KEXT0000  KEYIN     *P1:24,*EF,*DV,EXTRDSRC,*V2LON:
                    ANSEXTRA
.         
          MATCH     SP70,ANSEXTRA            * Test for spaces
          IF        @EQUAL
            PACK      ANSEXTRA,SP70
            DISPLAY   *P1:24,*EF,EXTRDSRC,*V2LON,"N"
            GOTO      KEXT9100
          ENDIF
.
          REP       "yYnN",ANSEXTRA
