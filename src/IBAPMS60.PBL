          MOVE      TMPAPGR2,TPAPGR2N
          MATCH     TPAPGR2N,TMPAPGRX
          IF        @EQUAL
            MOVE    "10",TPAPGR2N
          ENDIF
.
          PRINT     *001,"|",DDATE," ",TMPBTIME:
                    *021,"|",TMPCHURN:
                    *030,"|",CHNAME:
                    *057,"|",TMPCHSEX:
                    *061,"|",TPAPGR1N," ",TPAPGR2N:
                    *068,"|",TMPEGEST:
                    *073,"|",TMPWEIGH:
                    *080,"|",TMPMMURN:
                    *090,"|",MONAME:
                    *117,"|",TMPDLVTY:
                    *132,"|"
