          PACK      KEY16 WITH FROMDATE,SP8
          CALL      RDSDSCH2
LOOP1     CALL      RDKDSCH2
          BRANCH    OVRCD OF SORTD
.
          UNPACK    DDATE INTO XCENT,XYEAR,XMON,XDAY
          PACK      XDATE INTO XCENT,XYEAR,XMON,XDAY
          MOVE      XDATE,TODATE
.
          DISPLAY   *P46:24,XDAY,SLASH,XMON,SLASH,XYEAR,SP2,DADMNO;
.
          MATCH     XDATE,ENDDATE
          GOTO      SORTD IF LESS
.
