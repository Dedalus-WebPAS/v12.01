          IF        OVRCD=0
            MOVE      TDESC,UNITDESC
          ENDIF
          MATCH     SP70,ACLSSFT
          IF        @EQUAL
            MOVE      SP70,UNITDESC
          ENDIF
.
          PACK      KEY5,ANSC,ANSL,ACLAIM
          CALL      RDCODE1
          IF        OVRCD=0
