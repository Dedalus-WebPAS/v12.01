          WRITE     HTMLFILE;"<tr><td nowrap height=30>":
                             "<A HREF=",LINKPRG,".pbl":
                             "?reportno=",LINKREP:
                             "&template=",LINKTEM:
                             "&urnumber=",URNUMBER:
                             "&admissno=",ADMISSNO,">":
                             "<img src=../images/patweb97-1.gif border=0 ":
                             "hspace=4 align=absmiddle></a>":
                             "<A HREF=",RLINKPRG,".pbl":
                             "?reportno=",RLINKREP:
                             "&template=",RLINKTEM:
                             "&urnumber=",URNUMBER:
