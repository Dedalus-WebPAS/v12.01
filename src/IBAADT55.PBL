            ADD       ONE,CNT
            PACK      ELSTARR,ELSTARR,XELSTAR[CNT]
            IF        CNT < 5
              PACK      ELSTARR,ELSTARR,COMMA 
            ENDIF
          UNTIL       CNT = 5
