.            ENDIF
          UNTIL       CNT = 18
.
          CLEAR     NATOARR
          MOVE      ZERO,CNT
          REPEAT
            ADD       ONE,CNT
            PACK      NATOARR,NATOARR,XNATOSAR[CNT]
.            IF        CNT < 12 
