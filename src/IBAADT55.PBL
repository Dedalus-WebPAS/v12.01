          MOVE      ZERO,CNT
          REPEAT
            ADD       ONE,CNT
            PACK      PROCARR,PROCARR,XPROCAR[CNT]
            IF        CNT < 50
              PACK      PROCARR,PROCARR,COMMA
            ENDIF
          UNTIL       CNT = 50
.
          CLEAR     DIAGARR
          MOVE      ZERO,CNT
          REPEAT
            ADD       ONE,CNT
            PACK      DIAGARR,DIAGARR,XDIAGAR[CNT]
            IF        CNT < 100
              PACK      DIAGARR,DIAGARR,COMMA
            ENDIF
          UNTIL       CNT = 100
