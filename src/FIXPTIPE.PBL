          UNPACK    SP70,CCENT,CYEAR,CMON,CDAY
          CALL      KEYDATE
          BRANCH    OVRCD,KDATE900
.
          PACK      KEYNDATE,CCENT,CYEAR,CMON,CDAY
          REP       " 0",KEYNDATE
          GOTO      KDATE999
.
