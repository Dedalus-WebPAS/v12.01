PTOT0000  CALL      UND132
.
          PRINT     *1,"| Total Errors  : ",TOTALERR,*132,"|"
          PRINT     *1,"| Total Added   : ",TOTALADD,*132,"|"
          PRINT     *1,"| Total Updated : ",TOTALUPD,*132,"|"
.           
          CALL      UND132
