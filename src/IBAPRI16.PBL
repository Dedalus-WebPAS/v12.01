KPTH9500  DISPLAY   *P1:24,*EL,*B,"Upload File is Mandatory .  ";
          CALL      PHED0000
          PRINT     *N,"Upload File is Mandatory. ",FNAMEI
          CALL      HITENTER
          MOVE      ONE,EXIT
.
KPTH9999  RETURN
