.
          MOVE     SP30,IBGSDESC
          MOVE     "65",CCOL
          MOVE     "11",CVERT
          CALL     KIBGSA00
.
          PACK    KEY14,IBGSCODE,SP10
          CALL    RSIBGS1
.
          DISPLAY *P65:11,*EL,*V2LON,IBGSCODE:
          *P72:11,*HOFF,IBGSDESC
          MOVE    IBGSCODE,PRITGSTC
          MOVE    PRITGSTC,PRIVGSTC
          GOTO    GSTC9999
.
GSTC9999  RETURN
