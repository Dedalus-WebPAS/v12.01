.
          PRINT     *20,"Upload Private Practice MBS/AMA Items from Text File"
.
          PRINT     *20,"Upload File : ",PATHNAME
                    
          PRINT     *20,"Date        : ",CDATE," at ",CTIMEIS
.
          PACK      KEY10,WBUSERID,SP70
          CALL      RDWBSE1
          IF        OVRCD=1
            MOVE      WBUSERID,WBSENAM
          ENDIF
.
          PRINT     *20,"User Name   : ",WBSENAM
