          PACK      FRMTDATE,CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR
.
VEWCOM40  MATCH     "1",WTMHDELT
          IF        @EQUAL
            GOTO       VEWCOM10
          ENDIF
          MOVE       "",STRIKETG
          MOVE       "",STRENDTG
.
          REP       " +",CASEKEYS
          REP       " +",WTMHTYPE
          REP       " +",WTMHNOTE
.
          MOVE      CASEKEYS,DIM19
