                             "#"",WMDATE2,"#",":
                             "#"",WMDATE3,"#",":
                             "#"",WMCODE,"#",";
.                                            * Display Procedure Description
.                                            * comments only if they exist
          MATCH     SP70,WTWMPDSC
          STRIP     WMDESC
          MOVELPTR  WMDESC,F3
          SFORMAT   DISPVAR1,F3
          MOVE      WMDESC,DISPVAR1
