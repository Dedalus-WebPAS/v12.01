          APPEND    "Effective Date must be between ",WEBTITLE
          APPEND    DISPDAT1,WEBTITLE
          APPEND    " and ",WEBTITLE
          APPEND    DISPDAT2,WEBTITLE
          RESET     WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      CATHSS99
.
