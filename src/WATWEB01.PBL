            IF        !@EQUAL
              DAYSEP    BKRXCNDT,BKREEDAT,CALDAYS
              IF        CALDAYS < 7
                GOTO      BOKFLA90
              ENDIF
            ENDIF
          ENDIF
.
          MATCH     SP70,DOCTCODE
          IF        !@EQUAL
            MATCH     WMDOCTOR,DOCTCODE
            GOTO      BOKFLA90 IF NOT EQUAL
          ENDIF
          MATCH     SP70,UNITCODE
          IF        !@EQUAL|@EOS
            MATCH     UNITCODE,WMUNIT
            GOTO      BOKFLA90 IF NOT EQUAL 
          ENDIF
          MOVE      ZERO,EXIT
          GOTO      BOKFLA99
.
BOKFLA90  MOVE      ONE,EXIT
          GOTO      BOKFLA99
.
