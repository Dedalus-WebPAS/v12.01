.
          MOVE      ZERO,F1
          MATCH     SP70,XMLDATE1
          GOTO      XMLBOK85 IF EQUAL
.
          MATCH     SP70,XMLDATE2
          GOTO      XMLBOK85 IF EQUAL
.          
          DAYSEP    XMLDATE1,XMLDATE2,CALDAYS
          CALL      NRCDAY00
          ASSIGN    (CALDAYS-NRCDAYS),CALDAYS
