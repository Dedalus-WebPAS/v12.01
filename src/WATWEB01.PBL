          CALL      RKWTEPS1
          BRANCH    OVRCD,WATDWN90 
.
.  Set Nextpage and redirect
.
          MOVE      ".txt",KEY4
          CLEAR     KEY13
          APPEND    "extrc",KEY13
          APPEND    WATEX001,KEY13 
          APPEND    KEY4,KEY13   
