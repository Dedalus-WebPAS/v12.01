            CALL      IBACLOCK
            PACK      BKCFCDAT,CCC,CYY,CMM,CDD  * set created date
            REP       " 0",BKCFCDAT
            PACK      BKCFCTIM,CTIMEIS          * set created time
            CALL      WRBKCNF1         * Writes away the data
            CALL      UPVCM000
            MOVE      ZERO,EXIT
            GOTO      CNFBOK99
