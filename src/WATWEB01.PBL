                             "#"",PTELEB,"#",":               *23
                             "#"",PMPXMPNO,"#",":             *24
                             "#"",PMPXMBNO,"#",":             *25
                             "#"",PMPXMMNO,"#",":             *26
                             "#"",WTTXADMW,"#",":             *27
                             "#"",PSUBRB,"#",":               *28
                             "#"",WMNOTICE,"#",":             *29
                             "#"",NOTCDESC,"#",":             *30
                             "#"",PMPXFPNO,"#",":             *31
                             "#"",PMPXFBNO,"#",":             *32
