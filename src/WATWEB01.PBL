          UNPACK    DIM127,D1,LINKREP,DIM127
          UNPACK    DIM127,D1,LINKTEM,DIM127
          MOVE      ZERO,RECCOUNT
.
          MATCH     SP70,UNITCODE
          GOTO      DATTAB05 IF EQUAL
.
          MATCH     SP70,DOCTCODE
          GOTO      DATTAB05 IF EQUAL
.
          PACK      KEY9,DOCTCODE,UNITCODE,SP70
          CALL      RDPMDUN1
