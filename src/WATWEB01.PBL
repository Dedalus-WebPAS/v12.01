. Output Row Details
.------------------------------------------------------------
BOKROW00  MATCH     "       0",WMURNO
          IF        @EQUAL
            MOVE      WMURNO,KEY8           * visit number
            UNPACK    KEY8,KEY2,KEY6          * r5
            CALL      RDPRAM1
            MOVE      ZERO,PURNO
          ELSE
            PACK      KEY8,WMURNO           * r5
            UNPACK    KEY8,KEY2,KEY6          * r5
            CALL      RDMAST1
            CALL      RDPMPX21
          ENDIF
.
          CALL      GETSEX00
.
          PACK      AGEDATE,CCC,CYY,CMM,CDD
          REP       " 0",AGEDATE
          CALL      CALCAGE
.
          MOVE      "S ",CATEGORY
          MOVE      WTWMSRCR,CODE
          CALL      GETCOD00
          MOVE      TDESC,SRCRDESC
.
          MOVE      "WU",CATEGORY
          MOVE      WMUNIT,CODE
          CALL      GETCOD00
          MOVE      TDESC,UNITDESC
          MOVE      ACODE,UNITCOD1
.
          MOVE      "VL",CATEGORY
          MOVE      WTTXPLST,CODE
          CALL      GETCOD00
          MOVE      TDESC,PRIOSTAT
.
          MOVE      "TP",CATEGORY
          MOVE      WMPTY,CODE
          CALL      GETCOD00
          MOVE      TDESC,PRIODESC
