          GOTO      WATBOK97 IF EQUAL
          MATCH     Z70,BKREC004
          GOTO      WATBOK97 IF EQUAL
          MATCH     SP70,BKREC008
          GOTO      WATBOK98 IF EQUAL
          MATCH     Z70,BKREC008
          GOTO      WATBOK98 IF EQUAL
