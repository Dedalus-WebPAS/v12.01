          BRANCH    OVRCD,WSUS9999
.
          PACK      KEY21,WTSUURNO,WTSUCODE,WTSUCNT,SP70
          MATCH     KEY19,KEY21
          GOTO      WSUS9999 IF NOT EQUAL
.
          MATCH     SP70,WTSUTDAT
          GOTO      WSUS9999 IF EQUAL
.
          CALL      IBACLOCK
          PACK      TODAYDTE,CCC,CYY,CMM,CDD
          REP       " 0",TODAYDTE
          MATCH     WTSUTDAT,TODAYDTE
          GOTO      WSUS9999 IF NOT LESS
