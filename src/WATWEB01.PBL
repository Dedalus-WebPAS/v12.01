                             "#"",DISPSEX,"#",":              *3
                             "#"",PSAGEYRS,"#",":             *4
                             "#"",WMDATE1,"#",":              *5
                             "#"",WMDATE2,"#",":              *6
                             "#"",WMDATE3,"#",":              *7
                             "#"",WMCODE,"#",";               *8
