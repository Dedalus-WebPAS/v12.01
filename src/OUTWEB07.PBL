          APPEND    "Printing Complete. Invoice Number:",WEBTITLE
          APPEND    CNEXTINV,WEBTITLE
          APPEND    SP70,WEBTITLE
          RESET     WEBTITLE
.
          CALL      NXTPAG00
          GOTO      MAIN1000
.
