          MOVE      DISPOPTL,CPHDROPT              * printing heading field
          STRIP     DISPOPTL
          DISPLAY   *P53:2,*V2LON,*+,DISPOPTL,SP1;
          MOVE      FALSE,EXIT
          GOTO      OPTN9999
