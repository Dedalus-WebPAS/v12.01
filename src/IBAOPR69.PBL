.**********************************************************************
.*                             PRNT0000                               *
.*                Print records from the tempfile                     *
.**********************************************************************
PRNT0000  MOVE      ZERO,ENDFLAG
          COMPARE   "60",CLNO
          CALL      HEAD0000 IF NOT LESS
.
          MOVE      MON1,NUMBER
          ADD       MON2,NUMBER
          ADD       MON3,NUMBER
          ADD       MON4,NUMBER
          ADD       MON5,NUMBER
          ADD       MON6,NUMBER
          ADD       MON7,NUMBER
          ADD       MON8,NUMBER
          ADD       MON9,NUMBER
          ADD       MON10,NUMBER
          ADD       MON11,NUMBER
          ADD       MON12,NUMBER
          ADD       MON13,NUMBER
.
. --- print '--''s ----
.
PRNT0050  MATCH     XDOCT,Z6
          GOTO      PRNT5000 IF EQUAL
.
PRNT0070  MATCH     PRDOCT,XDOCT
          CALL      HEDD0000 IF NOT EQUAL
.
          MATCH     XTYPE,Z3
          GOTO      PRNT2000 IF NOT EQUAL
.
. ---- print '--''s ----
.
          PRINT     *1,DASH30;
          MOVE      ONE,COUNT
          MOVE      THIRTY1,CCOL
.
PRNT0100  PRINT     *CCOL,"------";
          ADD       SEVEN,CCOL
          ADD       ONE,COUNT
          COMPARE   COUNT,PERS                  * all pers yet ????
          GOTO      PRNT0100 IF NOT LESS 
          PRINT     *CCOL,"-------"             * total line
          ADD       ONE,CLNO
.
. ---- print line ----
.
          PRINT     *15,"       Total";
          MOVE      ONE,COUNT
          PRINT     *30;
.
PRNT0500  LOAD      MONVAR,COUNT,MON1,MON2,MON3,MON4:MON5,MON6,MON7,MON8:
                                 MON9,MON10,MON11,MON12,MON13
          PRINT     SP2,MONVAR;
          ADD       ONE,COUNT
.
          COMPARE   COUNT,PERS
          GOTO      PRNT0500 IF NOT LESS
          ADD       ONE,CLNO
          PRINT     SP2,NUMBER
.
