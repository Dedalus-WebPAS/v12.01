          MOVE      ONE,COUNT
          MOVE      THIRTY1,CCOL
.
PRNT0800  PRINT     *CCOL,"=======";
          ADD       SEVEN,CCOL
          ADD       ONE,COUNT
.
          COMPARE   COUNT,PERS                  * all pers yet ????
          GOTO      PRNT0800 IF NOT LESS 
          PRINT     *CCOL,"======="             * total line
          GOTO      PRNT9999
.
. ---- print normal line ----
.
