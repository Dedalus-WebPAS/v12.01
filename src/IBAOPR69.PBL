          CALL      RDCODE1
.
          PRINT     *5,TDESC,SP5;
          MOVE      ONE,COUNT
.
PRNT2100  LOAD      MONVAR,COUNT,MON1,MON2,MON3,MON4,MON5,MON6,MON7,MON8:
                                 MON9,MON10,MON11,MON12,MON13
          PRINT     SP2,MONVAR;
          ADD       ONE,COUNT
          COMPARE   COUNT,PERS
          GOTO      PRNT2100 IF NOT LESS
          ADD       ONE,CLNO
          PRINT     SP2,NUMBER
          ADD       "1",CLNO
          GOTO      PRNT9999
