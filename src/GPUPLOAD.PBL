          CALL      STLN0000                     * set up vars
          MOVE      CDATEIS,PMHLCDTE
          MOVE      CTIMEIS,PMHLCTIM
          MOVE      "GPUPLOAD  ",PMHLWEBC
          PACK      KEY20,PROVCODE,PROVPRAC,SP70
          CALL      WRPMHCL1                     * write record
          ADD       ONE,HCLCOUNT
          GOTO      WLND9500
