.******************************************************************************
.*                                  CHAC0000                                  *
.*                       Check if GP to be set to ACTIVE                      *
.******************************************************************************
CHAC0000  REP       UPPLOW,DIM10
.
          MATCH     "INACTIVE",DIM10
          GOTO      CHAC9000 IF EQUAL
.
          MATCH     "DECEASED",DIM10
          GOTO      CHAC9000 IF EQUAL
.
          MATCH     "RETIRED",DIM10
          GOTO      CHAC9000 IF EQUAL
.
          MOVE      ZERO,EXIT                    * active
          GOTO      CHAC9999
.
CHAC9000  MOVE      ONE,EXIT                     * inactive
CHAC9999  RETURN
