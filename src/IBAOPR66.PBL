            MOVE      ONE,COMFSTLN 
          ENDIF
.
          STRIP     OPDDDATA
.
          PACK      NEWTCOMM,TCOMLINE,SP1,OPDDDATA 
          RESET     NEWTCOMM
          MOVELPTR  NEWTCOMM,F3
          IF        F3 > 132
            GOTO      ACOMM400
          ENDIF
.
          MOVE      NEWTCOMM,TCOMLINE
          GOTO      ACOMM200
.
