          REP       " 0",DIM8
          PACK      KEY14,GLGSTC,DIM8,SP70
          CALL      RDIBGE1
          COMPARE   ZERO,OVRCD
          GOTO      GSTC0100 IF EQUAL
.
          CALL      RPIBGE1
          BRANCH    OVRCD,GSTC0200
.
          MATCH     GLGSTC,IBGECODE
          GOTO      GSTC0200 IF NOT EQUAL
.
