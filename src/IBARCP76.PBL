          MOVE      TAXAMNT,GLAMNT
.
. get BAS Code
.
          PACK      KEY6,GLGSTC,SP70
          CALL      RDIBGS1
          BRANCH    OVRCD,GSTC0200
.
          COMPARE   ZERO,IBGSACTV
          GOTO      GSTC0200 IF NOT EQUAL
.
          MOVE      IBGSBASC,SVBCBASC
.
          UNPACK    RCDTGSTA,GLSLEDG,GLSACCT
          PACK      GLACCT,GLSLEDG,GLSACCT
          GOTO      GSTC9999
.
GSTC0200  MOVE      ZERO,TAXAMNT
          GOTO      GSTC9999
