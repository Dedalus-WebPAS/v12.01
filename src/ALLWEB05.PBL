          WRITE     HTMLFILE;"t.addRow(#"",HTMLLINK,"#",":           * 0
                             "#"",ALHDSTAT,"#",":                    * 1
                             "#"",ALHDURNO,"#",":                    * 2
                             "#"",D16,"#",":                         * 3
                             "#"",ALHDRTYP,"#",":                    * 4
                             "#"",ALHDACTN,"#",":                    * 5
                             "#"",ALHDBNUM,"#",":                    * 6
                             "#"",ALHDMESI,"#",":                    * 7
                             "#"",ALHDVISN,"#",":                    * 8
                             "#"",FILESTAT,"#",":                    * 9
                             "#"",ALHDMTYP,"#",":                    * 10
                             "#"",DIM12,"#",":                       * 11
                             "#"",ALHDDEPT,"#",":                    * 12
                             "#"",TDESC,"#",":                       * 13
                             "#"",DIM8,"#",":                        * 14
                             "#"",HTMLLNK2,"#",":                    * 15
                             "#"",ALHDCONT,"#"":                     * 16
                             ");"
