...       ENDIF
.
          PACK      SAVKEY42,ALHDURNO,ALHDSTAT,ALHDVISN,ALHDCONT,ALHDDTTM:
                             ALHDRTYP,ALHDACTN,SP70
.
          CALL      DSED0000                   * Delete error records
