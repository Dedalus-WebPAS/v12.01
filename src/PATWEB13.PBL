          REP       " +",KEY3
          UNPACK    DIM127,D1,FLDPARA,DIM127
          MOVE      REPORTNO,F2
.
          REP       " +",STARTKEY
          REP       "'~",KEYWORDS
.
          WRITE     HTMLFILE;"patweb13.pbl":
                             "?reportno=",F2:
                             "&template=",FLDPARA:
                             "&startnum=",KEY3:
                             "&norecord=",KEY2:
                             "&startkey=",STARTKEY:
                             "&keywords=",KEYWORDS;
.
          REP       "+ ",STARTKEY
