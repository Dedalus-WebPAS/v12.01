          ENDIF
.
          MOVE      SP70,PREFSITE
          MATCH     SP70,ALREPSIT           * Preferred site
          IF        !@EQUAL
            PACK      KEY6,ALREPSIT,SP70
            CALL      RDSITA1
            IF        OVRCD=0
              MOVE      OSTDESC,PREFSITE
            ENDIF
          ENDIF
.
          PACK      KEY20,SP70
          MATCH     SP70,ALREPRTY
          IF        !@EQUAL
            PACK      KEY5,ANSP,ANSR,ALREPRTY
            CALL      RDCODE1
            IF        OVRCD = 0
              PACK      KEY20,TDESC
            ENDIF
          ENDIF
