          CALL      WAALRE00
.
          MATCH     "0",ALRESTAT
          IF        @EQUAL
            CALL      IBACLOCK
            PACK      ALREDACT,CCC,CYY,CMM,CDD
            REP       " 0",ALREDACT
          ENDIF
.
          MOVE      ONE,ALRESTAT          * Update status to active
