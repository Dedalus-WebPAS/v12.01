          CALL      RSALLNK1
URSK1010  CALL      RKALLNK1
          BRANCH    OVRCD,URSK2000
.
          MATCH     ALREVISN,ALLNVISN       * Linked bookings
          IF        @EQUAL
            PACK      SAVKEY16,ALLNVISN,ALLNLNKV,SP70
.
            CALL      DATK0000              * Get first booking date
.
            MOVE      SAVKEY16,KEY16
            CALL      RDALLNK1              * Re position on link records
            BRANCH    OVRCD,URSK2000
.
            GOTO      URSK9100
          ENDIF
