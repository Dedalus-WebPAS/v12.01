          CLEAR     WEBTITLE
          APPEND    "Error : Can't reinstate referral ",WEBTITLE
          APPEND    ALCNMDES,WEBTITLE
          APPEND    " Referral is Rejected.",WEBTITLE
          RESET     WEBTITLE
          RESET     ALCNMDES
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      REIS9999
