            MATCH     SP3,ALDEHOSP
            IF        !@EQUAL
              MOVE      ALDEHOSP,ALREHOSN   * Hospital code
            ENDIF                                             * end   *I-232086
          ENDIF
.
          MATCH     ALREF038,Z70
          IF        !@EQUAL
            MOVE      ALREF038,ALREHCP      * Responsible HCP
          ENDIF
.
          MATCH     ALREF087,Z70
          IF        !@EQUAL
