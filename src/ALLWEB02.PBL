          MATCH     SP70,INPDATE;
          GOTO      DSPDAT99 IF EQUAL
          GOTO      DSPDAT99 IF EOS
          UNPACK    INPDATE,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,F2
