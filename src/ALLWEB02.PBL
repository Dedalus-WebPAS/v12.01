          ENDIF
.
          MATCH     ALREF106,Z70
          IF        !@EQUAL
            MOVE      ALREF106,ALRECTYP     * Clinic Type 
          ENDIF
.
          MATCH     ALREF107,Z70
          IF        !@EQUAL
            MOVE      ALREF107,ALREHOSN     * Preferred Hospital
          ENDIF
.
          MATCH     ALREF117,Z70
          IF        !@EQUAL
            MOVE      ALREF117,ALRESTYP     * Slot Type
          ENDIF
.
          MATCH     ALREF119,Z70
          IF        !@EQUAL
            MOVE      ALREF119,ALREPSIT     * Preferred Site
          ENDIF
.
          MATCH     ALREF120,Z70
          IF        !@EQUAL
            MOVE      ALREF120,ALRECLID     * Clinic Id
          ENDIF
