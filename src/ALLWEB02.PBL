            MOVE      ALLRF022,ALRERHCP
            MOVE      ALLRF023,ALRERHCR
            MOVE      ALLRF024,ALRERHCT
            MOVE      ALLRF034,ALRESRCE
            MOVE      ALLRF035,ALRECOMP
            MOVE      ALLRF140,ALREPURC
            MOVE      ALLRF153,ALREORIG
            MOVE      PMSVX010,PMVXINGP
            MOVE      ALDAD001,ALDAFHOS
