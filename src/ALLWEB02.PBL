          IF        LOCKCNT<5
            DISPLAY   *W
            GOTO      INAREF10
          ENDIF
.
          MOVE      "Referral Record Locked.",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      INAREF99
.
