            MATCH     SP70,PMHCPRV1
            IF        @EQUAL
              MOVE      SP70,DISPPROV
            ELSE
              PACK      DISPPROV,LBRACK,PMHCPRV1,RBRACK
            ENDIF
          ENDIF
.
          PACK      KEY20,ALRERHCP,ALRERHCR,SP70
          CALL      RDPMHCL1
          IF        OVRCD=0
