                             "#"",FORMATNM,"#",":                  1
                             "#"",ALREURNO,"#",":                  2
                             "#"",TEMPDAT1,"#",":                  3
                             "#"",TEMPDAT2,"#",":                  4
                             "#"",LSTENDAT,"#",":                  5
                             "#"",PROBLEM,"#",";                   6
.
            MATCH     "1",DOCINDIC
            IF        @EQUAL
              CALL      DISHCS00
.             WRITE     HTMLFILE;"#"",DOCFNAME,"<br><font color=blue>",DOCFNAM3,"<br>",DOCFNAM4,"<br>",DOCFNAM5,"<br>",DOCFNAM6,"</font>#",";
            ELSE
              WRITE     HTMLFILE;"#"",DOCFNAME,"#",";              7
            ENDIF
