          CLEAR     WFLINE1
          APPEND    PSNAME,WFLINE1    * surname
.
SETKNX21  CMATCH    SP1,WFLINE1
          GOTO      SETKNX22 IF NOT EQUAL
          BUMP      WFLINE1,-1
          GOTO      SETKNX21
.
SETKNX22  RESET     WLINE1
          APPEND    SP1,WFLINE1
          APPEND    PTITL,WFLINE1     * title
          APPEND    SP40,WFLINE1
          RESET     WFLINE1,40
          LENSET    WFLINE1
          RESET     WFLINE1
