          PACK      WFLINE7A,SP30
          CLEAR     WFLINE7A
          APPEND    "TIME:",WFLINE7A
          APPEND    SP30,WFLINE7A
          LENSET    WFLINE7A
          RESET     WFLINE7A
.
          PACK      WFLINE8,SP40
          CLEAR     WFLINE8
          APPEND    "Presented: ",WFLINE8
          APPEND    VISITDAT,WFLINE8          * emr vis date
          RESET     WFLINE8,24
          APPEND    "ED:",WFLINE8
          APPEND    ADMISSNO,WFLINE8         * emr vis no.
          APPEND    SP40,WFLINE8
          LENSET    WFLINE8
