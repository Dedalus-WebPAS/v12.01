                           "#"",MHPFTDAT,"#",":                  5
                           "#"",MHPFRDNO,"#",":                  6
                           "#"",MHPFRDER,"#",":                  7
                           "#"",MHPFRDWN,"#",":                  8
                           "#"",MHPFLSNO,"#",":                  9
                           "#"",MHPFLSER,"#",":                  10
                           "#"",MHPFLSWN,"#");"            *     11
          GOTO      PMFL1000
