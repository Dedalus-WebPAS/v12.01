          ENDIF
.
          MOVE      "1",KEY1
          MOVE      " / ",DIM3A
          MATCH     SP3,MHPDECNT
          IF        @EQUAL
            MATCH     SP3,MHPDWCNT
            IF        @EQUAL
              MOVE      "0",KEY1
