.*********************************************************************
. LODT000 - Load temp file with clinics to process
.*********************************************************************
LODT0000  PACK      KEY25,SITECODE,VISFRDAT,SP70
          CALL      RDSSESA6
LODT1000  CALL      RDKSESA6
          BRANCH    OVRCD,LODT8000
.
          MATCH     SITECODE,OSESITE        * Site
          GOTO      LODT8000 IF NOT EQUAL
.
          MATCH     OSEDATE,VISTODAT        * Session date
          GOTO      LODT8000 IF LESS
.
          MATCH     SP70,CLINTYFR           * Clinic type from
          IF        !@EQUAL
            MATCH     CLINTYFR,OSESCTYP
            GOTO      LODT1000 IF LESS
          ENDIF
.
          MATCH     SP70,CLINTYTO           * Clinic type to
          IF        !@EQUAL
            MATCH     OSESCTYP,CLINTYTO
            GOTO      LODT1000 IF LESS
          ENDIF
.
          MOVE      OSESITE,TEMPSITE
          MOVE      OSECLIN,TEMPCLIN
          MOVE      OSEDATE,TEMPDATE
          MOVE      OSESTRT,TEMPSTRT
.
          MOVE      ZERO,TEMPUNBK
          MOVE      ZERO,TEMPEXTN
          MOVE      ZERO,TEMPEXTD
          MOVE      ZERO,TEMPUNAV
          MOVE      ZERO,TEMPUNAD
          MOVE      ZERO,TEMPUTIL
          MOVE      ZERO,TEMPNOTU
.
          PACK      KEY25,TEMPSITE,TEMPCLIN,TEMPDATE,TEMPSTRT,SP70
          CALL      RATEMP1
          IF        OVRCD=1
            CALL      WRTEMP1               * Clinic record
          ENDIF
.
          MOVE      OSESITE,TEMPSITE
          MOVE      OSECLIN,TEMPCLIN
          MOVE      Z70,TEMPDATE            * Clinic total
          MOVE      Z70,TEMPSTRT            * Clinic total
.
          MOVE      ZERO,TEMPUNBK
          MOVE      ZERO,TEMPEXTN
          MOVE      ZERO,TEMPEXTD
          MOVE      ZERO,TEMPUNAV
          MOVE      ZERO,TEMPUNAD
          MOVE      ZERO,TEMPUTIL
          MOVE      ZERO,TEMPNOTU
.
          PACK      KEY25,TEMPSITE,TEMPCLIN,TEMPDATE,TEMPSTRT,SP70
          CALL      RATEMP1
          IF        OVRCD=1
            CALL      WRTEMP1               * Clinic total
          ENDIF
.
          GOTO      LODT1000
.
LODT8000  MOVE      OSESITE,TEMPSITE
          MOVE      Z70,TEMPCLIN            * Grand total
          MOVE      Z70,TEMPDATE            * Grand total
          MOVE      Z70,TEMPSTRT            * Grand total
.
          MOVE      ZERO,TEMPUNBK
          MOVE      ZERO,TEMPEXTN
          MOVE      ZERO,TEMPEXTD
          MOVE      ZERO,TEMPUNAV
          MOVE      ZERO,TEMPUNAD
          MOVE      ZERO,TEMPUTIL
          MOVE      ZERO,TEMPNOTU
.
          PACK      KEY25,TEMPSITE,TEMPCLIN,TEMPDATE,TEMPSTRT,SP70
          CALL      RATEMP1
          IF        OVRCD=1
            CALL      WRTEMP1               * Grand total
          ENDIF
.
LODT9999  RETURN
+
.*********************************************************************
. SLOT000 - Get slot counts                
.*********************************************************************
SLOT0000  PACK      KEY25,SP70
          CALL      RSTEMP1
SLOT1000  CALL      RKTEMP1
          BRANCH    OVRCD,SLOT9999
.
          MATCH     Z70,TEMPCLIN            * Skip total record
          GOTO      SLOT1000 IF EQUAL
.
          MATCH     Z70,TEMPDATE            * Skip total record
          GOTO      SLOT1000 IF EQUAL
.
          PACK      SAVKEY25,TEMPSITE,TEMPCLIN,TEMPDATE,TEMPSTRT,SP70
.
. Loop all slots and update total counts
.
          MOVE      ZERO,PREVSLOT           * Previous slot number
          MOVE      ZERO,PREVSTAT           * Previous slot status
.
          PACK      KEY28,TEMPSITE,TEMPCLIN,TEMPDATE,TEMPSTRT,SP70
          CALL      RDSBOKA1
SLOT2000  CALL      RDKBOKA1                * next read on details file
          BRANCH    OVRCD,SLOT5000          * end of session
.
          MATCH     OBASITE,TEMPSITE
          GOTO      SLOT5000 IF NOT EQUAL
.
          MATCH     OBACLIN,TEMPCLIN
          GOTO      SLOT5000 IF NOT EQUAL
.
          MATCH     OBADATE,TEMPDATE
          GOTO      SLOT5000 IF NOT EQUAL
.
          MATCH     OBASTRT,TEMPSTRT
          GOTO      SLOT5000 IF NOT EQUAL
. 
          MATCH     SP70,VISTCODE
          IF        !@EQUAL
            MATCH     OBAVISIT,VISTCODE
            GOTO      SLOT3000 IF NOT EQUAL
          ENDIF
.
          IF        OBASTAT=0
            ADD       ONE,TEMPUNBK        * Number of slots with status
.                                         * 0 (Unbooked) to be deleted
          ENDIF
.
          IF        OBASTAT=3
            IF        PREVSLOT=OBAEXSL & PREVSTAT=0
              ADD       ONE,TEMPEXTN        * Number of slots with status
.                                           * 3 (Extended) to be deleted
              GOTO      SLOT4000
            ENDIF
            IF        PREVSLOT<>OBAEXSL
              ADD       ONE,TEMPEXTN        * Number of slots with status
.                                           * 3 (Extended) to be deleted
              GOTO      SLOT4000
            ENDIF
            ADD       ONE,TEMPEXTD        * Number of slots with status
.                                           * 3 (Extended) will not be deleted
          ENDIF
.
          IF        OBASTAT=7
            ADD       ONE,TEMPUNAV        * Number of slots with status
.                                         * 7 (Unavailable) to be deleted
          ENDIF
.
          IF        OBASTAT=1 | OBASTAT=4 | OBASTAT=5
            ADD       ONE,TEMPUTIL        * Number of utilised slots
.                                         * slots with a status 1, 4, or 5
          ENDIF
.
          IF        OBASTAT=0 | OBASTAT=2 | OBASTAT=6 | OBASTAT=7 | OBASTAT=9
            ADD       ONE,TEMPNOTU        * Number of slots not utilised
.                                         * slots with a status of 0,2,6,7 or 9
          ENDIF
.
          GOTO      SLOT4000
.
SLOT3000  IF        OBASTAT=3
            ADD       ONE,TEMPEXTD        * Number of slots with status
.                                         * 3 (Extended) will not be deleted
          ENDIF
.
          IF        OBASTAT=7
            ADD       ONE,TEMPUNAD        * Number of slots with status
.                                         * 7 (Unavailable) will not be deleted
          ENDIF
.
SLOT4000  MOVE      OBASLOT,PREVSLOT      * Previous slot number
          MOVE      OBASTAT,PREVSTAT      * Previous slot status
.
          GOTO      SLOT2000
.
SLOT5000  CALL      UPTEMP1
.
          MOVE      TEMPUNBK,SAVEUNBK     * Save counts for totals
          MOVE      TEMPEXTN,SAVEEXTN
          MOVE      TEMPEXTD,SAVEEXTD
          MOVE      TEMPUNAV,SAVEUNAV
          MOVE      TEMPUNAD,SAVEUNAD
          MOVE      TEMPUTIL,SAVEUTIL
          MOVE      TEMPNOTU,SAVENOTU
.
. Update Clinic total 
.
          MOVE      Z70,TEMPDATE            * Clinic total
          MOVE      Z70,TEMPSTRT            * Clinic total
.
          PACK      KEY25,TEMPSITE,TEMPCLIN,TEMPDATE,TEMPSTRT,SP70
          CALL      RDTEMP1
          BRANCH    OVRCD,SLOT6000
.
          ADD       SAVEUNBK,TEMPUNBK
          ADD       SAVEEXTN,TEMPEXTN
          ADD       SAVEEXTD,TEMPEXTD
          ADD       SAVEUNAV,TEMPUNAV
          ADD       SAVEUNAD,TEMPUNAD
          ADD       SAVEUTIL,TEMPUTIL
          ADD       SAVENOTU,TEMPNOTU
.
          CALL      UPTEMP1
.
. Update Grand total
.
SLOT6000  MOVE      Z70,TEMPCLIN            * Grand total
          MOVE      Z70,TEMPDATE            * Grand total
          MOVE      Z70,TEMPSTRT            * Grand total
.
          PACK      KEY25,TEMPSITE,TEMPCLIN,TEMPDATE,TEMPSTRT,SP70
          CALL      RDTEMP1
          BRANCH    OVRCD,SLOT7000
.
          ADD       SAVEUNBK,TEMPUNBK
          ADD       SAVEEXTN,TEMPEXTN
          ADD       SAVEEXTD,TEMPEXTD
          ADD       SAVEUNAV,TEMPUNAV
          ADD       SAVEUNAD,TEMPUNAD
          ADD       SAVEUTIL,TEMPUTIL
          ADD       SAVENOTU,TEMPNOTU
.
          CALL      UPTEMP1
.
SLOT7000  MOVE      SAVKEY25,KEY25
          CALL      RDTEMP1
          BRANCH    OVRCD,SLOT1000
.
          GOTO      SLOT1000
.
SLOT9999  RETURN
+
.*********************************************************************
. DELT000 - Delete slots  and update session slot counts
.*********************************************************************
DELT0000  MATCH     "Y",REPTONLY             * Run report only
          GOTO      DELT9999 IF EQUAL
.
          PACK      KEY25,SP70
          CALL      RDTEMP1
DELT1000  CALL      RKTEMP1
          BRANCH    OVRCD,DELT9999
.
          MATCH     Z70,TEMPCLIN            * Skip total record
          GOTO      DELT1000 IF EQUAL
.
          MATCH     Z70,TEMPDATE            * Skip total record
          GOTO      DELT1000 IF EQUAL
.
          MOVE      ZERO,PREVSLOT           * Previous slot number
          MOVE      ZERO,PREVSTAT           * Previous slot statu
.
          PACK      KEY28,TEMPSITE,TEMPCLIN,TEMPDATE,TEMPSTRT,SP70
          CALL      RDSBOKA1
DELT2000  CALL      RDKBOKA1                * next read on details file
          BRANCH    OVRCD,DELT1000          * end of session
.
          MATCH     OBASITE,TEMPSITE
          GOTO      DELT1000 IF NOT EQUAL
.
          MATCH     OBACLIN,TEMPCLIN
          GOTO      DELT1000 IF NOT EQUAL
.
          MATCH     OBADATE,TEMPDATE
          GOTO      DELT1000 IF NOT EQUAL
.
          MATCH     OBASTRT,TEMPSTRT
          GOTO      DELT1000 IF NOT EQUAL
.
          MATCH     SP70,VISTCODE
          IF        !@EQUAL
            MATCH     OBAVISIT,VISTCODE          * Visit type filer
            GOTO      DELT2000 IF NOT EQUAL
          ENDIF
.
. Empty, Unbooked Extended and Unavailable Slots
.
          MATCH     "1",SLOTTDEL   * Slot type to delete
          IF        @EQUAL
            IF        OBASTAT=0
              PACK      KEY28,OBASITE,OBACLIN,OBADATE,OBASTRT,OBASLOT,SP70
              CALL      DEBOKA1    * Delete Un Booked
            ENDIF
.
            IF        OBASTAT=3 & PREVSLOT=OBAEXSL & PREVSTAT=0
              PACK      KEY28,OBASITE,OBACLIN,OBADATE,OBASTRT,OBASLOT,SP70
              CALL      DEBOKA1    * Delete Un Booked Extended
            ENDIF
.
            IF        OBASTAT=7
              PACK      KEY28,OBASITE,OBACLIN,OBADATE,OBASTRT,OBASLOT,SP70
              CALL      DEBOKA1    * Delete unavailable
              CALL      DEOTBOX1   * Delete unavailable comment
            ENDIF
          ENDIF
.
. Empty Slots
.
          MATCH     "2",SLOTTDEL   * Slot type to delete
          IF        @EQUAL
            IF        OBASTAT=0
              PACK      KEY28,OBASITE,OBACLIN,OBADATE,OBASTRT,OBASLOT,SP70
              CALL      DEBOKA1    * Delete Un Booked
            ENDIF
.
            IF        OBASTAT=3 & PREVSLOT=OBAEXSL & PREVSTAT=0
              PACK      KEY28,OBASITE,OBACLIN,OBADATE,OBASTRT,OBASLOT,SP70
              CALL      DEBOKA1    * Delete Un Booked Extended
            ENDIF
          ENDIF
.
. Unbooked Extended Slots
.
          MATCH     "3",SLOTTDEL   * Slot type to delete
          IF        @EQUAL
            IF       OBASTAT=3 & PREVSLOT=OBAEXSL & PREVSTAT=0
              PACK      KEY28,OBASITE,OBACLIN,OBADATE,OBASTRT,OBASLOT,SP70
              CALL      DEBOKA1    * Delete Un Booked Extended
            ENDIF
.
            IF        OBASTAT=3 & PREVSLOT<>OBAEXSL
              PACK      KEY28,OBASITE,OBACLIN,OBADATE,OBASTRT,OBASLOT,SP70
              CALL      DEBOKA1    * Delete Un Booked Extended
            ENDIF
          ENDIF
.
. Unavailable Slots
.
          MATCH     "4",SLOTTDEL  * Unavailable Slots
          IF        @EQUAL
            IF        OBASTAT=7
              PACK      KEY28,OBASITE,OBACLIN,OBADATE,OBASTRT,OBASLOT,SP70
              CALL      DEBOKA1   * Delete unavailable
              CALL      DEOTBOX1   * Delete unavailable comment
            ENDIF
          ENDIF
.
DELT8000  MOVE      OBASLOT,PREVSLOT      * Previous slot number
          MOVE      OBASTAT,PREVSTAT      * Previous slot status
.
          GOTO      DELT2000
.
DELT9999  RETURN
+
.*********************************************************************
. SESS000 - Update session slot counts
.*********************************************************************
SESS0000  MATCH     "Y",REPTONLY   * Run report only 
          GOTO      SESS9999 IF EQUAL
.
          PACK      KEY25,SP70
          CALL      RDTEMP1
SESS1000  CALL      RKTEMP1
          BRANCH    OVRCD,SESS9999
.
          MATCH     Z70,TEMPCLIN            * Skip total record
          GOTO      SESS1000 IF EQUAL
.
          MATCH     Z70,TEMPDATE            * Skip total record
          GOTO      SESS1000 IF EQUAL
.
          PACK      KEY25,TEMPSITE,TEMPCLIN,TEMPDATE,TEMPSTRT,SP70
          CALL      RDSESA1
          BRANCH    OVRCD,SLOT1000
.
          MOVE      "00:00",OSETIMEU
          CALL      SLTCOUNT
.
          GOTO      SESS1000
.
SESS9999  RETURN
+
.*********************************************************************
. PRIN000 - Print the report                              
.*********************************************************************
PRIN0000  CALL      HEAD0000                * Print heading
.
          PACK      KEY25,SP70
          CALL      RSTEMP1
PRIN1000  CALL      RKTEMP1
          BRANCH    OVRCD,PRIN8000
.
          MOVE      ZERO,TOTALLIN           * Not a total line
.
          MATCH     Z70,TEMPCLIN
          IF        @EQUAL
            MOVE      SP70,TEMPCLIN
            MOVE      "Grand Total",DIM25
            MOVE      SP70,DIM11
            MOVE      SP70,TEMPSTRT
            MOVE      ONE,TOTALLIN          * Yes a total line
            GOTO      PRIN5000
          ENDIF
.
          MATCH     Z70,TEMPDATE
          IF        @EQUAL
            MOVE      SP70,TEMPCLIN
            MOVE      "Clinic Total",DIM25
            MOVE      SP70,DIM11
            MOVE      SP70,TEMPSTRT
            MOVE      ONE,TOTALLIN          * Yes a total line
            GOTO      PRIN5000
          ENDIF
.
          PACK      KEY20,TEMPSITE,TEMPCLIN,TEMPDATE,SP70
          CALL      RDCLIA1
          IF        OVRCD<>1
            MOVE      OCADESC,DIM25
            GOTO      PRIN2000
          ENDIF
.
          MOVE      OCACLIN,DIM25
          PACK      KEY20,TEMPSITE,TEMPCLIN,TEMPDATE,Z70
          CALL      RDSCLIA1
          CALL      RDPCLIA1                *To get Clinic ID Description
          BRANCH    OVRCD,PRIN2000
.
          MATCH     TEMPSITE,OCASITE
          GOTO      PRIN2000 IF NOT EQUAL
.
          MATCH     TEMPCLIN,OCACLIN
          GOTO      PRIN2000 IF NOT EQUAL
.
          MOVE      OCADESC,DIM25
.
PRIN2000  UNPACK    TEMPDATE,CCENT,CYEAR,CMON,CDAY
          CALL      PACDATE
          MOVE      CPCDATE,DIM11
.
          COMPARE   "55",CLNO                               * page full ?
          CALL      HEAD0000 IF NOT LESS                    * yes
.
PRIN5000  IF        TOTALLIN=1        
            CALL      UND132          * Yes a total line
            PRINT     *1,"| ",DIM25:
                      *62,"|",TEMPUNBK:
                      *72,"|",TEMPEXTN:
                      *82,"|",TEMPEXTD:
                      *92,"|",TEMPUNAV:
                      *102,"|",TEMPUNAD:
                      *112,"|",TEMPUTIL:
                      *122,"|",TEMPNOTU:
                      *132,"|"
            ADD       ONE,CLNO
            CALL      UND132          * Yes a total line
            GOTO      PRIN1000
          ENDIF
.
          PRINT     *1,"| ",TEMPCLIN,*10,"| ",DIM25:
                    *41,"| ",DIM11,*54,"| ",TEMPSTRT:
                    *62,"|",TEMPUNBK:
                    *72,"|",TEMPEXTN:
                    *82,"|",TEMPEXTD:
                    *92,"|",TEMPUNAV:
                    *102,"|",TEMPUNAD:
                    *112,"|",TEMPUTIL:
                    *122,"|",TEMPNOTU:
                    *132,"|"
          ADD       ONE,CLNO
.
          GOTO      PRIN1000
.
PRIN8000  PRINT     *35,"End of Report"
.
PRIN9999  RETURN
+
.*********************************************************************
. HEAD0000 - Print heading
.*********************************************************************
HEAD0000  CALL      HEAD132
          UNPACK    VISFRDAT,CCENT,CYEAR,CMON,CDAY
          CALL      PACDATE
          MOVE      CPCDATE,DISPFDAT
.
          UNPACK    VISTODAT,CCENT,CYEAR,CMON,CDAY
          CALL      PACDATE
          MOVE      CPCDATE,DISPTDAT
.
          STRIP     CLINTYFD
          PRINT     *35,"Site : ",SITEDESC           
          PRINT     *35,"Clinic Type From : ",*+,CLINTYFD," to ",CLINTYTD  
          PRINT     *35,"Slot Status : ",SLOTDDEL
          PRINT     *35,"Visit Type  : ",VISTDESC
          PRINT     *35,"Report Only : ",REPONLYD
          PRINT     *35,"Date From : ",DISPFDAT," to ",DISPTDAT
          ADD       SIX,CLNO
          CALL      UND132
.
          PRINT     *1,"| Clinic ",*10,"| Clinic Description":
                    *41,"| Date",*54,"| Time":
                    *62,"| Not":
                    *72,"| Ext.":
                    *82,"| Ext. No":
                    *92,"| Unavail.":
                    *102,"| Unavail.":
                    *112,"| Used":
                    *122,"| Not":
                    *132,"|"
          PRINT     *1,"|",*10,"|":
                    *41,"|",*54,"|":
                    *62,"| Booked":
                    *72,"| Delete":
                    *82,"| Delete":
                    *92,"| Delete":
                    *102,"|":
                    *112,"| ":
                    *122,"| Used":
                    *132,"|"
.
          ADD       TWO,CLNO
          CALL      UND132
.
HEAD9999  RETURN
.**************************************************************************
.* CREA0000 - Create a new temporary file                                 *
.**************************************************************************
.
CREA0000  CALL      KILL0000                     * remove existing file
.
          CALL      TFILENAM * Get temp file name in create
          MOVE      TFILNAME,TEMPFILE
.
          CLEAR     CMDLINE
          PACK      CMDLINE,ISBUILD,TEMPFILE,UKEY
          EXECUTE   CMDLINE,TASKID               * create temporary index file
.
          OPEN      REPTEMP1,TEMPFILE            * open temp index file
.
CREA9999  RETURN
+
.****************************************************************************
.* KILL0000 - Close and erase the temporary file                            *
.****************************************************************************
.
KILL0000  CLOSE     REPTEMP1                     * close file
.
          CLEAR     CMDLINE
          PACK      CMDLINE,ERASE,TEMPFILE       * set file erase command
          EXECUTE   CMDLINE,TASKID               * erase temp file
.
KILL9999 RETURN
+
.****************************************************************************
.*        IO ROUTINES FOR TEMPORARY FILE                                    *
.****************************************************************************
.
RSTEMP1   READ      REPTEMP1,KEY25;;
          RETURN
.
RKTEMP1   MOVE      ZERO,OVRCD
          READKS    REPTEMP1;TEMPSITE,TEMPCLIN,TEMPDATE,TEMPSTRT:
                             TEMPUNBK,TEMPEXTN,TEMPEXTD,TEMPUNAV:
                             TEMPUNAD,TEMPUTIL,TEMPNOTU
          GOTO      OVERCOND IF OVER
          RETURN
.
RATEMP1   MOVE      ZERO,OVRCD
          READ      REPTEMP1,KEY25;ANS
          GOTO      OVERCOND IF OVER
          RETURN
.
RDTEMP1   MOVE      ZERO,OVRCD
          READ      REPTEMP1,KEY25;TEMPSITE,TEMPCLIN,TEMPDATE,TEMPSTRT:
                             TEMPUNBK,TEMPEXTN,TEMPEXTD,TEMPUNAV:
                             TEMPUNAD,TEMPUTIL,TEMPNOTU
          GOTO      OVERCOND IF OVER
          RETURN
.
WRTEMP1   WRITE     REPTEMP1,KEY25;TEMPSITE,TEMPCLIN,TEMPDATE,TEMPSTRT:
                             TEMPUNBK,TEMPEXTN,TEMPEXTD,TEMPUNAV:
                             TEMPUNAD,TEMPUTIL,TEMPNOTU
          RETURN
.
UPTEMP1   UPDATE    REPTEMP1;TEMPSITE,TEMPCLIN,TEMPDATE,TEMPSTRT:
                             TEMPUNBK,TEMPEXTN,TEMPEXTD,TEMPUNAV:
                             TEMPUNAD,TEMPUTIL,TEMPNOTU
          RETURN
