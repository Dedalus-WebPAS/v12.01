          OPEN      RCPDTEA1,"rcpdteaf"
          IF        CFEETYP = 6
            OPEN      CONTROLF,"controlf"
            READ      CONTROLF,THIRTY6;*180,RCCNDPID,RCCNDPFU
          ENDIF
