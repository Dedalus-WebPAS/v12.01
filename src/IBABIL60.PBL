                    " 'To Be Invoiced' Section (Y/N) ? ":
                    *V2LON,ANS;
         REP        "yYnN",ANS
         MATCH      ANSN,ANS
         GOTO       BOPT9999 IF EQUAL
         MATCH      ANSY,ANS
         GOTO       BOPT9999 IF NOT EQUAL
.
         MOVE       ONE,INCLBOOK
.
