          MOVE      CCC,CCENT
          REP       " 0",CCENT
          MOVE      TEN6,CVERT
.
          CALL      KEYDATE                 * keyin the to date
          BRANCH    OVRCD,DATE0000
.
          PACK      TODATE,CCENT,CYEAR,CMON,CDAY
.
          MATCH     TODATE,CURDATE          * see if future date entered
          GOTO      DATE9000 IF LESS
