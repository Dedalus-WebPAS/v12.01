VURN0000  MOVE      PV1019,DIM8                  * get length of U/R number
          SQUEEZE   DIM8
          MOVELPTR  DIM8,FORM3
          IF        FORM3 = 0
            IF        DISPFLAG = 1
              DISPLAY   "PID.3 - U/R number not present"
            ENDIF
            MOVE      PIDSEGMT,ERRORSEG
            MOVE      "3",ERRORFLD
            MOVE      "U/R number not present",SAVERTXT
            CALL      EROR0000
            GOTO      VURN9999
          ENDIF
.
