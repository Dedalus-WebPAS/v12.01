. HEAD - print report                               Called by PRNT
.         Requires : FROMBAS,TOBAS   (print range display labels)
.                    FROMCODE,TOCODE (print range)
.********************************************************************
HEAD0000  CALL      HEAD80                      * print heading
.
          ADD       "3",CLNO
          IF        OPTIONA=1
            PRINT        *20,"Print by BAS Code":
                      *N,*20,"From : ",BASFROM:
                      *N,*20,"To   : ",BASEND
          ELSE
            PRINT        *20,"Print by BAS Description":
                      *N,*20,"From : ",DESCFROM:
                      *N,*20,"To   : ",DESCEND
          ENDIF
          ADD       THREE,CLNO
          CALL      UND80                       * print line
. BAS Code | Description                    | Active | BAS Code Type           |
.   123    | 123456789012345678901234567890 |  YES   | X - GST TYPE UNKNOWN    |
.
          PRINT     "| BAS Code",*12,"| Description",*45,"| Active":
                    *54,"| BAS Code Type",*80,"|"
          ADD       ONE,CLNO
          CALL      UND80                       * print line
.
HEAD9999  RETURN
.********************************************************************
. WRIT - print data line                            Called by PRNT
.********************************************************************
. BAS Code | Description                    | Active | BAS Code Type           |
.   123    | 123456789012345678901234567890 |  YES   | X - GST TYPE UNKNOWN    |
.
WRIT0000  MOVE      YES,YESNO
          LOAD      YESNO,IBBAACTV,NO
.
          MOVE      IBBATYPE,F1
          MOVE      GSTUNK,KEY16
          LOAD      KEY16,F1,GSTPAY,GSTREC
.
          PRINT     *1,"|   ",IBBABASC,*12,"| ",IBBADESC,*45,"|  ",YESNO:
                    *54,"| ",IBBATYPE," - ",KEY16,*80,"|"
          ADD       ONE,CLNO           * add one to line number
.
WRIT9999  RETURN
