.**********************************************************************
.  NREC - Get next record                              Called By PRNT
.**********************************************************************
NREC0000  BRANCH    OPTIONA,NREC0100,NREC0200
.
.---- print by type 1 ----
.
NREC0100  CALL      RKIBBA1
          BRANCH    OVRCD,NREC9500
.
          MATCH     IBBABASC,ENDBAS
          GOTO      NREC9500 IF LESS             * out of range ?
          GOTO      NREC9000
.
.---- print by type 2 ----
.
NREC0200  CALL      RKIBBA2
          BRANCH    OVRCD,NREC9500
.
          MATCH     Z70,ENDDESC
          MOVELPTR  IBBADESC,F3
          SETLPTR   IBBADESC,F2
          MATCH     IBBADESC,ENDDESC
          GOTO      NREC9500 IF LESS             * out of range ?
          RESET     IBBADESC,F3
          RESET     IBBADESC
          GOTO      NREC9000
.
NREC9000  MOVE      ZERO,OVRCD         * continue
          GOTO      NREC9999
.
NREC9500  MOVE      ONE,OVRCD          * quit
.
NREC9999  RETURN
