          MOVE      SP70,VAR
          CALL      DISPITEM
          GOTO      RBAS0000
.
RBAS0400  MOVE      "~~~",ENDBAS
          MOVE      "End                                 ",BASEND
.
RBAS9999  RETURN
. ---------------------------------------------------------
. SETSCR02 - set up for screen 02
. ---------------------------------------------------------
SETSCR02  MOVE      ONE,ACCEPT
          MOVE      TWO,OPTIONA
          RETURN
.----------------------------------------------------------------------
. Range of BAS Description
.----------------------------------------------------------------------
RBAD0000  MOVE      ZERO,EXIT
          MOVE      "    4",FLDID
          CALL      GETPOSIT
          MOVE      SP70,VAR
          CALL      DISPITEM
          KEYIN     *PCCOL:CVERT,*V2LON,FROMDESC
          ENDSET    FROMDESC
          APPEND    SP70,FROMDESC
          RESET     FROMDESC
          MATCH     EXITCHAR,FROMDESC
          GOTO      RBAD9500 IF EQUAL
.
          MATCH     SP70,FROMDESC
          GOTO      RBAD0100 IF EQUAL
          MOVE      FROMDESC,DESCFROM
          GOTO      RBAD0200
.
RBAD0100  MOVE      SP70,FROMDESC
          MOVE      "Start                           ",DESCFROM
.
RBAD0200  MOVE      DESCFROM,VAR
          CALL      DISPITEM
.
          MOVE      "    5",FLDID
          CALL      GETPOSIT
          MOVE      SP70,VAR
          CALL      DISPITEM
          KEYIN     *PCCOL:CVERT,*V2LON,ENDDESC
          ENDSET    ENDDESC
          APPEND    SP70,ENDDESC
          RESET     ENDDESC
          MATCH     EXITCHAR,ENDDESC
          GOTO      RBAD9500 IF EQUAL
.
          MATCH     SP70,ENDDESC
          GOTO      RBAD0300 IF EQUAL
          MOVE      ENDDESC,DESCEND
          MATCH     FROMDESC,ENDDESC
          GOTO      RBAD9999 IF NOT LESS
.
          BEEP
          MOVE      "To Description Must Be > From Description - ",DISPMSG
          CALL      MESSAGE1
          MOVE      "    5",FLDID
          CALL      GETPOSIT
          MOVE      SP70,VAR
          CALL      DISPITEM
          GOTO      RBAD0000
.
RBAD0300  MOVE      "~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~",ENDDESC
          MOVE      "End                                 ",DESCEND
          GOTO      RBAD9999
.
RBAD9500  MOVE      TWO,EXIT
.
RBAD9999
.         MOVE      "    4",FLDID
.         CALL      GETPOSIT
          RETURN
