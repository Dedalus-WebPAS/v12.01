.
          PACK      KEY8,URNUMBER,SP70
          CALL      RDMAST1
          IF        OVRCD=1
            CALL      CLPATMAS
            CALL      CLPMSPX2
            MOVE      ZERO,UNKFLAG
            MOVE      ZEROUR,URNUMBER
            MATCH     SP8,ADMISSNO
            IF        @EQUAL
              MOVE      EMVIADMN,ADMISSNO
            ENDIF
            GOTO      CHKUR990
          ENDIF
.
          MOVE      PURNO,KEY8
          CALL      RDPMPX21
          IF        OVRCD=1
