          GOTO      CKDT9999 IF EQUAL
.
          PACK      KEY16,EMVIDDAT,EMVIDTIM,SP70
          MATCH     SP70,KEY16
          GOTO      CKDT9999 IF EQUAL
          REP       ":0",KEY16
.
          MATCH     KEY16,SAVKEY16    * check discharge date/time
          IF        !@LESS
            GOTO      CKDT9100
          ENDIF
          GOTO      CKDT9999
