          MOVE      EMVCDATE,ICDRDDTE                                  *I-63036
          CALL      RDPTICO1                             * was RDICO1  *C-63036
          IF        OVRCD=1
            MOVE      "Invalid Procedure Code ",CODEDESC
            GOTO      LOKCOD99
          ENDIF
.
          MOVE      ODESC,CODEDESC
          GOTO      LOKCOD99
.
