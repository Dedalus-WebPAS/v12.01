.
          CALL      TIMEDIFF
.
          WRITE     HTMLFILE;CALCTIME;
.
          GOTO      EVCD9999
.
EVCD6100  CALL      VALEMP00
          GOTO      EVCD9999
.
EVCD9999  RETURN
