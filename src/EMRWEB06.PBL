          CALL      PATITMRD
          IF        OVRCD=1
            MOVE      "Invalid MBS Code",CODEDESC
            GOTO      LOKCOD99
          ENDIF
.
          MOVE      IDESC,CODEDESC
          GOTO      LOKCOD99
