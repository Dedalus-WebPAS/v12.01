+
.*****************************************************************************
.*                               POST0000                                    *
.*                   Post the BBSW JSON to Medicare                          *
.*****************************************************************************
POST0000  CLEAR     CMDLINE
          APPEND    WBB2DPTH,CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
.
          MATCH     "S",WBBSSTYP
          IF        @EQUAL
            APPEND    "/webPAS_PRODA_run PostBBSWSPE ",CMDLINE
            GOTO      POST1000
          ENDIF
.
          MATCH     "O",WBBSSTYP
          IF        @EQUAL
            APPEND    "/webPAS_PRODA_run PostBBSWGEN ",CMDLINE
            GOTO      POST1000
          ENDIF
.
          MOVE      "Invalid BBSW Type in webbbsaf.wbbsstyp",ERRORDSC
          GOTO      POST9100
.
POST1000  APPEND    WBB2DPTH,CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    "/ ",CMDLINE
          APPEND    WBBSTRID,CMDLINE
          RESET     CMDLINE
.
          EXECUTE   CMDLINE,TASKID
.
          MATCH     "0       ",TASKID
          IF        !@EQUAL
            MOVE      "Unable to execute webPAS_PRODA PostBBSW.",ERRORDSC
            GOTO      POST9100
          ENDIF
.
          MOVE      ZERO,EXIT
          GOTO      POST9999
.
POST9100  MOVE      "1",ERRORIND
          MOVE      ONE,EXIT
.
POST9999  RETURN
