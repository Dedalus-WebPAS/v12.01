          CALL      WEBINT00       * Initialise Fifo Etc.
.
          CALL      INIT0000       * Open Files
.
MAIN0000  COMMIT
.
          BEGIN
.
          DISPLAY   *W2
.
          MATCH     "1",PTCNWSIN
          GOTO      MAIN0000 IF NOT EQUAL
.
          MATCH     "1",PTCNBBSW
          GOTO      MAIN0000 IF NOT EQUAL
.
          MOVE      ZERO,EXIT
.
          PACK      KEY30,SP1,TWO,SP70
          CALL      RSWBBBS8
MAIN1000  CALL      RKWBBBS8
          BRANCH    OVRCD,MAIN9999
.
          COMPARE   TWO,WBBSFLAG
          GOTO      MAIN9999 IF NOT EQUAL
.
          PACK      KEY71,WBBSHOSP,SP70          * Read B2B Device Table
          CALL      RSWBB2B3
          CALL      RKWBB2B3
          BRANCH    OVRCD,MAIN1000
.
          MATCH     WBBSHOSP,WBB2HOSP            * Same Hospital
          GOTO      MAIN1000 IF NOT EQUAL
.
          MATCH     "ACTIVE",WBB2DSTA            * Device Active
          GOTO      MAIN1000 IF NOT EQUAL
.
          MATCH     SP70,WBB2DPTH
          GOTO      MAIN1000 IF EQUAL
.
          CALL      PROPN000                     * Valid Properties File?
.
          MATCH     "1",PRODAIND
          GOTO      MAIN1000 IF EQUAL
.
          PACK      SAVKEY30,WBBSFLAG,WBBSHOSP,WBBSMODL,WBBSUNIQ,WBBSINVN:
                             WBBSBATN,SP70
.
          OPEN      WBSPROPF,PRDAPATH
.
. ******************************************************************
. Validate and Extract BBSW files 
. [TransactionId].properties 
. [TransactionId]_bbsw
. ******************************************************************
.
          CALL      BBSW0000
.
. ******************************************************************
.
          PACK      KEY30,WBBSFLAG,WBBSHOSP,WBBSMODL,WBBSUNIQ,WBBSINVN:
                          WBBSBATN,SP70
          CALL      RDWBBBS8
.
          MATCH     "1",ERRORIND
          IF        @EQUAL
            MOVE      THREE,WBBSFLAG
          ELSE
            MOVE      FOUR,WBBSFLAG
          ENDIF
.
          CALL      IBACLOCK
          PACK      WBBSUDAT,CCC,CYY,CMM,CDD
          REP       " 0",WBBSUDAT
          MOVE      CTIMEIS,WBBSUTIM
          MOVE      USERID,WBBSUUID
          CALL      UPWBBBS8
.
.         Write a record to the claims audit/history table for this claim
.
MAIN2000  MATCH     "1",ERRORIND
          IF        @EQUAL
            MOVE      WBBSINVN,WBBAINVN
            CALL      IBACLOCK
            PACK      WBBADATE,CCC,CYY,CMM,CDD
            REP       " 0",WBBADATE
            MOVE      CTIMEIS,WBBATIME
            MOVE      WBBSBATN,WBBABATN
            MOVE      THREE,WBBASTAT
            MOVE      " 0",WBBATYPE
            MOVE      SP70,WBBAOPER
            MOVE      WBBSTRID,WBBATRID
            MOVE      SP70,WBBAEROR
            MOVE      ERRORDSC,WBBAEROT
            MOVE      WBBSMSID,WBBAMSID
            MOVE      WBBSMODL,WBBAMODL
            PACK      WBBASPAR,SP70,SP70
          ELSE
            MOVE      WBBSINVN,WBBAINVN
            CALL      IBACLOCK
            PACK      WBBADATE,CCC,CYY,CMM,CDD
            REP       " 0",WBBADATE
            MOVE      CTIMEIS,WBBATIME
            MOVE      WBBSBATN,WBBABATN
            MOVE      FOUR,WBBASTAT
            MOVE      " 0",WBBATYPE
            MOVE      SP70,WBBAOPER
            MOVE      WBBSTRID,WBBATRID
            MOVE      SP70,WBBAEROR
            PACK      WBBAEROT,SP70,SP70
            MOVE      WBBSMSID,WBBAMSID
            MOVE      WBBSMODL,WBBAMODL
            PACK      WBBAMSID,SP70,SP70
          ENDIF
.
          PACK      KEY27,WBBAINVN,WBBADATE,WBBATIME,WBBATYPE,WBBAMODL,SP70
          CALL      RAWBBBA1
          IF        OVRCD=0
            GOTO      MAIN2000
          ENDIF
.
          TRAP      OVERCOND IF IO
          CALL      WRWBBBA1
          TRAPCLR   IO
.
MAIN9000  CLOSE     WBSPROPF
.
          GOTO      MAIN1000
.
MAIN9999  CLOSE     WBSPROPF
.
          GOTO      MAIN0000
.
.------------------------------------------------------------
.  Open PRODA Text Files
.------------------------------------------------------------
PROPN000  MOVE      SP70,PRODAIND
.
          CLEAR     PRDAPATH
          APPEND    WBB2DPTH,PRDAPATH
          RESET     PRDAPATH
          STRIP     PRDAPATH
          ENDSET    PRDAPATH
          APPEND    SLASH,PRDAPATH
          APPEND    PRDAFILE,PRDAPATH
          RESET     PRDAPATH
.
          MOVE      ZERO,PRODAIND
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      WBSPROPF,PRDAPATH
          TRAPCLR   IO
          IF        OVRCD=1
            MOVE      ONE,PRODAIND
            GOTO      PROPN999
          ENDIF
          CLOSE     WBSPROPF
.
PROPN999  RETURN
