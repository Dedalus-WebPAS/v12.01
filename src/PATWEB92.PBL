            ENDIF
.
            WRITE     HTMLFILE;"#",#"",HTMLLNKL;
            WRITE     HTMLFILE;"#",#"",HTMLLNK3;
.
            CALL      PRILL000
            CALL      MBSC0000
.
.           IF        OECRFLAG<>1
