          APPEND    "<img src=../images/mv_",DIM200C
          APPEND    ICONFLAG,DIM200C
          APPEND    ".png>",DIM200C
          APPEND    "</a>",DIM200C
          RESET     DIM200C
.
          REP       "+ ",VENTVTYP
          REP       "+ ",VENTSDAT
          REP       "+ ",VENTSTIM
          REP       "+ ",ADMISSNO
          REP       "+ ",URNUMBER
          CALL      PATFLD00               *Returns KEY3 for folder colour code
.
          WRITE     HTMLFILE;*+,"t.addRow(#"",PATLINK,"#",":
                             "#"",URNUMBER,"#",":                      *1
                             "#"",ADMISSNO,"#",":                      *2
                             "#"",FORMATNM,"#",":                      *3
                             *+,"#"",WARDTEXT,"/",BEDTEXT,"#",":       *4
                             "#"",ADATE,"#",":                         *5
