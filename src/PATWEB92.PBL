                             "#"",STDATE,"#",":                * 4
                             "#"",OERDATE,"#",":               * 5
                             "#"",ADATE,"#",":                 * 6
                             "#"",PATLINK,"#",":               * 7
                             "#"",URNUMBER,"#",":              * 8
                             "#"",UNITDESC,"#",":              * 9
                             "#"",DAYSON,"#",":                * 10
                             "#"",PSEX,"#",":                  * 11
                             "#"",LEAVETYP,"#",":              * 12
                             "#"",LESTDESC,"#",":              * 13
                             "#"",KEY3,"#",":                  * 14
                             "#"",OERDATE,OERTIME,"#",":       * 15
                             "#"",XRETCLAS,"#",":              * 16
                             "#"",CNLVDAYS,"#",":              * 17
                             "#"",SVCNLVIN,"#",":              * 18
                             "#"",SVLVDYIN,"#");"              * 19
