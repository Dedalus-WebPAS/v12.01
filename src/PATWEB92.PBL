          ENDIF
.
          MATCH     "U",ACAUAUTY
          IF        @EQUAL
            MOVE      "Update",AUDITYPE
          ENDIF
.
          MATCH     "V",ACAUAUTY
          IF        @EQUAL
            MOVE      "View",AUDITYPE
          ENDIF
