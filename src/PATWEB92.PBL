            PACK      KEY5,KEY2,VENTVTYP,SP70
            CALL      RDCODE1
          ENDIF
          WRITE     HTMLFILE;"#"",TDESC,"#",";                   *8
.
          MOVE      SP70,TDESC
          MATCH     SP70,VENTISIT
