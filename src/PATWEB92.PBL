          ENDIF
.
          MATCH     "P",ACAUTREC
          IF        @EQUAL
            MOVE      "Form Printing",ACCTYPRE
          ENDIF
.
          MATCH     "R",ACAUTREC
          IF        @EQUAL
            MOVE      "Referral Details",ACCTYPRE
          ENDIF
.
          MATCH     "W",ACAUTREC
