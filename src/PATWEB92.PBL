                             "#"",VSIACDAT,VSIACTIM,"#",":
                             "#"",RECORDTY,"#",":                    * 5
                             "#"",AUORDATE,AUORTIME,"#",":
                             "#"",AUAMDATE,AUAMTIME,"#",";
