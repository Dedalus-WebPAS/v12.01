          MATCH     SP70,SVCNLVDT
          IF        @EQUAL|@EOS
            MOVE      DAYSON,CNLVDAYS
          ENDIF
.
          MOVE      SP70,SVLVDYIN
          COMPARE   DAYSON,SVLVDAYS
          IF        @LESS
            MOVE      "Red",SVLVDYIN
          ENDIF
.
          CALL      PATLN000                * Patent Name format  
.
