          IF        ASTAT=4
            CLEAR     ALRTIMG5
            APPEND    "<img class=Icon ",ALRTIMG5
            MATCH     WBSEHOSP,PMVXMHOS
            IF        @EQUAL
              APPEND    " alt='Current Inpatient On-Leave'",ALRTIMG5
