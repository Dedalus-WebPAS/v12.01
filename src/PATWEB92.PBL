.
            WRITE     HTMLFILE;"#",#"",DOCFNAME;
.
            IF        ELCHFLAG=ZERO
              WRITE     HTMLFILE;"#",#"<input type=checkbox name=oechk",DRECCNT:
                                 " disabled>";
            ELSE
              IF        ELCHFLAG=ONE
