          ENDIF
.
          MATCH     "C",VSIARTYP
          IF        @EQUAL
            MOVE      "After",RECORDTY
          ENDIF
.
          MATCH     "D",VSIARTYP
          IF        @EQUAL
            MOVE      "Delete",RECORDTY
          ENDIF
.
          WRITE     HTMLFILE;"t.addRow(#"",PATLINK,"#",":            * 0
