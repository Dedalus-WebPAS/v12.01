          CALL      IBACLOCK
          PACK      WBDADATE,CCC,CYY,CMM,CDD
          REP       " 0",WBDADATE
          MOVE      CTIMEIS,WBDATIME
          MOVE      SAVBTHN,WBDABATN
          MOVE      TWO,WBDASTAT
          MOVE      " 0",WBDATYPE
          MOVE      WBDBOPER,WBDAOPER
          MOVE      SP30,WBDATRID
          MOVE      SP10,WBDAEROR
          PACK      WBDAEROT,SP70,SP30
          MOVE      WBDWMODL,WBDAMODL
          MOVE      SP70,WBDASPAR
          CALL      WRWBDVA1
.
.         Check if this claim was the result of a resubmission
.
          MATCH     SP8,WBDWPBAT                 * blank previous batch ?
          GOTO      PROC9999 IF EQUAL            * yes - not a resubmission
.
.         Update the next batch number in the original source claim for this
.         resubmission
.
          MOVE      WBDWPBAT,SAVPRVBN            * save previous batch number
          PACK      KEY30,WBDWHOSP,SP1,EIGHT,WBDWMODL,WBDWUNIQ,WBDWINVN,SAVPRVBN
          CALL      RDWBDVW1
          BRANCH    OVRCD,PROC9999
.
          MOVE      SAVBTHN,WBDWNBAT
.
          CALL      UPWBDVW1
.
PROC9999  RETURN
