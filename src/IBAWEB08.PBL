.*****************************************************************************
.*  CHKDVA00
.*  Procedure to check if a patient has a DVA concession card
.*****************************************************************************
CHKDVA00  PACK      KEY19,PURNO,SP70
          CALL      RSPMCCD1           * Read concession card table
CHKDVA10  CALL      RKPMCCD1
          BRANCH    OVRCD,CHKDVA90
.
          MATCH     PURNO,PMCDURNO     * Check same UR
          GOTO      CHKDVA90 IF NOT EQUAL
.
          MATCH     SP70,PMCDEXDT
          IF        !@EQUAL & !@EOS
            MATCH     CURRDATE,PMCDEXDT     * check if exp. date is >= current date
            IF        @LESS
              GOTO      CHKDVA10
            ENDIF
          ENDIF
.
          MATCH     SP70,PMCDDVAC
          GOTO      CHKDVA10 IF EQUAL
.
.         PACK      KEY5,ANSD,ANSX,PMCDDVAC,SP70
.         CALL      RDCODE1
.         BRANCH    OVRCD,CHKDVA10
.
.         MATCH     ANSG,THCSCOD
.         GOTO      CHKDVA10 IF NOT EQUAL
.
          MOVE      PMCDCNUM,VCHSEG21
.
CHKDVA90  
.
.          MATCH     "P",CLMSEG05
.          IF        !@EQUAL
            MATCH     SP70,VCHSEG21
            IF        @EQUAL | @EOS
              MOVE      "VCH.7.4-veteranNumber not populated",ERRMSG
              MOVE      SP9,DIM9
              CALL      PERR0000
            ENDIF
.          ENDIF
.
CHKDVA99  RETURN
