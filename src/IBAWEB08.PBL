            PACK      ERRMSG,SEGMTYPE,DOT,SEGFIELD
            ENDSET    ERRMSG
            IF        PROVFLAG = 1
              APPEND    "-Hospital provider no. (",ERRMSG
            ELSE
              APPEND    FIELDESC,ERRMSG
            ENDIF
            APPEND    DIM8,ERRMSG
            APPEND    ") ",ERRMSG
            APPEND    "check digit is not valid",ERRMSG
            RESET     ERRMSG
            MOVE      SP9,DIM9
            CALL      PERR0000
            GOTO      VPRN9100
          ENDIF
.
          MOVE      ZERO,EXIT
          GOTO      VPRN9999
.
VPRN9100  MOVE      ONE,EXIT
.
VPRN9999  RETURN
