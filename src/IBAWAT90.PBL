          GOTO      CALC9999
.
CALC2222  MOVE      WMBOOK,KEY8        * get adm date for those discharge or
          CALL      RDMISS1            * removed
          BRANCH    OVRCD,CALC1111
          DAYSEP    WMDATE1,ADATE,CALDAYS
          GOTO      CALC9999
.
CALC3333  DAYSEP    WMDATE1,WMDATE4,CALDAYS
.
CALC9999  RETURN
