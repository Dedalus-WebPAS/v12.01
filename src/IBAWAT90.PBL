          PACK      KEY5,CODETP,WTCACODT
          CALL      RDCODE1
          BRANCH    OVRCD,NPTP1000
          MATCH     ANSP,TCINDC2                 * is it a Pending priority ?
          GOTO      NPTP1000 IF NOT EQUAL        * no - read next record
