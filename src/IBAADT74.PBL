          IF        PASSNUMB = 2
            WRITE     EXTFILE1,SEQ;XXHDHEAD,XXHDINVC,XXHDDATE,XXHDTIME:
                                   XXHDTAXT,XXHDREFN,XXHDHNAM,XXHDHABN:
