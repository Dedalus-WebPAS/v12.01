          IF        @EQUAL
            MOVE      SIIXQOH,SIIXREQ
            ASSIGN    SIIXMAX-SIIXREQ,SIIXQOH
          ELSE
            ASSIGN    SIIXMAX-SIIXQOH,SIIXREQ
          ENDIF
          IF        SIIXREQ<0
            MOVE      ZERO,SIIXREQ
          ENDIF
.
          MOVE      SIIXQOH,VAR
          CALL      DISPITEM
.
          MOVE      "   12",FLDID
          CALL      GETMLPOS
