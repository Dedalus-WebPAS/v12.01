NEXTPR00  MOVE      NORECORD,KEY2
          REP       " +",KEY2
          ASSIGN    STARTNUM+NORECORD,F3
          MOVE      F3,KEY3
          REP       " +",KEY3
          UNPACK    DIM127,D1,FLDPARA,DIM127
          MOVE      REPORTNO,F1
.
          REP       " +",STARTKEY
          REP       " +",FILTACTV
          REP       " +",FILTHOSP
.
          WRITE     HTMLFILE;"mrtweb01.pbl":
                                 "?reportno=",F1:
                                 "&template=",FLDPARA:
                                 "&startnum=",KEY3:
                                 "&norecord=",KEY2:
                                 "&startkey=",STARTKEY:
                                 "&filtactv=",FILTACTV:
                                 "&filthosp=",FILTHOSP:
                                 "&superlev=",SUPERLEV;
.
          REP       "+ ",STARTKEY
          REP       "+ ",FILTACTV
          REP       "+ ",FILTHOSP
.
NEXTPR99  RETURN
