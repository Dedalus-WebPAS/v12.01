            CALL      CLPMSPX2
          ENDIF
.
          IF        PTCNNHII=1
            MOVE      MRMAURNO,KEY8
            CALL      RDNHMAS2
            BRANCH    OVRCD,BLAB1000
          ENDIF
.
          MOVE      MRMAURNO,CPPURNO
