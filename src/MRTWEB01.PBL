          CALL      RSMRLOC1
LOCSEL10  CALL      RKMRLOC1
          BRANCH    OVRCD,LOCSEL99
.
          COMPARE   MRLOSTAT,ONE
          GOTO      LOCSEL10 IF EQUAL
.         
          IF        IBCNMHOS=1
            MATCH     SP70,FILTHOSP
            IF        !@EQUAL
              MATCH     FILTHOSP,MRLOHOSP
              GOTO      LOCSEL10 IF NOT EQUAL
