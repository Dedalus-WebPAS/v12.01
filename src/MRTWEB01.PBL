              ENDIF
            ENDIF
          ENDIF
.
          MATCH     FILTDEST,MRLOCODE    * Default Medical Records Location
          IF        @EQUAL
            WRITE     HTMLFILE;"<option value=#"",MRLOCODE,"#" selected>":
                                 MRLODESC,"</option>"
          ELSE
....        IF        IBCNMHOS=1
....          MATCH     WBSEHOSP,MRLOHOSP
....          GOTO      LOCSEL10 IF NOT EQUAL
....        ENDIF
            WRITE     HTMLFILE;"<option value=#"",MRLOCODE,"#">":
                                 MRLODESC,"</option>"
          ENDIF
          GOTO    LOCSEL10
.
