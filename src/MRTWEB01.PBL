          CLEAR     ERRORLST[ERRORCNT]
          APPEND    "Invalid MR Key - ",ERRORLST[ERRORCNT]
          APPEND    KEY17,ERRORLST[ERRORCNT]
          RESET     ERRORLST[ERRORCNT]
          GOTO      SAVTRC10
