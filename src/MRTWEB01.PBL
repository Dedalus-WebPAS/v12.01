          IF        ARRCNT>MRKEYEND
            GOTO      BLAB9999
          ENDIF
.
          MATCH     SP70,MRRECKEY[ARRCNT]
          GOTO      BLAB1000 IF EQUAL
.
          UNPACK    MRRECKEY[ARRCNT],KEY10,KEY28
.
