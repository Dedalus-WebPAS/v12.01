          IF        STARTNUM>=RECNUMB
            GOTO      MRTTB100
          ENDIF
.
          CALL      MRTRW000    * Display Locations Mantenance Table Details Row
          ADD       ONE,DISNUMB
          COMPARE   NORECORD,DISNUMB
          GOTO      MRTTB100 IF NOT EQUAL
          GOTO      MRTTB999
.
