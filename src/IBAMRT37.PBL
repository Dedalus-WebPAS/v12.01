                    *P1:13,"Current Location To   : ";
.
          MATCH     SP7,STRTLOCN                     * if spaces entered 
          GOTO      DISP4000 IF NOT EQUAL            * default to 'start'
          DISPLAY   *P25:12,*V2LON,CODEDES1;
