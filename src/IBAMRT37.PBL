                    *P1:4,"Home Location : ",*V2LON,HOMELOCN,*HOFF:
                    *P35:4,HOMEDESC:
                    *P1:6,*V2LON,"0 ",*HOFF,"= Exit":
                    *P1:7,*V2LON,"1 ",*HOFF,"= By Current Location":
                    *P1:8,*V2LON,"2 ",*HOFF,"= By Current Receiver":
                    *P1:10,"Please Select : ",*V2LON,OPTION,*HOFF;
