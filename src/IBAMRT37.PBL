                       *N,*N,*40,"DATE RANGE FROM : ",OPENDATE," TO ",CLOSDATE:
                       *N,*N,*40,"CODE RANGE FROM : ",CODEDES1,SP5,DESCRPT1:
                       *N,*51,"TO   : ",CODEDES2,SP5,DESCRPT2;
