. ***********************************************************
. NIHCErrorMessage
. ***********************************************************
.
          SCAN      "#"NIHCErrorMessage#"",NHCRLINE
          IF        @EQUAL
            RESET     NHCRLINE
.
            SCAN      ":",NHCRLINE
            IF        @EQUAL
              MOVE      NHCRLINE,NHCRWORK
              REP       ": ",NHCRWORK
              REP       "#" ",NHCRWORK
              LJUSTIFY  NHCRWORK
              MOVE      NHCRWORK,JSONERRM
              RESET     NHCRLINE
            ENDIF
.
            REP       "#" ",JSONERRM
            REP       ", ",JSONERRM
          ENDIF
