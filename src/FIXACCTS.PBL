          LOAD      LNO,LNO,STARTSCR,STARTSCR,STARTSCR:
                            STARTSCR,STARTSCR:
                            LNO,LNO,LNO,LNO,LNO:
                            LNO,LNO,LNO,LNO,LNO:
                            LNO,LNO,LNO,LNO,LNO:
                            SIXTY,SIXTY,SIXTY,SIXTY,SIXTY,SIXTY,SIXTY
          COMPARE   SIXTY,LNO
          CALL      CLRS0000 IF NOT LESS
.
