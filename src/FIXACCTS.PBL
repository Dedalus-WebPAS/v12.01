          GOTO      SELK0100
.
.---- option 1 ----
.
SELK1100  CALL      KAPMAA00                * get data
          BRANCH    EXIT,SELK1190,SELK9500  * entry ok ? (1=blank, 2=EXITCHAR)
.
SELK1190  MOVE      APMACRD,CREDFROM
          LOAD      CREDFROM,EXIT,SP70
          DISPLAY   *PCCOL:CVERT,*V2LON,CREDFROM;
          GOTO      SELK0100
.
