              PACK      KEY18,OPRTR001,HOSPCODE,SP70
              CALL      RDOPTRA1
              COMPARE   ZERO,OVRCD
              GOTO      OPRTRA92 IF EQUAL
            ENDIF
          ELSE
            MOVE      SP70,HOSPCODE
          ENDIF
.
