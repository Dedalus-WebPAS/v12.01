          PACK      SAVKEY33,OPTDCODE,OPTDITEM,OPTDHOSP,SP70
.
          PACK      KEY33,OPTDCODE,OPTDITEM,HOSPCODE,SP70
          CALL      RDOPTRB1
          IF        OVRCD = 1
