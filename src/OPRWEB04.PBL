          BRANCH    OVRCD,SPTNKA10,SPTNKA90
.
          ADD       ONE,RECNUMB
          IF        STARTNUM>=RECNUMB
            GOTO      SPTNKA10
          ENDIF
.
          MATCH     SP70,OPNRSNAM
          IF        @EQUAL
            MOVE      "&nbsp;",OPNRSNAM
          ENDIF
.
