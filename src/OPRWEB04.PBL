                                 "&startnum=",KEY3:
                                 "&norecord=",KEY2:
                                 "&oprtb001=",OPTHCODE:
                                 "&hospcode=",HOSPCODE;
.
          REP       "+ ",OPTHCODE
          REP       "+ ",HOSPCODE
.
NEXTTB99  RETURN
.
.-----------------------------------------------------------
. Link to Prev Group Codes           
.-----------------------------------------------------------
PREVPR00  MOVE      NORECORD,KEY2
          REP       " +",KEY2
          ASSIGN    STARTNUM-NORECORD,F3
          IF        F3<0
            MOVE      ZERO,F3
          ENDIF
          MOVE      F3,KEY3
          REP       " +",KEY3
          UNPACK    DIM127,D1,FLDPARA,DIM127
          MOVE      REPORTNO,F2
          REP       " +",OPRMROOM
          REP       " +",STARTKEY
          REP       " +",PREVIKEY
