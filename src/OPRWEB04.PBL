          MOVE      SP70,ITEMDESC
.
          MOVE      OPDFTYPE,DIM1
          REP       "1~2~",DIM1
.
          MATCH     "~",DIM1
          IF        @EQUAL
.
            MATCH     "2",OPDFTYPE
            IF        @EQUAL
              PACK      KEY18,OPDFITEM,OPDFHOSP,SP70
              CALL      RDOPEQP1
              IF        OVRCD=0
                MOVE      OPEQDESC,ITEMDESC
              ENDIF
              GOTO      PRBF0550
