.-----------------------------------------------------------
. Link to Prev Group Tray Codes           
.-----------------------------------------------------------
PREVTB00  MOVE      NORECORD,KEY2
          REP       " +",KEY2
          ASSIGN    STARTNUM-NORECORD,F3
          IF        F3<0
            MOVE      ZERO,F3
          ENDIF
          MOVE      F3,KEY3
          REP       " +",KEY3
          UNPACK    DIM127,D1,FLDPARA,DIM127
          MOVE      REPORTNO,F1
          REP       " +",OPTHCODE
          REP       " +",HOSPCODE
.
