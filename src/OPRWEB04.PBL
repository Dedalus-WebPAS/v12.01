          ENDIF
.
          BRANCH    LNK2PRT,SPTNKA20,SPTNKA30
.
          WRITE     HTMLFILE;"<tr valign=top>":
                              "<td><a href=",LINKPRG,".pbl?":
                               "&reportno=",LINKREP:
                               "&template=",LINKTEM:
                               "&itemkeys=",KEY9,">":
