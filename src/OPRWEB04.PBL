                                 "&norecord=",KEY2:
                                 "&prvcflag=",ONE:
                                 "&previkey=",PREVIKEY:
                                 "&startkey=",STARTKEY:
                                 "&oprua001=",OPRMROOM:
                                 "&opror002=",OPRMROOM:
                                 "&actvfilt=",ACTVFILT:
                                 "&hospcode=",HOSPCODE;
.
          REP       "+ ",OPRMROOM
          REP       "+ ",STARTKEY
          REP       "+ ",PREVIKEY
