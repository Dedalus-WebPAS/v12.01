             ENDIF
             CALL      PATMCHRD
.
              IF        EXIT=ZERO
                MOVE      MDESC,ITEMDESC
                GOTO      PRBF0550
              ENDIF
.
            GOTO      PRBF0520
.
            ENDIF
.
