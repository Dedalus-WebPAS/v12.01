                                 "&norecord=",KEY2:
                                 "&prvcflag=",ONE:
                                 "&nextikey=",NEXTIKEY:
                                 "&startkey=",STARTKEY:
                                 "&oprua001=",OPRMROOM:
                                 "&opror002=",OPRMROOM:
                                 "&actvfilt=",ACTVFILT:
                                 "&hospcode=",HOSPCODE;
.
          REP       "+ ",OPRMROOM
          REP       "+ ",STARTKEY
          REP       "+ ",NEXTIKEY
.
