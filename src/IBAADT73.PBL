.                                           * write "IP" recd to TempIP table
            ADD       ONE,ITEMCNT
            PACK      XXIPKEY,PINVNO,XXIPSTDT,XXIPENDT,ITEMCNT,SP30
            PACK      KEY29,XXIPKEY,SP30
            CALL      WRTMPIP1
          ENDIF
          ADD       ONE,ITMCOUNT
. 
          GOTO      WTIP1000
.
WTIP9999  RETURN
