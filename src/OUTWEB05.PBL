            MOVE      OMADAY,SCHDLDAY 
          ELSE
            MATCH     SCHDLDAY,DOMADAY    * Check if Day if Same
            IF        @EQUAL
              MOVE      "&nbsp;",DAYDESC
            ELSE
              LOAD      DAYDESC,OMADAY,MON,TUE,WED,THU,FRI,SAT,SUN
              MOVE      OMADAY,SCHDLDAY 
            ENDIF
          ENDIF
.
          PACK      KEY12,WBSESIT,OMATYP     * Get Clinic Type
          CALL      RDCTYA1
          IF        OVRCD=1
            MOVE      "&nbsp;",KEY30 
          ELSE
