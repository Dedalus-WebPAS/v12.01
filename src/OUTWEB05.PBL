          WRITE     HTMLFILE;"<tr valign=top>":
                             "<td><img src=../images/MaintenanceIcon.gif ":
                             " class=ListIcon onclick=#"UpdateParent('":
                             OCACLIN,"','",KEY30,"');#">":
                             OCACLIN,"</td>":
                             "<td>",KEY30,"</td><td>",DOCTOR,"</td>":
                             "<td>",KEY3,"</td>":
                             "</tr>"
. 
