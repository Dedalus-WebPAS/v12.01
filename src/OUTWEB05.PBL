          IF        @EQUAL
            MOVE      "No ",KEY3
          ELSE
            MOVE      "Yes",KEY3
          ENDIF
.
          MATCH     SP70,OCADOCT
          IF        @EQUAL 
            MOVE      "&nbsp;",DOCTOR
            GOTO      CIROW010 
          ENDIF   
.
          MOVE      OCADOCT,KEY10
          CALL      RDPMHCP1
          IF        OVRCD=1
            MOVE      "&nbsp;",DOCTOR
          ELSE 
            MOVE      PMHCTITL,FMTTITLE
            MOVE      PMHCGNAM,FMTGNAME
            MOVE      PMHCSNAM,FMTSNAME
            CALL      DOCNM000
            CLEAR     DOCTOR 
            APPEND    DOCFNAME,DOCTOR
          ENDIF
.
