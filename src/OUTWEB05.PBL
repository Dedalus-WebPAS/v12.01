                             "&srchclid=",SRCHCLID:
                             "&frstdate=",PREFDATE:
                             "&lastdate=",PRELDATE:
                             "&viewtype=",VIEWTYPE:
                             "&srchclty=",SRCHCLTY:
                             "&srchdowk=",SRCHDOWK:
                             "&otcli001=",OTCLI001:
                             "&otcli006=",OTCLI006:
                             "&otmas001=",OTMAS001:
                             "&otmas002=",OTMAS002:
                             "&mandtyid=",MANDTYID:
                             "&srchltyp=",SRCHLTYP;
          REP       "+ ",CLINICID
.
