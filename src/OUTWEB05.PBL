          ENDIF
          MOVE      ZERO,RECNUMB
          MOVE      ZERO,DISNUMB
.
          PACK      KEY12,WBSESIT,STARTKEY,SP70
          CALL      RDSCTYA1
          IF        OVRCD=0
            CALL      RDPCTYA1
          ENDIF
