MAIN0000  COMMIT
.
          BEGIN
.
          DISPLAY   *W2
.
          MATCH     "1",PTCNWSIN
          GOTO      MAIN0000 IF NOT EQUAL
.
          MATCH     "1",PTCNDVAW
          GOTO      MAIN0000 IF NOT EQUAL
.
          MOVE      ZERO,EXIT
.
          PACK      KEY51,ZERO,TWENTY,SP70
          CALL      RSWBETR1
MAIN1000  CALL      RKWBETR1
          BRANCH    OVRCD,MAIN9999
.
          MATCH     "0",WBETSTAT
          GOTO      MAIN9999 IF NOT EQUAL
.
          MATCH     "20",WBETTYPE
          GOTO      MAIN9999 IF NOT EQUAL
.
          PACK      KEY17,WBETINVN,WBETBATN,WBETMODL,SP70
          CALL      RDWBDVW2
          BRANCH    OVRCD,MAIN1000
