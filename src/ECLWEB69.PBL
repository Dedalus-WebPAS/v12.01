+
.------------------------------------------------------------------
. Get the Report Key from the DVRW JSON Response "status" "claimId"
. And reposition again at "claimAssessment"
.------------------------------------------------------------------
RKEY0000  READ      DVRRFILE,SEQ;DVRRLINE
          GOTO      RKEY5000 IF OVER
.
.         *************************************************
.         status
.         *************************************************
.
          SCAN      "status",DVRRLINE
          IF        @EQUAL
            RESET     DVRRLINE
.           UNPACK    DVRRLINE,DIM13,WBDDSTAT
.UPTOHERE
            SCAN      ":",DVRRLINE
            IF        @EQUAL
              MOVE      DVRRLINE,DVRRWORK
              REP       ": ",DVRRWORK
              REP       "#" ",DVRRWORK
              LJUSTIFY  DVRRWORK
              MOVE      DVRRWORK,WBDDSTAT
              RESET     DVRRLINE
            ENDIF
.
            REP       "#" ",WBDDSTAT
            REP       ", ",WBDDSTAT
          ENDIF
.
.         *************************************************
.         claimId
.         *************************************************
.
          SCAN      "claimId",DVRRLINE
          IF        @EQUAL
            RESET     DVRRLINE
.           UNPACK    DVRRLINE,DIM16,WBDDCLID
.
            SCAN      ":",DVRRLINE
            IF        @EQUAL
              MOVE      DVRRLINE,DVRRWORK
              REP       ": ",DVRRWORK
              REP       "#" ",DVRRWORK
              LJUSTIFY  DVRRWORK
              MOVE      DVRRWORK,WBDDCLID
              RESET     DVRRLINE
            ENDIF
.
            REP       "#" ",WBDDCLID
            REP       ", ",WBDDCLID
          ENDIF
.
          GOTO      RKEY0000
.
.         close and reopen dvrw response json file and read from the start
.         ----------------------------------------------------------------
.
RKEY5000  CLOSE     DVRRFILE
.
          CLEAR     CMDLINE
          APPEND    WBB2DPTH,CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    "/webPAS_claims/dvrw/",CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    WBETSTRD,CMDLINE
          APPEND    DVRRLITR,CMDLINE
          RESET     CMDLINE
.
          OPEN      DVRRFILE,CMDLINE
.
.         reposition at "claimAssessment" line
.         ----------------------------
.
RKEY6000  READ      DVRRFILE,SEQ;DVRRLINE
          GOTO      RKEY9999 IF OVER
.
          SCAN      "claimAssessment",DVRRLINE
          GOTO      RKEY9999 IF EQUAL
.
          GOTO      RKEY6000
.
RKEY9999  RETURN
