            MOVE      "E",WBETSTAT
.
            CALL      IBACLOCK
            PACK      WBETUDTE,CCC,CYY,CMM,CDD
            REP       " 0",WBETUDTE
            MOVE      CTIMEIS,WBETUTIM
            MOVE      USERID,WBETUUID
            CALL      UPWBETR1
.
            GOTO      MAIN1000
.
          ENDIF
