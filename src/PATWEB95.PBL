SELP0000  MATCH     "1",VIEWTYPE            * Week
          GOTO      SELP2000 IF EQUAL
          MATCH     "2",VIEWTYPE            * Month
          GOTO      SELP4000 IF EQUAL
          MATCH     "3",VIEWTYPE            * Weekly Mon - Fri
          GOTO      SELP2000 IF EQUAL
          MATCH     "4",VIEWTYPE
          IF        @EQUAL
            CALL      WEEK0000
            GOTO      SELP9999
          ENDIF
