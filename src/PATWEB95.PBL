          GOTO      HERA9999 IF EQUAL
.
          MATCH     SP70,HEROLOCI
          GOTO      HERA9999 IF EQUAL
.
          PACK      KEY7,ONE,PTWRD001,PTWRD002,SP70
          CALL      RDPMUHL1
          IF        OVRCD=1
            MOVE      ONE,PMULTYPE
            PACK      PMULWDCD,PTWRD001,SP70
            PACK      PMULBDCD,PTWRD002,SP70
            PACK      PMULUHID,HEROLOCI,SP70
            PACK      PMULSPAR,SP70
            CALL      WRPMUHL1
          ENDIF
.
HERA9999  RETURN
.
.------------------------------------------------------------------------------
. Update HERO location id record for ward/bed to PMSUHLFD
.------------------------------------------------------------------------------
HERU0000  MATCH     Z70,HEROLOCI
          GOTO      HERU9999 IF EQUAL
.
          MATCH     SP70,HEROLOCI
          IF        @EQUAL
            PACK      KEY7,ONE,PTWRD001,PTWRD002,SP70
            CALL      DEPMUHL1
            GOTO      HERU9999
          ENDIF
.
          PACK      KEY7,ONE,PTWRD001,PTWRD002,SP70
          CALL      RDPMUHL1
          IF        OVRCD=1
            MOVE      ONE,PMULTYPE
            PACK      PMULWDCD,PTWRD001,SP70
            PACK      PMULBDCD,PTWRD002,SP70
            PACK      PMULUHID,HEROLOCI,SP70
            PACK      PMULSPAR,SP70
            CALL      WRPMUHL1
          ELSE
            PACK      PMULUHID,HEROLOCI,SP70
            CALL      UPPMUHL1
          ENDIF
.
HERU9999  RETURN
