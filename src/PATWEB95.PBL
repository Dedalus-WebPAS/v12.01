          BRANCH    OVRCD,TABC9000
          MATCH     SP70,WBED
          GOTO      TABC9000 IF NOT EQUAL
.
          IF        CWRDSCN=1
            MATCH     SP70,WARDCODE
            IF        !@EQUAL
              MATCH     WARDCODE,WWARD        * show only supplied ward in scan
              GOTO      TABC9000 IF NOT EQUAL
            ENDIF
          ENDIF
.
          COMPARE   ONE,WACTIVE
          GOTO      TABC0100 IF EQUAL
.
          IF        IBCNMHOS=1
