                               "<td>",KEY3,"</td>":
                               "<td>",KEY4,"</td>":
                             "</tr>"
          GOTO      WRDRW999
.
WRDRW100  IF        DISNUMB>0
            WRITE     HTMLFILE;",";
          ENDIF
          CLEAR     KEY16
          APPEND    "Ward-",KEY16
          APPEND    WWARD,KEY16
          APPEND    SP70,KEY16
          RESET     KEY16
          WRITE     HTMLFILE;*+,"{ #"fullUrl#" : #"%BASEURL/Location/",KEY16,"#", ":
                                " #"resource#" : ";
.
          CALL      RESWRD00
.
          WRITE     HTMLFILE;*+," } "
.
          IF       FHRLOCIN=1
.
            MATCH     SP70,PTWDHOSN
            IF        !@EQUAL & !@EOS
.
              PACK      KEY3,PTWDHOSN,SP70
              CALL      RDPTHSP1
              IF        OVRCD=0
.
                WRITE     HTMLFILE;*+,",{ #"fullUrl#" : #"%BASEURL/Location/Hospital-",PTWDHOSN,"#", ":
                                      " #"resource#" : ";
.
                CALL      RESLOC00
.
                WRITE     HTMLFILE;*+," } "
.
              ENDIF
.
            ENDIF
.
          ENDIF
.
WRDRW999  RETURN
