.**********************************************************************
.*                         PROC0000                                   *
.*                    By clinic id and medical practice               *
.**********************************************************************
PROC0000  MOVE      ZERO,LOOPFLAG
          MOVE      ONE,SORTFLAG          * sort by clinic id
          MATCH     SP70,CLINFROM
          GOTO      PROC2000 IF NOT EQUAL
.
          MOVE      THREE,SORTFLAG       * sort by provider
          MATCH     SP70,FILTPROV
          GOTO      PROC1000 IF NOT EQUAL * sort by clinic id if all blank
.
          MOVE      ONE,SORTFLAG          * default sort by clinic id
          MATCH     SP70,PRACTIFR
          GOTO      PROC0100 IF EQUAL
          MOVE      TWO,SORTFLAG          * sort by medical practice
.
.******************************************************************************
.         Position read from 'On Hold' status
.
PROC0100  MOVE      " 2",HCCLSTAT
          PACK      KEY18,HCCLSTAT,FROMDATE,SP70
          CALL      RSHCCLM8
PROC0200  CALL      RKHCCLM8
          BRANCH    OVRCD,PROC9999
.
          MATCH     " 2",HCCLSTAT         * Status on hold?
          GOTO      PROC9999 IF NOT EQUAL
.
          MATCH     SP70,TODATE
          IF        !@EQUAL
            MATCH     HCCLDATE,TODATE 
            GOTO      PROC9999 IF LESS
          ENDIF
.
          MATCH     SP70,FILTPROV
          IF        !@EQUAL
            MATCH     HCCLPSRV,FILTPROV
            GOTO      PROC0200 IF NOT EQUAL
          ENDIF
.
          MATCH     IDDD,HCCLSITE         * Same site?
          GOTO      PROC0200 IF NOT EQUAL
.
          GOTO      PROC1800              * go to check clinic id and med.prac.
.
.******************************************************************************
.         By Provider number
.
PROC1000  MOVE      ONE,LOOPFLAG
          PACK      KEY26,FILTPROV,FROMDATE,SP70
          CALL      RSHCCLM4
PROC1200  CALL      RKHCCLM4
          BRANCH    OVRCD,PROC9999
.
          MATCH     FILTPROV,HCCLPSRV      * Same provider
          GOTO      PROC9999 IF NOT EQUAL
.
          MATCH     SP70,TODATE
          IF        !@EQUAL
            MATCH     HCCLDATE,TODATE 
            GOTO      PROC9999 IF LESS
          ENDIF
.
          MATCH     " 2",HCCLSTAT           * check status
          GOTO      PROC1200 IF NOT EQUAL   * not on hold
.
          MATCH     IDDD,HCCLSITE           * Same site?
          GOTO      PROC1200 IF NOT EQUAL
.
.         Check range of clinic id
.
PROC1800  MATCH     SP70,CLINFROM
          IF        !@EQUAL
            MATCH     CLINFROM,HCCLCLID    * clinic id within range?
            GOTO      PROC9000 IF LESS
          ENDIF
.
          MATCH     SP70,CLINICTO
          IF        !@EQUAL
            MATCH     HCCLCLID,CLINICTO    * clinic id within range?
            GOTO      PROC9000 IF LESS
          ENDIF
          GOTO      PROC4000
.
.******************************************************************************
.         By clinic id
.
PROC2000  MOVE      TWO,LOOPFLAG
          PACK      KEY28,IDDD,CLINFROM,FROMDATE,SP70
          CALL      RSHCCLM6
PROC2200  CALL      RKHCCLM6
          BRANCH    OVRCD,PROC9999
.
          MATCH     IDDD,HCCLSITE       * Same site?
          GOTO      PROC9999 IF NOT EQUAL
.
          MATCH     SP70,CLINICTO
          IF        !@EQUAL
            MATCH     HCCLCLID,CLINICTO    * clinic id within range?
            GOTO      PROC9999 IF LESS
          ENDIF
.
          MATCH     SP70,FROMDATE
          IF        !@EQUAL
            MATCH     FROMDATE,HCCLDATE 
            GOTO      PROC2200 IF LESS
          ENDIF
.
          MATCH     SP70,TODATE
          IF        !@EQUAL
            MATCH     HCCLDATE,TODATE 
            GOTO      PROC2200 IF LESS
          ENDIF
.
          MATCH     " 2",HCCLSTAT           * check status
          GOTO      PROC2200 IF NOT EQUAL   * not on hold
.
.         Check for provider number
.
          MATCH     SP70,FILTPROV
          IF        !@EQUAL
            MATCH     HCCLPSRV,FILTPROV
            GOTO      PROC2200 IF NOT EQUAL
          ENDIF
.
.         Check for range of medical practice
.
PROC4000  MATCH     SP70,PRACTIFR
          IF        !@EQUAL
            MATCH     PRACTIFR,HCCLMPRA
            GOTO      PROC9000 IF LESS
          ENDIF
.
          MATCH     SP70,PRACTITO
          IF        !@EQUAL
            MATCH     HCCLMPRA,PRACTITO
            GOTO      PROC9000 IF LESS
          ENDIF
.
          BRANCH    ALACCESS,PROC6000       * Access to all practices 
.
          PACK      KEY16,HCCLMPRA,WEBUSRID,SP70
          CALL      RDPRSEC1
          BRANCH    OVRCD,PROC9000          * No access to this practice
.
PROC6000  CALL      WTMP0000                * write to tempfile
.
PROC9000  BRANCH    LOOPFLAG,PROC1200,PROC2200
          GOTO      PROC0200
.
PROC9999  RETURN
+
.**********************************************************************
.*                         WTMP0000                                   *
.*                        Write to tempfile                           *
.**********************************************************************
WTMP0000  MOVE      HCCLCLID,TEMPCLID
          MOVE      HCCLMPRA,TEMPMPRA
          MOVE      HCCLPSRV,TEMPPSRV
          MOVE      HCCLBTCH,TEMPBTCH
          MOVE      HCCLPMCI,TEMPPMCI
          MOVE      HCCLPYEE,TEMPPYEE
          MOVE      HCCLCLMN,TEMPCLMN
.
          PACK      KEY13,TEMPBTCH,TEMPCLMN,SP70
          CALL      RATEMPA
          IF        OVRCD=1
            CALL      WRTEMPA
          ENDIF
.
WTMP9999  RETURN
.
.**********************************************************************
.*                         PRIN0000                                   *
.*                        Print Data                                  *
.**********************************************************************
PRIN0000  MOVE      ZERO,CPAGENO
          MOVE      ZERO,TOTCLMN
          MOVE      SP70,SAVEBTCH
          MOVE      SP70,SAVEVAR
          CALL      RSTEMPA
PRIN1000  CALL      RKTEMPA
          BRANCH    OVRCD,PRIN9000
.
          MATCH     SP70,SAVEVAR
          GOTO      PRIN1200 IF EQUAL            * first record
          CALL      CVAR0000                     * check if sort field changed
          BRANCH    EXIT,PRIN1100                * changed
.
          GOTO      PRIN1300
.
PRIN1100  CALL      UND132
          PRINT     *69,"| TOTAL",*80,"|",TOTCLMN,*94,"|":
                    *N,*69,"*----------",*80,"*-------------*"
          ADD       TWO,CLNO
          MOVE      ZERO,TOTCLMN
          MOVE      ONE,CNOUNDLN                 * no underline
.
PRIN1200  CALL      HEAD0000
          MOVE      ZERO,CNOUNDLN
          CALL      SVAR0000                     * save sort variable
.
PRIN1300  MOVE      TEMPCLMN,KEY8
          CALL      RDHCCLM1
          BRANCH    OVRCD,PRIN1000               * Invalid claim number
.
          MOVE      SP70,REFPROV
          MOVE      SP70,ALRERDAT
          PACK      KEY16,HCCLVISN,SP70
          CALL      RSALLNK2
          CALL      RKALLNK2                * get referral link
          BRANCH    OVRCD,PRIN1600
.
          MATCH     HCCLVISN,ALLNLNKV
          GOTO      PRIN1600 IF NOT EQUAL   * same visit?
.
          PACK      KEY8,ALLNVISN,SP70
          CALL      RDALREF1                * get referral record
          BRANCH    OVRCD,PRIN1600
          CALL      PROV0000              * Get referring provider
.
PRIN1600  MOVE      HCCLURNO,KEY8
          CALL      RDMAST1
          IF        OVRCD=0
            MOVE      PSNAME,PACSNAME
            MOVE      PGNAME,PACGNAME
            MOVE      PTITL,PACTITLE
            MOVE      ONE,PACFRMT
            CALL      PACNAME
            MOVE      PACFNAME,KEY23
          ENDIF
.
          PACK      KEY18,HCCLCLMN,SP70
          CALL      RSHCCIT1
PRIN2000  CALL      RKHCCIT1
          BRANCH    OVRCD,PRIN1000
.
          MATCH     HCCLCLMN,HCCICLMN
          GOTO      PRIN1000 IF NOT EQUAL        * Different claim number
.
          COMPARE   "48",CLNO
          CALL      HEAD0000 IF NOT LESS
.
          MATCH     SP70,HCCIIDAT               * service date
          IF        !@EQUAL
            UNPACK    HCCIIDAT,CCENT,CYEAR,CMON,CDAY
            CALL      PACDATE
          ENDIF
.
          MOVE      HCCICAMT,FORM102
          PRINT     *1,"|",HCCLCLMN,*10,"|",HCCLURNO:
                    *21,"|",KEY23,*46,"|",PMEDI:
                    *58,"|",CPCDATE:
                    *69,"|",HCCIITMN,*80,"|",FORM102;
.
          UNPACK    ALRERDAT,CCENT,CYEAR,CMON,CDAY * referral date
          CALL      PACDATE
          REP       " 0",CPDATE
.
          CALL      GETCMN00              * Get claim comment
          PRINT     *94,"|",REFPROV,*106,"|",CPCDATE:
                    *117,"|",KEY13,*132,"|"
          ADD       HCCICAMT,TOTCLMN              * total claim amount
.
          ADD       ONE,CLNO
          MOVE      ZERO,HEADFLAG
          GOTO      PRIN2000              * read next record
.
PRIN9000  COMPARE   ZERO,CPAGENO
          GOTO      PRIN9200 IF NOT EQUAL
          CALL      HEAD0000
          GOTO      PRIN9999
.
PRIN9200  CALL      UND132                   * finish off report
          PRINT     *69,"| TOTAL",*80,"|",TOTCLMN,*94,"|":
                    *N,*69,"*----------",*80,"*-------------*"
.
PRIN9999  RETURN
+
.*********************************************************************
.         Get claim comments
.*********************************************************************
GETCMN00  MOVE      SP70,KEY13
          PACK      KEY14,HCCIVISN,SP70
          CALL      RSVSCMT1
GETCMN10  CALL      RKVSCMT1
          BRANCH    OVRCD,GETCMN99
          MATCH     HCCIVISN,VSCTVIST     * Same visit number
          GOTO      GETCMN99 IF NOT EQUAL
          PACK      KEY13,VSCTDATA
GETCMN99  RETURN
.
.*********************************************************************
.         Get the referring provider from either the referring HCP/Practice
.         (if not blank), or the free format referring HCP field.
.*********************************************************************
PROV0000  MOVE      ZERO,EXIT
          MATCH     SP10,ALRERHCP                * referring HCP blank ?
          IF        !@EQUAL
            MATCH     SP10,ALRERHCR              * no - ref. HCP Practice blank?
            IF        !@EQUAL
              PACK      KEY20,ALRERHCP,ALRERHCR  * no
              CALL      RDPMHCL1                 * HCP link record found ?
              BRANCH    OVRCD,PROV5000           * no - get free format HCP
.
              MATCH     SP10,PMHLPRV1
              IF        !@EQUAL
                MOVE      PMHLPRV1,REFPROV 
                GOTO      PROV9999
              ENDIF
.
              MATCH     SP10,PMHLPRV2
              IF        !@EQUAL
                MOVE      PMHLPRV2,REFPROV
                GOTO      PROV9999
              ENDIF
.
              MATCH     SP10,PMHLPRV3
              IF        !@EQUAL
                MOVE      PMHLPRV3,REFPROV
                GOTO      PROV9999
              ENDIF
.
              MATCH     SP10,PMHLPRV4
              IF        !@EQUAL
                MOVE      PMHLPRV4,REFPROV
                GOTO      PROV9999
              ENDIF
.
              MATCH     SP10,PMHLPRV5
              IF        !@EQUAL
                MOVE      PMHLPRV5,REFPROV
                GOTO      PROV9999
              ENDIF
              GOTO      PROV8000                 * provider not found
            ENDIF
          ENDIF
.
PROV5000  MATCH     SP70,ALREHCPP
          IF        !@EQUAL
            MOVE      ALREHCPP,REFPROV
            GOTO      PROV9999
          ENDIF
PROV8000  MOVE      ONE,EXIT
PROV9999  RETURN
.
.*********************************************************************
.         Save sort variable
.*********************************************************************
SVAR0000  MOVE      SP70,SAVEVAR
          COMPARE   ONE,SORTFLAG
          GOTO      SVAR4000 IF EQUAL    * sort by clinic id
          COMPARE   TWO,SORTFLAG
          GOTO      SVAR6000 IF EQUAL    * sort by medical practice
.
.         Sort by service provider
.
          PACK      SAVEVAR,TEMPPSRV,SP70
          GOTO      SVAR9999
.
.         Sort by clinic id
.
SVAR4000  PACK      SAVEVAR,TEMPCLID,SP70
          GOTO      SVAR9999
.
.         Sort by medical practice
.
SVAR6000  PACK      SAVEVAR,TEMPMPRA,SP70
.
SVAR9999  RETURN
+
.*********************************************************************
.         check if sort variable change
.*********************************************************************
CVAR0000  MOVE      ZERO,EXIT
          COMPARE   ONE,SORTFLAG
          GOTO      CVAR4000 IF EQUAL    * sort by clinic id
          COMPARE   TWO,SORTFLAG
          GOTO      CVAR6000 IF EQUAL    * sort by medical practice
.
          MATCH     SAVEVAR,TEMPPSRV     * check service provider
          GOTO      CVAR9999 IF EQUAL
          GOTO      CVAR9000
.
CVAR4000  MATCH     SAVEVAR,TEMPCLID     * check clinic id
          GOTO      CVAR9999 IF EQUAL
          GOTO      CVAR9000
.
CVAR6000  MATCH     SAVEVAR,TEMPMPRA     * check medical practice
          GOTO      CVAR9999 IF EQUAL
.
CVAR9000  MOVE      ONE,EXIT
CVAR9999  RETURN

.*********************************************************************
.         print heading
.*********************************************************************
PRHD0000  COMPARE   ONE,SORTFLAG
          GOTO      PRHD4000 IF EQUAL    * sort by clinic id
          COMPARE   TWO,SORTFLAG
          GOTO      PRHD6000 IF EQUAL    * sort by medical practice
          GOTO      PRHD8000             * by provider no
.
PRHD4000  MOVE      SP70,OCADESC
          PACK      KEY20,IDDD,TEMPCLID,Z70
          CALL      RDSCLIA1
          CALL      RDPCLIA1                *To get Clinic ID Description
          BRANCH    OVRCD,PRHD4100
.
          MATCH     IDDD,OCASITE
          GOTO      PRHD4100 IF NOT EQUAL
.
          MATCH     TEMPCLID,OCACLIN
          GOTO      PRHD4100 IF NOT EQUAL
.
          MATCH     SP70,OCADESC            * Read HCP if no doctor
          GOTO      PRHD4200 IF NOT EQUAL
.
          PACK      KEY10,OCADOCT,SP70
          CALL      RDPMHCP1
          BRANCH    OVRCD,PRHD4200
          MOVE      PMHCSNAM,PACSNAME
          MOVE      PMHCGNAM,PACGNAME
          MOVE      PMHCTITL,PACTITLE
          MOVE      ONE,PACFRMT
          CALL      PACNAME
          MOVE      PACFNAME,OCADESC
          GOTO      PRHD4200
.
PRHD4100  MOVE      SP70,OCADESC
.
PRHD4200  PRINT     *N,*1,"Clinic Id : ",OCADESC,"(",TEMPPSRV,")"
          GOTO      PRHD9000
.
PRHD6000  PACK      PRPRDESC,SP70
          PACK      KEY6,TEMPMPRA
          CALL      RDPRPR1
          PRINT     *N,*1,"Medical Practice : ",PRPRDESC
          GOTO      PRHD9000
.
PRHD8000  MATCH     "All",HEADING0
          GOTO      PRHD9999 IF NOT EQUAL
          PRINT     *N,*1,"Provider No.: ",TEMPPSRV
.
PRHD9000  ADD       TWO,CLNO
PRHD9999  RETURN
+
.*********************************************************************
.         print a new page
.*********************************************************************
HEAD0000  CALL      HEAD132
.
          PRINT     *1,"Medical Practice From : ",HEADING7," To : ",HEADING8
          PRINT     *1,"Clinic From           : ",HEADING5," To : ",HEADING6
          PRINT     *1,"Provider Number       : ",HEADING0
.
          MATCH     SP70,FROMDATE
          IF        @EQUAL
            PRINT     *1,"Claim Date From       : Start   to";
          ELSE
            UNPACK    FROMDATE,CCENT,CYEAR,CMON,CDAY
            CALL      PACDATE
            PRINT     *1,"Claim Date From       : ",CPCDATE," to";
          ENDIF
.
          MATCH     SP70,TODATE
          IF        @EQUAL
            PRINT     *40,"Finish"
          ELSE
            UNPACK    TODATE,CCENT,CYEAR,CMON,CDAY
            CALL      PACDATE
            PRINT     *40,CPCDATE
          ENDIF
.
          ADD       FOUR,CLNO
.
          CALL      PRHD0000                     * print sort heading
          CALL      UND132
          PRINT     *1,"|Claim No",*10,"| U/R No":
                    *21,"|Patient Name",*46,"|Medicare ##",*58,"|Serv.Date":
                    *69,"|Item No",*80,"| Item Amount":
                    *94,"|Ref.Prov":
                    *106,"|Ref.Date":
                    *117,"|Claim Comnt.",*132,"|"
          CALL      UND132
          ADD       THREE,CLNO
          MOVE      ONE,HEADFLAG
.
HEAD9999  RETURN
