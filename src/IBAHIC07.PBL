.****************************************************************************
.*        IO ROUTINES FOR TEMPORARY FILE                                    *
.****************************************************************************
RATEMPA   RESET     KEY13
          MOVE      ZERO,OVRCD
          READ      HICTEMP1,KEY13;ANS
          GOTO      OVERCOND IF OVER
          RETURN
.
WRTEMPA   WRITE     HICTEMP1,KEY13;TEMPCLID,TEMPMPRA,TEMPPSRV,TEMPBTCH:
                                   TEMPPMCI,TEMPPYEE,TEMPCLMN
          RETURN
.
RSTEMPA   MOVE      SP70,KEY19
          MOVE      SP70,KEY23
          IF        SORTFLAG=1
            READ      HICTEMP2,KEY19;;
          ELSE
            IF        SORTFLAG=2
              READ      HICTEMP3,KEY19;;
            ELSE
              READ      HICTEMP4,KEY23;;
            ENDIF
          ENDIF
          RETURN
.
RKTEMPA   MOVE      ZERO,OVRCD
          IF        SORTFLAG=1
            READKS    HICTEMP2;TEMPCLID,TEMPMPRA,TEMPPSRV,TEMPBTCH:
                               TEMPPMCI,TEMPPYEE,TEMPCLMN
          ELSE
            IF        SORTFLAG=2
              READKS    HICTEMP3;TEMPCLID,TEMPMPRA,TEMPPSRV,TEMPBTCH:
                                 TEMPPMCI,TEMPPYEE,TEMPCLMN
            ELSE
              READKS    HICTEMP4;TEMPCLID,TEMPMPRA,TEMPPSRV,TEMPBTCH:
                                 TEMPPMCI,TEMPPYEE,TEMPCLMN
            ENDIF
          ENDIF
          GOTO      OVERCOND IF OVER
          RETURN
