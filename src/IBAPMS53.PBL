          TYPE      DINFIELD
          IF        !@EQUAL
            PACK      ERRORMSG,CINVALID,SP1,FLDNAME
            CALL      PERR0000
            GOTO      C36NUM99
          ENDIF
.
C36NUM20  MOVE      3,DINTLEN
