.------------------------------------------------------------------------------
. Validate an active/inactive flag field
.------------------------------------------------------------------------------
CACTIV00  MATCH     SP70,DINFIELD
          IF        @EQUAL | @EOS
            PACK      ERRORMSG,FLDNAME,SP1,CISBLANK
            CALL      PERR0000
            GOTO      CACTIV99
          ENDIF
.
          SQUEEZE   DINFIELD
          MOVELPTR  DINFIELD,FORM1
          IF        FORM1 > 1
            PACK      ERRORMSG,FLDNAME,SP1,CMUST01
            CALL      PERR0000
            GOTO      CACTIV99
          ENDIF
.
          PACK      KEY1,DINFIELD,SP70
          MATCH     "0",KEY1
          IF        !@EQUAL
            MATCH     "1",KEY1
            IF        !@EQUAL
              PACK      ERRORMSG,FLDNAME,SP1,CMUST01
              CALL      PERR0000
            ENDIF
          ENDIF
.
CACTIV99  RETURN
