PSTSRT    FILE      ASCII, FIXED=256
PSTROL    FILE      ASCII, FIXED=256
SORT1     INIT      "u7-22"
SORT2     INIT      "u1-6,13-22"
SORTSTR   DIM       10
TEMPFILE  IFILE     SQL, FIXED=95, KEY=SORTSTR
.
FNAMER    DIM       8
FNAMEI    DIM       8
.
CALDAYS   FORM      4
CMDLINE   DIM       50
CODEA     INIT      "A "
CODEBT    INIT      "BT"
CODECL    INIT      "CL"
CUDATE    DIM       8
CEFFDATE  DIM       8
CONTRCID  DIM       6
CNTRCEFR  FORM      1
.
DAYLEFT   FORM      3
DDATE     INIT      "        "
DIM4      DIM       4
DIM5      DIM       5
DIM8      DIM       8
DIM9      DIM       9
DIM10     DIM       10
DIM11     DIM       11
DIMHF     DIM       10
DIM19A    DIM       19
