            MATCH     "1",MHDLACTV          * if Active set MHDLSENT to "N"
            IF        !@EQUAL
              MOVE      "N",MHDLSENT        * update not sent
            ENDIF
          ENDIF
.
          MATCH     "1",SUPERLEV
