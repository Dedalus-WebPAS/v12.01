            MATCH     "1",MHDLACTV          * if Active set MHDLSENT to "N"
            IF        !@EQUAL
              MOVE      "N",MHDLSENT        * update not sent
            ENDIF
          ENDIF
.
          PACK      KEY8,CCC,CYY,CMM,CDD
          REP       " 0",KEY8
          MOVE      KEY8,MHDLUDAT             * date updated
          CLOCK     TIME,MHDLUTIM             * time updated
          MOVE      USERID,MHDLUUID           * web user id
