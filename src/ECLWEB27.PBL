                IF        OVRCD=1
                  CALL      WRWBPCP1
                ENDIF
              ENDIF
.
              MATCH     SP70,JSONOECO
              IF        !@EQUAL
                MOVE      "OEC",WBPCCPID
                MOVE      JSONOECO,WBPCCNAM
                MOVE      "ECO",WBPCSCID
.
                PACK      KEY9,WBPCCODE,WBPCCPID,WBPCSCID,SP70
                CALL      RDWBPCP1
                IF        OVRCD=1
                  CALL      WRWBPCP1
                ENDIF
              ENDIF
.
              MATCH     SP70,JSONOVSA
              IF        !@EQUAL
                MOVE      "OVS",WBPCCPID
                MOVE      JSONOVSA,WBPCCNAM
                MOVE      "AG ",WBPCSCID
.
                PACK      KEY9,WBPCCODE,WBPCCPID,WBPCSCID,SP70
                CALL      RDWBPCP1
                IF        OVRCD=1
                  CALL      WRWBPCP1
                ENDIF
              ENDIF
.
              MATCH     SP70,JSONOVSB
              IF        !@EQUAL
                MOVE      "OVS",WBPCCPID
                MOVE      JSONOVSB,WBPCCNAM
                MOVE      "MB ",WBPCSCID
.
                PACK      KEY9,WBPCCODE,WBPCCPID,WBPCSCID,SP70
                CALL      RDWBPCP1
                IF        OVRCD=1
                  CALL      WRWBPCP1
                ENDIF
              ENDIF
.
              MATCH     SP70,JSONOVSP
              IF        !@EQUAL
                MOVE      "OVS",WBPCCPID
                MOVE      JSONOVSP,WBPCCNAM
                MOVE      "PC ",WBPCSCID
.
                PACK      KEY9,WBPCCODE,WBPCCPID,WBPCSCID,SP70
                CALL      RDWBPCP1
                IF        OVRCD=1
                  CALL      WRWBPCP1
                ENDIF
              ENDIF
.
              MATCH     SP70,JSONOVSS
              IF        !@EQUAL
                MOVE      "OVS",WBPCCPID
                MOVE      JSONOVSS,WBPCCNAM
                MOVE      "SC ",WBPCSCID
.
                PACK      KEY9,WBPCCODE,WBPCCPID,WBPCSCID,SP70
                CALL      RDWBPCP1
                IF        OVRCD=1
                  CALL      WRWBPCP1
                ENDIF
              ENDIF
.
            ENDIF
.
.           ***********************************************
.           close bracket
.           ***********************************************
.
            SCAN      "]",GPRRLINE
            IF        @EQUAL
              GOTO      GPRPRS90
            ENDIF
.
GPRPRS70    GOTO      GPRPRS50
.
          ENDIF
.
GPRPRS90  GOTO      GPRPRS01
.
