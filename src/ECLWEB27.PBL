.------------------------------------------------------------
.  Parse GPRW Response JSON
.------------------------------------------------------------
.
GPRPRS00  MOVE      SP70,JSONCODE
          PACK      JSONTEXT,SP70,SP70,SP70,SP70,SP70,SP70,SP70,SP70
          MOVE      SP70,GPREINDC
.
GPRPRS01  READ      GPRRFILE,SEQ;GPRRLINE
          GOTO      GPRPRS99 IF OVER
.
. ***********************************************************
. Errors
. ***********************************************************
.
          SCAN      "highestSeverity",GPRRLINE
          IF        @EQUAL
.
            MOVE      "1",GPREINDC
.
GPRPRS05    READ      GPRRFILE,SEQ;GPRRLINE
            GOTO      GPRPRS90 IF OVER
.
.           *************************************************
.           serviceMessage
.           *************************************************
.
            SCAN      "serviceMessage",GPRRLINE
            IF        @EQUAL
.
GPRPRS07      READ      GPRRFILE,SEQ;GPRRLINE
              GOTO      GPRPRS20 IF OVER
.
.             ***********************************************
.             {    
.             ***********************************************
.
              SCAN      "{",GPRRLINE
              IF        @EQUAL
                MOVE      SP70,JSONCODE
                PACK      JSONTEXT,SP70,SP70,SP70,SP70,SP70,SP70,SP70,SP70
.
                GOTO      GPRPRS07
.
              ENDIF
.
.             ***********************************************
.             code
.             ***********************************************
.
              SCAN      "code",GPRRLINE
              IF        @EQUAL
                RESET     GPRRLINE
.               UNPACK    GPRRLINE,DIM14,JSONCODE
.
                SCAN      ":",GPRRLINE
                IF        @EQUAL
                  MOVE      GPRRLINE,GPRRWORK
                  REP       ": ",GPRRWORK
                  REP       "#" ",GPRRWORK
                  LJUSTIFY  GPRRWORK
                  MOVE      GPRRWORK,JSONCODE
                  RESET     GPRRLINE
                ENDIF
.
                GOTO      GPRPRS07
.
              ENDIF
.
.             ***********************************************
.             }
.             ***********************************************
.
              SCAN      "}",GPRRLINE
              IF        @EQUAL
.
                MOVE      SAVKEY51,KEY51
                CALL      RDWBETR1
.
                MOVE      JSONCODE,WBETEROR
.
                CALL      IBACLOCK
                PACK      WBETUDTE,CCC,CYY,CMM,CDD
                REP       " 0",WBETUDTE
                MOVE      CTIMEIS,WBETUTIM
                MOVE      USERID,WBETUUID
 
                CALL      UPWBETR1
.
                GOTO      GPRPRS07
.
              ENDIF
.
.             ***********************************************
.             ]
.             ***********************************************
.
              SCAN      "]",GPRRLINE
              IF        @EQUAL
                GOTO      GPRPRS90
              ENDIF
.
              GOTO      GPRPRS07
.
            ENDIF
.
GPRPRS20    GOTO      GPRPRS05   
.
          ENDIF
.
