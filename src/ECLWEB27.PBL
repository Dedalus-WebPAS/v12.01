SETBOLD   INIT      "(s7bT"
.
ALRCOMM   DIM       100
MULTILIN  FORM      1  
.--------------------------------------------------------------------------------
PRGID     INIT      "ECLWEB27"
VERSION   INIT      "V12.01.00"
PRGNAM    INIT      "Medicare WebServices GPRW Polling Response Server"
.--------------------------------------------------------------------------------
.
          CALL      WEBINT00       * Initialise Fifo Etc.
.
          CALL      INIT0000       * Open Files
.
MAIN0000  COMMIT
.
          BEGIN
.
          DISPLAY   *W2
.
          MATCH     "1",PTCNWSIN
          GOTO      MAIN0000 IF NOT EQUAL
.
          MOVE      ZERO,EXIT
.
          PACK      KEY51,ZERO,SP1,TWO,SP70
          CALL      RSWBETR1
MAIN1000  CALL      RKWBETR1
          BRANCH    OVRCD,MAIN9999
.
          MATCH     "0",WBETSTAT
          GOTO      MAIN9999 IF NOT EQUAL
.
          MATCH     " 2",WBETTYPE
          GOTO      MAIN9999 IF NOT EQUAL
.
          PACK      KEY71,WBETLOCN,SP70          * Read B2B Device Table
          CALL      RSWBB2B2
          CALL      RKWBB2B2
          BRANCH    OVRCD,MAIN1000
.
          MATCH     WBETLOCN,WBB2LOCN            * Same Hospital
          GOTO      MAIN1000 IF NOT EQUAL
.
          MATCH     "ACTIVE",WBB2DSTA            * Device Active
          GOTO      MAIN1000 IF NOT EQUAL
.
          MATCH     SP70,WBB2DPTH
          GOTO      MAIN1000 IF EQUAL
.
          CALL      PROPN000                     * Valid Properties File?
.
          MATCH     "1",PRODAIND
          GOTO      MAIN1000 IF EQUAL
.
          PACK      SAVKEY51,WBETSTAT,WBETTYPE,WBETLOCN,WBETCDTE,WBETCTIM:
                             WBETTRID,SP70
.
          MOVE      WBETLOCN,SAVELOCN
          MOVE      WBETSNID,SAVESNID
.
          OPEN      WBSPROPF,PRDAPATH
.
. Parse _gprw file (GPRW Response JSON file)
. ************************************************************
.
MAIN2000  CLEAR     CMDLINE
          APPEND    WBB2DPTH,CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    "/webPAS_claims/gprw/",CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    WBETSTRD,CMDLINE
          APPEND    GPRRLITR,CMDLINE
          RESET     CMDLINE
.
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      GPRRFILE,CMDLINE
          TRAPCLR   IO
          IF        OVRCD=1
            GOTO      MAIN3000
          ENDIF
.
          CALL      GPRPRS00                   * Parse GPRW Response JSON
.
          GOTO      MAIN8000
.
. Parse _gprw_errors file (GPRW Stderrs/Stdout file)
. ******************************************************
.
MAIN3000  CLEAR     CMDLINE
          APPEND    WBB2DPTH,CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    "/webPAS_claims/gprw/",CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    WBETSTRD,CMDLINE
          APPEND    GPRELITR,CMDLINE
          RESET     CMDLINE
.
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      GPREFILE,CMDLINE
          TRAPCLR   IO
          IF        OVRCD=1
            MOVE      "Response files not found.",GPRELINE  
            GOTO      MAIN3500
          ENDIF
.
MAIN3100  READ      GPREFILE,SEQ;GPRELINE
          IF        @OVER
            MOVE      "Refer to GPRW Details.",GPRELINE
            GOTO      MAIN3500 
          ENDIF
.
          SCAN      "error",GPRELINE
          IF        @EQUAL
            GOTO      MAIN3500
          ENDIF
.
          GOTO      MAIN3100
.
MAIN3500  MOVE      "1",GPREINDC
.
          MOVE      SAVKEY51,KEY51
          CALL      RDWBETR1
.
          MOVE      "EROR",WBETEROR
.
          CALL      IBACLOCK
          PACK      WBETUDTE,CCC,CYY,CMM,CDD
          REP       " 0",WBETUDTE
          MOVE      CTIMEIS,WBETUTIM
          MOVE      USERID,WBETUUID

          CALL      UPWBETR1
.
          GOTO      MAIN8000
.
. ******************************************************************************
.
MAIN8000  MOVE      SAVKEY51,KEY51
          CALL      RDWBETR1
.
          MATCH     "1",GPREINDC
          IF        @EQUAL
            MOVE      "E",WBETSTAT
          ELSE
            MOVE      "1",WBETSTAT
          ENDIF
.
          CALL      IBACLOCK                     * get current date/time
          PACK      WBETUDTE,CCC,CYY,CMM,CDD
          REP       " 0",WBETUDTE
          MOVE      CTIMEIS,WBETUTIM
          REP       " 0",WBETUTIM
.
          CALL      IBACLOCK
          PACK      WBETUDTE,CCC,CYY,CMM,CDD
          REP       " 0",WBETUDTE
          MOVE      CTIMEIS,WBETUTIM
          MOVE      USERID,WBETUUID

          CALL      UPWBETR1
.
MAIN9000  CLOSE     WBSPROPF
          CLOSE     GPRRFILE
          CLOSE     GPREFILE
.
          GOTO      MAIN1000
.
MAIN9999  CLOSE     WBSPROPF
          CLOSE     GPRRFILE
          CLOSE     GPREFILE
.
          GOTO      MAIN0000
