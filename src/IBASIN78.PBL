          ELSE
            MOVE      SP70,TOLAB
            DISPLAY   *PCCOL:CVERT,TOLAB;
.
            CALL      KSIFAB00
            PACK      TOCO15,SIFASUPP,SP70
            LOAD      TOCO15,EXIT,Z70,SP70
.
            PACK      TOLAB,SIFASUPP,SP70
            LOAD      TOLAB,EXIT,END,SP70
          ENDIF
.
KCODT999  RETURN
