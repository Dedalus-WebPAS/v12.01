.********************************************************************
. PRNT - print report                               Called by ML
.         Requires : FROMLAB,TOLAB   (print range display labels)
.                    FROMCO15,TOCO15 (print range)
.                    OPTIONA         (report type)
.********************************************************************
PRNT0000
          MATCH     FROMCO15,TOCO15         * check if range ok
          IF        @LESS
            CLEAR     DISPMSG
            APPEND    "Illegal Range Entered - ",DISPMSG
            CALL      MESSAGE1
            GOTO      PRNT9500
          ENDIF
.
          DISPLAY   *P50:24,"Printing :   ";
          MOVE      ZERO,CPAGENO                 * reset page no.
          CLOCK     TIME,CTIMEIS                 * get current time
          MOVE      "999",CLNO                   * init last line number
          MOVE      SP70,LASTCO15                * clear last item
.
          CALL      FILE0000                    * go to correct position in file
.
PRNT1000  CALL      NREC0000                     * get next record
          BRANCH    OVRCD,PRNT9000               * end of file ?
.
          IF        OPTIONA=1
            DISPLAY   *P61:24,*V2LON,SIFAISSU,*EL;
          ELSE
            DISPLAY   *P61:24,*V2LON,SIFASUPP,*EL;
          ENDIF
          COMPARE   "55",CLNO                    * end of page ?
          CALL      HEAD0000 IF NOT LESS         * start new page
.
          CALL      WRIT0000                     * print details
          GOTO      PRNT1000                     * get next record
.
PRNT9000  COMPARE   ZERO,CPAGENO
          CALL      HEAD0000 IF EQUAL          * print header if nothing printed
          CALL      UND80                        * print line
          PRINT     "****   End of Report   ****"
          MOVE      ZERO,EXIT
          GOTO      PRNT9999
.
PRNT9500  MOVE      ONE,EXIT
.
PRNT9999  RETURN
.**********************************************************************
.  FILE - Set file pointer                             Called By PRNT
.**********************************************************************
FILE0000  BRANCH    OPTIONA,FILE0100,FILE0200
.
.---- print by type 1 ----
.
FILE0100  PACK      KEY30,FROMCO15,SP70
          CALL      RSSIFA1
          CALL      RPSIFA1
          GOTO      FILE9999
.
.---- print by type 2 ----
.
FILE0200  PACK      KEY30,FROMCO15,SP70
          CALL      RSSIFA2
          CALL      RPSIFA2
          GOTO      FILE9999
.
FILE9999  RETURN
.**********************************************************************
.  NREC - Get next record                              Called By PRNT
.**********************************************************************
NREC0000  BRANCH    OPTIONA,NREC0100,NREC0200
.
.---- print by type 1 ----
.
NREC0100  CALL      RKSIFA1
          BRANCH    OVRCD,NREC9500
.
          MATCH     SIFAISSU,TOCO15
          GOTO      NREC9500 IF LESS             * out of range ?
          GOTO      NREC9000
.
.---- print by type 2 ----
.
NREC0200  CALL      RKSIFA2
          BRANCH    OVRCD,NREC9500
.
          MATCH     SIFASUPP,TOCO15
          GOTO      NREC9500 IF LESS             * out of range ?
          GOTO      NREC9000
.
NREC9000  MATCH     SP70,SIFASUPP
          GOTO      NREC0000 IF EQUAL
          MATCH     SP70,SIFAISSU
          GOTO      NREC0000 IF EQUAL
.
          MOVE      ZERO,OVRCD         * continue
          GOTO      NREC9999
.
NREC9500  MOVE      ONE,OVRCD          * quit
.
NREC9999  RETURN
.********************************************************************
. HEAD - print report                               Called by PRNT
.         Requires : FROMLAB,TOLAB   (print range display labels)
.                    FROMCO15,TOCO15 (print range)
.********************************************************************
HEAD0000  CALL      HEAD80                      * print heading
.
          ADD       "2",CLNO
          IF        OPTIONA=1
            PRINT        *20,"Print by Issue Unit":
                      *N,*20,"From : ",FROMLAB:
                      *N,*20,"To   : ",TOLAB
          ELSE
            PRINT        *20,"Print by Supply Unit":
                      *N,*20,"From : ",FROMLAB:
                      *N,*20,"To   : ",TOLAB
          ENDIF
          CALL      UND80                       * print line
          PRINT     "| Issue Unit      | Supply Unit     | Factor":
                    *80,"|"
          ADD       "2",CLNO
          CALL      UND80                       * print line
.
HEAD9999  RETURN
