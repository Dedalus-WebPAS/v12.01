.
          WRITE     HTMLFILE;"#"",HTMLLNK4,"#",":                      * 36
                             "#"",OPARATIM,"#",":                      * 37
                             "#"",OPARANAS,"#",":                      * 38
                             "#"",OPARTIRD,"#",":                      * 39
                             "#"",OPARTETC,"#",":                      * 40
