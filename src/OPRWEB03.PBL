          ELSE
            MATCH     SP70,BKRXPOBD
            IF        !@EQUAL
              WRITE     HTMLFILE;"#"",BKRXPOWD,"/",BKRXPOBD,"#",";     * 30
            ELSE
              WRITE     HTMLFILE;"#"",BKRXPOWD,"#",";
