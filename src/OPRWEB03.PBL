          ELSE
            MATCH     SP70,OPDAPOBD
            IF        !@EQUAL
              WRITE     HTMLFILE;"#"",OPDAPOWD,"/",OPDAPOBD,"#",";     * 35
            ELSE
              WRITE     HTMLFILE;"#"",OPDAPOWD,"#",";                  * 35
            ENDIF
          ENDIF
.
          CLEAR     HTMLLNK4
          APPEND    "javascript:linkToCase('",HTMLLNK4
          APPEND    LINKPRG,HTMLLNK4
          APPEND    ".pbl?reportno=",HTMLLNK4
          APPEND    LINKREP,HTMLLNK4
