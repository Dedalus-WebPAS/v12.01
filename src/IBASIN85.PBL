. Print Heading
.----------------------------------------------------------
HEAD0000  CALL      HEAD132                 * Print Page Header
          MOVE      "5",CLNO
          IF        CPAGENO=1
            STRIP     INCLDESC
            STRIP     SEQDESC
            CLEAR     VAR
            APPEND    "Report Including ",VAR
            APPEND    INCLDESC,VAR
            APPEND    " in ",VAR
            APPEND    SEQDESC,VAR
            APPEND    " Sequence",VAR
            RESET     VAR
            PRINT        "Catalog Code  Range : ",FROMCATA," to  ",TOCATA:
                      *N,"Product Group Range : ",FROMCODE,"   to  ",TOCODE:
                      *N,"Catalog Name  Range : ",FROMDESC," to  ",TODESC:
                      *N,"Date          Range : ",DPFRDATE," to  ",DPTODATE:
                      *N,"Warehouse           : ",SIWAWAR,SP1,WAREDESC:
                      *N,VAR
            ADD       FIVE,CLNO
          ELSE
            PRINT        "Warehouse           : ",SIWAWAR,SP1,SIWADES:
                      *N,VAR
            ADD       TWO,CLNO
          ENDIF
          CALL      UND132
          PRINT     "Catalog : ",SIIACAT,SP1,SIIADES," Issue Unit : ",SIIAISS
          CALL      UND132
          PRINT     "  Date       Time   Transaction       Opr/Port Program":
                    "  Period Reqn No. P/O No. C/C   Reference               ":
                    "Quantity       Amount"
          CALL      UND132
          RETURN
.------------------------------------------------------------
. Range Check
.------------------------------------------------------------
RANGE000  MOVE      ZERO,EXIT
          IF        !(REPTSEQ=1)
            MATCH     FCATALOG,SIIACAT
            GOTO      RANGE900 IF LESS
            MATCH     SIIACAT,TCATALOG
            GOTO      RANGE900 IF LESS
          ENDIF
.
          IF        !(REPTSEQ=2)
            MATCH     FDESCRP,SIIADES
            GOTO      RANGE900 IF LESS
            MATCH     SIIADES,TDESCRP
            GOTO      RANGE900 IF LESS
          ENDIF
.
          IF        !(REPTSEQ=3)
            MATCH     FGROUP,SIIASUB
            GOTO      RANGE900 IF LESS
            MATCH     SIIASUB,TGROUP
            GOTO      RANGE900 IF LESS
          ENDIF
.
          MOVE      SP70,SIIESID
          IF        SIIANON=0
            MATCH     SP5,SIWAWAR
            IF        !@EQUAL
              PACK      KEY12,SIIACAT,SIWAWAR
              CALL      RDSIIE1
              BRANCH    OVRCD,RANGE900
            ENDIF
          ELSE
            MOVE      ZERO,SIIESOH
            MOVE      ZERO,SIIESUS
          ENDIF
.
RANGE500  BRANCH    INCLTYPE,RANGE510,RANGE520,RANGE530,RANGE530,RANGE530:
                             RANGE530,RANGE530,RANGE530
          GOTO      RANGE999
.
. SIIANON   FORM      1      Non-Stock Item 0=No (ie Stocked Item)
.------------------------------------------------
RANGE510  COMPARE   ONE,SIIANON
          GOTO      RANGE900 IF EQUAL   * Stock Only
          GOTO      RANGE999
.
RANGE520  COMPARE   ZERO,SIIANON
          GOTO      RANGE900 IF EQUAL   * Non-Stock Only
          GOTO      RANGE999
.
RANGE530  ASSIGN    INCLTYPE-2,F2
.
          MATCH     SP70,SIIESID
          IF        @EQUAL
            RESET     SIIASTI,F2
            CMATCH    "1",SIIASTI
            GOTO      RANGE900 IF NOT EQUAL
            GOTO      RANGE999
          ELSE
            RESET     SIIESID,F2
            CMATCH    "1",SIIESID
            GOTO      RANGE900 IF NOT EQUAL
            GOTO      RANGE999
          ENDIF
.
RANGE900  MOVE      ONE,EXIT
.
RANGE999  RETURN
.
