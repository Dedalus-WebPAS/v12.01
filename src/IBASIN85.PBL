. Print Line
.----------------------------------------------------------
PRTLIN00  MOVE      "80",CLNO
          MOVE      SP70,KEY8
.
          PACK      KEY28,SIIACAT,FDATE,SIWAWAR,SP70
          CALL      RSSIIT2
PRTLIN10  CALL      RKSIIT2
          BRANCH    OVRCD,PRTLIN90
          MATCH     SIITCAT,SIIACAT
          GOTO      PRTLIN90 IF NOT EQUAL
          MATCH     SIITDAT,TDATE
          GOTO      PRTLIN90 IF LESS
          MATCH     SIWAWAR,SP5
          IF        !@EQUAL
            MATCH     SIITWAR,SIWAWAR
            GOTO      PRTLIN10 IF NOT EQUAL
          ENDIF
.
          COMPARE   "55",CLNO               * Number of Lines Test
          CALL      HEAD0000 IF NOT LESS
.
          ADD       ONE,CLNO
.
          UNPACK    SIITDAT,CCENT,CYEAR,CMON,CDAY
          CALL      PACDATE
.
          CALL      ITMDET00   * Get Transaction Description
.
          MOVE      "9,999,999.99",KEY12
          FEDIT     SIITQTY,KEY12
.
          MOVE      "99,999,999.99",KEY13
          FEDIT     SIITAMT,KEY13
.
          UNPACK    SIITPER,CCENT,CYEAR,CMON
          PRINT     CPCDATE,SP1,SIITTIM,SP1,KEY17,SP1,SIITOPR,"/":
                    SIITPOR,SP1,SIITPRG,SP1,CMON,"/",CCENT,CYEAR,SP1:
                    SIITREQ,SP1,SIITPUR,SP1,SIITCST,SP1,SIITREF,KEY12,KEY13
          GOTO      PRTLIN10
.
PRTLIN90  COMPARE   "80",CLNO
          GOTO      PRTLIN99 IF EQUAL
          CALL      UND132
          PRINT     "Totals        Received             Issued":
                    "           Returned           Variance"
          MOVE      "99,999,999.99",KEY13
          MOVE      "999,999,999.99",KEY14
          MOVE      "9999,999,999.99",KEY15
          MOVE      "9,999,999,999.99",KEY16
          ASSIGN    QTOTAL04+QTOTAL14+F12P2,F12P2
          FEDIT     F12P2,KEY13
.
          MOVE      ZERO,F12P2
          COMPARE   ZERO,QTOTAL11
          IF        @LESS
            ADD       QTOTAL11,F12P2
          ENDIF
          ASSIGN    QTOTAL05+QTOTAL06+QTOTAL07+QTOTAL12+F12P2,F12P2
          FEDIT     F12P2,KEY14
.
          MOVE      ZERO,F12P2
          COMPARE   ZERO,QTOTAL11
          IF        !@LESS
            ADD       QTOTAL11,F12P2
          ENDIF
.
          ASSIGN    QTOTAL08+QTOTAL13+F12P2,F12P2
          FEDIT     F12P2,KEY15
          ASSIGN    QTOTAL02+QTOTAL09+QTOTAL10,F12P2
          FEDIT     F12P2,KEY16
          PRINT     "Qty",*10,KEY13:
                    *28,KEY14:
                    *46,KEY15:
                    *64,KEY16
.
          MOVE      "99,999,999.99",KEY13
          MOVE      "999,999,999.99",KEY14
          MOVE      "9999,999,999.99",KEY15
          MOVE      "9,999,999,999.99",KEY16
          ASSIGN    ATOTAL04+ATOTAL14+F12P2,F12P2
          FEDIT     F12P2,KEY13
.
          MOVE      ZERO,F12P2
          COMPARE   ZERO,ATOTAL11
          IF        @LESS
            ADD       ATOTAL11,F12P2
          ENDIF
          ASSIGN    ATOTAL05+ATOTAL06+ATOTAL07+ATOTAL12,F12P2
          FEDIT     F12P2,KEY14
.
          MOVE      ZERO,F12P2
          COMPARE   ZERO,ATOTAL11
          IF        !@LESS
            ADD       ATOTAL11,F12P2
          ENDIF
          ASSIGN    ATOTAL05+ATOTAL06+ATOTAL07+ATOTAL12,F12P2
          ASSIGN    ATOTAL08+ATOTAL13,F12P2
          FEDIT     F12P2,KEY15
          ASSIGN    ATOTAL02+ATOTAL09+ATOTAL10,F12P2
          FEDIT     F12P2,KEY16
          PRINT     "Value",*10,KEY13:
                    *28,KEY14:
                    *46,KEY15:
                    *64,KEY16
          CALL      UND132
          CALL      CLRS0000
.
PRTLIN99  RETURN
