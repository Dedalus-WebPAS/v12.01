.------------------------------------------------------------------------------
. REPT - Print Out Report
.------------------------------------------------------------------------------
REPT0000  PACK      DISPMSG,SP70,SP70                 * save line 24
          CALL      MESSAGE2
.
          MATCH     FCATALOG,TCATALOG
          IF        @LESS
            DISPLAY   *P1:24,*B,*EF,"Illegal Catalog Range - ";
            CALL      HITENTER
            GOTO      REPT9500
          ENDIF
.
          MATCH     FGROUP,TGROUP
          IF        @LESS
            DISPLAY   *P1:24,*B,*EF,"Illegal Product Group Range - ";
            CALL      HITENTER
            GOTO      REPT9500
          ENDIF
.
          MATCH     FDESCRP,TDESCRP
          IF        @LESS
            DISPLAY   *P1:24,*B,*EF,"Illegal Description Range - ";
            CALL      HITENTER
            GOTO      REPT9500
          ENDIF
.
.
          DISPLAY   *P1:24,*EL,"Processing - ";
          MOVE      "599",CLNO                        * Set up print values
          MOVE      "0",CPAGENO
          CLOCK     TIME,CTIMEIS
.
          READ      CONTROLF,TWENTY3;*143,HDEFWH
          BRANCH    REPTSEQ,REPT1000,REPT2000,REPT3000
.
. Catalog Seq
.-------------------------
REPT1000  PACK      KEY7,FCATALOG,SP70
          MOVE      "80",CLNO                         * Set up print values
          CALL      RSSIIA1
          CALL      RPSIIA1
.
REPT1100  CALL      RKSIIA1
          BRANCH    OVRCD,REPT9000
          MATCH     SIIACAT,TCATALOG
          GOTO      REPT9000 IF LESS
.
          CALL      RANGE000              * Check Other Ranges
          BRANCH    EXIT,REPT1100         * Not in Range
.
          DISPLAY   *P15:24,*EL,*V2LON,SIIACAT;
          CALL      PRTLIN00
.
          GOTO      REPT1100
.
. Catalog Description Seq
.-------------------------
REPT2000  PACK      KEY67,FDESCRP,SP70
          CALL      RSSIIA3
          CALL      RPSIIA3
.
REPT2100  CALL      RKSIIA3
          BRANCH    OVRCD,REPT9000
          MATCH     SIIADES,TDESCRP
          GOTO      REPT9000 IF LESS
.
          CALL      RANGE000              * Check Other Ranges
          BRANCH    EXIT,REPT2100         * Not in Range
.
          DISPLAY   *P15:24,*EL,*V2LON,SIIACAT;
          CALL      PRTLIN00
.
          GOTO      REPT2100
.
. Product Group Sequence
.-----------------------
REPT3000  PACK      KEY12,FGROUP,SP70
          CALL      RSSIIA2
          CALL      RPSIIA2
.
REPT3100  CALL      RKSIIA2
          BRANCH    OVRCD,REPT9000
          MATCH     SIIASUB,TGROUP
          GOTO      REPT9000 IF LESS
.
          CALL      RANGE000              * Check Other Ranges
          BRANCH    EXIT,REPT3100         * Not in Range
.
          DISPLAY   *P15:24,*EL,*V2LON,SIIACAT;
          CALL      PRTLIN00
          GOTO      REPT3100
.
REPT9000
          COMPARE   ZERO,CPAGENO
          CALL      HEAD0000 IF EQUAL       * at least one page should print
.
          CALL      UND132                  * End of Report
          PRINT     *57,"***** End of Report *****"
          DISPLAY   *P1:24,*EL;             * Clear Processing Lines Display
.
          MOVE      ZERO,EXIT
          GOTO      REPT9999
.
REPT9500  MOVE      ONE,EXIT
.
REPT9999  CALL      MESSAGE3                * restore line 24
          RETURN
