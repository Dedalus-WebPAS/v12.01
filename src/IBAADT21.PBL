          MOVE      "41",CCOL
          MOVE      "19",CVERT
          CALL      KIBGSA00              * Keyin GST Code
.
          MATCH     SP6,IBGSCODE
          IF        @EQUAL
            DISPLAY   *P1:24,*EL,*V2LON,*B:
                      "GST Code is mandatory for GST payable Item. ";
            CALL      HITENTER
            GOTO      KGST4000
          ENDIF
          MOVE      IBGSCODE,PTITGSTC
          MOVE      SAVCOL,CCOL
          MOVE      SAVVERT,CVERT
.
