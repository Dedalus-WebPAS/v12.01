          CALL      RDSDRGA1                * position on the date range file
          CALL      RDKDRGA1
          BRANCH    OVRCD,GETDAT90
.
          MATCH     PRIDC003,DRGYR           * match financial years
          GOTO      GETDAT90 IF NOT EQUAL
.
          MOVE      DRGFDTE,FROMDATE
.
          PACK      KEY6,PRIDC003,Z2
          CALL      RDSDRGA1                * position on the date range file
          CALL      RDPDRGA1
          BRANCH    OVRCD,GETDAT90
.
          MATCH     PRIDC003,DRGYR           * match financial years
          GOTO      GETDAT90 IF NOT EQUAL
.
          MOVE      DRGTDTE,TODATE
          GOTO      GETDAT99
