          MOVE      "GI",CATEGORY     * Get Patient Indicator Description
          MOVE      PRDOPIND,CODE
          CALL      GETCOD00
          MATCH     SP70,TDESC
          IF        @EQUAL
            MOVE      PRDOPIND,TDESC
          ENDIF
