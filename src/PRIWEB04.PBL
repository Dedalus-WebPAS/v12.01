          MOVE      PRDOCLAM,CODE
          CALL      GETCOD00
          MATCH     SP70,TDESC
          IF        @EQUAL
            MOVE      PRDOCLAM,KEY30
          ELSE
            MOVE      TDESC,KEY30
          ENDIF
