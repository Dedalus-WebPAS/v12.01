          ELSE
            PACK      PRFNCODE,ANSN,PRDTDOCT,PRDTSETC,PRDTIFLG,PRDTITMN,PRDTSUBN
          ENDIF
.
          MOVE      PRDTPRAC,PRFNMPRA
          MOVE      PRDTIAMT,FSTAMNT
          MULT      "-1",FSTAMNT
          MOVE      ZERO,FGSTFLAG
          CALL      PRICALFS
.
          IF        IBCNUGST=2 & PRDTGSTM<>0
            MOVE      PRDTGSTM,FSTAMNT    
            MULT      "-1",FSTAMNT
            MOVE      ONE,FGSTFLAG
            CALL      PRICALFS
          ENDIF
.
