          MOVE      PRECINVN,TEMPINVN
          MOVE      PRECBATN,TEMPBATN
          MOVE      PRECUNIQ,TEMPADMN
          MOVE      PRECURNO,TEMPURNO
          MOVE      PRECHFND,TEMPHFND
          MOVE      PRECPBAT,TEMPPBAT
          MOVE      PRECHOSP,TEMPHOSP
