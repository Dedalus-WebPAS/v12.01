            MATCH     SP90,ECCLMARR[COUNTEC]
            IF        !@EQUAL
              MOVE      ECCLMARR[COUNTEC],KEYSTARR[COUNT]    * to be extracted
            ENDIF
            ADD      ONE,COUNT
            ADD      ONE,COUNTEC
