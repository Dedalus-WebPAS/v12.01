.
          MATCH     PURNO,PTETURNO
          GOTO      COMRW100 IF EQUAL
          PACK      DIM45,SP70
          PACK      KEY8,PTETURNO,SP8
          CALL      RDMAST1
          BRANCH    OVRCD,COMRW200
          MOVE      ONE,FRSTLINE
.
