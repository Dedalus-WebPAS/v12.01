.******************************************************************************
VUPAM000  MOVE      ZERO,ERRFLAG
          MOVE      ZERO,ERRDATE
.
          MATCH     SP70,XITEMNO
          IF        @EQUAL
            MOVE      "Blank Item Number",ERRORMSG
            CALL      PERR0000
            GOTO      VUPAM300
          ENDIF
.
          PACK      KEY17,XITEMNO,SP70
          CALL      RDSITEM1
VUPAM200  CALL      RDKITEM1
          IF        OVRCD = 1
            MOVE      "Invalid Item Number",ERRORMSG
            CALL      PERR0000
            GOTO      VUPAM300
          ENDIF
.
          MATCH     XITEMNO,ITEMNO
          IF        !@EQUAL
            MOVE      "Invalid Item Number",ERRORMSG
            CALL      PERR0000
            GOTO      VUPAM300
          ENDIF
.
VUPAM300  MATCH     SP70,XFLD0004
          IF        @EQUAL | @EOS 
            MOVE      "Blank MBS Amount",ERRORMSG
            CALL      PERR0000
            GOTO      VUPAM999
          ENDIF
.
          STRIP     XFLD0004
          MOVE      XFLD0004,WORKFLD
.
VUPAM320  TYPE      WORKFLD 
          IF        !@EQUAL
            GOTO      VUPAM900
          ENDIF
.
VUPAM350  SCAN      ".",WORKFLD
          IF        @EQUAL
            BUMP      WORKFLD
            MOVEFPTR  WORKFLD,FORM2
            MOVE      WORKFLD,WORKFLDD
            SUB       TWO,FORM2
            MOVE      XFLD0004,WORKFLD
            SETLPTR   WORKFLD,FORM2
            MOVE      WORKFLD,WORKFLDI
          ELSE
            MOVE      WORKFLD,WORKFLDI
            MOVE      SP70,WORKFLDD
          ENDIF
.
          SQUEEZE   WORKFLDI
          MOVELPTR  WORKFLDI,FORM2
          IF        FORM2 > 14
            GOTO      VUPAM900
          ENDIF
.
          SQUEEZE   WORKFLDD
          MOVELPTR  WORKFLDD,FORM2
          IF        FORM2 > 2
            GOTO      VUPAM900
          ENDIF
.
          GOTO      VUPAM999
.
VUPAM900  MOVE      "Invalid MBS Amount",ERRORMSG
          CALL      PERR0000
          GOTO      VUPAM999
.
VUPAM999  RETURN
