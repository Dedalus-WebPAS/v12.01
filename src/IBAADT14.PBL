.******************************************************************************
.* Update Suggested Classification ONLY                                       *
.******************************************************************************
USGCL000  MOVE      ZERO,LINECNTR
.
          OPEN      UITEMTXT,FNAMEI
.
USGCL100  READ      UITEMTXT,SEQ;XFLD0001,XFLD0002,XFLD0003,XFLD0004,XFLD0005:
                                 XFLD0006,XFLD0007,XFLD0008,XFLD0009,XFLD0010:
                                 XFLD0011,XFLD0012,XFLD0013,XFLD0014,XFLD0015:
                                 XFLD0016,XFLD0017,XFLD0018,XFLD0019,XFLD0020:
                                 XFLD0021,XFLD0022,XFLD0023,XFLD0024,XFLD0025:
                                 XFLD0026,XFLD0027,XFLD0028,XFLD0029,XFLD0030:
                                 XFLD0031,XFLD0032,XFLD0033,XFLD0034,XFLD0035:
                                 XFLD0036,XFLD0037,XFLD0038,XFLD0039,XFLD0040
          GOTO      USGCL999 IF OVER
.
          PACK      XFLD0001,XFLD0001,SP70   * ITEM NUMBER
          PACK      XFLD0002,XFLD0002,SP70   * ITEM ALPHA KEY
          PACK      XFLD0003,XFLD0003,SP70   * ITEM DESCRIPTION
          PACK      XFLD0004,XFLD0004,SP70   * MBS AMOUNT
.
          MATCH     SP70,XFLD0001
          GOTO      USGCL200 IF NOT EQUAL
.
          MATCH     SP70,XFLD0002
          GOTO      USGCL200 IF NOT EQUAL
.
          MATCH     SP70,XFLD0003
          GOTO      USGCL200 IF NOT EQUAL
.
          MATCH     SP70,XFLD0004
          GOTO      USGCL200 IF NOT EQUAL
.
          GOTO      USGCL100
.
USGCL200  ADD       ONE,LINECNTR            * Text file line counter for error
.
          PACK      XITEMNO,XFLD0001,SP70   * ITEM NUMBER
          PACK      XIALPHA,XFLD0002,SP70   * ITEM ALPHA KEY
          PACK      XITMDES,XFLD0003,SP70   * ITEM DESCRIPTION
.
          MOVE      SP70,DIM18
          MOVE      XFLD0004,DIM18
          RJUSTIFY  DIM18
          MOVE      ZERO,XIQIAMT
          STRIP     XFLD0004
          MOVE      XFLD0004,XIQIAMT        * MBS AMOUNT
.
          PACK      XIMSGRP,XFLD0005,SP70   * MISCELLANEOUS GROUP (CAT FI)
          PACK      XITCLSS,XFLD0006,SP70   * SUGGESTED CLASS (CAT A )
.
          UNPACK    XFLD0007,XDAY,XMON,XCENT,XYEAR
          PACK      XITEFDT,XCENT,XYEAR,XMON,XDAY,SP70
.
          PACK      ITEMDESC,XITMDES,SP70
          PACK      KEY75,XITEMNO,SP1,XIALPHA,SP1,ITEMDESC,SP1:
                          DIM18,SP1,XIMSGRP,SP1,XITCLSS,SP1,XITEFDT,SP70
.
          CALL      VALD0000                * Validation pass
          BRANCH    ERRFLAG,USGCL100        * validate fields
.
USGCL400  PACK      KEY17,XITEMNO,UPLDDATE,SP70
          CALL      RDITEM1
          IF        OVRCD = 0
            GOTO      USGCL440
          ENDIF
.
          PACK      KEY17,XITEMNO,UPLDDATE,Z70
          CALL      RDSITEM1
USGCL420  CALL      RDPITEM1
          BRANCH    OVRCD,USGCL800
.
          MATCH     XITEMNO,ITEMNO
          GOTO      USGCL800 IF NOT EQUAL
.
USGCL440  MATCH     SP70,PTITETDT                * Effective to date
          IF        !@EQUAL
            MATCH     UPLDDATE,PTITETDT
            GOTO      USGCL420 IF LESS
          ENDIF
.
USGCL450  MOVE      XITCLSS,ICLSS 
.
          CALL      IBACLOCK
          PACK      PTITUDAT,CCC,CYY,CMM,CDD
          REP       " 0",PTITUDAT           * date record updated
          MOVE      CTIMEIS,PTITUTIM        * time record updated
          MOVE      USERID,PTITUUID         * web user updated
.
          CALL      UPITEM1
.
          ADD       ONE,UPDCOUNT       * Total records updated
.
USGCL480  GOTO      USGCL420
.
USGCL800  GOTO      USGCL100
.
USGCL999  RETURN
