            PACK      KEY5,ANST,ANSB,XIBAND2,SP70
            CALL      RDCODE1
            IF        OVRCD=1
              MOVE      "Invalid Theatre Class 2",ERRORMSG
              CALL      PERR0000
            ENDIF
