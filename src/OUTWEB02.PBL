          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      RSHS9999
RSHS9004  MOVE      CASEKEYS,KEY28
          CALL      UUOTBOA1
          MOVE      "Invalid Booking Status ",WEBTITLE
          IF        OBASTAT=4
            MOVE      "Error: Appointment has been Attended",WEBTITLE
          ENDIF
          IF        OBASTAT=5
            MOVE      "Error: Appointment has been Non-Attended",WEBTITLE
          ENDIF
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      RSHS9999
