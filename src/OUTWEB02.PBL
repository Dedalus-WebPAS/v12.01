          CALL      RDUSEA1
          IF        OVRCD=1
            UNPACK    SESSKEYS,OUSSITE,OUSCLIN,OUSDATE,OUSSTRT
            CALL      MOVUSE00              * Move cgi vars to file vars
            MOVE      SP70,OUSSPARE
            CALL      WRUSEA1
          ELSE
            CALL      MOVUSE00              * Move cgi vars to file vars
            CALL      UPUSEA1
          ENDIF

UPDUSG99  RETURN
