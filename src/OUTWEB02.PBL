                             "#"",PTELEB,"#",":                  *18
                             "#"",PTMXCELL,"#",":                *19
                             "#"",CASEKEYS,"#",":                *20
                             "#"",INTERNAM,"#",":                *21
                             "#"",SPECANAM,"#",":                *22
                             "#"",STATNAME,"#",":                *23
                             "#"",PATFOLDR,"#",":                *24
