          WRITE     HTMLFILE;"<RETURN_VALUE TYPE=SIMPLE>":
                    ONE,"|",KEY11,"|",ALRECTYP,"|",ALRECLID,"|",ALREVISN,"|":
                    ALREUHC4:
                    "</RETURN_VALUE>"
.
          GOTO      CHKLET99
.
CHKLET80  WRITE     HTMLFILE;"<RETURN_VALUE TYPE=SIMPLE>":
                    ZERO:
                    "</RETURN_VALUE>"
.
