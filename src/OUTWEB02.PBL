          CALL      RLOTSES1
          BRANCH    OVRCD,SUBSCH98,SUBSCH90
          SUB       TCFORM6,OSESLOTS
          MATCH     ANSN,TCINDC1
          IF        @EQUAL
            SUB       ONE,OSESSLTN
          ENDIF
          MATCH     ANSR,TCINDC1
          IF        @EQUAL
            SUB       ONE,OSESSLTR
          ENDIF
          MATCH     ANSS,TCINDC1
          IF        @EQUAL
            SUB       ONE,OSESSLTS
          ENDIF
          CALL      UPSESA1
          CALL      UUOTSES1
          MOVE      ZERO,EXIT
          GOTO      SUBSCH99
