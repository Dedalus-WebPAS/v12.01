            GOTO      FLST1000 IF NOT EQUAL
          ENDIF
.
          MATCH     SP70,SRCHLTYP
          IF        !@EQUAL
            MATCH     OTHELTYP,SRCHLTYP
            GOTO      FLST1000 IF NOT EQUAL
          ENDIF
.
          CALL      CHKSUS00
          BRANCH    EXIT,FLST1000
.
