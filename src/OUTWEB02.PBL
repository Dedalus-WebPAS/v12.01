                             "#"",OTBBASTM,"#",":                *10
                             "#"",OTBBCITM,"#",":                *11
                             "#"",OTBBDPTM,"#",":                *12
                             "#"",OBASTAT,"#",":                 *13
                             "#"",FLUPDESC,"#",":                *14
                             "#"",SHOWDNA,"#",":                 *15
                             "#"",ALERTSTR,"#",":                *16
