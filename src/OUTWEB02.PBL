          GOTO      CHKDNA90 IF NOT EQUAL
          COMPARE   FIVE,OBASTAT
          GOTO      CHKDNA10 IF NOT EQUAL
          IF        FIRSTFLG=0
            MOVE      ONE,FIRSTFLG
            IF        WARNINGF=0
              MOVE      ONE,WARNINGF
              WRITE     HTMLFILE;"<RETURN_VALUE TYPE=WARNING>";
            ENDIF
