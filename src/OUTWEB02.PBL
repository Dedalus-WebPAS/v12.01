          GOTO      FLST5100 IF EQUAL
          COMPARE   TWO,OBASTAT
          GOTO      FLST5100 IF EQUAL
.
          MATCH     OBAOUTNO,ZEROVISN
          IF        !@EQUAL
            MATCH     LASTOUTN,OBAOUTNO
            GOTO      FLST5100 IF EQUAL
          ENDIF
          MOVE      OBAOUTNO,LASTOUTN
