          CALL      RDSCODE1
CCOD2100  CALL      RDKCODE1
          BRANCH    OVRCD,CCOD9000
.
          MATCH     CATgb,TCODE
          GOTO      CCOD9000 IF NOT EQUAL
.
          MATCH     SP1,ACODE
          GOTO      CCOD2100 IF EQUAL
.
          MATCH     "A",PTCOACTV
          GOTO      CCOD2100 IF NOT EQUAL
.
