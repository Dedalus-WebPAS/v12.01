                               "<td bgcolor=magenta>",SARRDESC,"&nbsp;</td>":
                               "<td bgcolor=magenta>",OTBBCITM,"&nbsp;</td>":
                               "<td bgcolor=magenta>",OTBBASTM,"&nbsp;</td>":
                               "<td bgcolor=magenta>",OTBBDPTM,"&nbsp;</td>":
                               "<td bgcolor=magenta>",ENCADDED,"&nbsp;</td>":
