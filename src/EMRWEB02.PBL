              PACK      DISPCVAL,CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR:
                                 SP1,DISPLYAT,SP1,EMCHCVAL
            ENDIF
            GOTO      DITB8000
          ENDIF
.
          IF        F2=8
            UNPACK    EMCHOVAL,KEY10,CCENT,CYEAR,CMON,CDAY,HOUR,MIN,SEC
            MOVE      CMON,F2
            LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
