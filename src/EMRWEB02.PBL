          IF        @EQUAL
            MATCH     SP70,PTALREAC
            IF        !@EQUAL
              PACK      KEY5,PTALCATG,PTALREAC,SP70
              CALL      RDCODE1
              IF        OVRCD<>1
                WRITE     HTMLFILE;TDESC;
