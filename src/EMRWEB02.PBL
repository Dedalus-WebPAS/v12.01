              APPEND    "to current date",WEBTITLE
              RESET     WEBTITLE
              CALL      WEBERR00
              MOVE      ONE,EXIT
              GOTO      CHKDTE99
            ENDIF 
.
            IF        !@EQUAL
              GOTO      CHKDTE99            * If not today do not check time.
            ENDIF
.
          ENDIF
.
          MATCH     SP70,EMVITIME
          IF        !@EQUAL
            MATCH     SP70,EMVIDATE
            GOTO      CHKDTE99 IF EQUAL
.
            UNPACK    EMVITIME,HOUR,ANS,MIN
            PACK      DIM4,HOUR,MIN
            REP       " 0",DIM4
            MOVE      DIM4,FORM4
            COMPARE   FORM4,TODTIME
            IF        @LESS
