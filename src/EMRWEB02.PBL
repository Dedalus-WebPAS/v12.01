          MATCH     "1",KEY1
          IF        @EQUAL
            MATCH     SP70,PTALDATE
            GOTO      ADRC9999 IF EQUAL
            UNPACK    PTALDATE,CCENT,CYEAR,CMON,CDAY
            MOVE      CMON,F2
