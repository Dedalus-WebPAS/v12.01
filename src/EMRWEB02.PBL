          IF        @EQUAL           
            MATCH     SP70,EMVIATNS
            IF        @EQUAL
              GOTO      CHKWHO99
            ELSE
              MOVE      EMVINSDT,CTYPDATE 
              MOVE      EMVINSTM,CTYPTIME
              GOTO      CHKWHO99
            ENDIF
          ELSE
            MATCH     SP70,EMVIATNS
            IF        @EQUAL
              MOVE      EMVIDRDT,CTYPDATE 
              MOVE      EMVIDRTM,CTYPTIME
              GOTO      CHKWHO99
            ENDIF
          ENDIF

          PACK      KEY16,EMVIDRDT,EMVIDRTM,SP70
          REP       ":0",KEY16
          PACK      SAVKEY16,EMVINSDT,EMVINSTM,SP70
          REP       ":0",SAVKEY16
.
          MATCH     SAVKEY16,KEY16    * Check Who was first 
          IF        @LESS
            MOVE      EMVIDRDT,CTYPDATE 
            MOVE      EMVIDRTM,CTYPTIME
          ELSE
            MOVE      EMVINSDT,CTYPDATE 
            MOVE      EMVINSTM,CTYPTIME
          ENDIF 
.
