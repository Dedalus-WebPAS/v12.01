              IF        !@EQUAL
                MOVE      ANST,CLEXSTAT
                MOVE      ONE,CLEXTYPE
                MOVE      EMVIADMN,CLEXVISN
                PACK      KEY11,CLEXSTAT,CLEXTYPE,CLEXVISN
