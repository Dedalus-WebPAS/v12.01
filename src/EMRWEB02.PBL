          IF        !@EQUAL & !@EOS
            MATCH     EMVIDATE,DSMAEDAT
            IF        @LESS
              GOTO      DISVAL92
            ENDIF
          ENDIF
.
          MOVE      PURNO,DSPTURNO
          MOVE      DISPT002,DSPTDIID
          MOVE      DISPT001,DSPTCODE
