          APPEND    "emrweb02.pbl?reportno=1&",REDIRECT
          APPEND    "&template=",REDIRECT
          APPEND    TEMPLATE,REDIRECT
          APPEND    "&urnumber=",REDIRECT
          APPEND    URNUMBER,REDIRECT
          APPEND    "&admissno=",REDIRECT
          APPEND    ADMISSNO,REDIRECT
          APPEND    "&showmand=1",REDIRECT
          RESET     REDIRECT
