          BRANCH    OVRCD,WESIS200
.
          MATCH     URNUMBER,PMCDURNO
          GOTO      WESIS200 IF NOT EQUAL 
.
          MATCH     XDATE,PMCDEXDT
          IF        !@LESS
            PACK      KEY5,CATct,PMCDCTYP
            CALL      RDCODE1
            BRANCH    OVRCD,WESIS100
            MATCH     "7",TCINDC1
            IF        @EQUAL
              MOVE    PMCDCNUM,CARDNUMB
