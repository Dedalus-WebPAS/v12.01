          IF        OVRCD=1
.           MOVE      ZERO,SEQFLAG
            GOTO      ADDCOD05
          ENDIF
.
          MATCH     EMVCD001,EMVCVIST
          IF        !@EQUAL
.           MOVE      ZERO,SEQFLAG
            GOTO      ADDCOD05
          ENDIF
.
          MATCH     EMVCD005,EMVCMNCD
          GOTO      ADDCOD90 IF EQUAL
.
          MOVE      ZERO,FORM2
          MOVE      EMVCD004,FORM2
          COMPARE   EMVCCSYS,FORM2
          IF        @EQUAL
            MOVE      ONE,SEQFLAG
.           GOTO      ADDCOD05
          ENDIF
.
          GOTO      ADDCOD04
.
