          APPEND    "Patient has an inappropriate identifier for ",WEBTITLE
          APPEND    "this function.",WEBTITLE
          RESET     WEBTITLE
          CALL      WEBERR00
          MOVE      URNUMBER,KEY8
          CALL      UUPTMAS1
          MOVE      ONE,EXIT
          GOTO      ALLEX999
