          ENDIF
.
          IF        F2=10
            PACK      KEY5,ANSE,ANSD,EMCHOVAL,SP70
            CALL      RDCODE1
            IF        OVRCD=1
              PACK      DISPOVAL,EMCHOVAL,SP70,SP70
            ELSE
