          IF        @EQUAL
            CALL      MVACCREM            * Add record ACC Audit file
            CALL      DEWCOM1             * delete patwc1af record
          ENDIF
.
          MOVE      EMVIADMN,KEY8
          CALL      RDPMWX11
          COMPARE   ZERO,OVRCD
          CALL      DEPMWX11 IF EQUAL
.
