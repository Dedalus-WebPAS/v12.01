            IF        @EQUAL
              MOVE      ONE,MONTH2
              GOTO      MONNUM99  
            ENDIF
.
            MATCH     FEB,MONTH
            IF        @EQUAL
              MOVE      TWO,MONTH2
              GOTO      MONNUM99
            ENDIF
.
            MATCH     MAR,MONTH
            IF        @EQUAL
              MOVE      THREE,MONTH2
              GOTO      MONNUM99
            ENDIF
.
            MATCH     APR,MONTH
            IF        @EQUAL
              MOVE      FOUR,MONTH2
              GOTO      MONNUM99
            ENDIF
.
            MATCH     MAY,MONTH
            IF        @EQUAL
              MOVE      FIVE,MONTH2
              GOTO      MONNUM99
            ENDIF
.
            MATCH     JUN,MONTH
            IF        @EQUAL
              MOVE      SIX,MONTH2
              GOTO      MONNUM99
            ENDIF
.
            MATCH     JUL,MONTH
            IF        @EQUAL
              MOVE      SEVEN,MONTH2
              GOTO      MONNUM99
            ENDIF
.
            MATCH     AUG,MONTH
            IF        @EQUAL
              MOVE      EIGHT,MONTH2
              GOTO      MONNUM99
            ENDIF
.
            MATCH     SEP,MONTH
            IF        @EQUAL
              MOVE      NINE,MONTH2
              GOTO      MONNUM99
            ENDIF
.
            MATCH     OCT,MONTH
            IF        @EQUAL
              MOVE      TEN,MONTH2
              GOTO      MONNUM99
            ENDIF
.
            MATCH     NOV,MONTH
            IF        @EQUAL
              MOVE      TEN1,MONTH2
              GOTO      MONNUM99
            ENDIF
.
            MATCH     DEC,MONTH
            IF        @EQUAL
              MOVE      TEN2,MONTH2
              GOTO      MONNUM99
            ENDIF

