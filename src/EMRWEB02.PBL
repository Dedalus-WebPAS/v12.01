            IF        !@EQUAL
              CALL      WINP0000
            ENDIF
          ENDIF
.
          IF        EMVISTAT = 2|EMVISTAT = 3
            IF        CCCNSVHM = 1
              OPEN      CCIEX7A1,"cciex7af"
