          IF        @EQUAL | @EOS
            MOVE      SP70,TDESC
            GOTO      DITB8010
          ENDIF
.
          PACK      KEY5,ANSF,ANSW,EMCHREAS,SP70
          CALL      RDCODE1
          IF        OVRCD=1
            MOVE      EMCHREAS,TDESC
          ENDIF
