.
            IF        NZMCCHGT=1 & NZMCPATP=0
              MOVE      "1",NZEXPITM              * Pack item
              IF        SUBPACK=1
                 MOVE     "0",NZEXPITM
              ENDIF
              GOTO      CHKMSC99
