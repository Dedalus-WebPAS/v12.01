          GOTO      BJRN9999
.
BJRN7000  CALL      CANBJR00                * Cancel Batch
.
          MOVE      ZERO,EXIT
          GOTO      BJRN9999
.
BJRN8000  CLEAR     WEBTITLE
          MOVE      "Bulk Journal",WEBTITLE
          RESET     WEBTITLE
.
          CALL      WEBHED00
          BRANCH    EXIT,BJRN9999
          CALL      WEBBOD00
          CALL      WEBEND00
.
          MOVE      ONE,EXIT
          GOTO      BJRN9999
.
