          APPEND    "javascript:DeleteItm('",HTMLLNK3
          APPEND    PMMITRAN,HTMLLNK3
          APPEND    "')",HTMLLNK3
          RESET     HTMLLNK3
          MOVE      ONE,DELFLAG1
          ADD       ONE,FORM3
          MOVE      FORM3,KEY3
          REP       " 0",KEY3
