          PACK      KEY10,NZSPUNTH,SP70
          CALL      RDOPDEA3
          IF        OVRCD=0
            PACK      KEY17,NZSPPROC,OPDADATE,SP70
          ENDIF
          CALL      PATITMRD
          IF        OVRCD=1
            PACK      IDESC,SP70
          ENDIF
