          MATCH      SP70,DISPWACL
          IF         @EQUAL
            WRITE      HTMLFILE;"<td class=HeadingCell width=120>Ward</td>";
            COMPARE   "1",IBCNMHOS
            IF        @EQUAL
              WRITE    HTMLFILE;"<td class=HeadingCell width=120>Hospital</td>";
            ENDIF
          ELSE
            WRITE      HTMLFILE;"<td class=HeadingCell width=120>Ward</td>";
