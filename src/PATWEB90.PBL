          MOVE       ONE,STARTFLG
          WRITE      HTMLFILE;"<table width=100% border=1 cellspacing=0 ":
                            "cellpadding=1 style=#"table-layout: fixed;#">"
.
          MOVE       ZERO,FORM1
.
          WRITE      HTMLFILE;"<tr>":
                              "<td class=HeadingCell width=200>Patient</td>":
                              "<td class=HeadingCell width=35>Type</td>";
