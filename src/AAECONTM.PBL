          MOVE      Z70,AAECN004
          MOVE      Z70,AAECN005
          MOVE      Z70,AAECN006
          MOVE      Z70,AAECN007
          MOVE      Z70,AAECN008
          MOVE      Z70,AAECN009
          MOVE      Z70,AAECN010
          MOVE      Z70,AAECN011
          MOVE      Z70,AAECN012
          MOVE      Z70,AAECN013
          MOVE      Z70,AAECN014
          MOVE      Z70,AAECN015
