                    *P13:24,"GL Sub: ",*V2LON,*P21:24,FROMGL,*HOFF:
                    *P35:24,"Comm/Subj.: ",*V2LON,*P47:24,FROMSIN
          IF        REPTSEQ=1
            CALL      REPGL000     * GL Report Sequence
          ELSE
            CALL      REPCS000     * Commodity/Subjective Sequence
          ENDIF
.
