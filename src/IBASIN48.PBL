          MOVE        "ME ",SICDCATA
          CALL        KSICDB00
          BRANCH      EXIT,EMESS900,EMESS999
          MOVE        SICDCODE,SIPAMES
EMESS900  MOVE        SP70,MESSDESC
          MATCH       SP70,SIPAMES
          GOTO        EMESS999 IF EQUAL
.
          MOVE        "ME ",SICDCATA
          PACK        KEY6,SICDCATA,SIPAMES,SP70
          CALL        RDSICD1
          IF          OVRCD=0
            MOVE        SICDDESC,MESSDESC
          ENDIF
