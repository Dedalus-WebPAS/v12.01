                GOTO      PTRN1000 IF NOT EQUAL
              ELSE
                GOTO      PTRN1000
              ENDIF
            ENDIF
          ENDIF
.
          MOVE      TMOVE,STMOVE
          RESET     TDATE
          REP       " 0",TDATE
          RESET     TDATE
.
          MOVE      TDATE,XDATE1
          MOVE      TDATE,LSTDATE
.
          MATCH     ANSY,PTMICMXP
          IF        @EQUAL
            BRANCH    ACDAYCS,PTRN1100    * sameday, no stepdown
            MOVE      KTTDATE,SAVEDATE   * save todate as PATCMSTP changes it
            CALL      PATCMSTP
            MOVE      SAVEDATE,KTTDATE
            GOTO      PTRN1100
          ENDIF
.
          IF         CSTDOWN = 1
            CALL      STEPDOWN
          ENDIF
.
PTRN1100  COMPARE   ZERO,ENDFLAG
          GOTO      PTRN1200 IF EQUAL
.
          MATCH     LSTDATE,TDATE
          GOTO      PTRN2100 IF EQUAL
.
PTRN1200  CMATCH    "A",TMOVE
          GOTO      PTRN3500 IF EQUAL
.
          MATCH     ADATE,DDATE
          IF        !@EQUAL
            CMATCH    "R",TMOVE
            GOTO      PTRN3500 IF EQUAL
            CMATCH    "L",TMOVE
            GOTO      PTRN2500 IF EQUAL
          ENDIF
.
          CMATCH    "C",TMOVE
          GOTO      PTRN2500 IF EQUAL
.
