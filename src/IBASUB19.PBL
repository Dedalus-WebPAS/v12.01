          PACK      KEY5,SIQACST,SP70
          CALL      RDSICA1
          ADD       "1",CLNO
          PRINT     "Cost Centre : ",SIQACST,SP2,SICADESC
.
          CALL      UND132                       * print line
          PRINT     "Req No",*10,"Catalog Description",*75,"Part Number":
                    *103,"   Quantity",*116,"Unit",*N:
                    *10,"Status",*40,"User":
                    *75,"Date",*90,"Time",*100,"Port"
          ADD       "2",CLNO
          CALL      UND132                       * print line
.
HEAA9999  RETURN
.********************************************************************
. WRIA - print data line                            Called by PRNA
.********************************************************************
WRIA0000
          COMPARE   "52",CLNO                    * end of page ?
          CALL      HEAA0000 IF NOT LESS         * start new page
.
          MOVE      SP70,SIIADES
          MATCH     SIQACAT,SP70
          IF        @EQUAL
            PACK    SIIADES,SIQAPD,SP70
          ELSE
            PACK      KEY7,SIQACAT,SP70
            CALL      RDSIIA1
          ENDIF
          MOVE      SIIADES,KEY55
.
          MOVE      "999,999,999",KEY11
          MOVE      SIQAQTY,CONTQTY
          FEDIT     CONTQTY,KEY11
.
          UNPACK    SIQADAT,CCENT,CYEAR,CMON,CDAY
          CALL      PACDATE
.
          MOVE      SP70,SECUSER
          PACK      KEY4,SIQAUID,SP70
          CALL      RDPASS1
.
          PRINT     SIQARID,*10,SIQACAT,SP1,KEY55,*75,SIQAPN:
                    *103,KEY11,*116,SIQAUNT,*N:
                    *10,SIQASTA,*40,SECUSER:
                    *75,CPCDATE,*90,SIQATIM,*100,SIQAPOR
          ADD       "2",CLNO           * add one to line number
.
          MOVE      "Notes : ",KEY8
          PACK      KEY14,SIQACST,SIQARID,SP70
          PACK      KEY16,SIQACST,SIQARID,SP70
          CALL      RSSIQB1
.
WRIA1000
          CALL      RKSIQB1
          BRANCH    OVRCD,WRIA9000
          PACK      KEY16,SIQBCST,SIQBRID,SIQBLIN,SP70
          MATCH     KEY16,KEY14
          GOTO      WRIA9000 IF NOT EQUAL
.
          COMPARE   "55",CLNO                    * end of page ?
          CALL      HEAA0000 IF NOT LESS         * start new page
          PRINT     *10,KEY8,SIQBCOM
          MOVE      SP70,KEY8
          ADD       "1",CLNO           * add one to line number
          GOTO      WRIA1000
.
WRIA9000  PRINT     SP1
          ADD       "1",CLNO           * add one to line number
.
WRIA9999  RETURN
.----------------------------------------------------------------------
. Keyin extraction type
.----------------------------------------------------------------------
KEXTR000
          PACK      DISPMSG,SP70,SP70
          CALL      MESSAGE2
.
KEXTR100
          DISPLAY   *P1:24,*V2LON,"0",*HOFF,"=All, ":
                           *V2LON,"1",*HOFF,"=Stocked, ":
                           *V2LON,"2",*HOFF,"=Not Stocked, ":
                           *V2LON,"3",*HOFF,"=Not Cataloged",*EL;
.
          KEYIN     *PCCOL:CVERT,*DV,SP1:
                    *PCCOL:CVERT,*V2LON,DIM1;
.
          MATCH     EXITCHAR,DIM1
          GOTO      KEXTR950 IF EQUAL
.
          MOVE      DIM1,EXTTYPE
          IF        EXTTYPE=0
            MOVE      "All                                     ",EXTDESC
            GOTO      KEXTR900
          ENDIF
.
          IF        EXTTYPE=1
            MOVE      "Stocked                                 ",EXTDESC
            GOTO      KEXTR900
          ENDIF
.
          IF        EXTTYPE=2
            MOVE      "Not Stocked                             ",EXTDESC
            GOTO      KEXTR900
          ENDIF
.
          IF        EXTTYPE=3
            MOVE      "Not Cataloged                           ",EXTDESC
            GOTO      KEXTR900
          ENDIF
.
          DISPLAY   *P1:24,*B,*EL,"Invalid Option - ";
          CALL      HITENTER
          GOTO      KEXTR100
.
KEXTR900  MOVE      ZERO,EXIT                    * entry ok
          GOTO      KEXTR999
.
KEXTR950  MOVE      TWO,EXIT                     * quit screen
.
KEXTR999
          CALL      MESSAGE3
          RETURN
.----------------------------------------------------------------------
. Keyin From Date
. Returns  : FROMDATE (Date entered)
.            FROMDATD (Date description)
.----------------------------------------------------------------------
KDATF000
          PACK      FROMDATE,CCC,CYY,CMM,SP70         * default to blank date
          UNPACK    FROMDATE,CCENT,CYEAR,CMON,CDAY    * set up date parameters
          CALL      KEYDATE                           * keyin date
.
          MATCH     SP70,CDAY
          IF        !@EQUAL & OVRCD=0
            PACK      FROMDATE,CCENT,CYEAR,CMON,CDAY  * pack desired date
            CALL      PACDATE                         * get date description
            PACK      FROMDATD,CPCDATE,SP70
          ELSE
            PACK      FROMDATE,SP70                   * default to start date
            PACK      FROMDATD,START,SP70             * get date description
          ENDIF
.
          MOVE      ZERO,EXIT
.
KDATF999  RETURN
.----------------------------------------------------------------------
. Keyin To Date
. Returns  : TODATE (Date entered)
.            TODATD (Date description)
.----------------------------------------------------------------------
KDATT000
          PACK      TODATE,CCC,CYY,CMM,SP70           * default to blank date
          UNPACK    TODATE,CCENT,CYEAR,CMON,CDAY      * set up date parameters
          CALL      KEYDATE                           * keyin date
.
          MATCH     SP70,CDAY
          IF        !@EQUAL & OVRCD=0
            PACK      TODATE,CCENT,CYEAR,CMON,CDAY    * pack desired date
            CALL      PACDATE                         * get date description
            PACK      TODATD,CPCDATE,SP70
          ELSE
            PACK      TODATE,Z70                      * default to start date
            PACK      TODATD,END,SP70                 * get date description
          ENDIF
.
          MOVE      ZERO,EXIT
.
KDATT999  RETURN
