.  NREA - Get next record                              Called By PRNA
.**********************************************************************
NREA0000  BRANCH    OPTIONA,NREA0100,NREA9999,NREA9999,NREA9999:
                            NREA9999,NREA9999,NREA9999,NREA9999
.
.---- print by type 1 ----
.
NREA0100  CALL      RKSIQA1
          BRANCH    OVRCD,NREA9500
.
          MATCH     SIQARID,COSTCOD
          GOTO      NREA9500 IF LESS             * out of range ?
.
          MATCH     SIQADAT,TODATE               * check Date range
          GOTO      NREA0100 IF LESS
          MATCH     FROMDATE,SIQADAT
          GOTO      NREA0100 IF LESS
.
          MATCH     SIQACST,COSTCOD              * check Cost Centre range
          GOTO      NREA0100 IF LESS
          MATCH     COSFCOD,SIQACST
          GOTO      NREA0100 IF LESS
.
          MATCH     SIQASTA,STATCOD              * check Status range
          GOTO      NREA0100 IF LESS
          MATCH     STAFCOD,SIQASTA
          GOTO      NREA0100 IF LESS
.
.---- all ----
.
          IF        EXTTYPE=0
            GOTO      NREA9000
          ENDIF
.
.---- stocked ----
.
          IF        EXTTYPE=1
            PACK      KEY7,SIQACAT,SP70
            CALL      RDSIIA1
            BRANCH    OVRCD,NREA0100
.
            COMPARE   ZERO,SIIANON
            GOTO      NREA9000 IF EQUAL
          ENDIF
.
.---- not stocked ----
.
          IF        EXTTYPE=2
            PACK      KEY7,SIQACAT,SP70
            CALL      RDSIIA1
            BRANCH    OVRCD,NREA0100
.
            COMPARE   ONE,SIIANON
            GOTO      NREA9000 IF EQUAL
          ENDIF
.
.---- not cataloged ----
.
          IF        EXTTYPE=3
            PACK      KEY7,SIQACAT,SP70
            CALL      RDSIIA1
            BRANCH    OVRCD,NREA9000
          ENDIF
.
          GOTO      NREA0100
.
NREA9000  MOVE      ZERO,OVRCD         * continue
          GOTO      NREA9999
.
NREA9500  MOVE      ONE,OVRCD          * quit
.
NREA9999  RETURN
