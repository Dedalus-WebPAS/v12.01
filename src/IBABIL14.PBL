.
CHKTB2    ENDSET    TFENDMB1
          APPEND    SP5,TFENDMB1
          RESET     TFENDMB1
.
          PACK      KEY5 WITH CODETB,TFENDMB1
          CALL      RDCODE1
          BRANCH    OVRCD OF INVTB2
.
          MOVE      TDESC,TBDESC1
          DISPLAY   *P26:16,*EL,*V2LON,TFENDMB1,*HOFF,*P35:16,TBDESC1;
          MOVE      TFENDMB1,SAVMBS1
          MOVE      MODE1,MODE
          GOTO      REKEY3
.
INVTB2    DISPLAY   *P1:24,*EL,*B,"Invalid Theatre Band. ";
          CALL      HITENTER
          GOTO      TLDAY
.
