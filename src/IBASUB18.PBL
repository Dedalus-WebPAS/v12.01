                           *V2LON,"1",*HOFF,"=Cataloged Only, ":
                           *V2LON,"2",*HOFF,"=Not Cataloged Only",*EL;
.
          KEYIN     *PCCOL:CVERT,*DV,SP1:
                    *PCCOL:CVERT,*V2LON,EXTTYPE;
.
          IF        EXTTYPE=0
            MOVE      "All                                     ",EXTDESC
            GOTO      KEXTR900
          ENDIF
