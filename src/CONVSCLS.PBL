          REP       " 0",SNAPFTSH
.
          TYPE      SNAPFTSH
          IF        !@EQUAL
            MOVE      "PMSCFTSH",TMPFIELD
            CALL      NERR0000
          ENDIF

          MOVE      ZERO,FORM3B
          MOVE      SNAPFTSH,FORM3B
          MOVE      FORM3B,SNAPFTSH
