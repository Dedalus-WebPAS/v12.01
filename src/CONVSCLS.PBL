          CALL      NERR0000
.
VNUM0800  REP       " 0",SNAPFTSL
.
          TYPE      SNAPFTSL
          IF        !@EQUAL
            MOVE      "PMSCFTSL",TMPFIELD
            CALL      NERR0000
          ENDIF
.
          MOVE      ZERO,FORM3A
          MOVE      SNAPFTSL,FORM3A
          MOVE      FORM3A,SNAPFTSL
