          PRINT     *1,PIPE,*3,SNAPCLCD,*13,PIPE,*15,SNAPVERS:
                    *28,PIPE,*30,SNAPICDE,*42,PIPE,*44,SNAPDEFF:
                    *59,PIPE,*61,ERORDESC,*132,PIPE
          ADD       ONE,CLNO                     * increment page line count
.
