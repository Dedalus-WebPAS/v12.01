.******************************************************************************
INP41000  
...MATCH     PTCNGDV2,GRPDTE
...       GOTO      INP41100 IF LESS        * Grouper date < ICD10v2 golive date
.
          MOVE      "drginp",KEY6
          PACK      KEY8,KEY6,PORT
          PREP      INPFIL41,KEY8
          GOTO      INP41900
.
...INP41100  MOVE      "dinp09",KEY6
...          PACK      KEY8,KEY6,PORT
...          PREP      INPFIL9,KEY8
.
...          MOVE      "dinp10",KEY6
...          PACK      KEY8,KEY6,PORT
...          PREP      INPFIL41,KEY8
.
INP41900  MOVE      ZERO,EXIT
INP41999  RETURN
