            CALL      RSPMMPR1                 * Add into Current File
            CALL      RPPMMPR1
            ADD       ONE,PMMPCOUN
            MOVE      ZERO,PMMPPAMT
            MOVE      ZERO,PMMPRAMT
          ENDIF
.
          MOVE      PMMPCOUN,PMMPR005
.
          CALL      UPMPRC00
          PACK      KEY28,PMMPCNID,PMMPCCOD,PMMPHFUN,PMMPHFTY,PMMPMPRC:
                          PMMPCOUN,SP70
          CALL      RAPMMPR1
          IF        OVRCD=1
            CALL      WRPMMPR1
          ELSE
            MATCH     "1",HEADERRC
            GOTO      MLPROC92 IF EQUAL
          ENDIF
          GOTO      MLPROC98
