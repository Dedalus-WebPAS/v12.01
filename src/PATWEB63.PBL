          MOVE      ZERO,NZCMCCNT
          MATCH     "1",HEADERRC
          IF        !@EQUAL
            PACK      KEY26,NZCMT001,NZCMT002,NZCMT003,NZCMT004,NZCMT005,Z70
            CALL      RSNZCMT1                 * Add into Current File
            CALL      RPNZCMT1
