          GOTO      CASM9999
.
CASM0900  MATCH     Z70,PMCMT063
          GOTO      CASM9999 IF EQUAL
.
          WRITE     HTMLFILE;PMCMT063;
          GOTO      CASM9999
.
CASM1000  CALL      TABRUL00               * Table of rules
          GOTO      CASM9999
.
