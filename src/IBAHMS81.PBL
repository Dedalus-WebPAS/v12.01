            UNPACK    DSCSCORE,XXDSS01,XXDSS02,XXDSS03,XXDSS04:
                               XXDSS05,XXDSS06,XXDSS07,XXDSS08:
                               XXDSS09,XXDSS10,XXDSS11,XXDSS12
            MOVE      TMBDAYS,XXBDAYS
            MOVE      TMPTLOS,XXPTLOS
            CALL      WRTEMP1
          ENDIF
