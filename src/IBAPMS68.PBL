          ELSE
            MOVE      YFLD0001,VINFIELD
            MOVE      50,VINTLEN
            MOVE      "Equipment Description",VFLDNAME
            CALL      CCHLEN00
          ENDIF
.
          MATCH     SP70,XFLD0003
          IF        !@EQUAL
            MOVE      XFLD0003,VINFIELD
            MOVE      3,VINTLEN
            MOVE      "Equipment Type",VFLDNAME
            CALL      CCHLEN00
            IF        VERRFLAG = 0
              MOVE      "Equipment Type. Category OE",VFLDNAME
              MOVE      "OE",DIM2
              MOVE      XFLD0003,DIM3
              CALL      CCATCH00
            ENDIF
          ENDIF
.
          MATCH     SP70,XFLD0004
          IF        !@EQUAL
            MOVE      XFLD0004,VINFIELD
            MOVE      3,VINTLEN
            MOVE      "Current Status",VFLDNAME
            CALL      CCHLEN00
            IF        VERRFLAG = 0
              MOVE      "Current Status. Category ER",VFLDNAME
              MOVE      "ER",DIM2
              MOVE      XFLD0004,DIM3
              CALL      CCATCH00
            ENDIF
          ENDIF
.
