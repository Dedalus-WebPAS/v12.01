.------------------------------------------------------------------------------
.         GTID0000  - get extract Id
.------------------------------------------------------------------------------
.
GTID0000  RESET     EXTID_56
          KEYIN     *P1:11,*EL,"Extract Id : ",*V2LON,EXTID_56;
.
          MATCH     SP70,EXTID_56
          GOTO      GTID9100 IF EOS
.
          CALL      CONTQST   
          BRANCH    CEXIT,GTID9000,GTID0000,GTID9100
          GOTO      GTID9000
.
GTID9000  MOVE      ZERO,EXIT
          GOTO      GTID9999
.
GTID9100  MOVE      ONE,EXIT
          GOTO      GTID9999
. 
GTID9999  RETURN
