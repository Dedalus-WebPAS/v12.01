.
          CALL      BBWE0000            * Get OS Invoice balance from patinvrf
.
          PACK      KEY8,MAVINVCE,SP70
          CALL      RDINV1
          BRANCH    OVRCD,WBBS9400
.
          PACK      KEY8,PINVADM,SP70
          CALL      RDEMVIS1
          BRANCH    OVRCD,WBBS9700
.
          GOTO      WBBS5500
.
