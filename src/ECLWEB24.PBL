            MOVE      SP70,WBBSNBAT
            CALL      IBACLOCK
            PACK      WBBSUDAT,CCC,CYY,CMM,CDD
            REP       " 0",WBBSUDAT
            MOVE      CTIMEIS,WBBSUTIM
            MOVE      USERID,WBBSUUID
            CALL      UPWBBBS1
          ENDIF
.
.         Delete the selected record
.
WDBB1000  PACK      KEY30,TEMPHOSP,SP1,ZERO,TEMPMODL,TEMPADMN,TEMPINVN,TEMPBATN
          CALL      DEWBBBS1
