          MOVE      WBIMURNO,TEMPURNO
          MOVE      WBIMHFND,TEMPHFND
          MOVE      WBIMPBAT,TEMPPBAT
          MOVE      WBIMHOSP,TEMPHOSP
.
          MATCH     SP70,TEMPPBAT
          GOTO      WDEL1000 IF EQUAL
          GOTO      WDEL1000 IF EOS
.
          PACK      KEY35,TEMPHOSP,TWENTY1,TEMPHFND,TEMPADMN,TEMPINVN,TEMPPBAT,SP70
          CALL      RDWBIMC1
          IF        OVRCD = 1
            GOTO      WDEL7000
          ELSE
