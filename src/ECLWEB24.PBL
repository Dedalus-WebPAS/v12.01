          CALL      DELETR00
.
          MOVE      "10",TTYPEARG
          CALL      DELETR00
.
          MOVE      " 4",TTYPEARG
          CALL      DELETR00
.
          MOVE      " 5",TTYPEARG
          CALL      DELETR00
.
          CALL      DELECO00
.
          MOVE      ZERO,EXIT
          GOTO      WIMC9999
.
