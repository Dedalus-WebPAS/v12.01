          IF        (FORM7P2O <= 0) & (PINVAMT > 0)
            GOTO      RSBE9000
          ENDIF
.
          PACK      KEY17,EMECINVN,SP8,ONE,SP70
          CALL      RAEMECT2                     * claim already waiting ?
          IF        OVRCD = 0
            GOTO      RSBE9000
          ENDIF
.
          GOTO      RSBE9999
