.----------------------------------------------------------------------
. Keyin Spacing
.----------------------------------------------------------------------
KSPAC000
          MOVE      "S",SPACING
          MATCH     "G",DETAILED
          IF        @EQUAL
            KEYIN     *PCCOL:CVERT,*V2LON,*RV,SPACING;
            REP       UPPLOW,SPACING
            PACK      ANS,SPACING,SP70
            MATCH     SP70,ANS
            GOTO      KSPAC950 IF EQUAL
            MATCH     EXITCHAR,ANS
            GOTO      KSPAC950 IF EQUAL
.
            REP       "1x2xD1S1",ANS
            MOVE      ZERO,F1
            MOVE      ANS,F1
            BRANCH    F1,KSPAC900
            BEEP
            GOTO      KSPAC000
          ELSE
            IF        ACCEPT=0
              BEEP
              CLEAR     DISPMSG
              APPEND "Only Single Spacing For a Comprehensive Report - ",DISPMSG
              CALL      MESSAGE1
            ENDIF
          ENDIF
.
KSPAC900  MOVE     ZERO,EXIT
          GOTO     KSPAC999
.
KSPAC950  MOVE     TWO,EXIT
.
KSPAC999  RETURN
