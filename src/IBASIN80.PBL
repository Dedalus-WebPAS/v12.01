          CALL      RPSIIA3
          GOTO      FILE9999
.
.---- print by type 3 ----
.
FILE0300  PACK      KEY12,FROMCOD7,SP70
          CALL      RSSIIA2
          CALL      RPSIIA2
          GOTO      FILE9999
.
.---- print by type 4 ----
.
FILE0400  PACK      KEY15,FROMRACK,SP70
          CALL      RSSIIE3
          CALL      RPSIIE3
