          GOTO      P13TSUB IF NOT EQUAL
.
          BRANCH    OPTION OF PRSUB1,PRSUB1,PRSUB1,PRSUB2,PRSUB2:
                              PRSUB3,PRSUB3,ENDSUB1,PRSUB4,PRSUB4
          GOTO      PRSUB1
.
