.    PRINT SUB TOTALS
.
PRNTSUB   MOVE      "DT",TCODE
          CLEAR     TDESC
          APPEND    "UNKNOWN ",TDESC
          APPEND    DOCTOR,TDESC
          APPEND    " TYPE",TDESC
          APPEND    SP20,TDESC
          RESET     TDESC
          MATCH     SP3,SDRTYPE
          GOTO      UNKDC IF EQUAL
.
          PACK      KEY5 WITH TCODE,SDRTYPE
          CALL      RDCODE1
UNKDC     MOVE      DRTYPE,SDRTYPE
          MOVE      ZERO,SNAVDAY
.
          COMPARE   ZERO,STYADMNS
          GOTO      PRTTSUB IF EQUAL
.
          MOVE      STYBEDS,STAVDAY
          DIV       STYADMNS,STAVDAY
          MOVE      STAVDAY,SNAVDAY
.
