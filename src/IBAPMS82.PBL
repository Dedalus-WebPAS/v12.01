.******************************************************************************
.            CHKWRD00 - Checking range of ward
.******************************************************************************
CHKWRD00  MOVE      ZERO,EXIT
          MATCH     SP3,WARDFROM
          IF        !@EQUAL
            MOVE      WARDFROM,WARDFPRT
            MATCH     WARDFROM,XWARD
            GOTO      CHKWRD90 IF LESS
          ELSE
            MOVE      "Start ",WARDFPRT
          ENDIF
.
          MATCH     "zzz",WARDTO
          IF        @EQUAL
            MOVE      "Finish",WARDTPRT
            GOTO      CHKWRD99
          ELSE
            MOVE      WARDTO,WARDTPRT
            MATCH     XWARD,WARDTO
            GOTO      CHKWRD99 IF NOT LESS
          ENDIF
