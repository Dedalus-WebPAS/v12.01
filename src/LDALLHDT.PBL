          GOTO      CHKS0500                     * check next status
.
CHKS9000  MOVE      ZERO,EXIT
          GOTO      CHKS9999
.
CHKS9100  MOVE      SP70,PRINVISN
          MOVE      SP70,PRINENCT
          MOVE      SP70,PRINAUDD
          MOVE      SP70,PRINAUDT
          MOVE      "ERROR - Data already loaded for date range",PRINDESC
          CALL      PERR0000
.
          DISPLAY   *P1:24,*EL,*B,"ERROR - Data loaded for date range. ";
          CALL      HITENTER
.
          MOVE      ONE,EXIT
          GOTO      CHKS9999
.
