          IF        @EQUAL
            MOVE      ONE,YESSCONO           * Yes Consent Obtained
          ENDIF
.
          MATCH     "M",TCINDC2
          IF        @EQUAL
            MOVE      ONE,YESSMEDH           * Yes Medical History Received
          ENDIF
.
          GOTO      OBSV1000
