          BRANCH    OVRCD,DSIIA200
          MATCH     SIIACAT,SIIECAT
          GOTO      DSIIA200 IF NOT EQUAL
.
          IF        SIIESOH>0
            DISPLAY   *P1:24,"Catalog has Stock on Hand - W/h ",SIIEWAR," - ";
            CALL      HITENTER
            GOTO      DSIIA900
          ENDIF
.
          IF        SIIESOO>0
            DISPLAY   *P1:24,"Catalog has Stock on Order - W/h ",SIIEWAR," - ";
            CALL      HITENTER
            GOTO      DSIIA900
          ENDIF
.
          PACK      KEY12,SIIECAT,SIIEWAR,SP70
          CALL      SISIWK1
          IF        OVRCD=0
            DISPLAY   *P1:24,"Catalog set for Stocktake - W/h ",SIIEWAR," - ";
            CALL      HITENTER
            GOTO      DSIIA900
          ENDIF
          GOTO      DSIIA100
.
DSIIA200
          PACK      KEY25,SIIACAT,SP70
          DISPLAY   *P1:24,"Checking : ",*V2LON,"SIBA",*EF;
.
DSIIA300
