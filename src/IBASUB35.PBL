              CALL      MESSAGE4
              BRANCH    EXIT,DSIIA900
              GOTO      DSIIA550
            ENDIF
.
            IF        1=SIITTYP
              BEEP
              CLEAR     DISPMSG
              APPEND    "Stock Adjustment Record Found - ",DISPMSG
              RESET     DISPMSG
              CALL      MESSAGE4
              BRANCH    EXIT,DSIIA900
              GOTO      DSIIA550
            ENDIF
.
            IF        2=SIITTYP
              BEEP
              CLEAR     DISPMSG
              APPEND    "Stocktake Adjustment Record Found - ",DISPMSG
              RESET     DISPMSG
              CALL      MESSAGE4
              BRANCH    EXIT,DSIIA900
              GOTO      DSIIA550
            ENDIF
.
            IF        3=SIITTYP
              BEEP
              CLEAR     DISPMSG
              APPEND    "Stocktake Balance Record Found - ",DISPMSG
              RESET     DISPMSG
              CALL      MESSAGE4
              BRANCH    EXIT,DSIIA900
              GOTO      DSIIA550
            ENDIF
.
            UNPACK      SIITDAT,CCENT,CYEAR,CMON,CDAY
            CALL        PACDATE
            DISPLAY    *P1:24,"Item history record exists for ",CPCDATE," - ";
            CALL       HITENTER
            GOTO       DSIIA900
