          IF        STARTNUM>=RECNUMB
            GOTO      REFN0010
          ENDIF
.
          CALL      RFRW0000                * Display Of Rows
          ADD       ONE,DISNUMB
          COMPARE   NORECORD,DISNUMB
          GOTO      REFN0010 IF NOT EQUAL
.
