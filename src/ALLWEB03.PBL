          PACK      KEY16,PMENVISN,PMENENCT,SP70
          CALL      RAPMENC1
          IF        OVRCD=1
            CALL      WRPMENC1 
          ELSE
            GOTO      IPCONT10
          ENDIF
.
          MOVE      ZERO,EXIT
          GOTO      IPCONT99
.
