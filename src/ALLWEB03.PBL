             MATCH     WBSEDALL,ALREDEPT
             GOTO      WRSE0016 IF EQUAL
             GOTO      WRSE0015
          ELSE
             MATCH     WBSEDALL,ALREDEPT
             GOTO      WRSE0016 IF EQUAL
             GOTO      WRSE0010
          ENDIF
