.------------------------------------------------------------
. Set Medical Warning Date
.------------------------------------------------------------
SETMWD00  REP       " 0",PTMWDATE
          UNPACK    PTMWDATE,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,F2
          LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
          MATCH     "00",CDAY
          IF        @EQUAL
            MOVE      SP70,CDAY
          ENDIF
          MATCH     "00",CMON
          IF        @EQUAL
            MOVE      SP70,MTHNAM3
          ENDIF
          PACK      KEY4,CCENT,CYEAR
          MATCH     "0000",KEY4
          IF        @EQUAL
            MOVE      SP70,CCENT
            MOVE      SP70,CYEAR
          ENDIF
          RETURN
