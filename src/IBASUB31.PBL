.---------------------------------------------------------------------------
. KYEAR - keyin year
.---------------------------------------------------------------------------
KYEAR000  CALL      RINPECON           * default to current year
          MOVE      SIPECYR,SIPEYEAR
          KEYIN     *PCCOL:CVERT,*V2LON,*RV,*JR,SIPEYEAR
          PACK      SIPEYEAR,SIPEYEAR,SP70
.
          MATCH     SP70,SIPEYEAR
          GOTO      KYEAR990 IF EQUAL
          PACK      KEY4,SP3,EXITCHAR * exitchar entered ?
          MATCH     KEY4,SIPEYEAR
          GOTO      KYEAR990 IF EQUAL
.
          TYPE      SIPEYEAR           * valid numeric ?
          GOTO      KYEAR000 IF NOT EQUAL
.
          MOVE      SIPEYEAR,SIPEYF4
          IF        SIPEYF4<100
            SQUEEZE   SIPEYEAR
            MOVE      SIPEYEAR,FORM2
            PACK      SIPEYEAR,CCC,FORM2,SP70
          ENDIF
.
          REP       " 0",SIPEYEAR
          MOVE      ZERO,EXIT          * year ok
          GOTO      KYEAR999
.
KYEAR990  MOVE      TWO,EXIT           * no year entered
.
KYEAR999  RETURN
.---------------------------------------------------------------------------
. KPERD - keyin period
.         Returns SIPEPER - Period
.                 EXIT    - 0=ok, 1=nothing entered, 2=exitchar
.---------------------------------------------------------------------------
KPERD000  KEYIN     *PCCOL:CVERT,*V2LON,*RV,*JR,SIPEPER
          PACK      SIPEPER,SIPEPER,SP70
.
          PACK      KEY2,SP1,EXITCHAR
          MATCH     KEY2,SIPEPER
          GOTO      KPERD995 IF EQUAL
.
          MOVE      ZERO,F2
          MOVE      SIPEPER,F2
          COMPARE   ZERO,F2            * no period entered ?
          GOTO      KPERD990 IF EQUAL
.                                      * period greater than total no. of perds
          IF        F2>SIPENUM
            BEEP
            MOVE      "Invalid Period - ",DISPMSG
            CALL      MESSAGE1
            GOTO      KPERD000
          ENDIF
.
          MOVE      F2,SIPEPER         * check if period exists
          REP       " 0",SIPEPER
          PACK      KEY6,SIPEYEAR,SIPEPER,SP70
          REP       " 0",KEY6
          CALL      RDSIPE1
          IF        OVRCD=1
            BEEP
            MOVE      "Period NOT Created - ",DISPMSG
            CALL      MESSAGE1
            GOTO      KPERD000
          ENDIF
.
          MOVE      ZERO,EXIT          * period ok
          GOTO      KPERD999
.
KPERD990  MOVE      ONE,EXIT           * no period entered
          GOTO      KPERD999
.
KPERD995  MOVE      TWO,EXIT           * EXITCHAR entered
.
KPERD999  RETURN
.------------------------------------------------------------------------------
. REPT - Print Out Report
.------------------------------------------------------------------------------
REPT0000  OPEN      CONTROLF,"controlf"
          READ      CONTROLF,SIXTY3;*143,HNONUSAG
          PACK      DAT6,SIPEYEAR,SIPEPER,SP70
.
          PACK      DISPMSG,SP70,SP70                 * save line 24
          CALL      MESSAGE2
.
          MATCH     FCATALOG,TCATALOG
          IF        @LESS
            DISPLAY   *P1:24,*B,*EF,"Illegal Catalog Range - ";
            CALL      HITENTER
            GOTO      REPT9500
          ENDIF
.
          MATCH     FPRODGP,TPRODGP
          IF        @LESS
            DISPLAY   *P1:24,*B,*EF,"Illegal Product Group Range - ";
            CALL      HITENTER
            GOTO      REPT9500
          ENDIF
.
          MATCH     FDESCRP,TDESCRP
          IF        @LESS
            DISPLAY   *P1:24,*B,*EF,"Illegal Description Range - ";
            CALL      HITENTER
            GOTO      REPT9500
          ENDIF
.
.
          DISPLAY   *P1:24,*EL,"Processing - ";
          CALL      MAKEXP00                          * Create Export File
          MOVE      "599",CLNO                        * Set up print values
          MOVE      "0",CPAGENO
          CLOCK     TIME,CTIMEIS
.
          MOVE      SIPEYEAR,F4
          SUB       ONE,F4
          MOVE      F4,LASTYEAR
          REP       " 0",LASTYEAR
.
          MOVE      ZERO,ATOTAL01
          MOVE      ZERO,ATOTAL02
          MOVE      ZERO,ATOTAL03
          MOVE      ZERO,ATOTAL04
          MOVE      ZERO,ATOTAL05
          MOVE      ZERO,ATOTAL06
          MOVE      ZERO,ATOTAL07
          MOVE      ZERO,ATOTAL08
          MOVE      ZERO,ATOTAL09
          MOVE      ZERO,ATOTAL10
          MOVE      ZERO,ATOTAL11
          MOVE      ZERO,ATOTAL12
.
          BRANCH    REPTSEQ,REPT1000,REPT2000,REPT3000
.
. Catalog Seq
.-------------------------
REPT1000  MATCH     FCATALOG,SP70
          IF        @EQUAL & (INCLTYPE=0 | INCLTYPE =2) & NONCATA=1
            MOVE      INCLTYPE,DIM1
            MOVE      "2",INCLTYPE
            MOVE      SIWAWAR,DIM5
            CALL      LOAD0000
            MOVE      DIM1,INCLTYPE
            CALL      SNON0000               * Set up Non-Type  Variables
            CALL      CALCN000
            CALL      PRTLIN00
            MOVE      DIM5,SIWAWAR
          ENDIF
.
          PACK      KEY7,FCATALOG,SP70
          CALL      RSSIIA1
          CALL      RPSIIA1
.
REPT1100  CALL      RKSIIA1
          BRANCH    OVRCD,REPT9000
          MATCH     SIIACAT,TCATALOG
          GOTO      REPT9000 IF LESS
.
REPT1200  CALL      RANGE000              * Check Other Ranges
          BRANCH    EXIT,REPT1100         * Not in Range
.
          DISPLAY   *P15:24,*EL,*V2LON,SIIACAT;
          CALL      CALC0000
.
REPT1250  CALL      PRTLIN00
          GOTO      REPT1100
.
. Catalog Description Seq
.-------------------------
.
REPT2000  MATCH     FCATALOG,SP70
          IF        @EQUAL & (INCLTYPE=0 | INCLTYPE =2) & NONCATA=1
            MOVE      INCLTYPE,DIM1
            MOVE      "2",INCLTYPE
            MOVE      SIWAWAR,DIM5
            CALL      LOAD0000
            MOVE      DIM1,INCLTYPE
            CALL      SNON0000               * Set up Non-Type  Variables
            CALL      CALCN000
            CALL      PRTLIN00
            MOVE      DIM5,SIWAWAR
          ENDIF
.
          PACK      KEY67,FDESCRP,SP70
          CALL      RSSIIA3
          CALL      RPSIIA3
.
.
REPT2100  CALL      RKSIIA3
          BRANCH    OVRCD,REPT9000
.
          MATCH     SIIADES,TDESCRP
          GOTO      REPT9000 IF LESS
.
REPT2200  CALL      RANGE000              * Check Other Ranges
          BRANCH    EXIT,REPT2100         * Not in Range
.
          DISPLAY   *P15:24,*EL,*V2LON,SIIACAT;
          CALL      CALC0000
.
REPT2300  CALL      PRTLIN00
.
          GOTO      REPT2100
.
. Product Group Sequence
.-----------------------
REPT3000  CALL      ZTOT0000                 * Zero Totals
          MOVE      SIWAWAR,DIM5
          CALL      LOAD0000                 * Load Usage Data
.
          MOVE      TRUE,FIRSTRUN
          PACK      KEY28,SP70
          CALL      RSSIAL1
REPT3100  CALL      RKSIAL1
          IF        OVRCD=1
            MOVE      CHECKSUB,SIIASUB
            CALL      PRTLIN00
            CALL      KLALL000
            GOTO      REPT9000
          ENDIF
.
          IF        FIRSTRUN=1
            MOVE      SIALSUB,SIIASUB
            CALL      HEAD0000
            MOVE      SIALSUB,CHECKSUB
            MOVE      SIALCAT,CHECKCAT
            MOVE      FALSE,FIRSTRUN
          ENDIF
.
          MATCH     SIALSUB,CHECKSUB
          IF        !@EQUAL
            MOVE      CHECKSUB,SIIASUB
            CALL      PRTLIN00               * Print Current Catalog
            CALL      PTOT0000               * New Subjective
            CALL      ATOT0000               * Add Current Line to totals
            MOVE      SIALSUB,SIIASUB
            CALL      HEAD0000               * New Subjective
            GOTO      REPT3100
          ENDIF
.
          MATCH     SIALCAT,CHECKCAT
          IF        !@EQUAL
            MOVE      SIALSUB,SIIASUB
            CALL      PRTLIN00
            MOVE      SIALCAT,CHECKCAT       * New Current Catalog Code
          ENDIF
.
          CALL      ATOT0000                 * Add to totals
          GOTO      REPT3100
.
REPT9000  COMPARE   ZERO,CPAGENO
          CALL      HEAD0000 IF EQUAL       * at least one page should print
.
          CALL      UND132                  * End of Report
          PRINT     *1,"|",*45,"|":
                    *70,"|",ATOTAL05,ATOTAL06,ATOTAL07,ATOTAL08:
                    *132,"|"
          CALL      UND132
          PRINT     *57,"***** End of Report *****",*N:
                    *1,"WARNING : Information reported is restricted ":
                    "to User Security Access"
.
          CALL      TRAN0000                * Transfer Export File
.
          MOVE      ZERO,EXIT
          GOTO      REPT9999
.
REPT9500  MOVE      ONE,EXIT
.
REPT9999  CALL      MESSAGE3                * restore line 24
          RETURN
