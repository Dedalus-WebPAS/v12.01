          MOVE      SISADESC,KEY30
.
          PRINT     *1,"|",CHECKSUB,SP1,KEY30,*45,"|":
                    *70,"|",ATOTAL09,ATOTAL10,ATOTAL11,ATOTAL12:
                    *132,"|"
          MOVE      SIALSUB,CHECKSUB
          MOVE      SIALCAT,CHECKCAT
          MOVE      "599",CLNO
          MOVE      ZERO,ATOTAL01
          MOVE      ZERO,ATOTAL02
          MOVE      ZERO,ATOTAL03
          MOVE      ZERO,ATOTAL04
          MOVE      ZERO,ATOTAL09
          MOVE      ZERO,ATOTAL10
          MOVE      ZERO,ATOTAL11
          MOVE      ZERO,ATOTAL12
.
