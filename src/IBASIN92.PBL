.
EGRP2000  MOVE      "    2",FLDID
          CALL      GETPOSIT
          MOVE      ZERO,CKEYTYP
          CALL      KSISAA00
          BRANCH    EXIT,EGRP3000,EGRP9999
          MOVE      SISACODE,TGROUP
          MOVE      SISACODE,TOCODE
          MOVE      SISADESC,TONAME
          GOTO      EGRP9999
.
EGRP3000  MOVE      Z70,TGROUP
          MOVE      "End  ",TOCODE
          MOVE      SP70,TONAME
.
EGRP9999  MOVE      "    1",FLDID
          CALL      GETPOSIT
          RETURN
.------------------------------------------------------------------------------
. REPT - Print Out Report
.------------------------------------------------------------------------------
REPT0000  PACK      DISPMSG,SP70,SP70                 * save line 24
          CALL      MESSAGE2
.
          MATCH     FCATALOG,TCATALOG
          IF        @LESS
            DISPLAY   *P1:24,*B,*EF,"Illegal Catalog Range - ";
            CALL      HITENTER
            GOTO      REPT9500
          ENDIF
.
          MATCH     FGROUP,TGROUP
          IF        @LESS
            DISPLAY   *P1:24,*B,*EF,"Illegal Product Group Range - ";
            CALL      HITENTER
            GOTO      REPT9500
          ENDIF
.
          MATCH     FDESCRP,TDESCRP
          IF        @LESS
            DISPLAY   *P1:24,*B,*EF,"Illegal Description Range - ";
            CALL      HITENTER
            GOTO      REPT9500
          ENDIF
.
.
          DISPLAY   *P1:24,*EL,"Processing - ";
          MOVE      "599",CLNO                        * Set up print values
          MOVE      "0",CPAGENO
          CLOCK     TIME,CTIMEIS
.
          READ      CONTROLF,TWENTY3;*143,HDEFWH
          BRANCH    REPTSEQ,REPT1000,REPT2000,REPT3000
.
. Catalog Seq
.-------------------------
REPT1000  PACK      KEY7,FCATALOG,SP70
          MOVE      "80",CLNO                         * Set up print values
          CALL      RSSIIA1
          CALL      RPSIIA1
.
REPT1100  CALL      RKSIIA1
          BRANCH    OVRCD,REPT9000
          MATCH     SIIACAT,TCATALOG
          GOTO      REPT9000 IF LESS
.
          CALL      RANGE000              * Check Other Ranges
          BRANCH    EXIT,REPT1100         * Not in Range
.
          DISPLAY   *P15:24,*EL,*V2LON,SIIACAT;
          CALL      PRTLIN00
.
          GOTO      REPT1100
.
. Catalog Description Seq
.-------------------------
REPT2000  PACK      KEY67,FDESCRP,SP70
          CALL      RSSIIA3
          CALL      RPSIIA3
.
REPT2100  CALL      RKSIIA3
          BRANCH    OVRCD,REPT9000
          MATCH     SIIADES,TDESCRP
          GOTO      REPT9000 IF LESS
.
          CALL      RANGE000              * Check Other Ranges
          BRANCH    EXIT,REPT2100         * Not in Range
.
          DISPLAY   *P15:24,*EL,*V2LON,SIIACAT;
          CALL      PRTLIN00
.
          GOTO      REPT2100
.
. Product Group Sequence
.-----------------------
REPT3000  PACK      KEY12,FGROUP,SP70
          CALL      RSSIIA2
          CALL      RPSIIA2
.
REPT3100  CALL      RKSIIA2
          BRANCH    OVRCD,REPT9000
          MATCH     SIIASUB,TGROUP
          GOTO      REPT9000 IF LESS
.
          CALL      RANGE000              * Check Other Ranges
          BRANCH    EXIT,REPT3100         * Not in Range
.
          DISPLAY   *P15:24,*EL,*V2LON,SIIACAT;
          CALL      PRTLIN00
          GOTO      REPT3100
.
REPT9000
          COMPARE   ZERO,CPAGENO
          CALL      HEAD0000 IF EQUAL       * at least one page should print
.
          CALL      UND132                  * End of Report
          PRINT     *57,"***** End of Report *****"
          DISPLAY   *P1:24,*EL;             * Clear Processing Lines Display
.
          MOVE      ZERO,EXIT
          GOTO      REPT9999
.
REPT9500  MOVE      ONE,EXIT
.
REPT9999  CALL      MESSAGE3                * restore line 24
          RETURN
.----------------------------------------------------------
. Print Line
.----------------------------------------------------------
PRTLIN00  MOVE      SP70,KEY8
          IF        SIIANON=0
            PACK      KEY28,SIWAWAR,SIIACAT,Z70
          ELSE
            PACK      KEY28,SP5,SIIACAT,Z70
          ENDIF
          CALL      RSSIIT1
PRTLIN05  CALL      RPSIIT1
          BRANCH    OVRCD,PRTLIN10
          IF        SIIANON=0
            MATCH     SIWAWAR,SIITWAR
            GOTO      PRTLIN10 IF NOT EQUAL
          ENDIF
          MATCH     SIIACAT,SIITCAT
          GOTO      PRTLIN10 IF NOT EQUAL
.
. ------- Start -> SRF126419 RKS
          IF        (SIITTYP=2) | (SIITTYP=3)
            GOTO      PRTLIN05               * Don't count stocktake transacs.
          ENDIF
. ------- End   -> SRF126419 RKS
.
          MOVE      SIITDAT,KEY8
          REP       " 0",KEY8
          REP       " 0",TODATE
          MATCH     TODATE,KEY8
          GOTO      PRTLIN99 IF NOT LESS
.
PRTLIN10  MATCH     SP70,KEY8
          IF        @EQUAL
            MOVE      "Not Used",CPCDATE
          ELSE
            UNPACK    KEY8,CCENT,CYEAR,CMON,CDAY
            CALL      PACDATE
          ENDIF
.
          COMPARE   "55",CLNO               * Number of Lines Test
          CALL      HEAD0000 IF NOT LESS
          ADD       "1",CLNO
          MOVE      "99,999,999.99",KEY13
          FEDIT     SIIESOH,KEY13
          IF        SIIANON=0
            MOVE       "Yes",KEY3
          ELSE
            MOVE       "   ",KEY3
            MOVE      "          N/A",KEY13
          ENDIF
          IF        (SIIASUS=0&SIIESUS=0)
            MOVE       SP70,KEY4
          ELSE
            MOVE       "Yes",KEY4
          ENDIF
          MOVE      SIIADES,KEY50
          PRINT     "! ",SIIACAT,SP1,KEY50,"! ",SIIERAS," ! ",SIIAISS," ! ":
                    KEY13," ! ",CPCDATE," ! ",KEY3," ! ",KEY4," !"
PRTLIN99  RETURN
.----------------------------------------------------------
. Print Heading
.----------------------------------------------------------
HEAD0000  CALL      HEAD132                 * Print Page Header
          MOVE      "5",CLNO
          IF        CPAGENO=1
            STRIP     INCLDESC
            STRIP     SEQDESC
            CLEAR     VAR
            APPEND    "Report Including ",VAR
            APPEND    INCLDESC,VAR
            APPEND    " in ",VAR
            APPEND    SEQDESC,VAR
            APPEND    " Sequence",VAR
            RESET     VAR
            PRINT        "Catalog Code  Range : ",FROMCATA," to  ",TOCATA:
                      *N,"Product Group Range : ",FROMCODE,"   to  ",TOCODE:
                      *N,"Catalog Name  Range : ",FROMDESC," to  ",TODESC:
                      *N,"Warehouse           : ",SIWAWAR,SP1,SIWADES:
                      *N,VAR
            ADD       FIVE,CLNO
          ELSE
            PRINT        "Warehouse           : ",SIWAWAR,SP1,SIWADES:
                      *N,VAR
            ADD       TWO,CLNO
          ENDIF
          CALL      UND132
          PRINT     "! Catalog                                           ":
                    "        !   R & S  ! Issue Unit      ! Stock On Hand":
                    " ! Last Used  ! Stk ! Susp !"
          CALL      UND132
          RETURN
.------------------------------------------------------------
. Range Check
.------------------------------------------------------------
RANGE000  MOVE      ZERO,EXIT
          IF        !(REPTSEQ=1)
            MATCH     FCATALOG,SIIACAT
            GOTO      RANGE900 IF LESS
            MATCH     SIIACAT,TCATALOG
            GOTO      RANGE900 IF LESS
          ENDIF
.
          IF        !(REPTSEQ=2)
            MATCH     FDESCRP,SIIADES
            GOTO      RANGE900 IF LESS
            MATCH     SIIADES,TDESCRP
            GOTO      RANGE900 IF LESS
          ENDIF
.
          IF        !(REPTSEQ=3)
            MATCH     FGROUP,SIIASUB
            GOTO      RANGE900 IF LESS
            MATCH     SIIASUB,TGROUP
            GOTO      RANGE900 IF LESS
          ENDIF
.
          MOVE      SP70,SIIESID
          IF        SIIANON=0
            PACK      KEY12,SIIACAT,SIWAWAR
            CALL      RDSIIE1
            BRANCH    OVRCD,RANGE900
          ELSE
            MOVE      ZERO,SIIESOH
            MOVE      ZERO,SIIESUS
          ENDIF
.
RANGE500  BRANCH    INCLTYPE,RANGE510,RANGE520,RANGE530,RANGE530,RANGE530:
                             RANGE530,RANGE530,RANGE530
          GOTO      RANGE999
.
. SIIANON   FORM      1      Non-Stock Item 0=No (ie Stocked Item)
.------------------------------------------------
RANGE510  COMPARE   ONE,SIIANON
          GOTO      RANGE900 IF EQUAL   * Stock Only
          GOTO      RANGE999
.
RANGE520  COMPARE   ZERO,SIIANON
          GOTO      RANGE900 IF EQUAL   * Non-Stock Only
          GOTO      RANGE999
.
RANGE530  ASSIGN    INCLTYPE-2,F2
.
          MATCH     SP70,SIIESID
          IF        @EQUAL
            RESET     SIIASTI,F2
            CMATCH    "1",SIIASTI
            GOTO      RANGE900 IF NOT EQUAL
            GOTO      RANGE999
          ELSE
            RESET     SIIESID,F2
            CMATCH    "1",SIIESID
            GOTO      RANGE900 IF NOT EQUAL
            GOTO      RANGE999
          ENDIF
.
RANGE900  MOVE      ONE,EXIT
.
RANGE999  RETURN
ESEQU000  MOVE      SP70,DISPMSG
          CALL      MESSAGE2
ESEQU010  DISPLAY   *P1:24,*EL,"Report Sequence : ":
                    *V2LON,"1. ",*HOFF,"Catalog Code, ":
                    *V2LON,"2. ",*HOFF,"Catalog Name, ":
                    *V2LON,"3. ",*HOFF,"Product Group"
          MOVE      ONE,REPTSEQ
          KEYIN     *PCCOL:CVERT,*V2LON,*RV,REPTSEQ
          BRANCH    REPTSEQ,ESEQU100,ESEQU200,ESEQU300
          BEEP
          GOTO      ESEQU010
.
ESEQU100  MOVE      "Catalog Code",SEQDESC
          GOTO      ESEQU999
ESEQU200  MOVE      "Catalog Name",SEQDESC
          GOTO      ESEQU999
ESEQU300  MOVE      "Product Group",SEQDESC
.
ESEQU999  CALL      MESSAGE3
          RETURN
EINCL000  CALL      DCODE000
.
EINCL010  MOVE      ZERO,INCLTYPE
          KEYIN     *PCCOL:CVERT,*V2LON,*RV,INCLTYPE
          COMPARE   ZERO,INCLTYPE
          GOTO      EINCL900 IF EQUAL
          BRANCH    INCLTYPE,EINCL100,EINCL200
          ASSIGN    INCLTYPE-2,F1
          PACK      KEY6,ANSC,ANSA,ANST,F1,SP70
          CALL      RDSICD1
          IF        OVRCD=1
            BEEP
            GOTO    EINCL010
          ENDIF
          MOVE      SICDDESC,INCLDESC
          GOTO      EINCL999
.
EINCL100  MOVE      "Stocked Catalogs",INCLDESC
          GOTO      EINCL999
EINCL200  BEEP
          GOTO      EINCL010
.
.         MOVE      "Non-Stocked Catalogs",INCLDESC
.         GOTO      EINCL999
.
EINCL900  BEEP
          GOTO      EINCL010
.
.         MOVE      "All Catalogs",INCLDESC
.         GOTO      EINCL999
.
EINCL999  CALL      PUTSCR00
          RETURN
.
DCODE000  MOVE      "13",HTOP
          MOVE      "44",HLEF
          MOVE      "24",HBOT
          MOVE      "80",HRIG
          CALL      GETSCR00
.
          BOXCLR    HLEF,HTOP,HRIG,HBOT
          BOX       16,HLEF,HTOP,HRIG,HBOT
          DISPLAY   *P52:14,*V2LON,*ULON,"Include Options",*HOFF:
                    *P46:15,*V2LON,"0. ",*HOFF,"All Catalogs (N/A)":
                    *P46:16,*V2LON,"1. ",*HOFF,"Stocked Only":
                    *P46:17,*V2LON,"2. ",*HOFF,"Non-Stocked Only (N/A)"
          MOVE      ONE,F1
DCODE100  PACK      KEY6,ANSC,ANSA,ANST,F1,SP70
          CALL      RDSICD1
          IF        OVRCD=1
            MOVE      "No Description on Code",SICDDESC
          ENDIF
          ASSIGN    F1+17,F2
          ADD       "2",F1
          DISPLAY   *P46:F2,*V2LON,F1,". ",*HOFF,SICDDESC
          SUB       "2",F1
          ADD       ONE,F1
          COMPARE   SEVEN,F1
          GOTO      DCODE100 IF NOT EQUAL
          RETURN
ECAT0000  IF        ACCEPT=1
            MOVE      SP70,FCATALOG
            MOVE      "Start",FROMCATA
            MOVE      SP70,FCATADES
            MOVE      Z70,TCATALOG
            MOVE      "End  ",TOCATA
            MOVE      SP70,FCATADES
            GOTO      ECAT9999
          ENDIF
.
          MOVE      "   20",FLDID
          CALL      GETPOSIT
          MOVE      ZERO,CKEYTYP
          MATCH     SIWAWAR,SP70
          IF        @EQUAL
            CALL      KSIIAA00
          ELSE
            CALL      KSIIEA00
          ENDIF
          BRANCH    EXIT,ECAT1000,ECAT9999
          MOVE      SIIACAT,FCATALOG
          MOVE      SIIACAT,FROMCATA
          MOVE      SIIADES,FCATADES
          GOTO      ECAT2000
.
ECAT1000  MOVE      SP70,FCATALOG
          MOVE      "Start",FROMCATA
          MOVE      SP70,FCATADES
.
ECAT2000  MOVE      "   21",FLDID
          CALL      GETPOSIT
.
          MOVE      ZERO,CKEYTYP
          MATCH     SIWAWAR,SP70
          IF        @EQUAL
            CALL      KSIIAA00
          ELSE
            CALL      KSIIEA00
          ENDIF
          BRANCH    EXIT,ECAT3000,ECAT9999
          MOVE      SIIACAT,TCATALOG
          MOVE      SIIACAT,TOCATA
          MOVE      SIIADES,TCATADES
          GOTO      ECAT9999
.
ECAT3000  MOVE      Z70,TCATALOG
          MOVE      "End  ",TOCATA
          MOVE      SP70,FCATADES
.
ECAT9999  MOVE      "   20",FLDID
          CALL      GETPOSIT
          RETURN
EDES0000  IF        ACCEPT=1
            MOVE      SP70,FDESCRP
            MOVE      "Start",FROMDESC
            MOVE      Z70,TDESCRP
            MOVE      "End  ",TODESC
            GOTO      EDES9999
          ENDIF
.
          MOVE      "   22",FLDID
          CALL      GETPOSIT
.
          KEYIN     *PCCOL:CVERT,*V2LON,*MASK,*RV,FDESCRP
          ENDSET    FDESCRP
          APPEND    SP70,FDESCRP
          RESET     FDESCRP
          MATCH     SP70,FDESCRP
          GOTO      EDES1000 IF EQUAL
          CMATCH    EXITCHAR,FDESCRP
          GOTO      EDES9000 IF EQUAL
          MOVE      FDESCRP,FROMDESC
          GOTO      EDES2000
.
EDES1000  MOVE      SP70,FDESCRP
          MOVE      "Start",FROMDESC
.
EDES2000  MOVE      "   23",FLDID
          CALL      GETPOSIT
.
          KEYIN     *PCCOL:CVERT,*V2LON,*MASK,*RV,TDESCRP
          ENDSET    TDESCRP
          APPEND    SP70,TDESCRP
          RESET     TDESCRP
          MATCH     SP70,TDESCRP
          GOTO      EDES1000 IF EQUAL
          CMATCH    EXITCHAR,TDESCRP
          GOTO      EDES9000 IF EQUAL
          MOVE      TDESCRP,TODESC
          GOTO      EDES9999
.
EDES3000  MOVE      Z70,TDESCRP
          MOVE      "End  ",TODESC
          GOTO      EDES9999
EDES9000  MOVE      TWO,EXIT
.
EDES9999  MOVE      "   22",FLDID
          CALL      GETPOSIT
          RETURN
