          IF        @EQUAL
            MOVE      TCINDC3,FORM1
            IF        FORM1<0 | FORM1>9 | FORM1=3
              MOVE      "Invalid source of referral",ERRMSG
              CALL      EMSG0000              * Print an error message
              GOTO      WEPD8500
