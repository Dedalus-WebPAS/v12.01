          MOVE      "10",EVERT
          MOVE      SP6,CKYIDEF6
          MOVE      ZERO,CKYIMAND
.
HFGP1000  DISPLAY   *P1:EVERT,"Health Fund Group :",*EF;
          CALL      PATHGPKY                * Keyin a health fund group
          BRANCH    EXIT,HFGP5000,HFGP9100,HFGP9200
