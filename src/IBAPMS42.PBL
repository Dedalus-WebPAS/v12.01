          PRINT     *1,"| ",LOADURNO,*12,"| ",LOADADMN,*23,"| ",LOADINVN:
                    *34,"| ",LOADPINV,*57,"| ",ERRORDES,*132,"|"
          GOTO      MESP9999
.
