          PACK     KEY15,PRGID,SCRID,SP70
          CALL     RSSCPS1
DFBAL010
          CALL     RKSCPS1
          BRANCH   OVRCD,DFBAL900
          MATCH    PRGID,SCPSPID
          GOTO     DFBAL900 IF NOT EQUAL
          MATCH    SCRID,SCPSSID
          GOTO     DFBAL900 IF NOT EQUAL
          MATCH    SP70,SCPSMTY
          GOTO     DFBAL010 IF NOT EQUAL
.
          CALL     PSBAL000 * Display Field
          GOTO     DFBAL010
DFBAL900
          MOVE      "SIBT1",NAMSSFFI
          MOVE      "    1",ITMSSFFI
          CALL      FPBAL000
DFBAL999
          RETURN
