            PACK      KEY30,OPDADATE,OPDATIME,OPDACLIN,OPDACASE,ADMISSNO,SP70
            CALL      RDOPAAE1
            IF        OVRCD=1
              CALL      MVOPRA00
              CALL      ITMDES00
              PACK      OPAADATE,OPDADATE
              PACK      OPAATIME,OPDATIME
              PACK      OPAACLIN,OPDACLIN
              PACK      OPAACASE,OPDACASE
              PACK      OPAAADMN,ADMISSNO
              CALL      WROPAAE1
            ELSE
              CALL      MVOPRA00
              CALL      ITMDES00
              CALL      UPOPAAE1
            ENDIF
          ENDIF
