          MOVE      ADMISSNO,OPDAADMN
          MATCH     SP70,BKUNLADM
          IF        !@EQUAL
            PACK      OPDAADMN,BKUNLADM,SP70
          ENDIF          
          MOVE      ONE,TEAMNUMB
.
          MOVE      CURRDATE,OPDADATE
          MOVE      ADMISSNO,KEY8
          MATCH     SP70,BKUNLADM
          IF        !@EQUAL
            PACK      KEY8,BKUNLADM,SP70
          ENDIF
