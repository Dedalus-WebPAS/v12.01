.******************************************************************************
.         Only allow to Add MBS to the last Theatre Case/Booking Unique ID or
.         Un-invoiced previous Case/Booking Unique ID
.******************************************************************************
CINV0000  MOVE      ZERO,FORM1
          MATCH     "1",OPCNCOMB
          GOTO      CINV9999 IF NOT EQUAL  * Not using 'Combined CMBS&Misc.Chrg)
.
          MATCH     SP70,OPDAADMN
          GOTO      CINV9999 IF EQUAL
.
          MOVE      OPDAADMN,KEY8
          MOVE      OPDAUNIQ,KEY10
.
          PACK      KEY31,OPDAADMN,Z70
          CALL      RSOPDEA2
          CALL      RPOPDEA2                * Reading Through Operation Detail
          BRANCH    OVRCD,CINV8000
.
          MATCH     KEY8,OPDAADMN
          GOTO      CINV8000 IF NOT EQUAL
.
          MATCH     KEY10,OPDAUNIQ
          GOTO      CINV1000 IF NOT EQUAL   * this is not the last Theatre case
.
          MOVE      TWO,FORM1         * This is the last or Current Theatre case
.
.         This Unique ID is not the Last Case for the Admission, so we need to
.         check if this Unique ID has been invoiced
.
CINV1000  PACK      KEY22,KEY8,SP20
          CALL      RDSDTRN1
CINV2000  CALL      RDKDTRN1
          BRANCH    OVRCD,CINV8000          * no (more) details for admission
.
          MATCH     KEY8,DTADMNO
          GOTO      CINV8000 IF NOT EQUAL   * different admission
.
          MATCH     "1",PTDTCRST
          GOTO      CINV2000 IF EQUAL       * Fully credited
.
          MATCH     SP70,PTDTUNIQ
          GOTO      CINV2000 IF EQUAL
.
          MATCH     KEY10,PTDTUNIQ
          GOTO      CINV2000 IF NOT EQUAL
.
          COMPARE   TWO,FORM1
          GOTO      CINV9000 IF EQUAL       * Current case has been invoiced
.
CINV6000  MOVE      ONE,FORM1               * selected case has been invoiced
          GOTO      CINV9000
.
CINV8000  MOVE      ZERO,FORM1
CINV9000  CALL      RDOPDEA3                * reposition
          IF        OVRCD=1
            CALL      CLOPRDEA
          ENDIF
CINV9999  RETURN
