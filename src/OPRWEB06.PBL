.
          PACK      KEY10,OPDAUNIQ,SP70
          CALL      RDOPARD1
          BRANCH    OVRCD,RECTIM99
.
          MATCH     SP70,OPARTIRF
          IF        !@EQUAL & !@EOS
            GOTO      RECTIM30
          ENDIF
.
          MATCH     SP70,OPARTIRB
