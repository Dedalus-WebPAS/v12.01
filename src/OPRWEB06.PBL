            PACK      KEY18,OPTSSCDE,DIM9,OPDAHOSP,SP70
            CALL      RDOPDPH1
            IF        OVRCD=0        
              MOVE      OPDHDESC,PREFDESC
              MOVE      THREE,PREFTYPE   * Comments
              MOVE      ONE,INVLDITM
              GOTO      PRFDSC99
            ENDIF
          ENDIF
