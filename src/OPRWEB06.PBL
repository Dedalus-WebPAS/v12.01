          MOVE      "Invalid Code  ",OUTDESC
          PACK      KEY6,PMPOCODE,SP70
          CALL      RDPMADC1
          IF        OVRCD<>1
            PACK      OUTDESC,PMACDESC,SP70
          ENDIF
.
          MOVE      DNO,DANS
          MATCH     "1",PMACACHS
          IF        @EQUAL
            PACK      DANS,DYES
          ENDIF
