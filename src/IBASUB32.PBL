          CALL      GETPOSIT
          IF        EXIT=0
            MOVE      TODESC,VAR       
            CALL      DISPITEM
          ENDIF
          GOTO     SF00L001
.
SF00L110
          CALL      DEFAULTY
          CALL     SELECTED
          CALL      YORN0000
          MOVE       FYESNO,ANS
          REP        "0A1B",ANS
          REP        "A1B0",ANS
          MOVE       ANS,SUPCATYN
          MOVE      YES,YESNO
          MOVE      ZERO,FORM2
          MOVE      SUPCATYN,FORM2   
          LOAD      YESNO,FORM2,NO
          MOVE      YESNO,VAR
          CALL      DISPITEM
          GOTO     SF00L001
SF00L990  MOVE      FOUR,EXIT * Exit Char Exit
SF00L999  RETURN
.@@@@@ Function Restart Point @@@@@
.------------------------------------------------------------------------
. Open Nato X-Ref File if required
.------------------------------------------------------------------------
ONAT0000  OPEN      CONTROLF,"controlf"
.
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF FORMAT
          READ      CONTROLF,SIXTY3;*143,HNONUSAG
          TRAPCLR   FORMAT
          IF        OVRCD=1
            MOVE      ONE,HNONUSAG
            WRITAB    CONTROLF,SIXTY3;*143,HNONUSAG
          ENDIF
.
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF FORMAT
          READ      CONTROLF,SIXTY3;*141,INCNNATO
          TRAPCLR   FORMAT
          IF        OVRCD=1
            MOVE      ZERO,INCNNATO
            WRITAB    CONTROLF,SIXTY3;*141,INCNNATO
          ENDIF
.
          IF        INCNNATO=1
            MOVE      ZERO,OVRCD
            TRAP      OVERCOND IF IO
            OPEN      SINNATA1,"sinnataf"
            TRAPCLR   IO
            IF        OVRCD=1
              DISPLAY   *P1:24,*EL,*B,"Nato file does not exist. Run fixit.  ";
              CALL      HITENTER
              CHAIN     PGM
            ENDIF
.
            OPEN      SINNATA2,"sinnataf"
          ENDIF
.
ONAT9999  RETURN
.----------------------------------------------------------------------
. Set up screen 00
.----------------------------------------------------------------------
SETSCR00
          MOVE      ONE,ACCEPT              * set accept mode
.
          MOVE      ONE,REPTSEQ             * by catalog
          MOVE      "Catalog Code",SEQDESC
.
          MOVE      ZERO,INCLTYPE           * all catalog types
          MOVE      "All Catalogs",INCLDESC
.
          CALL      DCAT0000                * catalog range
.
          CALL      DGRP0000                * product group range
.
          CALL      DDES0000                * description range
.
          RETURN
WDESC000  MATCH     SP70,SIWAWAR
          IF        @EQUAL
            MOVE      "All Warehouses",WAREDESC
          ELSE
            MOVE      SIWADES,WAREDESC
          ENDIF
          RETURN
DEFAULTY    MOVE     ANSY,DEFYESNO
            RETURN
.----------------------------------------------------------------------
. Enter Product Group Range
.----------------------------------------------------------------------
EGRP0000  IF        ACCEPT=1
            CALL      DGRP0000         * default catalog range
            GOTO      EGRP9999
          ENDIF
.
          MOVE      "    1",FLDID
          CALL      GETPOSIT
          MOVE      ZERO,CKEYTYP
          CALL      KSISAA00
          BRANCH    EXIT,EGRP1000,EGRP9999
          MOVE      SISACODE,FGROUP
          MOVE      SISACODE,FROMCODE
          MOVE      SISADESC,FROMNAME
          GOTO      EGRP2000
.
EGRP1000  MOVE      SP70,FGROUP
          MOVE      "Start",FROMCODE
          MOVE      SP70,FROMNAME
.
EGRP2000  MOVE      "    2",FLDID
          CALL      GETPOSIT
          MOVE      ZERO,CKEYTYP
          CALL      KSISAA00
          BRANCH    EXIT,EGRP3000,EGRP9999
          MOVE      SISACODE,TGROUP
          MOVE      SISACODE,TOCODE
          MOVE      SISADESC,TONAME
          GOTO      EGRP9999
.
EGRP3000  MOVE      Z70,TGROUP
          MOVE      "End  ",TOCODE
          MOVE      SP70,TONAME
.
EGRP9999  MOVE      "    1",FLDID
          CALL      GETPOSIT
          RETURN
.----------------------------------------------------------------------
. Default range
.----------------------------------------------------------------------
DGRP0000
          MOVE      SP70,FGROUP
          MOVE      "Start",FROMCODE
          MOVE      SP70,FROMNAME
          MOVE      Z70,TGROUP
          MOVE      "End  ",TOCODE
          MOVE      SP70,TONAME
          RETURN
.------------------------------------------------------------------------------
. REPT - Print Out Report
.------------------------------------------------------------------------------
REPT0000  PACK      DISPMSG,SP70,SP70                 * save line 24
          CALL      MESSAGE2
.
          MATCH     FCATALOG,TCATALOG
          IF        @LESS
            DISPLAY   *P1:24,*B,*EF,"Illegal Catalog Range - ";
            CALL      HITENTER
            GOTO      REPT9500
          ENDIF
.
          MATCH     FGROUP,TGROUP
          IF        @LESS
            DISPLAY   *P1:24,*B,*EF,"Illegal Product Group Range - ";
            CALL      HITENTER
            GOTO      REPT9500
          ENDIF
.
          MATCH     FDESCRP,TDESCRP
          IF        @LESS
            DISPLAY   *P1:24,*B,*EF,"Illegal Description Range - ";
            CALL      HITENTER
            GOTO      REPT9500
          ENDIF
.
.
          DISPLAY   *P1:24,*EL,"Processing - ";
          CALL      MAKEXP00                          * Create Export File
          MOVE      "599",CLNO                        * Set up print values
          MOVE      "0",CPAGENO
          CLOCK     TIME,CTIMEIS
          MOVE      ZERO,ATOTAL11
          MOVE      ZERO,ATOTAL12
.
          BRANCH    REPTSEQ,REPT1000,REPT2000,REPT3000
.
. Catalog Seq
.-------------------------
REPT1000  PACK      KEY7,FCATALOG,SP70
          CALL      RSSIIA1
          CALL      RPSIIA1
.
REPT1100  CALL      RKSIIA1
          BRANCH    OVRCD,REPT9000
          MATCH     SIIACAT,TCATALOG
          GOTO      REPT9000 IF LESS
.
          CALL      RANGE000              * Check Other Ranges
          BRANCH    EXIT,REPT1100         * Not in Range
.
          DISPLAY   *P15:24,*EL,*V2LON,SIIACAT;
          CALL      PRTLIN00
.
          GOTO      REPT1100
.
. Catalog Description Seq
.-------------------------
REPT2000  PACK      KEY67,FDESCRP,SP70
          CALL      RSSIIA3
          CALL      RPSIIA3
.
REPT2100  CALL      RKSIIA3
          BRANCH    OVRCD,REPT9000
          MATCH     SIIADES,TDESCRP
          GOTO      REPT9000 IF LESS
.
          CALL      RANGE000              * Check Other Ranges
          BRANCH    EXIT,REPT2100         * Not in Range
.
          DISPLAY   *P15:24,*EL,*V2LON,SIIACAT;
          CALL      PRTLIN00
.
          GOTO      REPT2100
.
. Product Group Sequence
.-----------------------
REPT3000  CALL      KILL0000
          CALL      CREA0000
          PACK      KEY12,FGROUP,SP70
          CALL      RDSIIA2
          IF        OVRCD=0
            CALL      RPSIIA2
          ENDIF
          CALL      RKSIIA2
          MOVE      SIIASUB,LASTGRP
          MOVE      ZERO,TOTSOH
          MOVE      ZERO,TOTSOO
.
          CALL      RPSIIA2
.
REPT3100  CALL      RKSIIA2
          BRANCH    OVRCD,REPT9000
          MATCH     SIIASUB,TGROUP
          GOTO      REPT9000 IF LESS
.
          CALL      RANGE000              * Check Other Ranges
          BRANCH    EXIT,REPT3100         * Not in Range
.
          MATCH     LASTGRP,SIIASUB
          CALL      GRPTOT00 IF NOT EQUAL
.
          DISPLAY   *P15:24,*EL,*V2LON,SIIACAT;
          CALL      PRTLIN00
          GOTO      REPT3100
.
REPT9000  IF        CPAGENO=0
            CALL      HEAD0000              * at least one page should print
          ELSE
            COMPARE   THREE,REPTSEQ
            CALL      GRPTOT00 IF EQUAL
          ENDIF
.
          CALL      UND132
          PRINT     *1,"|",*66,"| ",*99,"| Total SOH Value",ATOTAL11:
                    *132,"|",*N:
                    *1,"|",*66,"| ",*99,"| Total SOO Value",ATOTAL12:
                    *132,"|"
          CALL      UND132                  * End of Report
          IF        REPTSEQ<>3
            PRINT     *57,"***** End of Report *****"
          ENDIF
.
          CALL      TRAN0000
.
          MOVE      ZERO,EXIT
          GOTO      REPT9999
.
REPT9500  MOVE      ONE,EXIT
.
REPT9999  CALL      MESSAGE3                * restore line 24
          IF        EXIT=0 & REPTSEQ=3
            CALL      PSUMM000              * Print summary
          ENDIF
          RETURN
.----------------------------------------------------------------------
. Delete Old Temp File
.----------------------------------------------------------------------
KILL0000  PACK      TEMPNAME,TEMPNAMA,PORT
          REP       " 0",TEMPNAME
          CLOSE     TEMPFIL1
          CLEAR     CMDLINE
          APPEND    "iserase ",CMDLINE
          APPEND    TEMPNAME,CMDLINE
          RESET     CMDLINE
.
          EXECUTE   CMDLINE,TASKID
.
KILL9999  RETURN
.----------------------------------------------------------------------
. Create New Temp File
.----------------------------------------------------------------------
CREA0000  CLEAR     CMDLINE
          APPEND    "isbuild ",CMDLINE
          APPEND    TEMPNAME,CMDLINE
          APPEND    " 28 UC1-6,7-11",CMDLINE                           *C-59696
          RESET     CMDLINE
.
          EXECUTE   CMDLINE,TASKID
          OPEN      TEMPFIL1,TEMPNAME
.
CREA9999  RETURN
.--------------------------------------------------
. Print Product Group Total
.--------------------------------------------------
GRPTOT00  MOVE      LASTGRP,KEY5
          CALL      RDSISA1
          CALL      UND132
          PRINT     *1,"| Product Group ",LASTGRP,SP1,SISADESC:
                    *66,"| ",*99,"| Total SOH Value",TOTSOH:
                    *132,"|",*N:
                    *1,"|",*66,"| ",*99,"| Total SOO Value",TOTSOO:
                    *132,"|"
          MOVE      "99",CLNO
.
. ------- Update temp file for summary
          PACK      KEY11,SISACOMM,SISACODE,SP70                       *C-59696
          CALL      RDTEMP1
          IF        OVRCD=0
            ADD       TOTSOH,TEMPSOH
            ADD       TOTSOO,TEMPSOO
            CALL      UPTEMP1
          ELSE
            UNPACK    KEY11,TEMPCOM,TEMPSUB                            *C-59696
            MOVE      TOTSOH,TEMPSOH
            MOVE      TOTSOO,TEMPSOO
            CALL      WRTEMP1
          ENDIF
.
          MOVE      SIIASUB,LASTGRP
          MOVE      ZERO,TOTSOH
          MOVE      ZERO,TOTSOO
          RETURN
.----------------------------------------------------------
. Print Summary
.----------------------------------------------------------
PSUMM000  MOVE      ONE,CNOUNDLN
          MOVE      ZERO,SBTOTSOH
          MOVE      ZERO,SBTOTSOO
          MOVE      ZERO,GRTOTSOH
          MOVE      ZERO,GRTOTSOO
          CALL      SUMHED00
          PACK      KEY11,SP70                                         *C-59696
          PACK      CHECKCOM,SP70
          CALL      RSTEMP1
PSUMM100  CALL      RKTEMP1
          BRANCH    OVRCD,PSUMM999
.
          MATCH     CHECKCOM,SP70
          IF        @EQUAL
            MOVE      TEMPCOM,CHECKCOM
          ENDIF
.
          MATCH     CHECKCOM,TEMPCOM
          IF        !@EQUAL
            CALL      PCOM0000
          ENDIF
.
          CALL      PSUB0000
          GOTO      PSUMM100
.
PSUMM999  CALL      PCOM0000            * Print Commodity Total
          CALL      PGRN0000            * Print Grand Total
          RETURN
.----------------------------------------------------------
. Print Summary Heading
.----------------------------------------------------------
SUMHED00  CALL      HEAD80
          PRINT     " "
          CALL      UND80
          PRINT     "! SUBJECTIVE CODE & DESCRIPTION":
                    *42,"!     SOH VALUE":
                    *61,"!     SOO VALUE",*80,"!"
          CALL      UND80
          MOVE      SEVEN,CLNO
.
SUMHED99  RETURN
.----------------------------------------------------------
. Print Subjective Total
.----------------------------------------------------------
PSUB0000  IF        CLNO>50
            IF        (SBTOTSOH<>0) & (SBTOTSOO<>0)
              CALL      UND80
            ENDIF
            CALL      HEAD80
            PRINT     " "
            CALL      UND80
            PRINT     "! SUBJECTIVE CODE & DESCRIPTION":
                      *42,"!     SOH VALUE":
                      *61,"!     SOO VALUE",*80,"!"
            CALL      UND80
            MOVE      SEVEN,CLNO
          ENDIF
.
          PACK      SISADESC,SP70
          PACK      KEY5,TEMPSUB
          CALL      RDSISA1
          PRINT     "! ",TEMPSUB,"  ",SISADESC:
                    *42,"! ",TEMPSOH:
                    *61,"!  ",TEMPSOO," !"
          ADD       ONE,CLNO
.
          ADD       TEMPSOH,SBTOTSOH
          ADD       TEMPSOO,SBTOTSOO
          ADD       TEMPSOH,GRTOTSOH
          ADD       TEMPSOO,GRTOTSOO
.
PSUB9999  RETURN
.----------------------------------------------------------
. Print Commodity Total
.----------------------------------------------------------
PCOM0000  CALL      UND80
          PRINT     "! COMMODITY ",CHECKCOM," TOTAL :":
                    *42,"! ",SBTOTSOH:
                    *61,"!  ",SBTOTSOO," !"
          CALL      UND80
          ADD       ONE,CLNO
.
          MOVE      ZERO,SBTOTSOH
          MOVE      ZERO,SBTOTSOO
          MOVE      TEMPCOM,CHECKCOM
.
PCOM9999  RETURN
.----------------------------------------------------------
. Print Grand Total
.----------------------------------------------------------
PGRN0000  PRINT     "! GRAND TOTAL :":
                      *42,"! ",GRTOTSOH:
                      *61,"!  ",GRTOTSOO," !"
          CALL      UND80
.
PGRN9999  RETURN
.----------------------------------------------------------
. Print Line
.----------------------------------------------------------
PRTLIN00  COMPARE   "55",CLNO               * Number of Lines Test
          CALL      HEAD0000 IF NOT LESS
.
          MOVE      SP70,APMASN1            * get preferred supplier name
          MOVE      SP70,APMASN2
          PACK      KEY5,SIIAPSC,SP70
          CALL      RDAPMA1
          STRIP     APMASN1
          PACK      KEY54,APMASN1,SP1,APMASN2,SP70
.
          MOVE      SP70,SIICCAT            * get preferred supplier part no
          PACK      KEY27,SIIACAT,SIIAPSC,SP70
          CALL      RSSIIC1
          CALL      RKSIIC1
          MATCH     SIIACAT,SIICCAT
          IF        !@EQUAL
            MOVE      SP70,SIICPNO
            MOVE      SP70,SIICSUT
          ENDIF
.
          MOVE      ONE,SIFACTOR            * calc average supply cost
          PACK      KEY30,SIIAISS,SIICSUT,SP70
          CALL      RDSIFA1
          ASSIGN    SIIAAVC,ATOTAL01
          ASSIGN    SIIAAVC*SIFACTOR,ATOTAL02
          ASSIGN    SIIAAVC*SIIESOH,ATOTAL03
          ASSIGN    SIIAAVC*SIIESOO,ATOTAL04
          PACK      KEY55,SIIADES,SP70
.
          CALL      WRTEXP00                * Write Export Details
.
          PRINT     *1,"| ",SIIACAT,SP1,KEY55,*66,"| ":
                            SIIAISS,ATOTAL01," | ",SIIESOH,ATOTAL03:
                    *132,"|",*N:
                    *1,"|         ",KEY54,*66,"| ":
                            SIICSUT,ATOTAL02," | ",SIIESOO,ATOTAL04:
                    *132,"|",*N:
                    *1,"|         ",SIICPNO,SP2,SIIERAS,*66,"| ":
                            SIIEROL,SIIEROQ," | ":
                    *132,"|"
          ADD       "3",CLNO
          ADD       ATOTAL03,ATOTAL11
          ADD       ATOTAL04,ATOTAL12
.
          ADD       ATOTAL03,TOTSOH
          ADD       ATOTAL04,TOTSOO
.
          RETURN
.----------------------------------------------------------
. Print Heading
.----------------------------------------------------------
HEAD0000  CALL      HEAD132                 * Print Page Header
          IF        CPAGENO=1
            STRIP       INCLDESC
            STRIP       SEQDESC
            STRIP       WAREDESC
            CLEAR       VAR
            APPEND      "Including ",VAR
            APPEND      INCLDESC,VAR
            APPEND      " in ",VAR
            APPEND      SEQDESC,VAR
            APPEND      " Sequence for ",VAR
            APPEND      WAREDESC,VAR
            RESET       VAR
            STRIP       FROMCATA
            STRIP       TOCATA
            STRIP       FROMCODE
            STRIP       TOCODE
            STRIP       FROMDESC
            STRIP       TODESC
            PRINT       *+,"Catalog Code  Range : ",FROMCATA," to ",TOCATA:
                        *N,"Product Group Range : ",FROMCODE," to ",TOCODE:
                        *N,"Catalog Name  Range : ",FROMDESC," to ",TODESC:
                        *N,VAR
            ADD       "4",CLNO
          ELSE
            PRINT       VAR
            ADD       "1",CLNO
          ENDIF
.
          CALL      UND132
          PRINT     *1,"| Catalog Description":
                    *66,"| Issue Unit & Value":
                    *99,"| Stock on Hand & Value":
                    *132,"|",*N:
                    *1,"|         Perferred Supplier Name":
                    *66,"| Supply Unit & Value":
                    *99,"| Stock on Order & Value":
                    *132,"|",*N:
                    *1,"|         Supplier Part Number           Rack & Shelf":
                    *66,"| Re Order Level & Quantity":
                    *99,"| ":
                    *132,"|"
          ADD       "3",CLNO
          CALL      UND132
.
HEAD9999  RETURN
.------------------------------------------------------------
. Range Check
.------------------------------------------------------------
RANGE000  MOVE      ZERO,EXIT
          IF        !(REPTSEQ=1)
            MATCH     FCATALOG,SIIACAT
            GOTO      RANGE900 IF LESS
            MATCH     SIIACAT,TCATALOG
            GOTO      RANGE900 IF LESS
          ENDIF
.
          IF        !(REPTSEQ=2)
            MATCH     FDESCRP,SIIADES
            GOTO      RANGE900 IF LESS
            MATCH     SIIADES,TDESCRP
            GOTO      RANGE900 IF LESS
          ENDIF
.
          IF        !(REPTSEQ=3)
            MATCH     FGROUP,SIIASUB
            GOTO      RANGE900 IF LESS
            MATCH     SIIASUB,TGROUP
            GOTO      RANGE900 IF LESS
          ENDIF
.
          MOVE      SP70,SIIESID
.         srf 18093-chk if supp susp catlgs is yes and supp for both Reqn and PO
          IF        SUPCATYN=0
            IF        SIIASUS=3
              GOTO      RANGE900            * don't display the record if supp
            ENDIF
          ENDIF
.                                           * srf 18093 - end
          MATCH     SP70,SIWAWAR
          IF        !@EQUAL
            PACK      KEY12,SIIACAT,SIWAWAR
            CALL      RDSIIE1
            BRANCH    OVRCD,RANGE900
.                                           * If SUPCATYN and Suspended, Bypass
            IF        SUPCATYN=0
              IF        SIIESUS=3
                GOTO      RANGE900          * don't display if supp.   *I-f0902
              ENDIF                                                    *I-f0902
            ENDIF                                                      *I-f0902
          ENDIF
.
          BRANCH    INCLTYPE,RANGE510,RANGE520,RANGE530,RANGE530,RANGE530:
                             RANGE530,RANGE530,RANGE530
          GOTO      RANGE999
.
. SIIANON   FORM      1      Non-Stock Item 0=No
.------------------------------------------------
RANGE510  COMPARE   ONE,SIIANON
          GOTO      RANGE900 IF EQUAL   * Stock Only
          GOTO      RANGE999
.
RANGE520  COMPARE   ZERO,SIIANON
          GOTO      RANGE900 IF EQUAL   * Non-Stock Only
          GOTO      RANGE999
.
RANGE530  ASSIGN    INCLTYPE-2,F2
.
          MATCH     SP70,SIIESID
          IF        @EQUAL
            RESET     SIIASTI,F2
            CMATCH    "1",SIIASTI
            GOTO      RANGE900 IF NOT EQUAL
            GOTO      RANGE999
          ELSE
            RESET     SIIESID,F2
            CMATCH    "1",SIIESID
            GOTO      RANGE900 IF NOT EQUAL
            GOTO      RANGE999
          ENDIF
.
RANGE900  MOVE      ONE,EXIT
.
RANGE999  RETURN
