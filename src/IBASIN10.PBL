            IF        OVRCD=0
              MOVE      SICDDESC,CONFDESC
            ENDIF
          ENDIF
.
          MATCH     SP70,SIORUC1
          IF        !(@EQUAL)
            CLEAR     KEY6
            APPEND    "or1",KEY6
            APPEND    SIORUC1,KEY6
            RESET     KEY6
            CALL      RDSICD1
            IF        OVRCD=0
              MOVE      SICDDESC,UDC1DESC
            ENDIF
          ENDIF
.
          MATCH     SP70,SIORUC2
          IF        !(@EQUAL)
            CLEAR     KEY6
            APPEND    "or2",KEY6
            APPEND    SIORUC2,KEY6
            RESET     KEY6
            CALL      RDSICD1
            IF        OVRCD=0
              MOVE      SICDDESC,UDC2DESC
            ENDIF
          ENDIF
.
          MOVE      ZERO,EXIT
          MOVE      ZERO,OVRCD
          GOTO      EORIG999
.
EORIG200  MOVE      SP70,DISPMSG
          CALL      MESSAGE2
          KEYIN     *P1:24,*EL,"Record Locked - (":
                    *V2LON,"R",*HOFF,")etry or e(":
                    *V2LON,"X",*HOFF,")it ? ",ANS
          CALL      MESSAGE3
          REP       UPPLOW,ANS
          MATCH     ANSR,ANS
          GOTO      EORIG100 IF EQUAL
          MATCH     ANSX,ANS
          GOTO      EORIG000 IF EQUAL
          MATCH     ANSE,ANS
          GOTO      EORIG200 IF NOT EQUAL
          GOTO      EORIG000
EORIG999  RETURN
.----------------------------------------------
. Enter Confirmation Code
.----------------------------------------------
ECONF000  MOVE        ZERO,CKEYTYP
          MOVE        "CF ",SICDCATA
          MOVE        SP70,SIORCON
          MOVE        SP70,CONFDESC
          CALL        KSICDB00
          BRANCH      EXIT,ECONF999,ECONF999
          MOVE        SICDCODE,SIORCON
          MOVE        SICDDESC,CONFDESC
.
ECONF999  RETURN
.

EDELV000  KEYIN     *PCCOL:CVERT,*V2LON,SIORDEL
          BRANCH    SIORDEL,EDELV100,EDELV100,EDELV100,EDELV100:
                            EDELV100,EDELV100,EDELV100,EDELV100
          BEEP
          GOTO      EDELV000
EDELV100  LOAD      DELVDESC,SIORDEL,HADEL1,HADEL2,HADEL3,HADEL4:
                    HADEL5,HADEL6,HADEL7,HADEL8
EDELV999  RETURN
.
.----------------------------------------------
. Enter Message Code
.----------------------------------------------
EMESS000  MOVE        ZERO,CKEYTYP
          MOVE        "ME ",SICDCATA
          MOVE        SP70,MESSDESC
          MOVE        SP70,SIORMESS
          CALL        KSICDB00
          BRANCH      EXIT,EMESS999,EMESS999
          MOVE        SICDCODE,SIORMESS
          MOVE        SICDDESC,MESSDESC
.
EMESS999  RETURN
.
.----------------------------------------------
. Enter Code
.----------------------------------------------
EUDC1000  MOVE        ZERO,CKEYTYP
          MOVE        "or1",SICDCATA
          MOVE        SP70,UDC1DESC
          CALL        KSICDB00
          BRANCH      EXIT,EUDC1999,EUDC1999
          MOVE        SICDCODE,SIORUC1
          MOVE        SICDDESC,UDC1DESC
.
EUDC1999  RETURN
.
.----------------------------------------------
. Enter Code
.----------------------------------------------
EUDC2000  MOVE        ZERO,CKEYTYP
          MOVE        "or2",SICDCATA
          MOVE        SP70,UDC2DESC
          CALL        KSICDB00
          BRANCH      EXIT,EUDC2999,EUDC2999
          MOVE        SICDCODE,SIORUC2
          MOVE        SICDDESC,UDC2DESC
.
EUDC2999  RETURN
.
POST0000  PACK     KEY3,SIORORIG
          CALL     RASIOR1
          IF       OVRCD=0
            CALL     UPSIOR1
            MOVE     THREE,AUDTTYPE
            CALL     WASIOR00                 * Audit Change
            CALL     UUSIOR1                  * Unlock
          ELSE
            CALL     WRSIOR1
            MOVE     ONE,AUDTTYPE
            CALL     WASIOR00                 * Audit Add
          ENDIF
          RETURN
DELT0000  MOVE     SP70,DISPMSG
          CALL     MESSAGE2
          CALL     DELQST
          CALL     MESSAGE3
          SUB      ONE,CEXIT
          BRANCH   CEXIT,DELT9000,DELT9100
.
          PACK     KEY3,SIORORIG
          CALL     RASIOR1
          IF       OVRCD=0
            CALL     DASIOR00                 * Delete Before Audit
            MOVE     FOUR,AUDTTYPE
            CALL     WASIOR00                 * Delete Audit
            CALL     DESIOR1
          ENDIF
          MOVE     TWO,EXIT
          GOTO     DELT9999
.
DELT9000  MOVE     ONE,EXIT
          GOTO     DELT9999
.
DELT9100  CALL     CANC0000
          MOVE     TWO,EXIT
          GOTO     DELT9999
.
DELT9999  RETURN
CANC0000  PACK     KEY3,SIORORIG
          CALL     RASIOR1
          IF       OVRCD=0
            CALL     DASIOR00                 * Delete Before Audit
            CALL     UUSIOR1                  * Unlovk
          ENDIF
          RETURN
