          GOTO     LP02L105 IF NOT EQUAL
          PACK     KEY67,SIIF2SKY,Z70
          CALL     RSSIIF2  * End
          IF       OVRCD=1
            CALL     RPSIIF2 
          ENDIF
          MOVE     ZERO,SECTOR
          WHILE    SECTOR<SIIF2SNO
            CALL     RPSIIF2 
            IF       OVRCD=1
              MOVE     SIIF2SNO,SECTOR
            ENDIF
            PACK     SIIF2MKY,SIIFCAT,SIIFALI                                                                
