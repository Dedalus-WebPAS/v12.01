SCOD0600  MOVE      ONE,INSSVHA
          MOVE      "HSA Customer Code ",KEY18
          PACK      HEADMESS,KEY18,SNCOVHA6
          PACK      KEY6,SNCOVHA6,SP70
          REP       " 0",KEY6
