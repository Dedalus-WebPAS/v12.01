SCOD0100  MOVE      ONE,INSSVHA
          MOVE      "HSA Customer Code ",KEY18
          PACK      HEADMESS,KEY18,INCNVHAC
          READ      CONTROLF,TWENTY6;*92,INCNTVHA
          GOTO      SCOD9999
.
SCOD0200  MOVE      ONE,INSSVHA
          MOVE      "HSA Customer Code ",KEY18
          PACK      HEADMESS,KEY18,INCNVHA2
          PACK      KEY6,INCNVHA2,SP70
