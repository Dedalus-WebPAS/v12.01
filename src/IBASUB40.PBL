SCOD0700  MOVE      ONE,INSSVHA
          MOVE      "HSA Customer Code ",KEY18
          PACK      HEADMESS,KEY18,SNCOVHA7
          PACK      KEY6,SNCOVHA7,SP70
          REP       " 0",KEY6
          MATCH     "000000",KEY6
          IF        @EQUAL
            DISPLAY     *P1:23,*EL,*B,"Invalid Customer Code. - ";
