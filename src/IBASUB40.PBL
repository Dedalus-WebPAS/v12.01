            GOTO        SCOD0000
          ENDIF
          READ      CONTROLF,TWENTY6;*92,INCNTVHA
          PACK      INCNVHAC,SNCOVHA7,SP70
          GOTO      SCOD9999
.
SCOD0800  MOVE      ONE,INSSVHA
          MOVE      "HSA Customer Code ",KEY18
          PACK      HEADMESS,KEY18,SNCOVHA8
          PACK      KEY6,SNCOVHA8,SP70
