          MATCH     SP70,NHDMC001
          GOTO      CHKDOM90 IF EQUAL
.
          RESET     NHDMC002
          MATCH     SP70,NHDMC002
          GOTO      CHKDOM91 IF EQUAL
.
          GOTO      CHKDOM98
.
