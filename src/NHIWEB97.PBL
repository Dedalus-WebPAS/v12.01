PRVGRP00  MOVE      NORECORD,KEY2
          REP       " +",KEY2
          ASSIGN    STARTNUM-NORECORD,F5                      * C CAR 41918
          IF        F5<0
            MOVE      ZERO,F5
          ENDIF
          MOVE      F5,KEY5
          REP       " +",KEY5                                 * end C CAR 41918
          UNPACK    DIM127,D1,FLDPARA,DIM127
          MOVE      REPORTNO,F1
.
