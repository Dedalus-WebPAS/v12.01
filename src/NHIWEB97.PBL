          REP       " +",KEY2
          ASSIGN    STARTNUM+NORECORD,F3
          UNPACK    DIM127,D1,FLDPARA,DIM127
          MOVE      RECNUMB,KEY5
          REP       " +",KEY5
          MOVE      REPORTNO,F1
.
          WRITE     HTMLFILE;"nhiweb97.pbl":
                                 "?reportno=",F1:
