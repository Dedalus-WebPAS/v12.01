                    "  (All waiting times are shown as number of weeks.)",*N
          ADD       TWO,CLNO
.
HEAD2000  PRINT     "Speciality       : ",*20,*+,SSPCDESC," to ":
                                             ESPCDESC
          PRINT     "Hospital         : ",*20,*+,SHSPDESC," to ":
                                             EHSPDESC
          PRINT     "Clinic Id        : ",*20,*+,SCLIDESC," to ":
                                             ECLIDESC
.
          ADD       THREE,CLNO  
          MOVE      ZERO,OUTHEAD
