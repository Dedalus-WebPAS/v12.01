.******************************************************************************
.         Check if more of the invoices are to be displayed
.
.******************************************************************************
DSPSUMMK  MATCH     "IBARSH",PGM
          GOTO      DSPSUMM1 IF EQUAL
.
          KEYIN     *P1:24,"Display more (",*V2LON,*DV,ANSY,*HOFF,*DV,SLASH:
                    *V2LON,*DV,ANSN,*HOFF,") ? ",*V2LON,ANS;
          REP       UPPLOW,ANS
.
          MATCH     ANSN,ANS
          GOTO      DSPSUMM1 IF EQUAL
          MATCH     ANSY,ANS
          IF        @EQUAL
