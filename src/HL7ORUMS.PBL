          IF        @EQUAL
            APPEND    "~",OUT127
            ADD       ONE,OUTSTLEN
            COMPARE   "127",OUTSTLEN
            CALL      WRTNTE00 IF EQUAL
            GOTO      FMTNO100 
          ENDIF
