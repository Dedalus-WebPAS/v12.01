          ENDIF
          CMATCH    "-",OBR01402
          IF        @EQUAL
            PACK      OBR01402,ZERO,ZERO,ZERO,ZERO
          ENDIF
          UNPACK    OBR01402,TEMPHRS,TEMPMIN
          PACK      TEMPHRS,TEMPHRS,SP70
