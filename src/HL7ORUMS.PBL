          MOVE      OBR00202,REHAPAP
          MOVE      OBR00201,REHAPOR
          MOVE      OBR00302,REHAFAP
          MOVE      OBR00301,REHAFOR
          MOVE      OBR00401,REHAUSC
          MOVE      OBR00403,REHAUCS
          MOVE      ONE,REHANOR             * 1=yes
          MOVE      SP8,REHACDT
          MOVE      SP5,REHACTM
          MOVE      OBR010,REHACID
          MOVE      OBR01401,REHASDT
          CMATCH    "+",OBR01402
          IF        @EQUAL
