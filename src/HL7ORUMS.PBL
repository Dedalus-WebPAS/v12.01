            ENDIF
            APPEND    "&AMP",OUT127
            ADD       FOUR,OUTSTLEN
            COMPARE   "127",OUTSTLEN
            CALL      WRTNTE00 IF EQUAL
            GOTO      FMTNO100 
          ENDIF
