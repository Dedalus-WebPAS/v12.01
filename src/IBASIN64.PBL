          IF        OVRCD=1
            PACK      KEY27,SIIACAT,INCNVHAS,SP70
            UNPACK    SP70,SIICCAT,SIICSUP
            CALL      RSSIIC1
            CALL      RKSIIC1
            PACK      KEY12,SIICCAT,SIICSUP,SP70
            MATCH     KEY12,KEY27
            GOTO      KCAT9999 IF EQUAL
            DISPLAY   *P1:24,*B,*EL,"Catalog ",*V2LON,SIIACAT,*HOFF:
                      " Not Linked To Supplier ",*V2LON,INCNVHAS,*HOFF," - ";
            CALL      HITENTER
            GOTO      KCAT0000
          ENDIF
