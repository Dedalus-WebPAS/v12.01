          MOVE      CCITEM,FIELD
          COMPARE   ZERO,FIELD
          RETURN    IF EQUAL
          GOTO      OVERCOND IF LESS
          BRANCH    FIELD OF KSOS11,KSOS12,KSOS13,KSOS14,KSOS15,KSOS16:
