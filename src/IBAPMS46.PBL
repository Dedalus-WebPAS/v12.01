+
.------------------------------------------------------------
. Print Report Heading
.------------------------------------------------------------
HEAD0000  CALL      IBACLOCK
          CALL      HEAD80
.
          UNPACK    STRTDATE,CCENT,CYEAR,CMON,CDAY
          CALL      PACDATE
          MOVE      CPCDATE,DISPFDAT
.
          UNPACK    ENDDDATE,CCENT,CYEAR,CMON,CDAY
          CALL      PACDATE
          MOVE      CPCDATE,DISPTDAT
.
          MATCH     "A4  ",TRIGTYPE
          IF        @EQUAL
            PRINT     *20,"Emergency Visit: All Objects"
            PRINT     *20,"ED Visit Date :",DISPFDAT," to ",DISPTDAT
          ENDIF
.
          MATCH     "A5  ",TRIGTYPE
          IF        @EQUAL
            PRINT     *20,"Non Admitted Patient: Update Outpatient Visit"
            PRINT     *20,"Booking Date :",DISPFDAT," to ",DISPTDAT
          ENDIF
.
          MATCH     "A14 ",TRIGTYPE
          IF        @EQUAL
            PRINT     *20,"Admitted Patient: Admission Details Update"
            PRINT     *20,"Discharge Date :",DISPFDAT," to ",DISPTDAT
          ENDIF
.
          MATCH     "A24 ",TRIGTYPE
          IF        @EQUAL
            PRINT     *20,"Admitted Patient: All Bed Transfers"
            PRINT     *20,"Admission Number :",VISTNUMB
          ENDIF
.
          MATCH     "A25 ",TRIGTYPE
          IF        @EQUAL
            PRINT     *20,"Admitted Patient: All Leave Records"
            PRINT     *20,"Discharge Date :",DISPFDAT," to ",DISPTDAT
          ENDIF
.
          MATCH     "A25X",TRIGTYPE
          IF        @EQUAL
            PRINT     *20,"Admitted Patient: All Leave Records"
            PRINT     *20,"Admission Number :",VISTNUMB
          ENDIF
.
          MATCH     "E29 ",TRIGTYPE
          IF        @EQUAL
            PRINT     *20,"Client : 01 Object By U/R Creation Date Range"
            PRINT     *20,"Creation Date :",DISPFDAT," to ",DISPTDAT
          ENDIF
.
          MATCH     "E29X",TRIGTYPE
          IF        @EQUAL
            PRINT     *20,"Client : 01 Object Single U/R"
            PRINT     *20,"U/R :",FROMURNO
          ENDIF
.
          ADD       TWO,CLNO
.
          IF        COPTION=1
            PRINT     *20,"Report Only"
          ELSE
            PRINT     *20,"Report and Trigger Records"
          ENDIF
.
          CALL      UND80
.
          MATCH     "A4  ",TRIGTYPE
          IF        @EQUAL
            PRINT     *1,"| Visit    ",*12,"| ED Visit Date",*80,"|"
          ENDIF
.
          MATCH     "A5  ",TRIGTYPE
          IF        @EQUAL
            PRINT     *1,"| Visit    ",*12,"| Booking Date",*80,"|"
          ENDIF
.
          MATCH     "A14 ",TRIGTYPE
          IF        @EQUAL
            PRINT     *1,"| Visit    ",*12,"| Discharge Date",*80,"|"
          ENDIF
.
          MATCH     "A24 ",TRIGTYPE
          IF        @EQUAL
            PRINT     *1,"| Visit    ",*12,"| Admission Date",*80,"|"
          ENDIF
.
          MATCH     "A25 ",TRIGTYPE
          IF        @EQUAL
            PRINT     *1,"| Visit    ",*12,"| Admission Date",*80,"|"
          ENDIF
.
          MATCH     "A25X",TRIGTYPE
          IF        @EQUAL
            PRINT     *1,"| Visit    ",*12,"| Admission Date",*80,"|"
          ENDIF
.
          MATCH     "E29 ",TRIGTYPE
          IF        @EQUAL
            PRINT     *1,"| U/R      ",*12,"| Creation Date",*80,"|"
          ENDIF
.
          MATCH     "E29X",TRIGTYPE
          IF        @EQUAL
            PRINT     *1,"| U/R      ",*12,"| Creation Date",*80,"|"
          ENDIF
.
          ADD       ONE,CLNO
          ADD       ONE,CLNO
.
          CALL      UND80
.
HEAD9999  RETURN
+
.-----------------------------------------------------------
. Write EDWARD emergency visit trigger record
.------------------------------------------------------------
WEDT0000  BRANCH    COPTION,WEDT9999        * Report Only
.
          MOVE      EMVIADMN,PMAWVISN
          MOVE      FOUR,PMAWTYPE
          PACK      PMAWOLDV,SP70,SP70,SP70,SP70
          PACK      PMAWNEWV,SP70,SP70,SP70,SP70
.
          CALL      IBACLOCK
          PACK      PMAWCDAT,CCC,CYY,CMM,CDD
          REP       " 0",PMAWCDAT
          MOVE      "00:00:00",PMAWCTIM
          MOVE      "IBAPMS46",PMAWCUID
          PACK      PMAWSPAR,SP70,SP70
.
          PACK      KEY27,PMAWCDAT,PMAWCTIM,PMAWVISN,PMAWTYPE,SP70
          CALL      RAPMADW1
          IF        OVRCD=1
            CALL      WRPMADW1
          ELSE
            CALL      UPPMADW1
          ENDIF
.
WEDT9999  RETURN
+
.-----------------------------------------------------------
. Write EDWARD update inpatient visit trigger - DADMNO
.------------------------------------------------------------
WEDV0000  BRANCH    COPTION,WEDV9999        * Report Only
.
          MOVE      DADMNO,PMAWVISN
          UNPACK    TRIGTYPE,D1,PMAWTYPE
          PACK      PMAWOLDV,SP70,SP70,SP70,SP70
          PACK      PMAWNEWV,SP70,SP70,SP70,SP70
.
          CALL      IBACLOCK
          PACK      PMAWCDAT,CCC,CYY,CMM,CDD
          REP       " 0",PMAWCDAT
          MOVE      "00:00:00",PMAWCTIM
          MOVE      "IBAPMS46",PMAWCUID
          PACK      PMAWSPAR,SP70,SP70
.
          PACK      KEY27,PMAWCDAT,PMAWCTIM,PMAWVISN,PMAWTYPE,SP70
          CALL      RAPMADW1
          IF        OVRCD=1
            CALL      WRPMADW1
          ELSE
            CALL      UPPMADW1
          ENDIF
.
WEDV9999  RETURN
+
.-----------------------------------------------------------
. Write EDWARD update inpatient visit trigger - AADMNO
.------------------------------------------------------------
WEAV0000  BRANCH    COPTION,WEAV9999        * Report Only
.
          MOVE      AADMNO,PMAWVISN
.
          MATCH     "A25X",TRIGTYPE
          IF        @EQUAL
            UNPACK    TRIGTYPE,D1,D2
            PACK      PMAWTYPE,D2,SP70
          ELSE
            UNPACK    TRIGTYPE,D1,PMAWTYPE
          ENDIF
.
          PACK      PMAWOLDV,SP70,SP70,SP70,SP70
          PACK      PMAWNEWV,SP70,SP70,SP70,SP70
.
          CALL      IBACLOCK
          PACK      PMAWCDAT,CCC,CYY,CMM,CDD
          REP       " 0",PMAWCDAT
          MOVE      "00:00:00",PMAWCTIM
          MOVE      "IBAPMS46",PMAWCUID
          PACK      PMAWSPAR,SP70,SP70
.
          PACK      KEY27,PMAWCDAT,PMAWCTIM,PMAWVISN,PMAWTYPE,SP70
          CALL      RAPMADW1
          IF        OVRCD=1
            CALL      WRPMADW1
          ELSE
            CALL      UPPMADW1
          ENDIF
.
WEAV9999  RETURN
.
.-----------------------------------------------------------
. Write EDWARD update OP visit trigger type 5
.------------------------------------------------------------
WEDA0000  BRANCH    COPTION,WEDV9999        * Report Only
.
          MOVE      OBAOUTNO,PMAWVISN
          UNPACK    TRIGTYPE,D1,PMAWTYPE
          PACK      PMAWOLDV,SP70,SP70,SP70,SP70
          PACK      PMAWNEWV,SP70,SP70,SP70,SP70
.
          CALL      IBACLOCK
          PACK      PMAWCDAT,CCC,CYY,CMM,CDD
          REP       " 0",PMAWCDAT
          MOVE      "00:00:00",PMAWCTIM
          MOVE      "IBAPMS46",PMAWCUID
          PACK      PMAWSPAR,SP70,SP70
.
          PACK      KEY27,PMAWCDAT,PMAWCTIM,PMAWVISN,PMAWTYPE,SP70
          CALL      RAPMADW1
          IF        OVRCD=1
            CALL      WRPMADW1
          ELSE
            CALL      UPPMADW1
          ENDIF
.
WEDA9999  RETURN
.
.-----------------------------------------------------------
. Write EDWARD client 01 object trigger type 29
.------------------------------------------------------------
WCLN0000  BRANCH    COPTION,WCLN9999        * Report Only
.
          MOVE      URURNO,PMEWURNO
          MATCH     "E29X",TRIGTYPE
          IF        @EQUAL
            UNPACK    TRIGTYPE,D1,D2
            PACK      PMEWTYPE,D2,SP70
          ELSE
            UNPACK    TRIGTYPE,D1,PMEWTYPE
          ENDIF
          PACK      PMEWOLDV,SP70,SP70,SP70,SP70
          PACK      PMEWNEWV,SP70,SP70,SP70,SP70
.
          CALL      IBACLOCK
          PACK      PMEWCDAT,CCC,CYY,CMM,CDD
          REP       " 0",PMEWCDAT
          CLOCK     TIME,PMEWCTIM
          MOVE      "IBAPMS46",PMEWCUID
          PACK      PMEWSPAR,SP70,SP70
.
          PACK      KEY27,PMEWCDAT,PMEWCTIM,PMEWURNO,PMEWTYPE,SP70
          CALL      RAPMEDW1
          IF        OVRCD=1
            CALL      WRPMEDW1
          ELSE
            CALL      UPPMEDW1
          ENDIF
.
WCLN9999  RETURN
