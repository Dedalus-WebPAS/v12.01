          GOTO      HEAD900
.
HEAD800   PRINT     *1,"|",*10,"|",*19,"|",*28,"|",*37,"|",*76,"|",*80,"|  Old":
                   *89,"|  New",*98,"|",*102,"Old",*115,"|",*119,"New",*132,"|":
