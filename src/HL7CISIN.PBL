.
          MOVE      "Unknown Code",FIELDSTR
          PACK      KEY10,H7CIUSID
          CALL      RDWBSE1
          IF        OVRCD = 0
            MOVE      WBSENAM,FIELDSTR           * web user name
            CALL      SPCHR000                   * convert special characters
          ENDIF
          APPEND    CARET,EVN005
          APPEND    FIELDSTR,EVN005
          RESET     EVN005
.
GEVN5999  RETURN
