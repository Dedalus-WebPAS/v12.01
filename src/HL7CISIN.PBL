.*****************************************************************************
.*  Calculate expected Discharge Date and Time
.*****************************************************************************
CLBKD000  PACK      ENDDTIME,SP70                * Expected Discharge Time
.
          PACK      KEY5,ANSC,ANSV,OBAVISIT,SP70
          CALL      RDCODE1
          IF        OVRCD=1
            MOVE      ONE,TCFORM6
          ENDIF
          MOVE      ZERO,LOOPCNTR
.
          MOVE      OBASITE,KEY6                 * get site prefix
          CALL      RDSITA1                      * site found ?
          BRANCH    OVRCD,CLBKD999
.
          PACK      FNAME,OSTFILE,FILSESA1
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      OUTSESA1,FNAME               * open O/P clinic master file
          TRAPCLR   IO
          BRANCH    OVRCD,CLBKD999               * file not found
.
          PACK      KEY25,OBASITE,OBACLIN,OBADATE,OBASTRT,SP70
          CALL      RDSESA1                      * session on file ?
          BRANCH    OVRCD,CLBKD999               * no - try clinic master
.
          PACK      FNAME,OSTFILE,FILHEDA1
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      OUTHEDA1,FNAME               * open O/P clinic master file
          TRAPCLR   IO
          BRANCH    OVRCD,CLBKD999               * file not found
.
          PACK      KEY28,OSESITE,OSECLIN,OSEDAY,OSESTRT,OSESSHNO,OSESSDAT,SP70
          CALL      RDOTHED1                     * master header on file ?
          BRANCH    OVRCD,CLBKD999               * no - try clinic master
.
          UNPACK    OBATIME,KEY2,KEY1,MIN   * HH:MM
.
CLBKD100  MOVE      KEY2,HOUR               * HH to Form
          MOVE      MIN,MINUTES             * MM to Form
.
          ADD       OTHEDURN,MINUTES   * Add Duration time to minutes
          ASSIGN    MINUTES-SIXTY,TIME
          IF        TIME >= 0
            IF        TIME >= 60
              MOVE      TIME,MINUTES
              ASSIGN    MINUTES-SIXTY,TIME
              MOVE      TIME,F2
              MOVE      F2,MIN
              ADD       TWO,HOUR                 * Add Two Hours
            ELSE
              MOVE      TIME,F2
              MOVE      F2,MIN
              ADD       ONE,HOUR                 * Add One Hour
            ENDIF
          ELSE
            MOVE      MINUTES,F2            * No Hour Change
            MOVE      F2,MIN
          ENDIF
.
          PACK      ENDDTIME,HOUR,MIN        * pack time save variable (HH:MM)
          REPLACE   " 0",ENDDTIME            * replace spaces with zero
.
          ADD       ONE,LOOPCNTR
.
          COMPARE   TCFORM6,LOOPCNTR
          IF        @LESS
            UNPACK    ENDDTIME,KEY2,MIN   * HH:MM
            GOTO      CLBKD100
          ENDIF
.
CLBKD999  RETURN
