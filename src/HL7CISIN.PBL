          IF        @EQUAL
            MOVE      ONE,DISPFLAG
            DISPLAY   *P1:4,*EF;
            GOTO      INIT9000
          ENDIF
.
          MATCH     ANSN,ANS
          IF        @EQUAL
            MOVE      ZERO,DISPFLAG
            CALL      BACK0000              * Display background screen
            GOTO      INIT9000
          ENDIF
.
          BEEP
          GOTO      INIT1000
.
