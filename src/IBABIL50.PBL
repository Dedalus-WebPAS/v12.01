          COMPARE   "51",ARRAYCNT
          IF        !@EQUAL
            WRITE     WORK1,SEQ;TPROCCD[ARRAYCNT],TPROCLOC[ARRAYCNT];
            GOTO      WREC3002
          ENDIF
.
          WRITE     WORK1,SEQ;TUPLTHEA,TRATYPE,DELIND
          GOTO      WREC4000
