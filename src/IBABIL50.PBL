          COMPARE   "50",ARRAYCNT
          IF        !@EQUAL
            WRITE     WORK1,SEQ;TRDIAG[ARRAYCNT],TRDIAGCD[ARRAYCNT];
            GOTO      WREC3001
          ENDIF                               
.
          WRITE     WORK1,SEQ;TPROCCD[1],TPROCLOC[1],TPRDATE8;
.
          MOVE      ONE,ARRAYCNT                * Write Proc/Location 2-50
