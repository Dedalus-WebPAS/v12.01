          ADD       ONE,EXTFLAG
.
          MATCH     "6",PT0DACRQ
          GOTO      PECS0100 IF EQUAL   
.
          MATCH     "7",PT0DACRQ
          GOTO      PECS0200 IF EQUAL      
.
          MATCH     "8",PT0DACRQ
          GOTO      PECS0300 IF EQUAL     
