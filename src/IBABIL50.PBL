          APPEND    "HCS_TO_DISK ",CMDLINE
          APPEND    FILENAME,CMDLINE
          APPEND    ".txt",CMDLINE
          APPEND    SP1,CMDLINE
          APPEND    DISKNAME,CMDLINE
          RESET     CMDLINE
