          MATCH     BLANKSTR,PV101901            * field null ?
          IF        @EQUAL
            MOVE      "PV1.19: Visit Number is null",ERRORDSC
            MOVE      "207",ERRORNUM
            CALL      EROR0000
          ENDIF
.
MPV17300  STRIP     PV102001
          MOVELPTR  PV102001,FORM3               * field populated ?
          COMPARE   ZERO,FORM3
          GOTO      MPV17400 IF EQUAL
