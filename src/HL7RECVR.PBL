          MOVELPTR  PID01104[2],FORM3            * field populated ?
          IF        FORM3 = 0
            GOTO      VPID8700                   * no
          ENDIF
.
          MATCH     BLANKSTR,PID01104[2]         * null field ?
          IF        @EQUAL
