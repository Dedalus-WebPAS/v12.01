              CLEAR     PID01001
            ELSE
              MOVE      "PID.10: Race incorrect length",ERRORDSC
              MOVE      "207",ERRORNUM
              CALL      EROR0000
            ENDIF
            GOTO      VADD0050
          ENDIF
