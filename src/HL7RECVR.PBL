          ENDIF
.
          MOVE      ZXP05701,DIM14
          MOVE      "ZXP.57",FIELDID
          MOVE      ZERO,DATFLAG1
          MOVE      ZERO,DATFLAG2
          CALL      VDTM0000                     * date/time validation
.
          MOVE      ZXP05701,CMEXPIRY
.
.         NDIS Expiry Date
.
VADD0780  SQUEEZE   ZXP06001
          MOVELPTR  ZXP06001,FORM3               * field populated ?
          IF        FORM3 = 0
