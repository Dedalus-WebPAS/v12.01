            IF        QUICKREG = 1
              PACK      PID01501,SP100,SP100,SP100,SP100,SP100
              CLEAR     PID01501
            ELSE
              MOVE      "PID.15: Preferred Language not numeric",ERRORDSC
              MOVE      "207",ERRORNUM
              CALL      EROR0000 
