.*****************************************************************************
.*                            VNTE0000             Called by: PROC0000       *
.*      Validate that the mandatory NTE fields are populated or null and     *
.*      load the data into a temporary table so that we can accommodate more *
.*      than 1 NTE segment/message.                                          *
.*****************************************************************************
.
.         Use the segment count for each NTE segment if no NTE.1 sent
.
VNTE0000  MOVE      NTESEGMT,RTMPSEGI            * load segment id
          ASSIGN    NTECOUNT,RTMPSEGC            * save previous count
          ADD       ONE,NTECOUNT                 * increment NTE segment count
          MOVE      ZERO,NTEFOUND                * set NTE found flag
.
.         Set ID (NTE.1)
.
          SQUEEZE   NTE00101
          MOVELPTR  NTE00101,FORM3
          COMPARE   ZERO,FORM3                   * field populated ?
          IF        @EQUAL
            MOVE      NTECOUNT,NTE00101
            GOTO      VNTE0100
          ENDIF
.
          COMPARE   FORM3,THREE
          IF        @LESS
            MOVE      "NTE.1: Set ID has too many characters",ERRORDSC
            MOVE      "207",ERRORNUM
            CALL      EROR0000
            GOTO      VNTE0200
          ENDIF
.
          MATCH     BLANKSTR,NTE00101            * field null ?
          IF        @EQUAL
            ADD       ONE,NTECOUNT               * no - increment count
            MOVE      NTECOUNT,NTE00101
            GOTO      VNTE0100
          ENDIF
.
          TYPE      NTE00101                     * field numeric ?
          IF        !@EQUAL
            MOVE      "NTE.1: Set ID is not numeric",ERRORDSC
            MOVE      "102",ERRORNUM
            CALL      EROR0000
            GOTO      VNTE0200
          ENDIF
.
VNTE0100  RJUSTIFY  NTE00101
          MOVE      NTE00101,FORM3
.
.         Check the NTE count is the same as the Set ID
.
          COMPARE   FORM3,NTECOUNT
          IF        !@EQUAL
            MOVE      "NTE.1: Set ID not in sequence",ERRORDSC
            MOVE      "207",ERRORNUM
            CALL      EROR0000
            GOTO      VNTE0200
          ENDIF
.
.         Check the new Set ID is one more than the previous Set ID
.
          SUB       ONE,FORM3
          COMPARE   FORM3,RTMPSEGC
          IF        !@EQUAL
            MOVE      "NTE.1: Set ID not in sequence",ERRORDSC
            MOVE      "207",ERRORNUM
            CALL      EROR0000
            GOTO      VNTE0200
          ENDIF
.
          MOVE      NTE00101,RTMPSEGC            * load NTE segment count
          MOVE      ONE,RTMPFLDN                 * load field number
          MOVE      ONE,RTMPFCNT                 * load field number count
          MOVE      ONE,RTMPCMPN                 * load component number
          PACK      RTMPVRBL,NTE00101,SP100,SP100,SP100,SP100,SP100
          CALL      WTMP0000
.
.         Comment Source (NTE.2)
.
VNTE0200  STRIP     NTE00201
.
          COMPARE   TEN,MESSGNUM
          GOTO      VNTE0300 IF NOT EQUAL        * only validate for M02
.
          MATCH     "1",NTE00201
          IF        !@EQUAL
            MOVE      "NTE.2: Comment Source must be 1 for HCP",ERRORDSC
            MOVE      "207",ERRORNUM
            CALL      EROR0000
            GOTO      VNTE0300
          ENDIF
.
          MOVE      NTE00101,RTMPSEGC            * load NTE segment count
          MOVE      TWO,RTMPFLDN                 * load field number
          MOVE      ONE,RTMPFCNT                 * load field number count
          MOVE      ONE,RTMPCMPN                 * load component number
          PACK      RTMPVRBL,NTE00201,SP100,SP100,SP100,SP100,SP100
          CALL      WTMP0000
.
.         Load Comments (NTE.3)
.
VNTE0300  STRIP     NTE00301
          MOVELPTR  NTE00301,FORM4
          COMPARE   ZERO,FORM4                   * field populated ?
          IF        @EQUAL
            PACK      NTE00301,SP100,SP100,SP100,SP100,SP100,SP100,SP100,SP100:
                               SP100,SP100,SP100,SP100,SP100,SP100,SP100,SP100:
                               SP100,SP100,SP100,SP100,SP100,SP100,SP100,SP100:
                               SP100,SP100,SP100,SP100,SP100,SP100,SP100,SP100:
                               SP100,SP100,SP100,SP100,SP100,SP100,SP100,SP100
            CLEAR     NTE00301
            GOTO      VNTE0350
          ENDIF
.
          MATCH     BLANKSTR,NTE00301            * field null ?
          IF        @EQUAL
            COMPARE   TEN,MESSGNUM
            GOTO      VNTE0350 IF EQUAL          * leave as "" for M02
.
            COMPARE   TEN2,MESSGNUM
            GOTO      VNTE0350 IF EQUAL          * leave as "" for S14
.
            PACK      NTE00301,SP100,SP100,SP100,SP100,SP100,SP100,SP100,SP100:
                               SP100,SP100,SP100,SP100,SP100,SP100,SP100,SP100:
                               SP100,SP100,SP100,SP100,SP100,SP100,SP100,SP100:
                               SP100,SP100,SP100,SP100,SP100,SP100,SP100,SP100:
                               SP100,SP100,SP100,SP100,SP100,SP100,SP100,SP100
            CLEAR     NTE00301
            GOTO      VNTE0350
          ENDIF
.
VNTE0350  IF        MESSGNUM = 10
            CALL      PTCO0000                   * process M02 HCP comments
          ELSE
            CALL      PODC0000                   * process appointment comments
          ENDIF
.
.         Comment Type (NTE.4)
.
VNTE0400  STRIP     NTE00401
.
          COMPARE   TEN,MESSGNUM
          GOTO      VNTE0900 IF NOT EQUAL        * only validate for M02
.
          MATCH     "1",NTE00201
          GOTO      VNTE0900 IF NOT EQUAL        * only validate for HCP
.
          MOVELPTR  NTE00401,FORM3
          COMPARE   ZERO,FORM3                   * field populated ?
          GOTO      VNTE0450 IF NOT EQUAL        * yes
.
          MATCH     BLANKSTR,NTE00401
          GOTO      VNTE0450 IF NOT EQUAL        * field "" ?
.
          MOVE      "NTE.4: Comment Type must be populated for HCP comments",ERRORDSC
          MOVE      "103",ERRORNUM
          CALL      EROR0000
          GOTO      VNTE0900
.
VNTE0450  PACK      KEY5,ANST,ANSZ,NTE00401,SP5
          CALL      RDCODE1                      * field valid on Cat TZ ?
          IF        OVRCD = 1
            MOVE      "NTE.4: Comment Type not valid on Category TZ",ERRORDSC
            MOVE      "103",ERRORNUM
            CALL      EROR0000
            GOTO      VNTE0900
          ENDIF
.
          MATCH     ANSI,PTCOACTV                * inactive ?
          IF        @EQUAL
            MOVE      "NTE.4: Comment Type not active on Category TZ",ERRORDSC
            MOVE      "103",ERRORNUM
            CALL      EROR0000
            GOTO      VNTE0900
          ENDIF
.
          MATCH     ANSR,TCINDC5                 * receiving this type ?
          GOTO      VNTE0500 IF EQUAL            * yes
.
          MATCH     ANSB,TCINDC5                 * sending/receiving this type ?
          GOTO      VNTE0500 IF EQUAL            * yes
.
          MOVE      "NTE.4: Indicator 5 on Category TZ code must be R or B",ERRORDSC
          MOVE      "103",ERRORNUM
          CALL      EROR0000
          GOTO      VNTE0900
.
VNTE0500  MOVE      NTE00101,RTMPSEGC            * load NTE segment count
          MOVE      FOUR,RTMPFLDN                 * load field number
          MOVE      ONE,RTMPFCNT                 * load field number count
          MOVE      ONE,RTMPCMPN                 * load component number
          PACK      RTMPVRBL,NTE00401,SP100,SP100,SP100,SP100,SP100
          CALL      WTMP0000
.
VNTE0900
.
VNTE9999  RETURN
+
.*****************************************************************************
.*                               PTCO0000                                    *
.*                                                                           *
.*                Process the hcp comments field                             *
.* Requires: NTE00301 - HCP Message Comments                                 *
.*****************************************************************************
.
PTCO0000  STRIP     NTE00301
          MOVE      NTE00301,OLDSTRNG
          CALL      CONV0000                     * convert any escape sequences
          MOVE      NEWSTRNG,NTE00301
          MOVELPTR  NTE00301,SAVFORM4
.
          COMPARE   ZERO,SAVFORM4                * field populated ?
          GOTO      PTCO9999 IF EQUAL            * no - finished
.
.         There are hcp comments in the field, so write to tempfile (pmstcoaf)
.         "" is allowed (used to signify deleting the comments for that type)
.
          CALL      WTCO0000
.
PTCO9999  RETURN
+
.*****************************************************************************
.*                           WTCO0000              Called by: PTCO0000       *
.*                    Write pmstcoaf record(s)                               *
.* Requires: PMSHCPFD - record exists on pmshcpaf for hcp                    *
.*           NTE00301 - HCP Message Comments                                 *
.*****************************************************************************
.
.         Bump through the text string on a character by character basis
.         and for each character check to see whether there is a line break.
.         Allow 70 characters/line, unless of course there is a line break
.         or the end of the string is reached.
.
WTCO0000  MOVE      ZERO,LINENUMB                * initialise line number
          MOVE      ZERO,CHRCOUNT                * initialise character count
          MOVE      SP100,DIM70
          CLEAR     DIM70
.
.         Check to see if we have a line break
.
WTCO0200  MOVE      ZERO,BREAKFLG                * initialise break found flag
          MATCH     LINBREAK,NTE00301
          IF        !@EQUAL | @LESS
            GOTO      WTCO0500
          ENDIF
.
.         We have found a line break, so if there is no data so far in the
.         the line (CHRCOUNT is zero), then ignore the line break, otherwise,
.         write the current line data
.
          IF        CHRCOUNT = 0
            BUMP      NTE00301,5                 * move past line break
            GOTO      WTCO9500 IF EOS            * eos - finished
            GOTO      WTCO0200                   * check for another line break
          ENDIF
.
          MOVE      ONE,BREAKFLG                 * set flag for break found
.
WTCO0300  CALL      WHLN0000                     * write a comment line
.
          MOVE      ZERO,CHRCOUNT                * re-initialise character count
.
WTCO0400  IF        BREAKFLG = 1
            BUMP      NTE00301,5                 * move past line break
          ELSE
            BUMP      NTE00301                   * get next character
          ENDIF
          GOTO      WTCO9000 IF EOS              * eos - finished
          GOTO      WTCO0200                     * check for another line break
.
.         We have a valid character, so load it into the text line and then
.         get the next character
.
WTCO0500  MOVE      NTE00301,ANS
          APPEND    ANS,DIM70                    * load current character
          ADD       ONE,CHRCOUNT                 * increment character count
.
          COMPARE   SEVENTY,CHRCOUNT             * full line yet ?
          GOTO      WTCO0400 IF LESS             * no
.
.         Check for blank space on char 70 so as not to split up words
.
WTCO1000  MATCH     SP1,ANS
          IF        !@EQUAL
            BUMP      DIM70,-1                   * go back to previous character
            LENSET    DIM70                      * set LL to FP position
            BUMP      NTE00301,-1
            MOVE      NTE00301,ANS
            MOVEFPTR  DIM70,FPPOSITN
            IF        FPPOSITN = 1
WTCO1500      SETLPTR   DIM70,70
              BUMP      NTE00301,69
              GOTO      WTCO0300
            ENDIF
            GOTO      WTCO1000                   * check for space again
          ELSE
            GOTO      WTCO0300                   * write comment if space
          ENDIF
.
WTCO9000  IF        CHRCOUNT > 0
            CALL      WHLN0000                   * write comment record
          ENDIF
.
WTCO9500  RESET     NTE00301
.
WTCO9999  RETURN
+
.*****************************************************************************
.*                                  WHLN0000       Called by: WTCO0000       *
.*        Write hcp comment line record to tempfile                          *
.*****************************************************************************
.
WHLN0000  ADD       ONE,LINENUMB                 * increment line number
          RESET     DIM70
.
          MOVE      THREE,RTMPFLDN
          MOVE      LINENUMB,RTMPFCNT
          MOVE      ONE,RTMPCMPN
          PACK      RTMPVRBL,DIM70,SP100,SP100,SP100,SP100,SP100
          CALL      WTMP0000
.
          MOVE      SP100,DIM70
          CLEAR     DIM70
.
WHLN9999  RETURN
