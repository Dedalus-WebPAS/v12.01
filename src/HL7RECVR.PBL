          MOVELPTR  PV100901,FORM3               * field populated ?
          COMPARE   ZERO,FORM3
          GOTO      EPV19999 IF EQUAL
.
          MATCH     BLANKSTR,PV100901            * field null ?
          GOTO      EPV19999 IF EQUAL
.
          PACK      KEY10,PV100901,SP70
          CALL      RDPMHCP1                     * field valid ?
