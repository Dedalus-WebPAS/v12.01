          LOAD      DIM8,CARDTYPE,CLEXPIRY,SNEXPIRY,DVEXPIRY,PNEXPIRY:
                                  SREXPIRY,CMEXPIRY,NDEXPIRY
          MOVELPTR  DIM8,FORM1
          IF        FORM1 <> 0
            LOAD      PMCDEXDT,CARDTYPE,CLEXPIRY,SNEXPIRY,DVEXPIRY,PNEXPIRY:
