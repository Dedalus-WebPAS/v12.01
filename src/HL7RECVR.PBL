          PACK      KEY3,EMVISITE,SP70
          CALL      RDEMSIT1
          IF        OVRCD<>1
            PACK      PMVXMHOS,EMSTHSNO,SP70
          ENDIF
.
          MOVE      PMVXVISN,KEY8
ZUNK2000  CALL      RAPMVX11
          IF        OVRCD=1
            CALL      WRPMVX11              * write visit extension record
          ELSE
            CALL      UPPMVX11
          ENDIF
