.
          MATCH     BLANKSTR,PRA00801            * field null ?
          IF        @EQUAL
            MOVE      "PRA.8: Date Entered Practice Date is mandatory",ERRORDSC
            MOVE      "101",ERRORNUM
            CALL      EROR0000
            GOTO      VPRA3000
          ENDIF
.
.         Make sure that there is the correct number of characters
.
          IF        FORM3 <> 8
