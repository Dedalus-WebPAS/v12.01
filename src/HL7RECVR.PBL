          MATCH     "I",STF00701                 * HCP Active ?
          IF        @EQUAL
            MOVE      "ZPR.15: HCP not active",ERRORDSC
            MOVE      "207",ERRORNUM
            CALL      EROR0000
            GOTO      VZPR9999
          ENDIF
