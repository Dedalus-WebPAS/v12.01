          GOTO      VZPR9999 IF EQUAL            * yes
.
          MATCH     SP8,PRA01001                 * Date Left Practice blank ?
          GOTO      VZPR9999 IF EQUAL            * yes
.
          MATCH     CURRDATE,PRA01001
          IF        @LESS
            MOVE      "ZPR.15: HCP not active in Practice",ERRORDSC
            MOVE      "207",ERRORNUM
            CALL      EROR0000
            GOTO      VZPR9999
          ENDIF
.
VZPR9999  RETURN
