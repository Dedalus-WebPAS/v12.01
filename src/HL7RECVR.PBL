          STRIP     PID01003
          MOVELPTR  PID01003,FORM3
          COMPARE   ZERO,FORM3                   * field populated ?
          IF        @EQUAL
            IF        QUICKREG = 1
              PACK      PID01001,SP100,SP100,SP100,SP100,SP100
              CLEAR     PID01001
            ELSE
              MOVE      "PID.10: Race coding system not populated",ERRORDSC
              MOVE      "101",ERRORNUM
