          MOVE      KEY6,OUTSITPR
          CALL      RDSITA1
          IF        OVRCD = 1
            MOVE      "User ID does not have a valid outpatient site",ERRORDSC
            MOVE      "103",ERRORNUM
            CALL      EROR0000
            GOTO      OUTSIT95
          ELSE
            MOVE      OSTFILE,OUTFILPR           * save outpatient file prefix
