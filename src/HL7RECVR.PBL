          CALL      RDCODE1
          IF        OVRCD = 1
            MOVE      "ZSC.4: Anaesthetic Key not valid",ERRORDSC
            MOVE      "103",ERRORNUM
            CALL      EROR0000
            GOTO      VZSC0150
          ENDIF
.
.         Verify the code is active
.
          MATCH     ANSI,PTCOACTV
          IF        @EQUAL
            MOVE      "ZSC.4: Anaesthetic Key not active",ERRORDSC
            MOVE      "207",ERRORNUM
            CALL      EROR0000
            GOTO      VZSC0150
          ENDIF
.
