.
          MOVE      ATIME,ADMNTIME
          REP       ": ",ADMNTIME
          SQUEEZE   ADMNTIME
          MOVELPTR  ADMNTIME,FORM2
          IF        FORM2 < 6
            ENDSET    ADMNTIME
            APPEND    "00",ADMNTIME
            RESET     ADMNTIME
          ENDIF
.
          IF        OPDASTAT = 4
            MOVE      DTIME,DSCHTIME
            REP       ": ",DSCHTIME
            SQUEEZE   DSCHTIME
            MOVELPTR  DSCHTIME,FORM2
            IF        FORM2 < 6
              ENDSET    DSCHTIME
              APPEND    "00",DSCHTIME
              RESET     DSCHTIME
            ENDIF
          ENDIF
.
