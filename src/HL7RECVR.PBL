          MOVELPTR  ZSC00601,FORM3               * field populated ?
          COMPARE   ZERO,FORM3
          GOTO      VZSC0300 IF EQUAL            * no
.
.         Check if the field is null
.
          MATCH     BLANKSTR,ZSC00601            * null field ?
          GOTO      VZSC0300 IF EQUAL            * yes
.
          MOVE      ZSC00601,DIM14
          MOVE      "ZSC.6 ",FIELDID
          MOVE      ZERO,DATFLAG1
          MOVE      TWO,DATFLAG2
          CALL      VDTM0000                     * time validation
.
