          MOVE      ZXP01701,FORM2
          IF        FORM2 = 0
            MOVE      "ZXP.17: Practice count cannot be zero",ERRORDSC
            MOVE      "207",ERRORNUM
            CALL      EROR0000
            GOTO      VZXP9999
          ENDIF
