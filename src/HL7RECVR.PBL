          MOVE      "HL7RECVR  ",USERID
          PACK      KEY10,USERID,SP10
          CALL      RDWBSE1
          IF        OVRCD<>0
            CALL      CLWEBSEC
          ENDIF
          MOVE      WBSEPCD,PASSCODE
.
          CALL      PMIGTNID              * get national id for dgate write
          MOVE      NMPNUMB,PTNINMPI
          MOVE      FORTY8,HL7TRGID
          MOVE      SP8,HL7INCLD
