          PACK      KEY5,ANSC,ANSL,PV102001,SP70
          CALL      RDCODE1                      * field valid ?
          IF        OVRCD = 1
            MOVE      "PV1.20: Financial Class code Cat CL not valid",ERRORDSC
            MOVE      "207",ERRORNUM
            CALL      EROR0000
            GOTO      MPV17400
          ENDIF
