          MATCH     SP70,XFLD0009
          IF        !@EQUAL
            SQUEEZE   XFLD0009
            TYPE      XFLD0009
            IF        !@EQUAL
              MOVE      "Invalid Private Patient Service Adjusment",ERRORMSG
              CALL      PERR0000
              MOVE      ONE,EXIT
            ENDIF
          ENDIF
.      
          PACK      XFLD0010,XFLD0010,SP70
