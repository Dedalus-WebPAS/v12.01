          PACK      XADMDRGC,XFLD0002,SP70
          PACK      XADMSDPY,XFLD0004,SP70
          PACK      XADMBICU,XFLD0005,SP70
.
          MOVE      SP70,KEY50
          UNPACK    XADMDRGD,KEY50
          PRINT     *1,"|",LINECNTR,*12,"|",ERRORMSG,*59,"|":
                    XADMCNTR,*71,"|",XADMDRGC,*80,"|",KEY50:
                    *132,"|"
          GOTO      PERR9999
.
