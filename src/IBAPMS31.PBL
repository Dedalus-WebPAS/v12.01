UPNM0000  PACK      KEY9,STWARD,CSTPER
          CALL      RDNUMW1               
          BRANCH    OVRCD,UPNM1000       
.
          ADD       NUMADMN,PADMN       
          ADD       NUMTRIN,PTRIN      
          ADD       NUMDSCH,PDSCH   
          ADD       NUMTOUT,PTROUT  
          ADD       NUMRETN,PRETN
          ADD       NUMLEAV,PLEAV
.
          CALL      UPNUMW1                  
          GOTO      UPNM9999                
.
UPNM1000  MOVE      STWARD,PWARD
          MOVE      CSTPER,PDATE
          MOVE      NUMADMN,PADMN       
          MOVE      NUMTRIN,PTRIN      
          MOVE      NUMDSCH,PDSCH   
          MOVE      NUMTOUT,PTROUT  
          MOVE      NUMRETN,PRETN
          MOVE      NUMLEAV,PLEAV
.
          CALL      WRNUMW1                  
.
UPNM9999  RETURN
