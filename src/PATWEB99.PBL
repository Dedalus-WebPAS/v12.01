          REP       " +",KEY2
          ASSIGN    STARTNUM-NORECORD,F3
          IF        F3<0
            MOVE      ZERO,F3
          ENDIF
          MOVE      F3,KEY3
          REP       " +",KEY3
          UNPACK    DIM127,D1,FLDPARA,DIM127
          MOVE      REPORTNO,F2
.
          REP       " +",PMHCP001
          REP       " +",RETNFLAG
.
          WRITE     HTMLFILE;"patweb99.pbl":
                                 "?reportno=",F2:
                                 "&template=",FLDPARA:
                                 "&startnum=",KEY3:
                                 "&norecord=",KEY2:
                                 "&pmhcp001=",PMHCP001:
                                 "&retnflag=",RETNFLAG;
.
          REP       "+ ",PMHCP001
          REP       "+ ",RETNFLAG
.
PREVCM99  RETURN
