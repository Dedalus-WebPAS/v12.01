          CALL      GPSRW000                * GP's Details Row
          ADD       ONE,DISNUMB
          COMPARE   NORECORD,DISNUMB
          IF        !@EQUAL
            IF        LPRACFLG=1
              GOTO      GPSRH100            * no linked practices
            ELSE
              GOTO      GPSRH250            * linked practices
            ENDIF
          ENDIF
