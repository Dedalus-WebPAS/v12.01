.
          MOVE      FIVE,EVERT
          DISPLAY   *PECOL:EVERT,*EL
          CALL      PATDOCKY
          IF        EXIT = 2
            MOVE      ONE,EXIT
            GOTO      KDOC9999
          ENDIF
.
