.***********************************************************************
.*  PTAB0000  :  print table                                           *
.***********************************************************************
PTAB0000  
.
          MOVE      SP30,DESCCLS
          MATCH     ANSA,XXPATCLS
          IF        @EQUAL
            MOVE      "Inpatients",DESCCLS
          ELSE
            MOVE      "Day Cases ",DESCCLS
          ENDIF
          IF        NOGPADD = 0
            CALL      GPAD0000
          ENDIF
          PRINT     *N,*1,DESCCLS
          PRINT     *1,"----------"
          CALL      UND132 
          PRINT       *1,"|",*3,"PROCEDURE",*46,"| U/R NUMBER":
                      *59,"| NAME AND ADDRESS",*97,"| ":
                      "CLINICAL DECISION",*118,"| PLANNED ADM":
                      *132,"|":
                      *N,*1,"|",*46,"|",*59,"|",*97,"|",*99,"DATE ON LIST":
                      *118,"|    DATE",*132,"|"
          CALL      UND132 
          ADD       EIGHT,CLNO
.          
PTAB9999  RETURN
