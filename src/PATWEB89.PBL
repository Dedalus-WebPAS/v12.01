.-----------------------------------------------------------
. WRHIS000 -  Write to Emergency History Audit Table
.----------------------------------------------------------------------------
WRHIS000  PACK      KEY22,EMVIADMN,Z70
          CALL      RSEMHIS1
          CALL      RPEMHIS1
          BRANCH    OVRCD,WRHIS999
.
          MOVE      EMVIADMN,DIM8
          MATCH     DIM8,EMHIVIS             * Same Visit Number

          GOTO      WRHIS999 IF NOT EQUAL
.
          CALL      IBACLOCK
          PACK      EMHIDAT,CCC,CYY,CMM,CDD
          REP       " 0",EMHIDAT                 * Current Date
.
          UNPACK    CTIMEIS,HOUR,D1,MIN,D1,SEC   * Current time
          PACK      EMHITIM,HOUR,MIN,SEC,SP70    * Time in hhmmss
.
          MOVE      "UPDIS",EMHIUPT              * Update Discharge Update Type
.
          MOVE      USERID,EMHIEUS               * Web User Id Created
          PACK      EMHICDAT,CCC,CYY,CMM,CDD     * Date Created
          REP       " 0",EMHICDAT
          CLOCK     TIME,EMHICTIM                * Time Created
          MOVE      USERID,EMHICUID              * Web User Id Created
          MOVE      SP70,EMHIUDAT                * Date Updated
          MOVE      SP70,EMHIUTIM                * Time Update
          MOVE      SP70,EMHIUUID                * Web User Id Updated
          MOVE      SP70,EMHISPA
.
          PACK      KEY22,EMHIVIS,EMHIDAT,EMHITIM,SP70
          CALL      RAEMHIS1
          IF        OVRCD=1
            CALL      WREMHIS1                 * write New History record exists
          ENDIF
.
WRHIS999  RETURN
.----------------------------------------------------------------------------
. CHAUD000 -  Write to Emergency Field Change Audit Table
.----------------------------------------------------------------------------
CHAUD000  CLOSE     PRSPMIA1                     * Free up some open files
          CLOSE     PMSIPLA1
.
          OPEN      EMRCHAA1,"emrchaaf"         * Open EMR Field Change Audit
.
          PACK      EMCHVISN,EMVIADMN,SP70     * Visit number
.
          CALL      IBACLOCK
          PACK      EMCHDATE,CCC,CYY,CMM,CDD   * Curremt Date
          REPLACE   " 0",EMCHDATE
          CLOCK     TIME,EMCHTIME              * Current Time
          MOVE      "07",EMCHUPTY              * Update Type - 07
          MOVE      SP70,EMCHREAS              * Reason for Change
          MOVE      USERID,EMCHUSID            * User Id
          MOVE      ORGLVALU,EMCHOVAL          * Original Value
          MOVE      CHNDVALU,EMCHCVAL          * Changed Value
.
          PACK      KEY24,EMCHVISN,EMCHDATE,EMCHTIME,SP70
          CALL      RAEMCHA1
          IF        OVRCD=1
            CALL      WREMCHA1
          ENDIF
.
CHAUD990  CLOSE     EMRCHAA1                   * Close EMR Field Change Audit
.
          OPEN      PRSPMIA1,"prspmiaf"        * Re open original files
          OPEN      PMSIPLA1,"pmsiplaf"
.
CHAUD999  RETURN
