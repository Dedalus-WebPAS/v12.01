          PACK      KEY35,URNUMBER,ADMISSNO,DIM3,Z70
          CALL      RSPTATR3
          CALL      RPPTATR3
          BRANCH    OVRCD,GPTATR97
.
          MATCH     URNUMBER,PTARURNO
          GOTO      GPTATR97 IF NOT EQUAL
.
          MATCH     ADMISSNO,PTARVISN
          GOTO      GPTATR97 IF NOT EQUAL
.
          MATCH     DIM3,PTARTYPE
          GOTO      GPTATR97 IF NOT EQUAL
.
          MATCH     "1",PTARDELR
          GOTO      GPTATR97 IF EQUAL
.
          GOTO      GPTATR99
.
