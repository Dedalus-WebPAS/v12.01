              CALL      MVACCXML            * Add record to accaudaf
            ENDIF
            GOTO      ACCD9999
          ENDIF
.
          MATCH     "A",ACCFLAG1            * Accident Details Flag
          IF        @EQUAL
            MATCH     "1",SERIFLAG
            IF        @EQUAL
              CALL      OUTCLM00
            ELSE
              CALL      ADET0000
            ENDIF
            CALL      NXTPAG00
            GOTO      ACCD9999
          ENDIF
.
          MATCH     "K",ACCDFLAG            * From Work Capacity Screen
          IF        @EQUAL
            CALL      WCAP0000
            GOTO      ACCD9999
          ENDIF
.
          MATCH     "C",ACCDFLAG            * Copy EMR diagnosis
          IF        @EQUAL
            CALL      CPEMR000
            GOTO      ACCD9999
          ENDIF
.
          MATCH     "S",ACCFLAG1            * Accident XML extract
          IF        @EQUAL
            MATCH     "1",SERIFLAG          * update ACC Details first
            IF        @EQUAL
              CALL      OUTCLM00
            ELSE
              CALL      ADET0000
            ENDIF
            CALL      ACCXML00              * (in ACCEXTRA.PBL)
            CALL      NXTPAG00
            GOTO      ACCD9999
          ENDIF
.
          MATCH     "T",ACCFLAG1            * Resubmit Accident XML extract
          IF        @EQUAL
            MOVE      ACCNUMBR,PTCLM011
            CALL      ACCXML00              * (in ACCEXTRA.PBL)
.         NZ only...
            IF        PTCNHDPS=1 
              CALL      MVACCXML            * Add record to accaudaf
            ENDIF
            GOTO      ACCD9999
          ENDIF
.
ACCD9999  RETURN
.
