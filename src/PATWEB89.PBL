          PACK      KEY8,AADMNO
          CALL      RDPTGTU1
          IF        OVRCD=0
            MOVE      ONE,PTGTGSTA           * Unknown Accommodation GST
            CALL      UPPTGTU1
          ELSE
            MOVE      AADMNO,PTGTADMN
            MOVE      ONE,PTGTGSTA           * Unknown Accommodation GST
            MOVE      ZERO,PTGTGSTM
            CALL      WRPTGTU1
          ENDIF
          GOTO       CGTU9000
