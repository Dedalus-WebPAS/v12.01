          APPEND    " Casemix Funding patient.",WEBTITLE
          CALL      ADMERR00
          MOVE      ONE,EXIT
          GOTO      CMIXA999
.
CMIXA930  MOVE      "Patient will not be funded by Casemix Funding. ",WEBTITLE
          CALL      ADMERR00
          MOVE      ONE,EXIT
          GOTO      CMIXA999
.
CMIXA950  MOVE      "Rates not setup for Casemix Funding Patient.",WEBTITLE
          CALL      ADMERR00
          MOVE      ONE,EXIT
.
