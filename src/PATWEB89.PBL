            MATCH     PTDOSDAT,CHCKDATE       * Doctors start date
            GOTO      ACTDOC91 IF LESS
.
            MATCH     SP70,PTDOEDAT           * Still active if no end date
            GOTO      ACTDOC90 IF EQUAL
