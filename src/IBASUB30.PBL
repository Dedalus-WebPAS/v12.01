                    *46,"|",TSUBTOTA," |",TSUBTOTB:
                    *80,"|"
          ADD       "1",CLNO                 * add one to line number
          CALL      UND80                    * print line
          GOTO      PTOT0600
.
PTOT0700  PRINT     "| Grand Totals                               ":
                    "|",TOTALREC," |",TOTALOUT:
                    *80,"|"
          ADD       "1",CLNO                   * add one to line number
          CALL      UND80                      * print line
.
PTOT9999  RETURN
