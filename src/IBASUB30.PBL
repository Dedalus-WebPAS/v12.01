SETSCR02  MOVE      "1",ACCEPT
          MOVE      "2",OPTIONA
          MOVE      "N",DEFYESNO
.
          MOVE      SP70,STRCST
          MOVE      SP70,ENDCST
          MOVE      SP70,STRWAR
          MOVE      SP70,ENDWAR
          MOVE      SP70,FROMCST
          MOVE      SP70,TOCST
          MOVE      SP70,FROMWAR
          MOVE      SP70,TOWAR
          MOVE      SP70,PDESC1
          MOVE      SP70,PDESC2
          MOVE      SP70,SIWAWAR
          MOVE      SP70,SICACODE
          RETURN
