FILA0000  BRANCH    OPTIONA,FILA0100,FILA9999,FILA9999,FILA9999:
                            FILA9999,FILA9999,FILA9999,FILA9999
.
.---- print by type 1 ----
.
FILA0100  PACK      KEY20,SP70
          CALL      RSNEZA1
          CALL      RPNEZA1
          GOTO      FILA9999
.
FILA9999  RETURN
.**********************************************************************
.  NREA - Get next record                              Called By PRNA
.**********************************************************************
NREA0000  BRANCH    OPTIONA,NREA0100,NREA9999,NREA9999,NREA9999:
                            NREA9999,NREA9999,NREA9999,NREA9999
.
.---- print by type 1 ----
.
NREA0100  CALL      RKNEZA1
          BRANCH    OVRCD,NREA9500
.
          GOTO      NREA9000
.
NREA9000  MOVE      ZERO,OVRCD         * continue
          GOTO      NREA9999
.
NREA9500  MOVE      ONE,OVRCD          * quit
.
NREA9999  RETURN
.********************************************************************
. HEAA - print report                               Called by PRNA
.********************************************************************
HEAA0000  CALL      HEAD132                      * print heading
.
          IF        CPAGENO=1
            UNPACK    FROMDATE,CCENT,CYEAR,CMON,CDAY
            CALL      PACDATE
            MOVE      CPCDATE,KEY10
            UNPACK    TODATE,CCENT,CYEAR,CMON,CDAY
            CALL      PACDATE
            STRIP     FROMWARE
            STRIP     TOWARE
            STRIP     FROMCATA
            STRIP     TOCATA
            STRIP     FROMRAS
            STRIP     TORAS
            ADD       "5",CLNO
            PRINT     *40,*+,"Print Stock Check Extract Report":
                      *N,*40,"Date Range       : ",KEY10," to ",CPCDATE:
                      *N,*40,"Warehouse Range  : ",FROMWARE," to ",TOWARE:
                      *N,*40,"Catalog Range    : ",FROMCATA," to ",TOCATA:
                      *N,*40,"Rack/Shelf Range : ",FROMRAS," to ",TORAS
          ENDIF
.
          MOVE      SP70,SIWADES
          PACK      KEY5,NEZAWAR,SP70
          CALL      RDSIWA1
.
          ADD       "1",CLNO
          PRINT     *1,"Warehouse : ",NEZAWAR,SP2,SIWADES
.
          CALL      UND132                       * print line
          PRINT     "| Rack & Shelf | Catalog                             ":
                    "                                      |   Stock on Hand":
                    "   |   Stock Counted":
                    *132,"|"
          ADD       "1",CLNO
          CALL      UND132                       * print line
.
HEAA9999  RETURN
.********************************************************************
. WRIA - print data line                            Called by PRNA
.********************************************************************
WRIA0000
          MATCH     SKEY5,NEZAWAR
          IF        !@EQUAL
            PACK      SKEY5,NEZAWAR,SP70
            MOVE      "999",CLNO
          ENDIF
          COMPARE   "55",CLNO                    * end of page ?
          CALL      HEAA0000 IF NOT LESS         * start new page
.
          MOVE      SP70,SIIADES
          PACK      KEY7,NEZACAT,SP70
          CALL      RDSIIA1
.
          PRINT     "| ",NEZARAS,"     | ",NEZACAT,SP2,SIIADES:
                    "     | ",NEZASOH,"   | ":
                    *132,"|"
          ADD       "1",CLNO           * add one to line number
.
WRIA9999  RETURN
.**********************************************************************
. NEZTMP0A IO routines                           Called by Lots
.**********************************************************************
RSNEZA1   RESET     KEY20
          READ      NEZTMP0A,KEY20;;
          RETURN
.
RANEZA1   RESET     KEY20
          MOVE      ZERO,OVRCD
          READ      NEZTMP0A,KEY20;ANS
          GOTO      OVERCOND IF OVER
          RETURN
.
RDNEZA1   RESET     KEY20
          MOVE      ZERO,OVRCD
          READ      NEZTMP0A,KEY20;NEZAWAR,NEZARAS,NEZACAT,NEZASOH
          GOTO      OVERCOND IF OVER
          RETURN
.
RKNEZA1   MOVE      ZERO,OVRCD
          READKS    NEZTMP0A;NEZAWAR,NEZARAS,NEZACAT,NEZASOH
          GOTO      OVERCOND IF OVER
          RETURN
.
RPNEZA1   MOVE      ZERO,OVRCD
          READKP    NEZTMP0A;NEZAWAR,NEZARAS,NEZACAT,NEZASOH
          GOTO      OVERCOND IF OVER
          RETURN
.
UPNEZA1   UPDATE    NEZTMP0A;NEZAWAR,NEZARAS,NEZACAT,NEZASOH
          RETURN
.
WRNEZA1   RESET     KEY20
          WRITE     NEZTMP0A,KEY20;NEZAWAR,NEZARAS,NEZACAT,NEZASOH
          RETURN
.
DENEZA1   RESET     KEY20
          DELETE    NEZTMP0A,KEY20
          RETURN
