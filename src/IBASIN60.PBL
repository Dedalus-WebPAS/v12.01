.********************************************************************
. PRNA - print report for NEZA                      Called by ML
.         Returns  : EXIT            (1=illegal range entered)
.********************************************************************
PRNA0000  PACK      DISPMSG,SP70,SP70            * save line 24
          CALL      MESSAGE2
.
          MATCH     FROMDATE,TODATE              * check range
          IF        @LESS
            DISPLAY   *P1:24,*B,*EF,"Illegal Date Range - ";
            CALL      HITENTER
            GOTO      PRNA9500
          ENDIF
.
          MATCH     FWAREHS,TWAREHS              * check range
          IF        @LESS
            DISPLAY   *P1:24,*B,*EF,"Illegal Warehouse Range - ";
            CALL      HITENTER
            GOTO      PRNA9500
          ENDIF
.
          MATCH     FCATALOG,TCATALOG            * check range
          IF        @LESS
            DISPLAY   *P1:24,*B,*EF,"Illegal Catalog Range - ";
            CALL      HITENTER
            GOTO      PRNA9500
          ENDIF
.
          MATCH     FRACKSH,TRACKSH              * check range
          IF        @LESS
            DISPLAY   *P1:24,*B,*EF,"Illegal Rack and Shelf Range - ";
            CALL      HITENTER
            GOTO      PRNA9500
          ENDIF
.
          CALL      EXTR0000                     * extract data
.
          DISPLAY   *P1:24,"Printing :   ";
          MOVE      ZERO,CPAGENO                 * reset page no.
          CLOCK     TIME,CTIMEIS                 * get current time
          MOVE      "999",CLNO                   * init last line number
          MOVE      SP70,SKEY5                   * init last warehouse
.
          CALL      FILA0000                     * go to correct posit in file
.
PRNA1000  CALL      NREA0000                     * get next record
          BRANCH    OVRCD,PRNA8000               * end of file ?
.
          DISPLAY   *P12:24,*V2LON,NEZAWAR,SLASH,NEZACAT,*EL;
.
          CALL      WRIA0000                     * print details
          GOTO      PRNA1000                     * get next record
.
PRNA8000  COMPARE   ZERO,CPAGENO
          CALL      HEAA0000 IF EQUAL            * print head if nothing printed
          CALL      UND132                       * print line
          PRINT     "****   End of Report   ****"
.
.
PRNA9000  MOVE      ZERO,EXIT                    * exit screen
          GOTO      PRNA9999
.
PRNA9500  MOVE      ONE,EXIT                     * continue
.
PRNA9999
          CLOSE     NEZTMP0A           * delete temp file
          EXECUTE   KILLTMPA,TASKID
.
          CALL      MESSAGE3                     * restore line 24
          RETURN
.**********************************************************************
.  EXTR - Extract Data                                 Called By PRNA
.**********************************************************************
EXTR0000  PACK      FILENAMA,NAMEA,PORT,SP70     * set up and open temp file
          REP       " 0",FILENAMA
          PACK      MAKETMPA,ISBUILD,FILENAMA,TMP0ADAT
          PACK      KILLTMPA,ISERASE,FILENAMA
          EXECUTE   KILLTMPA,TASKID
          EXECUTE   MAKETMPA,TASKID
          OPEN      NEZTMP0A,FILENAMA
.
          PACK      KEY12,FCATALOG,FWAREHS,SP70
          CALL      RSSIIE1
          CALL      RPSIIE1
.
EXTR1000  CALL      RKSIIE1
          BRANCH    OVRCD,EXTR9999          * no more items to find
          MATCH     SIIECAT,TCATALOG
          GOTO      EXTR9999 IF LESS
.
          MATCH     FWAREHS,SIIEWAR         * check warehouse range
          GOTO      EXTR1000 IF LESS
          MATCH     SIIEWAR,TWAREHS
          GOTO      EXTR1000 IF LESS
.
          MATCH     FRACKSH,SIIERAS         * check rack and shelf range
          GOTO      EXTR1000 IF LESS
          MATCH     SIIERAS,TRACKSH
          GOTO      EXTR1000 IF LESS
.
. check warehouse security
.
          MATCH     SP70,SIIEWAR
          IF        !@EQUAL
            PACK      KEY9,PASSCODE,SIIEWAR,SP70
            CALL      RDSIWS1
            BRANCH    OVRCD,EXTR1000
          ENDIF
.
          PACK      KEY12,SIIEWAR,SIIECAT,SP70
          PACK      KEY28,SIIEWAR,SIIECAT,FROMDATE,SP70
          CALL      RSSIIT1
          CALL      RKSIIT1
          BRANCH    OVRCD,EXTR1000
          PACK      KEY28,SIITWAR,SIITCAT,SP70
          MATCH     KEY12,KEY28
          GOTO      EXTR1000 IF NOT EQUAL
.
          MATCH     FROMDATE,SIITDAT        * check date range
          GOTO      EXTR1000 IF LESS
          MATCH     SIITDAT,TODATE
          GOTO      EXTR1000 IF LESS
.
          DISPLAY   *P1:24,"Extracting : ":
                    *V2LON,SIITWAR,SLASH,SIITCAT,SLASH,SIITDAT,*EL;
.
          PACK      KEY20,SIITWAR,SIIERAS,SIITCAT,SP70
          UNPACK    KEY20,NEZAWAR,NEZARAS,NEZACAT
          CALL      RDNEZA1
          IF        OVRCD=1
            MOVE      SIIESOH,NEZASOH
            CALL      WRNEZA1
          ENDIF
          GOTO      EXTR1000
.
EXTR9999  RETURN
