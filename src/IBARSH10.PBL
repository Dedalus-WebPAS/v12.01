.
          MATCH     "All",CPTOCLAM
          IF        @EQUAL
            DISPLAY   *P26:15,*V2LON,"All";
            GOTO      ENZT3000
          ENDIF
.
          MATCH     SP70,CPTOCLAM
          IF        @EQUAL
            DISPLAY   *P26:15,*V2LON,"Blank";
            GOTO      ENZT3000
          ENDIF
.
          PACK      KEY5,ANSC,ANSL,CPTOCLAM,SP70
