          ENDIF
.
          PACK      KEY14,CPTOHFND,ZERO,ZERO,ZERO,ZERO,SP70
          CALL      RDFUND1
          BRANCH    OVRCD,ENZT9000
.
          DISPLAY   *P35:16,HFNAME;
.
.         Keyin Health Fund Table
.
ENZT4000  KEYIN     *P26:17,*EL,*V2LON,CPTOHFTB;
          PACK      CPTOHFTB,CPTOHFTB,SP70
.
          MATCH     "All",CPTOHFTB
          IF        @EQUAL
            DISPLAY   *P26:17,*V2LON,"All";
            GOTO      ENZT5000
          ENDIF
.
          MATCH     SP70,CPTOHFTB
