          IF        CLMFLG = 0
            MATCH     FCCOD,PMCCCCOD     * check if this is the claim code
            GOTO      DUPL1000 IF NOT EQUAL * no.
            MOVE      TCCOD,PMCCCCOD
          ENDIF
.
.         Check if we are doing a single health fund only
.
          IF        FNDFLG = 0
            MATCH     FHFUN,PMCCHFUN     * check if this is the health fund
            GOTO      DUPL1000 IF NOT EQUAL * No.
            MOVE      THFUN,PMCCHFUN
          ENDIF
