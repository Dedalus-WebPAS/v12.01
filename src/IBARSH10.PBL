          DISPLAY   *P26:6,*EL,*P26:7,*EL;
          MOVE      TWENTY6,ECOL
          MOVE      SIX,EVERT
          MOVE      ZERO,CKYIMODE
          CALL      PATFNDKY
          BRANCH    EXIT,KEYD2005,KEYD9000
