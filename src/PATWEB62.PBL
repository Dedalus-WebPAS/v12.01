          GOTO      GTLWU999 IF EQUAL
.
          WRITE     HTMLFILE;WUSRLOCK;        
          GOTO      GTLWU999
.
GTLWU200  MATCH     SP10,WUSRLOCK
          GOTO      GTLWU291 IF EQUAL
.
          PACK      KEY10,WUSRLOCK,SP10
          CALL      RDWBSE1
          BRANCH    OVRCD,GTLWU210
          GOTO      GTLWU220 
.
GTLWU210  MOVE      "Unknown User",WBSENAM
.
