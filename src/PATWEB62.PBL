          APPEND    "Web: ",KEY60
          APPEND    WUSRLOCK,KEY60
          APPEND    " - ",KEY60
          APPEND    WBSENAM,KEY60
          RESET     KEY60
          WRITE     HTMLFILE;KEY60;
          GOTO      GTLWU999
.
GTLWU291  WRITE     HTMLFILE;"&nbsp";
          GOTO      GTLWU999
.
