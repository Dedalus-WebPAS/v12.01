            STRIP     DIM11
            MOVELPTR  DIM11,FORM2
            IF        FORM2 > 5
              MOVE      "Misc. Charge code (",ERRORDSC
              ENDSET    ERRORDSC
              APPEND    DIM11,ERRORDSC
              APPEND    ") more than 5 characters in length",ERRORDSC
              RESET     ERRORDSC
              GOTO      GEEQ9500
            ENDIF
