          APPEND    "displayOEC('",HTMLLNK3
          APPEND    WBOCVISN,HTMLLNK3
          APPEND    WBOCCNTR,HTMLLNK3
          APPEND    "','",HTMLLNK3
          APPEND    URNUMBER,HTMLLNK3
          APPEND    "','",HTMLLNK3
          APPEND    ADMISSNO,HTMLLNK3
          APPEND    "');",HTMLLNK3
          RESET     HTMLLNK3
