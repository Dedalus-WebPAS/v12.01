          APPEND    "','",HTMLLINK
          APPEND    EMVIADMN,HTMLLINK
          APPEND    "')",HTMLLINK
          RESET     HTMLLINK
.
          WRITE     HTMLFILE;"t.addRow(#"",HTMLLINK,"#",":
                             "#"",DIM11,SP1,KEY5,"#",":
                             "#"",FORMATNM,ALRTIMGT,"#",":
                             "#"",COMPDIAG,"#",":
                             "#"",EMLODESC,"#",":
                             "#"",DIM70,"#",":
                             "#"",DIM70A,"#",":
                             "#"",KEY20,"#",":
                             "#"",TDESC,"#",":
                             "#"",FOLDERSF,"#")"
.
          GOTO      VWCC0101
.
