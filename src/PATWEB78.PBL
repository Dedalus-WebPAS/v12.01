          IF        @EQUAL
            MOVE      "Waiting",DIM70
          ENDIF          
          MATCH     " 1",PMOESTAT
          IF        @EQUAL
            MOVE      "Extracted",DIM70
          ENDIF
          MATCH     " 2",PMOESTAT
          IF        @EQUAL
            MOVE      "Received",DIM70
          ENDIF
          MATCH     " 3",PMOESTAT
          IF        @EQUAL
            MOVE      "SA Error: ",DIM12
