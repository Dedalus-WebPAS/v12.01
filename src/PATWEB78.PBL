          ENDIF
          MATCH     " 4",PMOESTAT
          IF        @EQUAL
            MOVE      "Data Error: ",DIM12
            PACK      DIM12,DIM12
            PACK      DIM70,DIM12,SP1,PMOEERRC,SP1,PMOEERRD,SP70
          ENDIF
          MATCH     " 5",PMOESTAT
          IF        @EQUAL
            MOVE      "Rejected Reported",DIM70
          ENDIF
          MATCH     " 6",PMOESTAT
          IF        @EQUAL
            MOVE      "Complete Reported",DIM70
          ENDIF
          MATCH     " 7",PMOESTAT
          IF        @EQUAL
