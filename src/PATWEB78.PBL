          IF        @EQUAL
            MOVE      "Casemix Verification Added - ",DIM50
          ELSE
            MOVE      "Casemix Verification Updated - ",DIM50
          ENDIF
.
.         Casemix Verification
.
          MATCH     SP70,PTARVAL1
          IF        @EQUAL
            MOVE      SP70,DIM3
          ELSE
            MATCH     "1",PTARVAL1
            IF        @EQUAL
              MOVE      "Yes",DIM3
            ENDIF
            MATCH     "0",PTARVAL1
            IF        @EQUAL
              MOVE      "No ",DIM3
