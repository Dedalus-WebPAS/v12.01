          MOVE "Assessment Date must be within 3 days of discharge.",WEBTITLE
          CALL      POPERR00
          MOVE      ONE,EXIT
          GOTO      CKFDDT99
.
CKFDDT93  CLEAR     WEBTITLE
          MOVE "Assessment Date more than 7 days after discharge date.",WEBTITLE
          CALL      POPERR00
          MOVE      ONE,EXIT
          GOTO      CKFDDT99
.
CKFDDT98  MOVE      ZERO,EXIT
CKFDDT99  RETURN
