          IF        @EQUAL
            CALL      RKWBELF4
          ELSE
            CALL      RKWBELF3
          ENDIF
          BRANCH    OVRCD,FFTB9990
.
          MATCH     SP70,SRCHSNAM
          IF        !@EQUAL
            CMATCH    WBEFSNAM,SRCHSNAM
            GOTO      FFTB9990 IF NOT EQUAL
          ENDIF
.
          MATCH     SP70,SRCHGNAM
          IF        !@EQUAL
