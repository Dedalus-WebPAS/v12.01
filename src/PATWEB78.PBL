          APPEND    "&startnum=",HTMLLINK
          MOVE      SP70,KEY3
          MOVE      ZERO,F3
          ASSIGN    STARTNUM+ZERO,F3
          MOVE      F3,KEY3
          REP       " +",KEY3
          APPEND    KEY3,HTMLLINK
