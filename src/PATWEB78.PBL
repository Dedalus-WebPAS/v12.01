          CALL      RDEMLOC1
          IF        OVRCD=1
            MOVE      SP70,EMLODESC
          ENDIF
.
          MATCH     SP3,PTCRACTN
          IF        !@EQUAL
            PACK      KEY5,CTAc,PTCRACTN
            CALL      RDCODE1
            IF        OVRCD=1
              MOVE      SP70,TDESC
            ENDIF
          ELSE
            MOVE        SP70,TDESC
          ENDIF
.
