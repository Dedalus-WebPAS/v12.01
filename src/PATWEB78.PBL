          IF        OVRCD=1
            MOVE      "C",PTEMNEWF        * 1st record flag change
            PACK      KEY18,PTEMADMN,PTEMEPNO,PTEMCDTE
            CALL      WRPTECM1            * Write an episode morb chan file rec
          ENDIF
