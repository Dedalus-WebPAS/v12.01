            GOTO      FFTB5000 IF NOT EQUAL
          ENDIF
.
          MATCH     SP70,PMEFVISN
          GOTO      FFTB0010 IF NOT EQUAL
.
          MOVE      SP70,ELGRESIN 
          PACK      DIM20,PMEFELGN,SP70
          LJUSTIFY  DIM20
.
          PACK      KEY23,DIM20,SP70
