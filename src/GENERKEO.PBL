.----------------------------------------------------------------------
.   Program       : GENERKEO
.   CAR Number    : 188016
.   Modifications : 
.         V9.11.00  22/01/2009  Mike Laming   CAR 188016
.                   New Program - External Organisation Table Keyword generator
.----------------------------------------------------------------------
.
.----------------------------------------------------------------------
.  data includes
.----------------------------------------------------------------------
.
          INC       STDGENDF
          INC       PATCODFD/INC
          INC       PATEORFD/INC
          INC       PATKEOFD/INC
.
.----------------------------------------------------------------------
.  local variables
.----------------------------------------------------------------------
.
.
PRGID     INIT      "GENERKEO"
PRGNAM    INIT      "Fix patkeoaf"
VERSION   INIT      "V12.01.00"
.
.----------------------------------------------------------------------
.  program code
.----------------------------------------------------------------------
.
.---- initialise screen and confirm desire to execute program ----
.
ML0000    CALL      DISPHEAD
.
          DISPLAY   *P1:10,"Program generates External Organisation Keywords"
          CALL      CONTQST
          SUB       ONE,CEXIT
          BRANCH    CEXIT,ML9999,ML9999
.
.---- open files ----
.
          OPEN      PATCODE1,"patcodes"
          OPEN      PATEORA1,"pateoraf"
          OPEN      PATKEOA1,"patkeoaf"
          OPEN      PATKEOA2,"patkeoaf"
.
.---- loop through pateoraf ----
.
          MOVE      ZERO,SECTOR
.
          PACK      KEY7,SP70
ML1000    CALL      RSPTEOR1           * read next record
ML1100    CALL      RKPTEOR1
          BRANCH    OVRCD,ML9999
.
          ADD       ONE,SECTOR
          IF        SECTOR%100=1
            DISPLAY   *P1:24,*V2LON,SECTOR," - ",PTERCODE,*EF;
          ENDIF
.
          PROC      PATKEOUP
          GOTO      ML1100
.
.---- exit program ----
.
ML9999    CHAIN     PGM
.----------------------------------------------------------------------
.  code includes
.----------------------------------------------------------------------
.
          INC       PATCODIO/INC
          INC       PATEORIO/INC
          INC       PATKEOIO/INC
          INC       PATKEOPR
          INC       STDGENCD
.
