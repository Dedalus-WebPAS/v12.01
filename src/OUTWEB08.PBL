          CALL      WEBERR00
          GOTO      SHWBOK99
.
SHWBOK96  MOVE      "INVALID SESSION KEY",WEBTITLE
          CALL      WEBERR00
          GOTO      SHWBOK99
.
SHWBOK97  MOVE      "Error: No Appointment Found for Visit No",WEBTITLE
          CALL      WEBERR00
          GOTO      SHWBOK99
.
SHWBOK98  MOVE      "INVALID NHI RECORD",WEBTITLE
