          IF        @EQUAL
            PACK      CALCDATE,CCC,CYY,CMM,CDD
            REP       " 0",CALCDATE
            ADD       HUND90,CALCDATE
            UNPACK    CALCDATE,CCENT,CYEAR,CMON,CDAY
            MOVE      CMON,F2
            LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
            WRITE     HTMLFILE;CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR;
            GOTO      STAG9999
