          CALL      RSOTLKB2
          CALL      RKOTLKB2
          BRANCH    OVRCD,SCAN9999
.
          MATCH     OBAURNO,OTLKURNO
          GOTO      SCAN9999 IF NOT EQUAL
.
          MATCH     DOBAOUTN,OTLKLBOK
          GOTO      SCAN9999 IF NOT EQUAL
.
          MOVE      ONE,OTLKBSTA
          PACK      OTLKUDAT,CCC,CYY,CMM,CDD * current date
          REP       " 0",OTLKUDAT
          MOVE      CTIMEIS,OTLKUTIM
          MOVE      WBSEUID,OTLKUUID
          CALL      UPOTLKB2
