            APPEND    "javascript:UpdateRequest2('",HTMLLNK2
            APPEND    OTARURNO,HTMLLNK2
            APPEND    "','",HTMLLNK2
            APPEND    OTARREFN,HTMLLNK2
            APPEND    "','",HTMLLNK2
