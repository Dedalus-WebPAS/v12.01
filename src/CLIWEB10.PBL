          BRANCH    OVRCD,FHRCOM90    * Note does not exist
          PACK      KEY21,REDCRDT,REDCRTM,REDCRUN,REDCSID,SP70
          MATCH     KEY21,KEY24
          GOTO      FHRCOM90 IF NOT EQUAL
          IF        FIRSTCOM=0
            MOVE      ONE,FIRSTCOM
            WRITE     HTMLFILE;*+,",#"comment#" : #"";
