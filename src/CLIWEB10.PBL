. Security
.------------------------------------------------------------
CHKSEC00  MOVE      ZERO,F2
          MOVE      "OBR04",KEY5
          PACK      KEY32,REHALAB,KEY5,REHAUCS,REHAUSC
          CALL      RDRECT1
          IF        OVRCD=0
            SQUEEZE   RECTSLV
            MOVE      RECTSLV,F2
          ENDIF
          PACK      KEY18,WBSEUID,PRGID
          CALL      RDWBST1
          IF        OVRCD=1
            MOVE      ZERO,WBSTLEV
          ENDIF
          IF        WBSTLEV<F2
            GOTO      CHKSEC80
          ENDIF
          MOVE      ZERO,EXIT
          GOTO      CHKSEC99
.
CHKSEC80  MOVE      ONE,EXIT
.
CHKSEC99  RETURN
