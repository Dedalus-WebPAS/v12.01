          OPEN      WEBBBFA1,"webbbfaf"
          OPEN      WEBBBFA2,"webbbfaf"
          OPEN      WEBBBFA3,"webbbfaf"
. 
          OPEN      WEBBRDA2,"webbrdaf"
.
          RETURN
+
.------------------------------------------------------------------
. Get the Report Key from the BPRW JSON Response "status" "claimId"
. And reposition again at "claimAssessment"
.------------------------------------------------------------------
RKEY0000  READ      BPRRFILE,SEQ;BPRRLINE
          GOTO      RKEY5000 IF OVER
.
.         *************************************************
.         status
.         *************************************************
.
          SCAN      "status",BPRRLINE
          IF        @EQUAL
            RESET     BPRRLINE
.           UNPACK    BPRRLINE,DIM13,WBBDSTAT
.
            SCAN      ":",BPRRLINE
            IF        @EQUAL
              MOVE      BPRRLINE,BPRRWORK
              REP       ": ",BPRRWORK
              REP       "#" ",BPRRWORK
              LJUSTIFY  BPRRWORK
              MOVE      BPRRWORK,WBBDSTAT
              RESET     BPRRLINE
            ENDIF
.
            REP       "#" ",WBBDSTAT
            REP       ", ",WBBDSTAT
          ENDIF
.
.         *************************************************
.         claimId
.         *************************************************
.
          SCAN      "claimId",BPRRLINE
          IF        @EQUAL
            RESET     BPRRLINE
.           UNPACK    BPRRLINE,DIM16,WBBDCLID
.
            SCAN      ":",BPRRLINE
            IF        @EQUAL
              MOVE      BPRRLINE,BPRRWORK
              REP       ": ",BPRRWORK
              REP       "#" ",BPRRWORK
              LJUSTIFY  BPRRWORK
              MOVE      BPRRWORK,WBBDCLID
              RESET     BPRRLINE
            ENDIF
.
            REP       "#" ",WBBDCLID
            REP       ", ",WBBDCLID
          ENDIF
.
          GOTO      RKEY0000
.
.         close and reopen bprw response json file and read from the start
.         ----------------------------------------------------------------
.
RKEY5000  CLOSE     BPRRFILE
.
          CLEAR     CMDLINE
          APPEND    WBB2DPTH,CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    "/webPAS_claims/bprw/",CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    WBETSTRD,CMDLINE
          APPEND    BPRRLITR,CMDLINE
          RESET     CMDLINE
.
          OPEN      BPRRFILE,CMDLINE
.
.         reposition at "claimAssessment" line
.         ----------------------------
.
RKEY6000  READ      BPRRFILE,SEQ;BPRRLINE
          GOTO      RKEY9999 IF OVER
.
          SCAN      "claimAssessment",BPRRLINE
          GOTO      RKEY9999 IF EQUAL
.
          GOTO      RKEY6000
.
RKEY9999  RETURN
