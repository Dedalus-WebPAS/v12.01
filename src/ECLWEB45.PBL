VERSION   INIT      "V12.01.00"
PRGNAM    INIT      "Medicare WebServices BBSW BPRW Polling Response Server"
.--------------------------------------------------------------------------------
.
          CALL      WEBINT00       * Initialise Fifo Etc.
.
          CALL      INIT0000       * Open Files
.
MAIN0000  COMMIT
.
          BEGIN
.
          DISPLAY   *W2
.
          MATCH     "1",PTCNWSIN
          GOTO      MAIN0000 IF NOT EQUAL
.
          MATCH     "1",PTCNBBSW
          GOTO      MAIN0000 IF NOT EQUAL
.
          MOVE      ZERO,EXIT
.
          PACK      KEY51,ZERO,TEN1,SP70
          CALL      RSWBETR1
MAIN1000  CALL      RKWBETR1
          BRANCH    OVRCD,MAIN9999
.
          MATCH     "0",WBETSTAT
          GOTO      MAIN9999 IF NOT EQUAL
.
          MATCH     "11",WBETTYPE
          GOTO      MAIN9999 IF NOT EQUAL
.
          PACK      KEY17,WBETINVN,WBETBATN,WBETMODL,SP70
          CALL      RDWBBBS2
          BRANCH    OVRCD,MAIN1000
.
          MATCH     WBBSTRID,WBETTRID
          IF        !@EQUAL
.
            PACK      KEY51,WBETSTAT,WBETTYPE,WBETLOCN,WBETCDTE,WBETCTIM:
                      WBETTRID,SP70
            CALL      RDWBETR1
.
            MOVE      "E",WBETSTAT
.
            CALL      IBACLOCK
            PACK      WBETUDTE,CCC,CYY,CMM,CDD
            REP       " 0",WBETUDTE
            MOVE      CTIMEIS,WBETUTIM
            MOVE      USERID,WBETUUID
            CALL      UPWBETR1
.
            GOTO      MAIN1000
.
          ENDIF
.
          PACK      KEY71,WBBSHOSP,SP70          * Read B2B Device Table
          CALL      RSWBB2B3
          CALL      RKWBB2B3
          BRANCH    OVRCD,MAIN1000
.
          MATCH     WBBSHOSP,WBB2HOSP            * Same Hospital
          GOTO      MAIN1000 IF NOT EQUAL
.
          MATCH     "ACTIVE",WBB2DSTA            * Device Active
          GOTO      MAIN1000 IF NOT EQUAL
.
          MATCH     SP70,WBB2DPTH
          GOTO      MAIN1000 IF EQUAL
.
          CALL      PROPN000                     * Valid Properties File?
.
          MATCH     "1",PRODAIND
          GOTO      MAIN1000 IF EQUAL
.
          PACK      SAVKEY51,WBETSTAT,WBETTYPE,WBETLOCN,WBETCDTE,WBETCTIM:
                             WBETTRID,SP70
.
          OPEN      WBSPROPF,PRDAPATH
.
. Parse _bprw_response file (BBSW BPRW Response JSON file)
. ************************************************************
.
MAIN2000  CLEAR     CMDLINE
          APPEND    WBB2DPTH,CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    "/webPAS_claims/bprw/",CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    WBETSTRD,CMDLINE
          APPEND    BPRRLITR,CMDLINE
          RESET     CMDLINE
.
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      BPRRFILE,CMDLINE
          TRAPCLR   IO
          IF        OVRCD=1
            GOTO      MAIN3000
          ENDIF
.
          CALL      BPRPRS00                   * Parse BBSW BPRW Response JSON
.
          GOTO      MAIN8000
.
. Parse _bprw_errors file (BBSW BPRW Stderrs/Stdout file)
. ******************************************************
.
MAIN3000  CLEAR     CMDLINE
          APPEND    WBB2DPTH,CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    "/webPAS_claims/bprw/",CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    WBETSTRD,CMDLINE
          APPEND    BPRELITR,CMDLINE
          RESET     CMDLINE
.
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      BPREFILE,CMDLINE
          TRAPCLR   IO
          IF        OVRCD=1
            MOVE      "Response files not found.",BPRELINE  
            GOTO      MAIN3500
          ENDIF
.
MAIN3100  READ      BPREFILE,SEQ;BPRELINE
          IF        @OVER
            MOVE      "Refer to BBSW BPRW Details.",BPRELINE
            GOTO      MAIN3500 
          ENDIF
.
          SCAN      "error",BPRELINE
          IF        @EQUAL
            GOTO      MAIN3500
          ENDIF
.
          GOTO      MAIN3100
.
MAIN3500  
.         MOVE      "1",BPREINDC
.
          MOVE      WBBSINVN,WBBAINVN
          CALL      IBACLOCK
          PACK      WBBADATE,CCC,CYY,CMM,CDD
          REP       " 0",WBBADATE
          MOVE      CTIMEIS,WBBATIME
          MOVE      WBBSBATN,WBBABATN
          MOVE      THREE,WBBASTAT
          MOVE      " 5",WBBATYPE
          MOVE      SP70,WBBAOPER
          MOVE      WBBSTRID,WBBATRID
          MOVE      SP70,WBBAEROR
          MOVE      BPRELINE,WBBAEROT
          MOVE      WBBSMSID,WBBAMSID
          MOVE      WBBSMODL,WBBAMODL
          PACK      WBBASPAR,SP70,SP70
.
          PACK      KEY27,WBBAINVN,WBBADATE,WBBATIME,WBBATYPE,WBBAMODL,SP70
          CALL      RAWBBBA1
          IF        OVRCD=0
            GOTO      MAIN3500
          ENDIF
.
          TRAP      OVERCOND IF IO
          CALL      WRWBBBA1
          TRAPCLR   IO
.
          GOTO      MAIN8000
.
. ******************************************************************************
.
MAIN8000  PACK      KEY30,WBBSFLAG,WBBSHOSP,WBBSMODL,WBBSUNIQ,WBBSINVN:
                          WBBSBATN,SP70
          CALL      RDWBBBS8
          IF        OVRCD=0
.
            MATCH     "1",BPREINDC
            IF        @EQUAL
              MOVE      THREE,WBBSFLAG
            ENDIF

            CALL      IBACLOCK
            PACK      WBBSUDAT,CCC,CYY,CMM,CDD
            REP       " 0",WBBSUDAT
            MOVE      CTIMEIS,WBBSUTIM
            MOVE      USERID,WBBSUUID
            CALL      UPWBBBS8
.
          ENDIF
.
          PACK      KEY51,WBETSTAT,WBETTYPE,WBETLOCN,WBETCDTE,WBETCTIM:
                    WBETTRID,SP70
          CALL      RDWBETR1
.
          MOVE      "1",WBETSTAT
.
          CALL      IBACLOCK
          PACK      WBETUDTE,CCC,CYY,CMM,CDD
          REP       " 0",WBETUDTE
          MOVE      CTIMEIS,WBETUTIM
          MOVE      USERID,WBETUUID
          CALL      UPWBETR1
.
MAIN9000  CLOSE     WBSPROPF
          CLOSE     BPRRFILE
          CLOSE     BPREFILE
.
          GOTO      MAIN1000
.
MAIN9999  CLOSE     WBSPROPF
          CLOSE     BPRRFILE
          CLOSE     BPREFILE
.
          GOTO      MAIN0000
+
.------------------------------------------------------------
.  Parse BBSW BPRW Response JSON
.------------------------------------------------------------
.
BPRPRS00  MOVE      SP70,JSONCODE
          PACK      JSONTEXT,SP70,SP70,SP70,SP70,SP70,SP70,SP70,SP70
          MOVE      SP70,BPREINDC
.
BPRPRS01  READ      BPRRFILE,SEQ;BPRRLINE
          GOTO      BPRPRS99 IF OVER
.
. ***********************************************************
. Errors
. ***********************************************************
.
          SCAN      "highestSeverity",BPRRLINE
          IF        @EQUAL
.
.           MOVE      "1",BPREINDC
.
BPRPRS05    READ      BPRRFILE,SEQ;BPRRLINE
            GOTO      BPRPRS90 IF OVER
.
.           *************************************************
.           serviceMessage
.           *************************************************
.
            SCAN      "serviceMessage",BPRRLINE
            IF        @EQUAL
.
BPRPRS07      READ      BPRRFILE,SEQ;BPRRLINE
              GOTO      BPRPRS20 IF OVER
.
.             ***********************************************
.             {    
.             ***********************************************
.
              SCAN      "{",BPRRLINE
              IF        @EQUAL
                MOVE      SP70,JSONCODE
                PACK      JSONTEXT,SP70,SP70,SP70,SP70,SP70,SP70,SP70,SP70
.
                GOTO      BPRPRS07
.
              ENDIF
.
.             ***********************************************
.             code
.             ***********************************************
.
              SCAN      "#"code#"",BPRRLINE
              IF        @EQUAL
                RESET     BPRRLINE
.               UNPACK    BPRRLINE,DIM14,JSONCODE
.
                SCAN      ":",BPRRLINE
                IF        @EQUAL
                  MOVE      BPRRLINE,BPRRWORK
                  REP       ": ",BPRRWORK
                  REP       "#" ",BPRRWORK
                  LJUSTIFY  BPRRWORK
                  MOVE      BPRRWORK,JSONCODE
                  RESET     BPRRLINE
                ENDIF
.
                GOTO      BPRPRS07
.
              ENDIF
.
.             ***********************************************
.             text
.             ***********************************************
.
              SCAN      "reason",BPRRLINE
              IF        @EQUAL
                RESET     BPRRLINE
.               UNPACK    BPRRLINE,DIM16,JSONTEXT
.
                SCAN      ":",BPRRLINE
                IF        @EQUAL
                  MOVE      BPRRLINE,BPRRWORK
                  REP       ": ",BPRRWORK
                  REP       "#" ",BPRRWORK
                  LJUSTIFY  BPRRWORK
                  MOVE      BPRRWORK,JSONTEXT
                  RESET     BPRRLINE
                ENDIF
.
                REP       ", ",JSONTEXT
                REP       "#" ",JSONTEXT
.
                GOTO      BPRPRS07
.
              ENDIF
.
.             ***********************************************
.             }
.             ***********************************************
.
              SCAN      "}",BPRRLINE
              IF        @EQUAL
.
BPRPRS10        MOVE      WBBSINVN,WBBAINVN
                CALL      IBACLOCK
                PACK      WBBADATE,CCC,CYY,CMM,CDD
                REP       " 0",WBBADATE
                MOVE      CTIMEIS,WBBATIME
                MOVE      WBBSBATN,WBBABATN
                MOVE      THREE,WBBASTAT
                MOVE      " 4",WBBATYPE
                MOVE      SP70,WBBAOPER
                MOVE      WBBSTRID,WBBATRID
                MOVE      JSONCODE,WBBAEROR
                MOVE      JSONTEXT,WBBAEROT
                MOVE      WBBSMSID,WBBAMSID
                MOVE      WBBSMODL,WBBAMODL
                PACK      WBBASPAR,SP70,SP70
.
                PACK      KEY27,WBBAINVN,WBBADATE,WBBATIME,WBBATYPE,WBBAMODL:
                                SP70
                CALL      RAWBBBA1
                IF        OVRCD=0
                  GOTO      BPRPRS10
                ENDIF
.
                TRAP      OVERCOND IF IO
                CALL      WRWBBBA1
                TRAPCLR   IO
.
                GOTO      BPRPRS07
.
              ENDIF
.
.             ***********************************************
.             ]
.             ***********************************************
.
              SCAN      "]",BPRRLINE
              IF        @EQUAL
                GOTO      BPRPRS20
              ENDIF
.
              GOTO      BPRPRS07
.
            ENDIF
.
BPRPRS20    GOTO      BPRPRS05   
.
          ENDIF
.
. ***********************************************************
. DHSEIN
. ***********************************************************
.
          SCAN      "DHSEIN",BPRRLINE
          IF        @EQUAL
            MOVE      SP70,JSONCODE
            RESET     BPRRLINE
            UNPACK    BPRRLINE,JSONTEXT
            REP       "#" ",JSONTEXT
.
.           MOVE      "1",BPREINDC
.
BPRPRS30    MOVE      WBBSINVN,WBBAINVN
            CALL      IBACLOCK
            PACK      WBBADATE,CCC,CYY,CMM,CDD
            REP       " 0",WBBADATE
            MOVE      CTIMEIS,WBBATIME
            MOVE      WBBSBATN,WBBABATN
            MOVE      THREE,WBBASTAT
            MOVE      " 5",WBBATYPE
            MOVE      SP70,WBBAOPER
            MOVE      WBBSTRID,WBBATRID
            MOVE      JSONCODE,WBBAEROR
            MOVE      JSONTEXT,WBBAEROT
            MOVE      WBBSMSID,WBBAMSID
            MOVE      WBBSMODL,WBBAMODL
            PACK      WBBASPAR,SP70,SP70
.
            PACK      KEY27,WBBAINVN,WBBADATE,WBBATIME,WBBATYPE,WBBAMODL,SP70
            CALL      RAWBBBA1
            IF        OVRCD=0
              GOTO      BPRPRS30
            ENDIF
.
            TRAP      OVERCOND IF IO
            CALL      WRWBBBA1
            TRAPCLR   IO
.
          ENDIF
.
. ***********************************************************
. 401
. ***********************************************************
.
          SCAN      "401",BPRRLINE
          IF        @EQUAL
            MOVE      "401 ",JSONCODE
            RESET     BPRRLINE
            UNPACK    BPRRLINE,JSONTEXT
            REP       "#" ",JSONTEXT
.
.           MOVE      "1",BPREINDC
.
BPRPRS40    MOVE      WBBSINVN,WBBAINVN
            CALL      IBACLOCK
            PACK      WBBADATE,CCC,CYY,CMM,CDD
            REP       " 0",WBBADATE
            MOVE      CTIMEIS,WBBATIME
            MOVE      WBBSBATN,WBBABATN
            MOVE      THREE,WBBASTAT
            MOVE      " 5",WBBATYPE
            MOVE      SP70,WBBAOPER
            MOVE      WBBSTRID,WBBATRID
            MOVE      JSONCODE,WBBAEROR
            MOVE      JSONTEXT,WBBAEROT
            MOVE      WBBSMSID,WBBAMSID
            MOVE      WBBSMODL,WBBAMODL
            PACK      WBBASPAR,SP70,SP70
.
            PACK      KEY27,WBBAINVN,WBBADATE,WBBATIME,WBBATYPE,WBBAMODL,SP70
            CALL      RAWBBBA1
            IF        OVRCD=0
              GOTO      BPRPRS40
            ENDIF
.
            TRAP      OVERCOND IF IO
            CALL      WRWBBBA1
            TRAPCLR   IO
.
          ENDIF
.
. ***********************************************************
. claimAssessment
. ***********************************************************
.
          SCAN      "claimAssessment",BPRRLINE
          IF        @EQUAL
.
            CALL      CLWEBBBD
.
            CALL      RKEY0000
.
BPRPRS50    READ      BPRRFILE,SEQ;BPRRLINE
            GOTO      BPRPRS90 IF OVER
.
.           ***********************************************************
.           medicalEvent
.           ***********************************************************
.
            SCAN      "medicalEvent",BPRRLINE
            IF        @EQUAL
.
              CALL      CLWEBBBE
.
BPR50EAA      READ      BPRRFILE,SEQ;BPRRLINE
              GOTO      BPRPRS70 IF OVER
.
.             ***********************************************************
.             service
.             ***********************************************************
.
              SCAN      "service",BPRRLINE
              IF        @EQUAL
.
                CALL      CLWEBBBF
.
BPR50BBB        READ      BPRRFILE,SEQ;BPRRLINE
                GOTO      BPR50EAZ IF OVER
.
.UPTOHERE claimAssessment/medicalEvent/service
.

.
.               ***********************************************
.               numberOfPatientsSeen
.               ***********************************************
.
                SCAN      "numberOfPatientsSeen",BPRRLINE
                IF        @EQUAL
                  RESET     BPRRLINE
.                 UNPACK    BPRRLINE,DIM37,WBBFNOFP
.
                  SCAN      ":",BPRRLINE
                  IF        @EQUAL
                    MOVE      BPRRLINE,BPRRWORK
                    REP       ": ",BPRRWORK
                    REP       "#" ",BPRRWORK
                    LJUSTIFY  BPRRWORK
                    MOVE      BPRRWORK,WBBFNOFP
                    RESET     BPRRLINE
                  ENDIF
.
                  REP       "#" ",WBBFNOFP
                  REP       ", ",WBBFNOFP
                ENDIF
.
.               ***********************************************
.               itemNumber
.               ***********************************************
.
.                SCAN      "itemNumber",BPRRLINE
.                IF        @EQUAL
.                  RESET     BPRRLINE
..                 UNPACK    BPRRLINE,DIM27,WBBFITMN
..
.                  SCAN      ":",BPRRLINE
.                  IF        @EQUAL
.                    MOVE      BPRRLINE,BPRRWORK
.                    REP       ": ",BPRRWORK
.                    REP       "#" ",BPRRWORK
.                    LJUSTIFY  BPRRWORK
.                    MOVE      BPRRWORK,WBBFITMN
.                    RESET     BPRRLINE
.                  ENDIF
..
.                  REP       "#" ",WBBFITMN
.                  REP       ", ",WBBFITMN
.                ENDIF
.
.               ***********************************************
.               itemNumber
.               ***********************************************
.
                SCAN      "itemNumber",BPRRLINE
                IF        @EQUAL
                  RESET     BPRRLINE
.                 UNPACK    BPRRLINE,DIM27,WBBFITMN
.
                  SCAN      ":",BPRRLINE
                  IF        @EQUAL
                    MOVE      BPRRLINE,BPRRWORK
                    REP       ": ",BPRRWORK
                    REP       "#" ",BPRRWORK
                    LJUSTIFY  BPRRWORK
                    MOVE      BPRRWORK,WBBFITMN
                    RESET     BPRRLINE
                  ENDIF
.
                  REP       "#" ",WBBFITMN
                  REP       ", ",WBBFITMN
                ENDIF
.
.               ***********************************************
.               id
.               ***********************************************
.
                SCAN      "#"id#"",BPRRLINE
                IF        @EQUAL
                  RESET     BPRRLINE
.                 UNPACK    BPRRLINE,DIM19,WBBFSVID
.
                  SCAN      ":",BPRRLINE
                  IF        @EQUAL
                    MOVE      BPRRLINE,BPRRWORK
                    REP       ": ",BPRRWORK
                    REP       "#" ",BPRRWORK
                    LJUSTIFY  BPRRWORK
                    MOVE      BPRRWORK,WBBFSVID
                    RESET     BPRRLINE
                  ENDIF
.
                  REP       "#" ",WBBFSVID
                  REP       ", ",WBBFSVID
                ENDIF
.
.               ***********************************************
.               chargeAmount
.               ***********************************************
.
                SCAN      "chargeAmount",BPRRLINE
                IF        @EQUAL
                  RESET     BPRRLINE
.                 UNPACK    BPRRLINE,DIM29,WBBFCHAM
.
                  SCAN      ":",BPRRLINE
                  IF        @EQUAL
                    MOVE      BPRRLINE,BPRRWORK
                    REP       ": ",BPRRWORK
                    REP       "#" ",BPRRWORK
                    LJUSTIFY  BPRRWORK
                    MOVE      BPRRWORK,WBBFCHAM
                    RESET     BPRRLINE
                  ENDIF
.
                  REP       "#" ",WBBFCHAM
                  REP       ", ",WBBFCHAM
                ENDIF
.
.               ***********************************************
.               benefitPaid
.               ***********************************************
.
                SCAN      "benefitPaid",BPRRLINE
                IF        @EQUAL
                  RESET     BPRRLINE
.                 UNPACK    BPRRLINE,DIM28,WBBFBEPD
.
                  SCAN      ":",BPRRLINE
                  IF        @EQUAL
                    MOVE      BPRRLINE,BPRRWORK
                    REP       ": ",BPRRWORK
                    REP       "#" ",BPRRWORK
                    LJUSTIFY  BPRRWORK
                    MOVE      BPRRWORK,WBBFBEPD
                    RESET     BPRRLINE
                  ENDIF
.
                  REP       "#" ",WBBFBEPD
                  REP       ", ",WBBFBEPD
                ENDIF
.
.               ***********************************************
.               assessmentCode
.               ***********************************************
.
                SCAN      "assessmentCode",BPRRLINE
                IF        @EQUAL
                  RESET     BPRRLINE
.                 UNPACK    BPRRLINE,DIM31,WBBFASSC
.
                  SCAN      ":",BPRRLINE
                  IF        @EQUAL
                    MOVE      BPRRLINE,BPRRWORK
                    REP       ": ",BPRRWORK
                    REP       "#" ",BPRRWORK
                    LJUSTIFY  BPRRWORK
                    MOVE      BPRRWORK,WBBFASSC
                    RESET     BPRRLINE
                  ENDIF
.
                  REP       "#" ",WBBFASSC
                  REP       ", ",WBBFASSC
                ENDIF
.
.               ***********************************************
.               close curly bracket
.               ***********************************************
.
                SCAN      "}",BPRRLINE
                IF        @EQUAL
.
.                 get latest report counter from webbbdaf
.                 --------------------------------------
.
                  PACK      WBBDCLID,WBBDCLID,SP70
                  PACK      WBETSTRD,WBETSTRD,SP70
.
                  MOVE      ZERO,FORM3
BPR50BBC          ADD       ONE,FORM3
                  MOVE      FORM3,WBBDRPTC
.
                  PACK      KEY33,WBBDCLID,WBBDRPTC,WBETSTRD,SP70
                  CALL      RAWBBBD1
                  IF        OVRCD=0
                    GOTO      BPR50BBC
                  ENDIF
.
.                 get latest medicalEvent counter from webbbeaf
.                 ----------------------------------------------
.
                  MOVE      WBBDCLID,WBBECLID
                  MOVE      FORM3,WBBERPTC
                  MOVE      WBETSTRD,WBBERKEY
.
                  MOVE      ZERO,FORM2
BPR50BBD          ADD       ONE,FORM2
                  MOVE      FORM2,WBBEMEVC
.
                  PACK      KEY35,WBBECLID,WBBERPTC,WBBEMEVC,WBBERKEY,SP70
                  CALL      RAWBBBE1
                  IF        OVRCD=0
                    GOTO      BPR50BBD
                  ENDIF
.
.                 write to webbbfaf
.                 -----------------
.
                  MOVE      WBBDCLID,WBBFCLID
                  MOVE      FORM3,WBBFRPTC
                  MOVE      FORM2,WBBFMEVC
                  MOVE      WBETSTRD,WBBFRKEY
                  MOVE      WBETTRID,WBBFTRID
                  MOVE      WBETMSID,WBBFMSID
.
                  MOVE      ZERO,FORM2
BPR50BBE          ADD       ONE,FORM2
                  MOVE      FORM2,WBBFSRVC
.
                  PACK      KEY37,WBBFCLID,WBBFRPTC,WBBFMEVC,WBBFSRVC:
                                  WBBFRKEY,SP70
                  CALL      RAWBBBF1
                  IF        OVRCD=0
                    GOTO      BPR50BBE
                  ENDIF
.
                  CALL      WRWBBBF1
.
                  CALL      CLWEBBBF
.
                ENDIF
.
.               ***********************************************
.               close square bracket
.               ***********************************************
.
                SCAN      "]",BPRRLINE
                IF        @EQUAL
                  GOTO      BPR50EAZ
                ENDIF
.
BPR50BBZ        GOTO      BPR50BBB
.
              ENDIF
.
.             ***********************************************************
.             patient
.             ***********************************************************
.
              SCAN      "patient",BPRRLINE
              IF        @EQUAL
.
BPR50AAA        READ      BPRRFILE,SEQ;BPRRLINE
                GOTO      BPR50EAZ IF OVER
.
.               ***********************************************************
.               currentMember
.               ***********************************************************
.
                SCAN      "#"currentMember#"",BPRRLINE
                IF        @EQUAL
.
BPR50ABB          READ      BPRRFILE,SEQ;BPRRLINE
                  GOTO      BPR50AAZ IF OVER
.
.                 ***********************************************
.                 givenName
.                 ***********************************************
.
                  SCAN      "givenName",BPRRLINE
                  IF        @EQUAL
                    RESET     BPRRLINE
.                   UNPACK    BPRRLINE,DIM26,WBBEPTGN
.
                    SCAN      ":",BPRRLINE
                    IF        @EQUAL
                      MOVE      BPRRLINE,BPRRWORK
                      REP       ": ",BPRRWORK
                      REP       "#" ",BPRRWORK
                      LJUSTIFY  BPRRWORK
                      MOVE      BPRRWORK,WBBEPTGN
                      RESET     BPRRLINE
                    ENDIF
.
                    REP       "#" ",WBBEPTGN
                    REP       ", ",WBBEPTGN
                  ENDIF
.
.                 ***********************************************
.                 familyName
.                 ***********************************************
.
                  SCAN      "familyName",BPRRLINE
                  IF        @EQUAL
                    RESET     BPRRLINE
.                   UNPACK    BPRRLINE,DIM27,WBBEPTFN
.
                    SCAN      ":",BPRRLINE
                    IF        @EQUAL
                      MOVE      BPRRLINE,BPRRWORK
                      REP       ": ",BPRRWORK
                      REP       "#" ",BPRRWORK
                      LJUSTIFY  BPRRWORK
                      MOVE      BPRRWORK,WBBEPTFN
                      RESET     BPRRLINE
                    ENDIF
.
                    REP       "#" ",WBBEPTFN
                    REP       ", ",WBBEPTFN
                  ENDIF
.
.                 ***********************************************
.                 close bracket
.                 ***********************************************
.
                  SCAN      "}",BPRRLINE
                  IF        @EQUAL
                    GOTO      BPR50AAZ
                  ENDIF
.
BPR50ABZ          GOTO      BPR50ABB
.
                ENDIF
.
.               ***********************************************************
.               currentMembership
.               ***********************************************************
.
                SCAN      "currentMembership",BPRRLINE
                IF        @EQUAL
.
BPR50ACC          READ      BPRRFILE,SEQ;BPRRLINE
                  GOTO      BPR50AAZ IF OVER
.
.                 ***********************************************
.                 memberRefNumber
.                 ***********************************************
.
                  SCAN      "memberRefNumber",BPRRLINE
                  IF        @EQUAL
                    RESET     BPRRLINE
.                   UNPACK    BPRRLINE,DIM32,WBBEPTMR
.
                    SCAN      ":",BPRRLINE
                    IF        @EQUAL
                      MOVE      BPRRLINE,BPRRWORK
                      REP       ": ",BPRRWORK
                      REP       "#" ",BPRRWORK
                      LJUSTIFY  BPRRWORK
                      MOVE      BPRRWORK,WBBEPTMR
                      RESET     BPRRLINE
                    ENDIF
.
                    REP       "#" ",WBBEPTMR
                    REP       ", ",WBBEPTMR
                  ENDIF
.
.                 ***********************************************
.                 memberNumber
.                 ***********************************************
.
                  SCAN      "memberNumber",BPRRLINE
