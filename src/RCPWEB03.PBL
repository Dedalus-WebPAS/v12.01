          APPEND    PINVNO,DIM127
          APPEND    " Please use Reassign Debt to adjust.",DIM127
          APPEND    SP70,DIM127
          RESET     DIM127
          ADD       ONE,WARCOUNT
          MOVE      DIM127,WEBWARNG[WARCOUNT]
.
