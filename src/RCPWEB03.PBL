HSEL0000  PACK      KEY4,SP70 
          CALL      RDSHOSP1
HSEL0100  CALL      RDKHOSP1
          BRANCH    OVRCD,HSEL9999
.
          MOVE      HOSDESC,KEY50
          PACK      KEY6,QUOTE,HOSCODE,QUOTE
          MATCH     Z70,MULDHOSP
          IF        !@EQUAL
            MATCH     MULDHOSP,HOSCODE
          ELSE
            PACK      KEY2,CHOSPNUM
