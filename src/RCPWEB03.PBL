MVSUSP00  CALL      CLRCPSRT                * clear Suspense Receipt record
          MATCH     "1",KEY1
          GOTO      MVSUSP10 IF EQUAL
.
          MOVE      "3",KEY1                * determine suspense record type
          PACK      KEY3,RCDTREGI,SP70
          CALL      RDREGA1
          IF        OVRCD<>1
            COMPARE   "99",REGIBACD
            IF        @EQUAL
              MOVE      "2",KEY1
            ENDIF
          ENDIF
.
