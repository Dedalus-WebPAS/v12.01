ML7200    CALL      SELW0000
          BRANCH    EXIT,ML9999
.
          CALL      DELW0000
          CALL      SETW0000
          CALL      EXTR0000
          GOTO      ML9999
.
ML7400    CALL      SELW0000
          CALL      SENW0000
          GOTO      ML9999
.
ML7600    CALL      DSEL0000
          BRANCH    EXIT,ML9999
.
          CALL      DELW0000
.
