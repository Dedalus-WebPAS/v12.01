          GOTO      GLKEY110 IF EQUAL
          CMATCH    "2",SUBSECTN              * Group Codes
          GOTO      GLKEY120 IF EQUAL
          CMATCH    "3",SUBSECTN              * Item Codes
          GOTO      GLKEY130 IF EQUAL
          GOTO      GLKEY999
.
GLKEY110  PACK      PRGLI002,SP70
          GOTO      GLKEY999
.
