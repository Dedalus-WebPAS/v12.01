          IF        !@EQUAL
            APPEND    "&keywords=",HTMLLINK
            REP       "'~",KEYWORDS
            REP       " +",KEYWORDS
            STRIP     KEYWORDS
            APPEND    KEYWORDS,HTMLLINK
            REP       "~'",KEYWORDS
            REP       "+ ",KEYWORDS
          ENDIF
.
          RESET     HTMLLINK
          RETURN

