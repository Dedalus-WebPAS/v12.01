.****************************************************************************
.*                                OPTN0000             Called by: ML0000    *
.*                        get user options or exit                          *
.*                                                                          *
.*    Returns:  EXIT    = FALSE (0)  Run clean up                           *
.*                        TRUE  (1)  Exit option selected                   *
.****************************************************************************
OPTN0000  MOVE      ZERO,EXIT
          DISPLAY   *P1:3,*EF,*P1:4,*V2LON,ZERO,*HOFF," = Exit":
                    *P1:5,*V2LON,ONE,*HOFF," = Print Nurse Handover Report"
.
OPTN0500  KEYIN     *P1:8,*EL,"Select Option : ",*DV,UNDLN1:
                    *P17:8,*V2LON,COPTION
.
          COMPARE   ZERO,COPTION                 * exit selected ?
          GOTO      OPTN9500 IF EQUAL            * yes
.
          BRANCH    COPTION,OPTN9000    * run clean-up
.
          BEEP
          GOTO      OPTN0500
.
OPTN9000  MOVE      ZERO,EXIT
          GOTO      OPTN9999
.
OPTN9500  MOVE      ONE,EXIT
.
OPTN9999  RETURN
+
.******************************************************************************
.         Keyin a ward
.******************************************************************************
KYWRD000  MOVE      ZERO,EXIT
          MOVE      ZERO,ALLWARDS
          MOVE      ZERO,NEWPAGE
          MOVE      ZERO,PG1ONLY
          DISPLAY   *P1:10,*EF,"Enter Ward     : ";
.
KYWRD100  MOVE      SP10,SWARD
          KEYIN     *P18:10,*EL,*DV,UNDLN3,*P18:10,*V2LON,SWARD;
.         
          ENDSET    SWARD
          APPEND    SP3,SWARD
          RESET     SWARD
.         
          DISPLAY   *P18:10,*V2LON,SWARD
.
          CMATCH    "?",SWARD                    * Wards scan ?
          GOTO      KYWRD800 IF EQUAL            * yes
.         
          MATCH     SP3,SWARD
          IF        @EQUAL
            DISPLAY   *P18:10,*V2LON,"All";
            MOVE      "1",ALLWARDS
            DISPLAY   *P1:12,*EF,"New page for each Ward ? (Y/N) : ";
            KEYIN     *P34:12,*EL,*DV,UNDLN1,*P34:12,*V2LON,ANS;
            REP       "y1Y1n0N0",ANS
            MOVE      ANS,NEWPAGE
            GOTO      KYWRD999
          ENDIF
.
           
          MOVE      SP20,WBDESC
          PACK      KEY6,SWARD,SP3
          CALL      RDWARD1
          BRANCH    OVRCD,KYWRD200
.
          BRANCH    WACTIVE,KYWRD150
.
          DISPLAY   *P24:10,*EL,WBDESC;
          DISPLAY   *P1:12,*EF,"Headings on page 1 only? (Y/N) : ";
          KEYIN     *P34:12,*EL,*DV,UNDLN1,*P34:12,*V2LON,ANS;
          REP       "y1Y1n0N0",ANS
          MOVE      ANS,PG1ONLY
          GOTO      KYWRD999
.
.
KYWRD150  DISPLAY   *P24:10,*EL,*B,*V2LON,"** Ward is INACTIVE **",*W2
          CLEAR     ERRMSG
          MOVE      "Ward ",KEY5
          MOVE      " is Inactive",KEY12
          APPEND    KEY5,ERRMSG
          APPEND    SWARD,ERRMSG
          APPEND    KEY12,ERRMSG
          RESET     ERRMSG
          GOTO      KYWRD900
.
KYWRD200  DISPLAY   *P24:10,*EL,*B,*V2LON,"** Ward does not exist **",*W2
          CLEAR     ERRMSG
          MOVE      "Ward ",KEY5
          MOVE      " does not exist on file",KEY23
          APPEND    KEY5,ERRMSG
          APPEND    SWARD,ERRMSG
          APPEND    KEY23,ERRMSG
          RESET     ERRMSG
          GOTO      KYWRD900
.
.         Wards scan
.
KYWRD800  CALL      GETSCR00
          CALL      PATWRDDS
          CALL      PUTSCR00
          GOTO      KYWRD100
.
.
KYWRD900  MOVE      ONE,EXIT
          COMPARE   "63",CLNO
          CALL      HEAD0000 IF NOT LESS
          PRINT     *3,ERRMSG
          
KYWRD999  RETURN
+
.****************************************************************************
.*                               CREA0000                                   *
.*                        Create Temporary Files                            *
.****************************************************************************
.                                           * to handle Oracle temp tables
CREA0000  MOVE      ZERO,EXIT
          CALL      TFILENAM
          MOVE      TFILNAME,TMPFNAME
.
          TRAP      OVERCOND IF IO
          OPEN      TEMP1,TMPFNAME
          BRANCH    OVRCD,CREA2000
          TRAPCLR   IO
          CALL      CLER0000                * clear existing temp file
          GOTO      CREA9000
.
CREA2000  PACK      CMDLINE,SP30,SP30
          CLEAR     CMDLINE
          APPEND    "isbuild ",CMDLINE
          APPEND    TMPFNAME,CMDLINE
          APPEND    " 166 U1-3,4-6,7-14",CMDLINE
          RESET     CMDLINE
          EXECUTE   CMDLINE,TASKID
.
CREA9000  OPEN      TEMP1,TMPFNAME
.
CREA9999  RETURN
.
.****************************************************************************
.*                               KILL0000                                   *
.*                        Remove Temporary Files                            *
.****************************************************************************
.
KILL0000  CLOSE     TEMP1
          PACK      CMDLINE,SP30,SP30
          CLEAR     CMDLINE
          APPEND    "iserase ",CMDLINE
          APPEND    TMPFNAME,CMDLINE
          RESET     CMDLINE
          EXECUTE   CMDLINE,TASKID
.
          MATCH     "0",TASKID
          GOTO      KILL9999 IF EQUAL
.
          CALL      CLER0000                * to handle Oracle temp tables
.
KILL9999  RETURN
+
.******************************************************************************
.*                             CLER0000                 Called by:            *
.*               close and erase the temporary file                           *
.******************************************************************************
.
CLER0000  PACK      KEY14,SP20
          CALL      RSTEMP1
          CALL      RKTEMP1
          BRANCH    OVRCD,CLER9999
.
          PACK      KEY14,XWARD,XWBED,XADMNO,SP20
          CALL      DETEMP1
.
          GOTO      CLER0000
.
CLER9999  RETURN
