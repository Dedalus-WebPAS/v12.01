PROC0000  MOVE      SP10,SAVPRVBN
.
          MOVE      SAVBTHN,EMECBATN             * load the batch number
          MOVE      TWO,EMECFLAG                 * update status to "extracted"
          MOVE      LISTAMNT,EMECAMTC            * load claim amount
          CALL      UPEMECT1                     * update record
.
.         Write a record to the claims audit/history table for this claim
.
          MOVE      EMECINVN,EMEAINVN
          CALL      IBACLOCK
          PACK      EMEADATE,CCC,CYY,CMM,CDD
          REP       " 0",EMEADATE
          MOVE      CTIMEIS,EMEATIME
          MOVE      SAVBTHN,EMEABATN
          MOVE      TWO,EMEASTAT
          MOVE      " 0",EMEATYPE
          MOVE      EMIBOPER,EMEAOPER
          MOVE      SP30,EMEATRID
          MOVE      SP10,EMEAEROR
          PACK      EMEAEROT,SP70,SP30
          MOVE      EMECMODL,EMEAMODL
          MOVE      SP70,EMEASPAR
          CALL      WREMECA1
.
.         Check if this claim was the result of a resubmission
.
          MATCH     SP8,EMECPBAT                 * blank previous batch ?
          GOTO      PROC9999 IF EQUAL            * yes - not a resubmission
.
.         Update the next batch number in the original source claim for this
.         resubmission
.
          MOVE      EMECPBAT,SAVPRVBN            * save previous batch number
          PACK      KEY30,EMECHOSP,SP1,FIVE,EMECMODL,EMECUNIQ,EMECINVN,SAVPRVBN
          CALL      RDEMECT1
          BRANCH    OVRCD,PROC9999
.
          MOVE      SAVBTHN,EMECNBAT
          CALL      UPEMECT1
.
PROC9999  RETURN
