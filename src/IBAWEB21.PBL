          IF        @EQUAL
            MOVE      "Admitted to ward/other admitted patient unit",XEDSTDES
            GOTO      EDED9999
          ENDIF
.
          MATCH     "2 ",PTCDUDF5
          IF        @EQUAL
            MOVE      "ED service event completed; departed under own care",XEDSTDES
            GOTO      EDED9999
          ENDIF
.
