          MOVE     ZERO,COUNT
          MOVE     SIWAWAR,KEY5
.
SINORD05  CALL     RLSIWA1
          BRANCH   OVRCD,SINORD99,SINORD10
          MOVE     SIWANPO,ORDERNO
          SUB      ONE,ORDERNO
.
          REPEAT
            ADD      ONE,ORDERNO
            MOVE     ORDERNO,KEY7
            CALL     RASIPA1
          UNTIL    (OVRCD=1)
.
          MOVE     ORDERNO,SIPAPON
          ADD      ONE,ORDERNO
          MOVE     ORDERNO,SIWANPO
          CALL     UPSIWA1
          CALL     UUSIWA1
          MOVE     ZERO,OVRCD
          GOTO     SINORD99
.
