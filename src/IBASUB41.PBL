          PRINT     "Cost Centre From : ",FROMCST," to ",TOCST
          PRINT     "Usage Date  From : ",FROMDAT," to ",TODAT
          PRINT     "Cost Centre      : ",SICACODE,SP1,SICADESC
          CALL      UND132
          PRINT     "Catalog                                Unit        Avg":
                    ". Cost     Qty          Value ABC":
                    "  Cur. Max        Value Rec. Max        Value"
          CALL      UND132
