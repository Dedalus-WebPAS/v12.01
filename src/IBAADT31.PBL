.******************************************************************************
.         PEDY0000 - Print line for range of days
.******************************************************************************
PEDY0000  IF        COLNUM = 1
            PRINT     *10,STRDAY,*15,"-",*17,ENDDAY;
          ENDIF
.
          IF        COLNUM = 2
            PRINT     *25,STRDAY,*30,"-",*32,ENDDAY;
          ENDIF
.
          IF        COLNUM = 3
            PRINT     *40,STRDAY,*45,"-",*47,ENDDAY;
          ENDIF
.
          IF        COLNUM = 4
            PRINT     *55,STRDAY,*60,"-",*62,ENDDAY;
          ENDIF
.
          IF        COLNUM = 5
            PRINT     *70,STRDAY,*75,"-",*77,ENDDAY;
          ENDIF
PEDY9999  RETURN
