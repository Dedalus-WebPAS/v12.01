          GOTO      WDGN1000 IF EQUAL         * already exists
.
          MOVE      "07",MHDICSYS             * Using ICD10
          IF        FORM1=7 | FORM1=8
            MOVE      "10",MHDICSYS
            MATCH     SP70,PTCNGDV2
            IF        !@EQUAL
              MOVE      "11",MHDICSYS         * Using ICD10 version 2
            ENDIF
            MATCH     SP70,PTCNGDV3
            IF        !@EQUAL
              MOVE      "12",MHDICSYS         * Using ICD10 version 3
