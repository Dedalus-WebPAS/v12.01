.------------------------------------------------------------
. Set Parameters
.------------------------------------------------------------
SETPAR00  MATCH     "reportno=",QRYNAME
          IF        @EQUAL
            MOVE      QRYDATA,REPORTNO
            GOTO      SETPAR99
          ENDIF
.
          MATCH     "template=",QRYNAME
          IF        @EQUAL
            MOVE      QRYDATA,TEMPLATE
            GOTO      SETPAR99
          ENDIF
.
          MATCH     "updttype=",QRYNAME
          IF        @EQUAL
            MOVE      QRYDATA,UPDTTYPE
            GOTO      SETPAR99
          ENDIF
.
          MATCH     "nextpage=",QRYNAME
          IF        @EQUAL
            MOVE      QRYDATA,NEXTPAGE
            GOTO      SETPAR99
          ENDIF 
.
          MATCH     "redirect=",QRYNAME
          IF        @EQUAL
            MOVE      QRYDATA,REDIRECT
            GOTO      SETPAR99
          ENDIF
.
          MATCH     "srchcntr=",QRYNAME
          IF        @EQUAL
            PACK      SRCHCNTR,QRYDATA,SP70
            GOTO      SETPAR99
          ENDIF
.
          MATCH     "srchhacg=",QRYNAME
          IF        @EQUAL
            MOVE      QRYDATA,SRCHHACG
            GOTO      SETPAR99
          ENDIF
.
          MATCH     "pmadj",QRYNAME
          IF        @EQUAL
            CALL      SPPAJ000
            GOTO      SETPAR99
          ENDIF
.
          MATCH     "pmhac",QRYNAME
          IF        @EQUAL
            CALL      SPPHA000
            GOTO      SETPAR99
          ENDIF
.
          MATCH     "pmhde",QRYNAME
          IF        @EQUAL
            CALL      SPPHE000
            GOTO      SETPAR99
          ENDIF
.
SETPAR99  RETURN
