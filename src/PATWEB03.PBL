          GOTO      DIPV1000 IF NOT EQUAL
.
          MATCH     SP70,PMUGCLAM
          IF        !@EQUAL
            MATCH     PMUGCLAM,ACLAIM
            GOTO      DIPV1000 IF NOT EQUAL
          ENDIF
.
          MATCH     SP70,PMUGFUND
          IF        !@EQUAL
            MATCH     PMUGFUND,AFUNDH
            GOTO      DIPV1000 IF NOT EQUAL
.
            PACK      KEY14 WITH AFUNDH,AFNDTB,SP10
            CALL      RDFUND1
            BRANCH    OVRCD,DIPV1000
.
            MATCH     PMUGTABT,PTHFBCAT
            GOTO      DIPV1000 IF NOT EQUAL
          ENDIF
.
