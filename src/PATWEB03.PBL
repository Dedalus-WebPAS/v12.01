                    SP1,"|",ZERO,"|",ZERO,"|":
                    "</RETURN_VALUE>"
          GOTO      CHKPRG99
.
CHKPRG90  WRITE     HTMLFILE;"<RETURN_VALUE TYPE=ERROR>":
                    " Invalid U/R Number":
                    "</RETURN_VALUE>"
          GOTO      CHKPRG99
.
CHKPRG91  WRITE     HTMLFILE;"<RETURN_VALUE TYPE=ERROR>":
                    " Invalid Visit Type Validation":
                    "</RETURN_VALUE>"
          GOTO      CHKPRG99
.
CHKPRG99  RETURN
