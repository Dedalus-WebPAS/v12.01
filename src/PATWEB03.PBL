ADVCA200  ***** check new key doesn't already exist ******
          PACK      KEY27,PMDAU001,PMDAU002,PMDAU003,PMDAU004,SP70
          CALL      RAPMDAU1
          IF        OVRCD<>1
            GOTO      ADVCA930
          ENDIF
.
          PACK      KEY27,PMDAU001,OLDAU002,OLDAU003,PMDAU004,SP70
          CALL      RDPMDAU1
          IF        OVRCD = 1
            GOTO      ADVCA920
          ENDIF
