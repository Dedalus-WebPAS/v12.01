            GOTO      VALA9000 IF LESS
          ENDIF
.
          IF        ASTAT<>3
            GOTO      VALA2000
          ENDIF
.
          MATCH     PMDTC006,DDATE
          GOTO      VALA9010 IF LESS
.
          MATCH     DDATE,PMDTC006
          IF        @EQUAL
            MATCH     DTIME,PMDTC007
            GOTO      VALA9010 IF LESS
          ENDIF
.
VALA2000  MOVE      ZERO,EXIT
          GOTO      VALA9999
.
