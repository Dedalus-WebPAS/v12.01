          GOTO      PKEY0600 IF EQUAL
.
          PACK      KEY4,KEY8
          CALL      RDSYFI1
          BRANCH    OVRCD,PKEY8000
          COMPARE   TWO,SYFITIO
          GOTO      PKEY0500 IF EQUAL
.
          MATCH     KEY8,DIM8
          GOTO      PKEY8000 IF EQUAL
          GOTO      PKEY0700
.
PKEY0500  CALL      RDSYFL2
          BRANCH    OVRCD,PKEY8000
          MATCH     SYFLV4N,DIM8
          GOTO      PKEY8000 IF EQUAL
          MOVE      SYFLV4N,KEY8
          GOTO      PKEY0700
