          APPEND    "          MOVE      ",DIM80
          PACK      KEY17,KEY8,COMMA,DIM8
          SQUEEZE   KEY17
          APPEND    KEY17,DIM80
          RESET     DIM80
          STRIP     DIM80
          PRINT     *+,DIM80
          GOTO      PKEY8000
.
