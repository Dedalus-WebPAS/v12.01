          CLEAR     DIM80
          MOVE      SCFLFLD,KEY8
          PACK      KEY4,KEY8
          CALL      RDSYFI1
          BRANCH    OVRCD,PKEY1010
          COMPARE   TWO,SYFITIO
          GOTO      PKEY1010 IF NOT EQUAL
          CALL      RDSYFL2
          BRANCH    OVRCD,PKEY1010
          MOVE      SYFLV4N,KEY8
