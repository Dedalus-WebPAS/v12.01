          MOVE      "08",CVERT
          MOVE      TWO,CKEYTYP
          MOVE      SP8,SCPRCAF
          MOVE      SP70,SCFMDES
          DISPLAY   *PCCOL:CVERT,*V2LON,SCPRCAF,SP1,*HOFF,SCFMDES
