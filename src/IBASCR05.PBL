EWRTA000  PACK      KEY16,SCFMFID,SCFMPID
          CALL      RASCFM1
          IF        OVRCD=1
            CALL      WRSCFM1
          ELSE
            CALL      UPSCFM1
          ENDIF
          CALL      DSCFD100
          IF        SCFMTYP=0
            CALL      WSCFD100
          ENDIF
.
EWRTA999  RETURN
