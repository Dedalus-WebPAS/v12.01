APFLD000  
          MOVE      ZERO,SECTOR
          PACK      KEY15,COPYTPID,COPYTSID,Z70
          CALL      RSSCFL1
          CALL      RPSCFL1
          IF        OVRCD=0
            MATCH     COPYTPID,SCFLPID
            IF        @EQUAL
              MATCH     COPYTSID,SCFLSID
              IF        @EQUAL
                MOVE      SCFLFNO,SECTOR
              ENDIF
            ENDIF
          ENDIF
.
          PACK      KEY15,SCPRPID,SCMASID,COPYFFNO,SP70
          CALL      RSSCFL1
