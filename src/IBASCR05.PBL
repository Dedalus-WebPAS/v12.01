          GOTO      GSKEY900 IF NOT EQUAL
          MATCH     SCMASID,SCFLSID
          GOTO      GSKEY900 IF NOT EQUAL
.
          COMPARE   ZERO,SCFLTYP
          GOTO      GSKEY100 IF NOT EQUAL
          PACK      KEY15,SCFLPID,SCFLSID,SCFLFNO
          CALL      RDSCWP1
          IF        SCFLMUL=1
            MOVE      ZERO,IDXMUL
            MOVE      ZERO,EXIT
