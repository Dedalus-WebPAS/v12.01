          LOAD      SEGMENT,COUNT,SYIDFL1,SYIDFL2,SYIDFL3,SYIDFL4:
                                  SYIDFL5,SYIDFL6,SYIDFL7,SYIDFL8
          MATCH     SP70,SEGMENT
          GOTO      PKEY0620 IF EQUAL
.
