. WPRG - Write to Files
.-------------------------------------------------
WPRG0000  PACK      KEY8,SCPRPID,SP70
          CALL      RASCPR1
          IF        OVRCD=0
            CALL      UPSCPR1
          ELSE
            CALL      WRSCPR1
          ENDIF
WPRG9999  RETURN
.-------------------------------------------------
. DPRG - Delete Program
.-------------------------------------------------
DPRG0000  PACK      KEY8,SCPRPID,SP70
          CALL      RASCPR1
          IF        OVRCD=0
            CALL      DESCPR1
          ENDIF
          CALL      PRDEL000
.
DPRG9999  RETURN
