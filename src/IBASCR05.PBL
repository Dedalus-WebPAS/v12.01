          ENDIF
DPITM010  MOVE      SCPSROW,CVERT
          MOVE      ONE,LLCNT
          WHILE     LLCNT<=SCPSLEN
            MOVE      SCPSCOL,CCOL
            ADD       LLCNT,CCOL
            SUB       ONE,CCOL
            PUTVAR    ANSX,SCPSATT,CCOL,CVERT
            ADD       ONE,LLCNT
          DO
          GOTO      DPITM999
.
DPITM100  MOVE      SCPSROW,CTOP
          MOVE      SCPSCOL,CLEF 
          MOVE      SCPSLEN,LLCNT
          MOVE      SCPSNOR,CVERT
          ASSIGN    CLEF+LLCNT+1,CRIG
          ASSIGN    CTOP+CVERT+1,CBOT
          MOVE      SCPSCOL,CCOL 
          BOX       16,CLEF,CTOP,CRIG,CBOT
          ADD       ONE,CLEF
          SUB       "12",CRIG
          DISPLAY   *PCLEF:CBOT,*HON,SP1,SP10:
                    *PCRIG:CBOT,SP5
          ADD       SIX,CRIG
          DISPLAY   *PCRIG:CBOT,*HON,SP5
.
DPITM999
          MOVE      SVERT,CVERT
          MOVE      SCOL,CCOL
          RETURN
