            MOVE       ZERO,MULTIBSQ[F2]
            MOVE       ZERO,MULTIBTY[F2]
            MOVE       SP70,MULTITIO[F2]
            MOVE       SP70,MULTIMTY[F2]
            MOVE       SP70,MULTIFNO[F2]
          DO
          MOVE       ZERO,MULTIIDX
.
          CLEAR     ENDLABEL
          APPEND    KYLAB,ENDLABEL
          APPEND    "L900",ENDLABEL
          RESET     ENDLABEL
          PACK      SAVKEY15,SCPRPID,SCMASID,SP70
          MOVE      "100",LABELNO3
.
