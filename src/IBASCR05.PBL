          GOTO      PKEY1000 IF EQUAL
.
          PRINT     "          CALL      ",SCFLCAL
.
          UNPACK    SCFLCAL,DIM1,KEY5,DIM2
          CALL      RDSYKV1
          BRANCH    OVRCD,PKEY8000
.
          PACK      KEY8,SYKVSYS,SYKVFIL,SYKVKFLD
          MOVE      SCFLFLD,DIM8
          PACK      KEY4,DIM8
          CALL      RDSYFI1
          BRANCH    OVRCD,PKEY0400
.
          COMPARE   TWO,SYFITIO
          GOTO      PKEY0400 IF NOT EQUAL
          PACK      KEY8,DIM8,SP70
          CALL      RDSYFL2
          BRANCH    OVRCD,PKEY0400
          MOVE      SYFLV4N,DIM8
          PACK      KEY8,SYKVSYS,SYKVFIL,SYKVKFLD
.
