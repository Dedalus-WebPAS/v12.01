          IF        @EQUAL
            PRINT     "          MOVE      #"",SCFLFNO,"#",FLDID"
            PRINT     "          CALL      GETPOSIT"
            PRINT     "          IF        EXIT=1"
            PRINT     "            MOVE      #"Mandatory Field Missing.":
                       " Fatal - #",DISPMSG"
            PRINT     "            BEEP"
