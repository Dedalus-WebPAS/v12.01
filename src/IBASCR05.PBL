          GOTO      GSCLR900 IF NOT EQUAL
.
          MATCH     "1",SCMACLR
          GOTO      GSCLR110 IF NOT EQUAL 
.
          IF        SCFLTYP>1
            GOTO      GSCLR100
          ENDIF
          GOTO      GSCLR120
.
