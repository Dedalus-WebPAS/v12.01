          GOTO      GSSEL900 IF NOT EQUAL
          IF        SCFLTYP=6|SCFLTYP=2
            GOTO      GSSEL300
          ENDIF
          IF        SCFLMUL=1
            IF        !(SCFLTYP=0)
              GOTO      GSSEL300
