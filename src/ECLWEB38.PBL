.---------------------------------------------------------------
. Print Statement of Claim and Benefit Form
.---------------------------------------------------------------
PCICBF00  MATCH     SP70,PTCNPCIP
          GOTO      PCICBF99 IF EQUAL
.
          MATCH     SP70,WBPITRID
          GOTO      PCICBF99 IF EQUAL
.
          PACK      KEY46,WBPITRID,SP70
          CALL      RSWBPCE2
          CALL      RKWBPCE2
          BRANCH    OVRCD,PCICBF99
.
          MATCH     WBPITRID,WBPETRID
          GOTO      PCICBF99 IF NOT EQUAL
.
          MATCH     "MEDICARE_ASSESSED",WBPESTAT
          GOTO      PCICBF99 IF NOT EQUAL
.
          MOVE      PTCNPCIP,SCHDPRNT
          MOVE      "iba       ",USERID
.
          MATCH     "1",WBPIMODL
          IF        @EQUAL
.
            PACK      KEY8,WBPIINVN,SP70
            CALL      RDINV1
            IF        OVRCD=0
              MATCH     SP70,PTINCUID
              IF        !@EQUAL
                MOVE      PTINCUID,USERID
              ENDIF
            ENDIF
.
          ELSE
.
            PACK      KEY8,WBPIINVN,SP70
            CALL      RDPRIN1 
            IF        OVRCD=0
.
              MATCH     SP70,PRINCUID
              IF        !@EQUAL
                MOVE      PRINCUID,USERID
              ENDIF
.
            ENDIF
.
          ENDIF
.
          MOVE      "IBAWEB27",SCHDPGID
          MOVE      "Generating a PCIW Claim and Benefit Form",SCHDDESC
          PACK      SCHDDATE,CCC,CYY,CMM,CDD
          REP       " 0",SCHDDATE
          CLOCK     TIME,SCHDTIME
.
.         Write Keyin parameters
.
          MATCH     "1",WBPIMODL
          IF        @EQUAL
            MOVE      "1",KEYSTARR[1]          * 1 for ED
          ELSE
            MOVE      "2",KEYSTARR[1]
          ENDIF
.
          MOVE      WBPIINVN,KEYSTARR[2]     * Invoice number
          MOVE      SP70,KEYSTARR[3]
          MOVE      SP70,KEYSTARR[4]
.
          MOVE      FOUR,KEYSTCNT         * Number of Keyins
.
          CALL      SCHPRO00   * Schedule Extract
.
PCICBF99  RETURN
