                                WBPHMODL,SP70
                CALL      RAWBPCA1
                IF        OVRCD=0
                  GOTO      PCIPRS10
                ENDIF
.
                TRAP      OVERCOND IF IO
                CALL      WRWBPCA1
                TRAPCLR   IO
.
                GOTO      PCIPRS07
.
              ENDIF
.
.             ***********************************************
.             ]
.             ***********************************************
.
              SCAN      "]",PCIRLINE
              IF        @EQUAL
                GOTO      PCIPRS20
              ENDIF
.
              GOTO      PCIPRS07
.
            ENDIF
