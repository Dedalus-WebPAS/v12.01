          GOTO      PKEY5000 IF OVER
.
.         *************************************************
.         transactionId
.         *************************************************
.
          MOVE      WBPITRID,WBPETRID
.
.         ***********************************************
.         claimId
.         ***********************************************
.
          SCAN      "claimId",PCIRLINE
          IF        @EQUAL
            RESET     PCIRLINE
.           UNPACK    PCIRLINE,DIM17,WBPECLID
.
            SCAN      ":",PCIRLINE
            IF        @EQUAL
              MOVE      PCIRLINE,PCIRWORK
              REP       ": ",PCIRWORK
              REP       "#" ",PCIRWORK
              LJUSTIFY  PCIRWORK
              MOVE      PCIRWORK,WBPECLID
              RESET     PCIRLINE
            ENDIF
.
            REP       "#" ",WBPECLID
            REP       ", ",WBPECLID
          ENDIF
.
.         ***********************************************
.         status
.         ***********************************************
.
          SCAN      "status",PCIRLINE
          IF        @EQUAL
            RESET     PCIRLINE
.           UNPACK    PCIRLINE,DIM14,WBPESTAT
.
            SCAN      ":",PCIRLINE
            IF        @EQUAL
              MOVE      PCIRLINE,PCIRWORK
              REP       ": ",PCIRWORK
              REP       "#" ",PCIRWORK
              LJUSTIFY  PCIRWORK
              MOVE      PCIRWORK,WBPESTAT
              RESET     PCIRLINE
            ENDIF
.
            REP       "#" ",WBPESTAT
            REP       ", ",WBPESTAT
          ENDIF
.
          GOTO      PKEY0000
.
