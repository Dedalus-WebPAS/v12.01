MAIN0000  COMMIT
.
          BEGIN
.
          DISPLAY   *W2
.
          MATCH     "1",PTCNWSIN
          GOTO      MAIN0000 IF NOT EQUAL
.
          MATCH     "1",PTCNPCIW
          GOTO      MAIN0000 IF NOT EQUAL
.
          MOVE      ZERO,EXIT
.
          PACK      KEY30,SP1,FOUR,SP70
          CALL      RSWBPCI8
MAIN1000  CALL      RKWBPCI8
          BRANCH    OVRCD,MAIN9999
.
          COMPARE   FOUR,WBPIFLAG
          GOTO      MAIN9999 IF NOT EQUAL
.
          MATCH     SP70,WBPITRID
          GOTO      MAIN1000 IF EQUAL
.
          PACK      KEY71,WBPIHOSP,SP70          * Read B2B Device Table
          CALL      RSWBB2B3
          CALL      RKWBB2B3
          BRANCH    OVRCD,MAIN1000
.
          MATCH     WBPIHOSP,WBB2HOSP            * Same Hospital
          GOTO      MAIN1000 IF NOT EQUAL
.
          MATCH     "ACTIVE",WBB2DSTA            * Device Active
          GOTO      MAIN1000 IF NOT EQUAL
.
          MATCH     SP70,WBB2DPTH
          GOTO      MAIN1000 IF EQUAL
.
          CALL      PROPN000                     * Valid Properties File?
.
          MATCH     "1",PRODAIND
          GOTO      MAIN1000 IF EQUAL
.
          PACK      SAVKEY30,WBPIFLAG,WBPIHOSP,WBPIMODL,WBPIUNIQ,WBPIINVN:
                             WBPIBATN,SP70
.
          OPEN      WBSPROPF,PRDAPATH
.
          MOVE      SP70,PCIPINDC
.
. Parse _pciw file (PCIW Response JSON file)
. **************************************************
.
MAIN2000  CLEAR     CMDLINE
          APPEND    WBB2DPTH,CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    "/webPAS_claims/pciw/",CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    WBPITRID,CMDLINE
          APPEND    PCIRLITR,CMDLINE
          RESET     CMDLINE
.
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      PCIRFILE,CMDLINE
          TRAPCLR   IO
          IF        OVRCD=1
            GOTO      MAIN3000
          ENDIF
.
          CALL      PCIPRS00                   * Parse PCIW Response JSON
          CALL      PCICBF00                   * Statement of Claim/Benefit
          CALL      PCIPEN00                   * Automatic Pendable to Pended
.
          GOTO      MAIN8000
.
. Parse _pciw_errors file (PCIW Stderrs/Stdout file)
. ******************************************************
.
MAIN3000  CLEAR     CMDLINE
          APPEND    WBB2DPTH,CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    "/webPAS_claims/pciw/",CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    WBPITRID,CMDLINE
          APPEND    PCIELITR,CMDLINE
          RESET     CMDLINE
.
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      PCIEFILE,CMDLINE
          TRAPCLR   IO
          IF        OVRCD=1
            MOVE      "Response files not found.",PCIELINE  
            GOTO      MAIN3500
          ENDIF
.
MAIN3100  READ      PCIEFILE,SEQ;PCIELINE
          IF        @OVER
            MOVE      "Refer to PCIW Details.",PCIELINE
            GOTO      MAIN3500 
          ENDIF
.
          SCAN      "error",PCIELINE
          IF        @EQUAL
            GOTO      MAIN3500
          ENDIF
.
          GOTO      MAIN3100
.
MAIN3500  MOVE      "1",PCIEINDC
.
          MOVE      WBPIINVN,WBPHINVN
          CALL      IBACLOCK
          PACK      WBPHDATE,CCC,CYY,CMM,CDD
          REP       " 0",WBPHDATE
          MOVE      CTIMEIS,WBPHTIME
          MOVE      WBPIBATN,WBPHBATN
          MOVE      THREE,WBPHSTAT
          MOVE      " 7",WBPHTYPE
          MOVE      SP70,WBPHOPER
          MOVE      WBPITRID,WBPHTRID
          MOVE      SP70,WBPHEROR
          MOVE      PCIELINE,WBPHEROT
          MOVE      WBPIMSID,WBPHMSID
          PACK      WBPHSPAR,SP70,SP70
.
          PACK      KEY27,WBPHINVN,WBPHDATE,WBPHTIME,WBPHTYPE,WBPHMODL,SP70
          CALL      RAWBPCA1
          IF        OVRCD=0
            GOTO      MAIN3500
          ENDIF
.
          TRAP      OVERCOND IF IO
          CALL      WRWBPCA1
          TRAPCLR   IO
.
          GOTO      MAIN8000
.
