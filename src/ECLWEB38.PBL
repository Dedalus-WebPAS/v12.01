.
MAIN8000  PACK      KEY30,WBPIFLAG,WBPIHOSP,WBPIMODL,WBPIUNIQ,WBPIINVN:
                          WBPIBATN,SP70
          CALL      RDWBPCI8
.
          MATCH     "1",PCIEINDC
          IF        @EQUAL
            MOVE      THREE,WBPIFLAG
          ELSE
            MATCH     "1",PCIPINDC
            IF        @EQUAL
              MOVE      TWO,WBPIFLAG
            ELSE
              MOVE      FIVE,WBPIFLAG
            ENDIF
          ENDIF

          CALL      IBACLOCK
          PACK      WBPIUDAT,CCC,CYY,CMM,CDD
          REP       " 0",WBPIUDAT
          MOVE      CTIMEIS,WBPIUTIM
          MOVE      USERID,WBPIUUID
          CALL      UPWBPCI8
.
