.
            MOVE      "1",PCIEINDC
.
PCIPRS05    READ      PCIRFILE,SEQ;PCIRLINE
            GOTO      PCIPRS90 IF OVER
.
.           *************************************************
.           serviceMessage
.           *************************************************
.
            SCAN      "serviceMessage",PCIRLINE
            IF        @EQUAL
.
PCIPRS07      READ      PCIRFILE,SEQ;PCIRLINE
              GOTO      PCIPRS20 IF OVER
.
.             ***********************************************
.             {    
.             ***********************************************
.
              SCAN      "{",PCIRLINE
              IF        @EQUAL
                MOVE      SP70,JSONCODE
                PACK      JSONTEXT,SP70,SP70,SP70,SP70,SP70,SP70,SP70,SP70
.
                GOTO      PCIPRS07
.
              ENDIF
.
.             ***********************************************
.             code
.             ***********************************************
.
              SCAN      "code",PCIRLINE
              IF        @EQUAL
                RESET     PCIRLINE
.               UNPACK    PCIRLINE,DIM14,JSONCODE
.
                SCAN      ":",PCIRLINE
                IF        @EQUAL
                  MOVE      PCIRLINE,PCIRWORK
                  REP       ": ",PCIRWORK
                  REP       "#" ",PCIRWORK
                  LJUSTIFY  PCIRWORK
                  MOVE      PCIRWORK,JSONCODE
                  RESET     PCIRLINE
                ENDIF
.
                GOTO      PCIPRS07
.
              ENDIF
.
.             ***********************************************
.             text
.             ***********************************************
.
              SCAN      "reason",PCIRLINE
              IF        @EQUAL
                RESET     PCIRLINE
.               UNPACK    PCIRLINE,DIM16,JSONTEXT
.
                SCAN      ":",PCIRLINE
                IF        @EQUAL
                  MOVE      PCIRLINE,PCIRWORK
                  REP       ": ",PCIRWORK
                  REP       "#" ",PCIRWORK
                  LJUSTIFY  PCIRWORK
                  MOVE      PCIRWORK,JSONTEXT
                  RESET     PCIRLINE
                ENDIF
.
                REP       ", ",JSONTEXT
                REP       "#" ",JSONTEXT
.
                GOTO      PCIPRS07
.
              ENDIF
.
.             ***********************************************
.             }
.             ***********************************************
.
              SCAN      "}",PCIRLINE
              IF        @EQUAL
.
PCIPRS10        MOVE      WBPIINVN,WBPHINVN
                CALL      IBACLOCK
                PACK      WBPHDATE,CCC,CYY,CMM,CDD
                REP       " 0",WBPHDATE
                MOVE      CTIMEIS,WBPHTIME
                MOVE      WBPIBATN,WBPHBATN
                MOVE      THREE,WBPHSTAT
                MOVE      " 6",WBPHTYPE
                MOVE      SP70,WBPHOPER
                MOVE      WBPITRID,WBPHTRID
                MOVE      JSONCODE,WBPHEROR
                MOVE      JSONTEXT,WBPHEROT
                MOVE      WBPIMSID,WBPHMSID
                PACK      WBPHSPAR,SP70,SP70
.
