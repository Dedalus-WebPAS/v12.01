                  REP       ", ",WBPGASCD
                ENDIF
.
.               ***********************************************
.               id
.               ***********************************************
.
                SCAN      "id",PCIRLINE
                IF        @EQUAL
                  RESET     PCIRLINE
.                 UNPACK    PCIRLINE,DIM20,WBPGSVID
.
                  SCAN      ":",PCIRLINE
                  IF        @EQUAL
                    MOVE      PCIRLINE,PCIRWORK
                    REP       ": ",PCIRWORK
                    REP       "#" ",PCIRWORK
                    LJUSTIFY  PCIRWORK
                    MOVE      PCIRWORK,WBPGSVID
                    RESET     PCIRLINE
                  ENDIF
.
                  REP       "#" ",WBPGSVID
                  REP       ", ",WBPGSVID
                ENDIF
.
.               ***********************************************
.               close curly bracket
.               ***********************************************
.
                SCAN      "}",PCIRLINE
                IF        @EQUAL
.
.                 get latest report counter from webpcfaf
.                 ---------------------------------------
.
                  MOVE      WBPECLID,WBPFCLID
                  MOVE      WBPETRID,WBPFTRID
                  MOVE      WBPIMSID,WBPFMSID
.
                  MOVE      ZERO,FORM3
PCIP53BB          ADD       ONE,FORM3
                  MOVE      FORM3,WBPFCNTR
.
                  PACK      KEY49,WBPFCLID,WBPFTRID,WBPFCNTR,SP70
                  CALL      RAWBPCF1
                  IF        OVRCD=0
                    GOTO      PCIP53BB
                  ENDIF
.
.                 write to webpcgaf
.                 -----------------
.
                  MOVE      WBPECLID,WBPGCLID
                  MOVE      WBPETRID,WBPGTRID
                  MOVE      FORM3,WBPGCNTR
                  MOVE      WBPIMSID,WBPGMSID
.
                  MOVE      ZERO,FORM2
PCIP53CC          ADD       ONE,FORM2
                  MOVE      FORM2,WBPGSCNT
.
                  PACK      KEY51,WBPGCLID,WBPGTRID,WBPGCNTR,WBPGSCNT,SP70
                  CALL      RAWBPCG1
                  IF        OVRCD=0
                    GOTO      PCIP53CC
                  ENDIF
.
                  CALL      WRWBPCG1
.
                  CALL      CLWEBPCG
.
                ENDIF
.
.               ***********************************************
.               close square bracket
.               ***********************************************
.
                SCAN      "]",PCIRLINE
                IF        @EQUAL
                  GOTO      PCIPR53Z
                ENDIF
.
PCIP53ZZ        GOTO      PCIP53AA
.
              ENDIF
.
.             ***********************************************
.             id
.             ***********************************************
.
              SCAN      "id",PCIRLINE
              IF        @EQUAL
                RESET     PCIRLINE
.               UNPACK    PCIRLINE,DIM16,WBPFMEID
.
                SCAN      ":",PCIRLINE
                IF        @EQUAL
                  MOVE      PCIRLINE,PCIRWORK
                  REP       ": ",PCIRWORK
                  REP       "#" ",PCIRWORK
                  LJUSTIFY  PCIRWORK
                  MOVE      PCIRWORK,WBPFMEID
                  RESET     PCIRLINE
                ENDIF
.
                REP       "#" ",WBPFMEID
                REP       ", ",WBPFMEID
              ENDIF
.
.             ***********************************************
.             eventDate
.             ***********************************************
.
              SCAN      "eventDate",PCIRLINE
              IF        @EQUAL
                RESET     PCIRLINE
.               UNPACK    PCIRLINE,DIM23,WBPFEVDT
.
                SCAN      ":",PCIRLINE
                IF        @EQUAL
                  MOVE      PCIRLINE,PCIRWORK
                  REP       ": ",PCIRWORK
                  REP       "#" ",PCIRWORK
                  REP       "- ",PCIRWORK
                  SQUEEZE   PCIRWORK
                  LJUSTIFY  PCIRWORK
                  MOVE      PCIRWORK,WBPFEVDT
                  RESET     PCIRLINE
                ENDIF
.
                REP       "#" ",WBPFEVDT
                REP       ", ",WBPFEVDT
              ENDIF
.
.             ***********************************************
.             close curly bracket
.             ***********************************************
.
              SCAN      "}",PCIRLINE
              IF        @EQUAL
.
.               write to webpcfaf
.               ----------------- 
.
                MOVE      WBPECLID,WBPFCLID
                MOVE      WBPETRID,WBPFTRID
                MOVE      WBPIMSID,WBPFMSID
.
                MOVE      ZERO,FORM3
PCIPR53B        ADD       ONE,FORM3
                MOVE      FORM3,WBPFCNTR
.
                PACK      KEY49,WBPFCLID,WBPFTRID,WBPFCNTR,SP70
                CALL      RAWBPCF1
                IF        OVRCD=0
                  GOTO      PCIPR53B
                ENDIF
.
                CALL      WRWBPCF1
.
                CALL      CLWEBPCF
.
              ENDIF
.
.             ***********************************************
.             close square bracket
.             ***********************************************
.
              SCAN      "]",PCIRLINE
              IF        @EQUAL
                GOTO      PCIPRS70
              ENDIF
.
PCIPR53Z      GOTO      PCIPR53A
.
            ENDIF
.
.           *************************************************
.           claimId
.           *************************************************
.
            SCAN      "claimId",PCIRLINE
            IF        @EQUAL
              RESET     PCIRLINE
.             UNPACK    PCIRLINE,DIM17,WBPECLID
.
              SCAN      ":",PCIRLINE
              IF        @EQUAL
                MOVE      PCIRLINE,PCIRWORK
                REP       ": ",PCIRWORK
                REP       "#" ",PCIRWORK
                LJUSTIFY  PCIRWORK
                MOVE      PCIRWORK,WBPECLID
                RESET     PCIRLINE
              ENDIF
.
              REP       "#" ",WBPECLID
              REP       ", ",WBPECLID
            ENDIF
.
.           ***********************************************
.           close curly bracket
.           ***********************************************
.
            SCAN      "}",PCIRLINE
            IF        @EQUAL
.
.             write to webpceaf
.             -----------------
.
              MOVE      WBPIMSID,WBPEMSID
.
              PACK      KEY46,WBPECLID,WBPETRID,SP70
              CALL      RAWBPCE1
              IF        OVRCD=1
                CALL      WRWBPCE1
              ENDIF
.
.             write to webpcaaf
.             -----------------
.
PCIPRS62      MOVE      WBPIINVN,WBPHINVN
              CALL      IBACLOCK
              PACK      WBPHDATE,CCC,CYY,CMM,CDD
              REP       " 0",WBPHDATE
              MOVE      CTIMEIS,WBPHTIME
              MOVE      WBPIBATN,WBPHBATN
.
              MOVE      FIVE,WBPHSTAT
.
              MOVE      " 0",WBPHTYPE
              MOVE      SP70,WBPHOPER
              MOVE      WBPITRID,WBPHTRID
              MOVE      SP70,WBPHEROR
              PACK      WBPHEROT,WBPESTAT,SP70
              MOVE      WBPIMODL,WBPHMODL
              MOVE      WBPIMSID,WBPHMSID
              PACK      WBPHSPAR,SP70,SP70
.
              PACK      KEY27,WBPHINVN,WBPHDATE,WBPHTIME,WBPHTYPE,WBPHMODL,SP70
              CALL      RAWBPCA1
              IF        OVRCD=0
                GOTO      PCIPRS62
              ENDIF
.
              TRAP      OVERCOND IF IO
              CALL      WRWBPCA1
              TRAPCLR   IO
.
PCIPRS69      GOTO      PCIPRS90
            ENDIF
.
PCIPRS70    GOTO      PCIPRS50
.
          ENDIF
.
PCIPRS90  GOTO      PCIPRS01
.
PCIPRS99  RETURN
