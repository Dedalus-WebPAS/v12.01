          DISPLAY   *P40:4,*EF:
                    *P1:6,*V2LON,"0",*HOFF," = Exit":
                    *P1:7,*V2LON,"1",*HOFF," = Create HF IMC Extract ":
                    *P1:8,*V2LON,"2",*HOFF," = Create Medicare IMC Extract ":
                    *P1:10,"Select Option : ";
