.----------------------------------------------------------------------
KSIWA000
          CALL      KSIWSA00                     * keyin SIWA with security
          BRANCH    EXIT,KSIWA950,KSIWA950
.
          REP       " 0",SIWANPO
          MATCH     "0000000",SIWANPO
          IF        @EQUAL
            CLEAR     DISPMSG
            APPEND    "Warehouse May Not Have Purchase Orders - ",DISPMSG
            CALL      MESSAGE1
