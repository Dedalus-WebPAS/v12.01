.**************************************************************************
.*                               PROC0000              Called by: ML0000  *
.*                                                                        *
.**************************************************************************
PROC0000  PACK      KEY16,FROMDATE,SP70
          CALL      RSHCEFA2
PROC1000  CALL      RKHCEFA2
          BRANCH    OVRCD,PROC9999
.
          MATCH     FROMDATE,HCEAPDAT      * Check payment date
          GOTO      PROC9999 IF NOT EQUAL
.
.         Read payment file
.
          PACK      KEY13,HCEARNUM,HCEACNTR,SP70
          CALL      RSHCEFC2
PROC2000  CALL      RKHCEFC2
          BRANCH    OVRCD,PROC1000
.
          MATCH     HCEARNUM,HCECRNUM      * Same run number?
          GOTO      PROC1000 IF NOT EQUAL
.
          MATCH     HCEACNTR,HCECCNTR      * Same counter number?
          GOTO      PROC1000 IF NOT EQUAL
.
          PACK      KEY33,HCECBTCH,FILTPROV,SP70
          CALL      RSHCBTC1
          CALL      RKHCBTC1
          BRANCH    OVRCD,PROC2000
.
          MATCH     HCECBTCH,HCBTBTCH            * Same batch no ?
          GOTO      PROC2000 IF NOT EQUAL
.
          MATCH     SP70,FILTPROV
          IF        !@EQUAL
            MATCH     FILTPROV,HCBTPSRV
            GOTO      PROC2000 IF NOT EQUAL
          ENDIF
.
          MATCH     IDDD,HCBTSITE
          GOTO      PROC2000 IF NOT EQUAL
.
          BRANCH    ALACCESS,PROC4000       * Access to all practices 
.
          PACK      KEY16,HCBTMPRA,WEBUSRID,SP70
          CALL      RDPRSEC1
          BRANCH    OVRCD,PROC2000          * No access to this practice
.
PROC4000  MATCH     SP70,CLINICTO
          IF        !@EQUAL
            MATCH     HCBTCLID,CLINICTO
            GOTO      PROC2000 IF LESS
          ENDIF
.
          MATCH     SP70,CLINFROM
          IF        @EQUAL
            MATCH     CLINFROM,HCBTCLID
            GOTO      PROC2000 IF LESS
          ENDIF
.
.
          MATCH     SP70,PRACTIFR
          IF        !@EQUAL
            MATCH     PRACTIFR,HCBTMPRA
            GOTO      PROC2000 IF LESS
          ENDIF
.
          MATCH     SP70,PRACTITO
          IF        !@EQUAL
            MATCH     HCBTMPRA,PRACTITO
            GOTO      PROC2000 IF LESS
          ENDIF
.
          MOVE      HCBTBTCH,TEMPBTCH
          MOVE      HCBTPMCI,TEMPPMCI
          MOVE      HCBTPYEE,TEMPPYEE
          MOVE      HCBTPSRV,TEMPPSRV
          MOVE      HCBTMPRA,TEMPMPRA
          MOVE      HCBTCAMT,TEMPCAMT
          MOVE      HCECDAMT,TEMPAMTR
          MOVE      HCBTDATE,TEMPDATE
          MOVE      HCBTCLID,TEMPCLID
          MOVE      SP70,TEMPCOST
.
          PACK      KEY18,HCBTSITE,HCBTCLID,SP70
          CALL      RDSMASA1
          CALL      RDKMASA1
          BRANCH    OVRCD,PROC8000
          MATCH     OMASITE,HCBTSITE          * Same site?
          GOTO      PROC8000 IF NOT EQUAL
          MATCH     OMACLIN,HCBTCLID          * Same clinic id?
          GOTO      PROC8000 IF NOT EQUAL
          MOVE      OTMACCTR,TEMPCOST         * Cost center
.
PROC8000  PACK      KEY11,TEMPCLID,TEMPBTCH
          CALL      RATEMP1
          IF        OVRCD=1
            CALL      WRTEMP1
          ENDIF
          GOTO      PROC2000
.
PROC9999  RETURN
+
.**********************************************************************
.*                         PRIN0000                                   *
.*                        Print Data                                  *
.**********************************************************************
PRIN0000  MOVE      ZERO,CPAGENO
          MOVE      SP70,SAVEVAR
          MOVE      ZERO,TOTAMNT
          MOVE      ZERO,TOTRECV
.
          PACK      KEY11,SP70
          PACK      KEY15,SP70
          CALL      RSTEMP1
PRIN1000  CALL      RKTEMP1
          BRANCH    OVRCD,PRIN9000
.
          MATCH     SP70,SAVEVAR
          GOTO      PRIN1200 IF EQUAL            * first record
          CALL      CVAR0000                     * check if sort field changed
          BRANCH    EXIT,PRIN1100                * changed
.
          GOTO      PRIN1300
.
PRIN1100  CALL      UND132
          PRINT     *71,"| TOTAL",*84,"|",TOTAMNT,*101,"|",TOTRECV,*118,"|":
                    *N,*71,"*------------":
                    *84,"*----------------",*101,"*----------------*"
          ADD       TWO,CLNO
          MOVE      ZERO,TOTAMNT
          MOVE      ZERO,TOTRECV
          MOVE      ONE,CNOUNDLN                 * no underline
.
PRIN1200  CALL      HEAD0000
          MOVE      ZERO,CNOUNDLN
          CALL      SVAR0000                     * save sort variable
.
PRIN1300  MOVE      SP70,PRPRDESC
          PACK      KEY6,TEMPMPRA,SP70
          CALL      RDPRPR1
.
          UNPACK    TEMPDATE,CCENT,CYEAR,CMON,CDAY
          CALL      PACDATE
.
          COMPARE   "55",CLNO
          CALL      HEAD0000 IF NOT LESS
.
          CALL      CLID0000              * get name of clinic id
          UNPACK    PRPRDESC,KEY25
          PRINT     *1,"| ",KEY25,*31,"| ",OCADESC:
                    *63,"| ",TEMPBTCH,*71,"| ",CPCDATE:
                    *84,"|",TEMPCAMT,*101,"|",TEMPAMTR:
                    *118,"| ",TEMPCOST,*132,"|"
          ADD       TEMPCAMT,TOTAMNT
          ADD       TEMPAMTR,TOTRECV
.
          ADD       ONE,CLNO
          COMPARE   "55",CLNO
          CALL      HEAD0000 IF NOT LESS
.
          GOTO      PRIN1000              * read next record
.
PRIN9000  COMPARE   ZERO,CPAGENO
          GOTO      PRIN9999 IF NOT EQUAL
          CALL      HEAD0000
PRIN9999  RETURN
+
.*********************************************************************
.         Save sort variable
.*********************************************************************
SVAR0000  MOVE      SP70,SAVEVAR
          COMPARE   ONE,SORTFLAG
          GOTO      SVAR4000 IF EQUAL    * sort by clinic id
          COMPARE   TWO,SORTFLAG
          GOTO      SVAR6000 IF EQUAL    * sort by medical practice
.
.         Sort by service provider
.
          PACK      SAVEVAR,TEMPPSRV,SP70
          GOTO      SVAR9999
.
.         Sort by clinic id
.
SVAR4000  PACK      SAVEVAR,TEMPCLID,SP70
          GOTO      SVAR9999
.
.         Sort by medical practice
.
SVAR6000  PACK      SAVEVAR,TEMPMPRA,SP70
.
SVAR9999  RETURN
+
.*********************************************************************
.         check if sort variable change
.*********************************************************************
CVAR0000  MOVE      ZERO,EXIT
          COMPARE   ONE,SORTFLAG
          GOTO      CVAR4000 IF EQUAL    * sort by clinic id
          COMPARE   TWO,SORTFLAG
          GOTO      CVAR6000 IF EQUAL    * sort by medical practice
.
          MATCH     SAVEVAR,TEMPPSRV     * check service provider
          GOTO      CVAR9999 IF EQUAL
          GOTO      CVAR9000
.
CVAR4000  MATCH     SAVEVAR,TEMPCLID     * check clinic id
          GOTO      CVAR9999 IF EQUAL
          GOTO      CVAR9000
.
CVAR6000  MATCH     SAVEVAR,TEMPMPRA     * check medical practice
          GOTO      CVAR9999 IF EQUAL
.
CVAR9000  MOVE      ONE,EXIT
CVAR9999  RETURN

.*********************************************************************
.         Get clinic id
.*********************************************************************
CLID0000  MOVE      SP70,OCADESC
          PACK      KEY20,IDDD,TEMPCLID,Z70
          CALL      RDSCLIA1
          CALL      RDPCLIA1                *To get Clinic ID Description
          BRANCH    OVRCD,CLID9000
.
          MATCH     IDDD,OCASITE
          GOTO      CLID9000 IF NOT EQUAL
.
          MATCH     TEMPCLID,OCACLIN
          GOTO      CLID9000 IF NOT EQUAL
.
          MATCH     SP70,OCADESC
          GOTO      CLID9999 IF NOT EQUAL
.
          PACK      KEY10,OCADOCT,SP70
          CALL      RDPMHCP1
          BRANCH    OVRCD,CLID9000
.
          MOVE      PMHCSNAM,PACSNAME
          MOVE      PMHCGNAM,PACGNAME
          MOVE      PMHCTITL,PACTITLE
          MOVE      ONE,PACFRMT
          CALL      PACNAME
          MOVE      PACFNAME,OCADESC
          GOTO      CLID9999
.
CLID9000  MOVE      SP70,OCADESC
CLID9999  RETURN
+
.*********************************************************************
.         print a new page
.*********************************************************************
HEAD0000  CALL      HEAD132
.
          MOVE      SP70,CPCDATE
          MATCH     SP70,FROMDATE
          IF        !@EQUAL
            UNPACK    FROMDATE,CCENT,CYEAR,CMON,CDAY
            CALL      PACDATE
          ENDIF
          PRINT     *1,"Payment Received Date For: ",CPCDATE
.
          PRINT     *1,"Medical Practice From : ",HEADING5," To : ",HEADING6
          PRINT     *1,"Clinic From           : ",HEADING1," To : ",HEADING2
          PRINT     *1,"Provider Number       : ",HEADING0
.
          ADD       FOUR,CLNO
.
          CALL      PRHD0000                     * print sort heading
          CALL      UND132
.
          PRINT     *1,"| Medical Practice ",*31,"| Clinic ID":
                    *63,"| Batch ",*71,"|   Batch":
                    *84,"|    Batch",*101,"|   EFT Payment":
                    *118,"| Cost Centre",*132,"|":
                    *N,*1,"| ",*31,"|":
                    *63,"|  No.",*71,"|   Date":
                    *84,"|    Amount",*101,"|    Received":
                    *118,"|",*132,"|"
          ADD       TWO,CLNO
          CALL      UND132
.
HEAD9999  RETURN
