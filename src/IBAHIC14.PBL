.****************************************************************************
.*        IO ROUTINES FOR TEMPORARY FILE                                    *
.****************************************************************************
.
RATEMP1   RESET     KEY11
          MOVE      ZERO,OVRCD
          READ      HICTEMP1,KEY11;ANS
          GOTO      OVERCOND IF OVER
          RETURN
.
RSTEMP1   MOVE      SP70,KEY11
          MOVE      SP70,KEY15
          IF        SORTFLAG=1
            READ      HICTEMP1,KEY11;;
          ELSE
            IF        SORTFLAG=2
              READ      HICTEMP2,KEY11;;
            ELSE
              READ      HICTEMP3,KEY15;;
            ENDIF
          ENDIF
          RETURN
.
RKTEMP1   MOVE      ZERO,OVRCD
          IF        SORTFLAG=1
            READKS    HICTEMP1;TEMPBTCH,TEMPPMCI,TEMPPYEE,TEMPPSRV:
                               TEMPMPRA,TEMPCAMT,TEMPAMTR,TEMPCOST:
                               TEMPDATE,TEMPCLID
          ELSE
            IF        SORTFLAG=2
              READKS    HICTEMP2;TEMPBTCH,TEMPPMCI,TEMPPYEE,TEMPPSRV:
                                 TEMPMPRA,TEMPCAMT,TEMPAMTR,TEMPCOST:
                                 TEMPDATE,TEMPCLID
            ELSE
              READKS    HICTEMP3;TEMPBTCH,TEMPPMCI,TEMPPYEE,TEMPPSRV:
                                 TEMPMPRA,TEMPCAMT,TEMPAMTR,TEMPCOST:
                                 TEMPDATE,TEMPCLID
            ENDIF
          ENDIF
          GOTO      OVERCOND IF OVER
          RETURN
.
WRTEMP1   WRITE     HICTEMP1,KEY11;TEMPBTCH,TEMPPMCI,TEMPPYEE,TEMPPSRV:
                             TEMPMPRA,TEMPCAMT,TEMPAMTR,TEMPCOST:
                             TEMPDATE,TEMPCLID
          RETURN
.
