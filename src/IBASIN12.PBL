          MATCH     HDEFWH,SIIEWAR     * Default Warehouse ?
          IF        @EQUAL
            MOVE      SP70,KEY5
          ENDIF
.
          MATCH     SP70,KEY5          * Save first or default warehouse
          IF        @EQUAL
            MOVE      SIIEWAR,KEY5
            MOVE      SIIERAS,DEFRAS
            MOVE      SIIEROL,DEFROL
            MOVE      SIIEROQ,DEFROQ
          ENDIF
          GOTO      ECATB200
