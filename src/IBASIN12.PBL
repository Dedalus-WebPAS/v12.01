.----------------------------------------------------------------------
SETBO000  MOVE      ZERO,BOCOST
          MOVE      ZERO,BOTOTAL
          PACK      KEY25,SIIACAT,SIWAWAR,SP70
          CALL      RSSIBA3
          REPEAT
            CALL      RKSIBA3
            PACK      KEY12,SIBACAT,SIBAWAR,SP70
            MATCH     KEY12,KEY25
            IF        !@EQUAL | OVRCD=1
              MOVE      ONE,OVRCD
            ELSE
              ADD       ONE,BOCOST
              ADD       SIBAQTY,BOTOTAL
            ENDIF
          UNTIL     OVRCD=1
.
          ADD       ONE,SIIESUS
          BRANCH    SIIESUS,SETBO100,SETBO200,SETBO300,SETBO400
.
SETBO100  MOVE      "Not Suspend               ",SUSPDES1
          GOTO      SETBO999
.
