OUTDET00  MOVE     SIPCPON,KEY7
          CALL     RDSIPA1
.
          MOVE     ONE,SIFACTOR
          MATCH    SP7,SIPCCAT
          IF       !@EQUAL
            PACK     KEY7,SIPCCAT,SP70
            CALL     RDSIIA1
            IF       OVRCD=0
              MOVE     SIIADES,SIPCPD
              PACK     KEY30,SIIAISS,SIPCSUT,SP70
              CALL     RDSIFA1
            ENDIF
          ENDIF
.
          ASSIGN   SIPCOQT-(SIPCRQT/SIFACTOR),OUTQTY
.
          ASSIGN   SIPCECT*OUTQTY,F12P2
          IF       SIPCECT<0
            MOVE     "      T.B.A,",ITEMCOST
          ELSE
            MOVE     "9,999,999.99",ITEMCOST
            FEDIT    F12P2,ITEMCOST
            MATCH    "*",ITEMCOST
            IF       @EQUAL
              MOVE     F12P2,F9P2
              MOVE     F9P2,ITEMCOST
            ENDIF
          ENDIF
          RETURN
