          CALL      RSSIIE1
          CALL      RKSIIE1
          MATCH     SIIACAT,SIIECAT
          IF        !@EQUAL
            MOVE      SP70,SIIERAS
          ENDIF
.
          MOVE      "RACK/SHELF: ",KEY14
          PACK      LLINE8,KEY14,SIIERAS,SP70
.
          CALL      STLAB000           * store and print labels
.
WRIT9999  RETURN
.----------------------------------------------------------------------
. Read Label Parameters
.----------------------------------------------------------------------
RDLAB000
          READ      CONTROLF,TWENTY3;*188,HLABPG,HLABPRT:
                                     *239,HCATLAB
          MOVE      "37",TABPOS2
          MOVE      "73",TABPOS3
          IF        2=HCATLAB
            MOVE      "43",TABPOS2
            MOVE      "85",TABPOS3
          ENDIF
          IF        3=HCATLAB
            MOVE      "30",TABPOS2
            MOVE      "66",TABPOS3
          ENDIF
.
          MOVE        ONE,LENIND
          MOVE        ZERO,LABNUM
          UNPACK      SP70,ALINEA,ALINEB
          UNPACK      SP70,ALINE1,ALINE2
          UNPACK      SP70,ALINE3,ALINE4
          UNPACK      SP70,ALINE5,ALINE6
          UNPACK      SP70,ALINE7,ALINE8
          UNPACK      SP70,BLINEA,BLINEB
          UNPACK      SP70,BLINE1,BLINE2
          UNPACK      SP70,BLINE3,BLINE4
          UNPACK      SP70,BLINE5,BLINE6
          UNPACK      SP70,BLINE7,BLINE8
          UNPACK      SP70,CLINEA,CLINEB
          UNPACK      SP70,CLINE1,CLINE2
          UNPACK      SP70,CLINE3,CLINE4
          UNPACK      SP70,CLINE5,CLINE6
          UNPACK      SP70,CLINE7,CLINE8
.
RDLAB999  RETURN
.********************************************************************
. BKLIN - Break up KEY60 into two neat halves (first being 27 char long)
.         Requires - KEY60   (original line)
.         Returns  - KEY27   (line 1)
.                    KEY60   (line 2)
.********************************************************************
BKLIN000  RESET     KEY60,28
          CMATCH    SP70,KEY60
          WHILE     !@EQUAL&!@EOS
            BUMP      KEY60,-1
            IF        !@EOS
              CMATCH    SP70,KEY60
            ENDIF
          DO
.
          IF        @EOS
            SETLPTR   KEY60
            RESET     KEY60
            UNPACK    KEY60,KEY27,KEY60
          ELSE
            LENSET    KEY60
            RESET     KEY60
            MOVE      KEY60,KEY27
            PACK      KEY27,KEY27,SP70
.
            ENDSET    KEY60
            SETLPTR   KEY60
            BUMP      KEY60
            PACK      KEY60,KEY60,SP70
          ENDIF
.
BKLIN999  RETURN
.----------------------------------------------------------------------
. Keyin from code (SIIA)
.----------------------------------------------------------------------
KCODF000
          MATCH     SIWAWAR,SP70
          IF        @EQUAL
            CALL      KSIIAA00
          ELSE
            CALL      KSIIEA00                     * keyin SIIA
            MOVE      SIIECAT,SIIACAT
          ENDIF
          BRANCH    EXIT,KCODF100,KCODF950
.
          PACK      FROMLAB,SIIACAT,SP70         * item entered
          PACK      FROMCOD7,SIIACAT,SP70
          PACK      FROMDESC,SIIADES,SP70
          GOTO      KCODF900
.
KCODF100  PACK      FROMLAB,START,SP70           * nothing entered
          PACK      FROMCOD7,SP70
          PACK      FROMDESC,SP70
          GOTO      KCODF900
.
KCODF900  MOVE      ZERO,EXIT                    * entry ok
          GOTO      KCODF999
.
KCODF950  MOVE      TWO,EXIT                     * quit screen
.
KCODF999  RETURN
.----------------------------------------------------------------------
. Keyin to code (SIIA)
.----------------------------------------------------------------------
KCODT000
          MATCH     SIWAWAR,SP70
          IF        @EQUAL
            CALL      KSIIAA00
          ELSE
            CALL      KSIIEA00                     * keyin SIIA
            MOVE      SIIECAT,SIIACAT
          ENDIF
          BRANCH    EXIT,KCODT100,KCODT950
.
          PACK      TOLAB,SIIACAT,SP70           * item entered
          PACK      TOCOD7,SIIACAT,SP70
          PACK      TODESC,SIIADES,SP70
          GOTO      KCODT900
.
KCODT100  PACK      TOLAB,END,SP70               * nothing entered
          PACK      TOCOD7,Z70
          PACK      TODESC,SP70
          GOTO      KCODT900
.
KCODT900  MOVE      ZERO,EXIT                    * entry ok
          GOTO      KCODT999
.
KCODT950  MOVE      TWO,EXIT                     * quit screen
.
KCODT999  RETURN
.**********************************************************************
. ALIGN- print lineup routine
. Requires IBACODFD/INC IBAPRNFD/INC
.          IBAIOCOD/INC IBAIOPRN/INC IBAPRINT
.  Note : already includes IBAPASFD & IBAPASIO
.**********************************************************************
ALIGN000
          PACK      DISPMSG,SP70,SP70       * save line 24
          CALL      MESSAGE2
.
          CALL      OPNPRINT
.
          CALL      RDLAB000                * read parameters
          CALL      LULAB000                * print routine
          CALL      CLSPRINT
.
ALIGN999
          CALL      MESSAGE3                * restore line 24
          RETURN
.----------------------------------------------------------------------
. Keyin COPY
.----------------------------------------------------------------------
KCOPY000
          READ      CONTROLF,TWENTY3;*188,HLABPG,HLABPRT
          MOVE      HLABPRT,NOCOPIES
          KEYIN     *PCCOL:CVERT,*V2LON,*RV,NOCOPIES
.
          IF        NOCOPIES<1
            BEEP
            CLEAR     DISPMSG
            APPEND    "Must Be Greater Than Zero - ",DISPMSG
            CALL      MESSAGE1
            GOTO      KCOPY000
          ENDIF
.
KCOPY999  RETURN
EINCL000  CALL      DCODE000
.
EINCL010  MOVE      ZERO,INCLTYPE
          KEYIN     *PCCOL:CVERT,*V2LON,*RV,INCLTYPE
          COMPARE   ZERO,INCLTYPE
          GOTO      EINCL900 IF EQUAL
          BRANCH    INCLTYPE,EINCL100,EINCL200
          ASSIGN    INCLTYPE-2,F1
          PACK      KEY6,ANSC,ANSA,ANST,F1,SP70
          CALL      RDSICD1
          IF        OVRCD=1
            BEEP
            GOTO    EINCL010
          ENDIF
          MOVE      SICDDESC,INCLDESC
          GOTO      EINCL999
.
EINCL100  MOVE      "Stocked Catalogs",INCLDESC
          GOTO      EINCL999
.
EINCL200  MOVE      "Non-Stocked Catalogs",INCLDESC
          GOTO      EINCL999
.
EINCL900  MOVE      "All Catalogs",INCLDESC
          GOTO      EINCL999
.
EINCL999  CALL      PUTSCR00
          RETURN
.
DCODE000  MOVE      "13",HTOP
          MOVE      "44",HLEF
          MOVE      "24",HBOT
          MOVE      "80",HRIG
          CALL      GETSCR00
.
          BOXCLR    HLEF,HTOP,HRIG,HBOT
          BOX       16,HLEF,HTOP,HRIG,HBOT
          DISPLAY   *P52:14,*V2LON,*ULON,"Include Options",*HOFF:
                    *P46:15,*V2LON,"0. ",*HOFF,"All Catalogs":
                    *P46:16,*V2LON,"1. ",*HOFF,"Stocked Only":
                    *P46:17,*V2LON,"2. ",*HOFF,"Non-Stocked Only"
          MOVE      ONE,F1
DCODE100  PACK      KEY6,ANSC,ANSA,ANST,F1,SP70
          CALL      RDSICD1
          IF        OVRCD=1
            MOVE      "No Description on Code",SICDDESC
          ENDIF
          ASSIGN    F1+17,F2
          ADD       "2",F1
          DISPLAY   *P46:F2,*V2LON,F1,". ",*HOFF,SICDDESC
          SUB       "2",F1
          ADD       ONE,F1
          COMPARE   SEVEN,F1
          GOTO      DCODE100 IF NOT EQUAL
          RETURN
