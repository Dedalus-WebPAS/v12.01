          APPEND    PTCNWSDR,PRDAPATH
          RESET     PRDAPATH
          STRIP     PRDAPATH
          ENDSET    PRDAPATH
          APPEND    SLASH,PRDAPATH
          APPEND    PRDAFILE,PRDAPATH
          RESET     PRDAPATH
.
          MOVE      ZERO,PRODAIND
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      WBSPROPF,PRDAPATH
          TRAPCLR   IO
          IF        OVRCD=1
            MOVE      ONE,PRODAIND
            GOTO      PROPN500
          ENDIF
          CLOSE     WBSPROPF
.
          CLEAR     ACTVPATH
          APPEND    PTCNWSDR,ACTVPATH
          RESET     ACTVPATH
          STRIP     ACTVPATH
          ENDSET    ACTVPATH
          APPEND    SLASH,ACTVPATH
          APPEND    ACTVFILE,ACTVPATH
          RESET     ACTVPATH
.
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      WBSACTVF,ACTVPATH
          TRAPCLR   IO
          IF        OVRCD=1
            MOVE      TWO,PRODAIND
            GOTO      PROPN500
          ENDIF
          CLOSE     WBSACTVF
.
          CLEAR     TOKNPATH
          APPEND    PTCNWSDR,TOKNPATH
          RESET     TOKNPATH
          STRIP     TOKNPATH
          ENDSET    TOKNPATH
          APPEND    SLASH,TOKNPATH
          APPEND    TOKNFILE,TOKNPATH
          RESET     TOKNPATH
.
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      WBSTOKNF,TOKNPATH
          TRAPCLR   IO
          IF        OVRCD=1
            MOVE      THREE,PRODAIND
            GOTO      PROPN500
          ENDIF
          CLOSE     WBSTOKNF
.
          CLEAR     ACTOPATH
          APPEND    PTCNWSDR,ACTOPATH
          RESET     ACTOPATH
          STRIP     ACTOPATH
          ENDSET    ACTOPATH
          APPEND    SLASH,ACTOPATH
          APPEND    ACTOFILE,ACTOPATH
          RESET     ACTOPATH
.
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      WBSACTOF,ACTOPATH
          TRAPCLR   IO
          IF        OVRCD=1
            MOVE      FOUR,PRODAIND
            GOTO      PROPN500
          ENDIF
          CLOSE     WBSACTOF
.
          CLEAR     TOKOPATH
          APPEND    PTCNWSDR,TOKOPATH
          RESET     TOKOPATH
          STRIP     TOKOPATH
          ENDSET    TOKOPATH
          APPEND    SLASH,TOKOPATH
          APPEND    TOKOFILE,TOKOPATH
          RESET     TOKOPATH
.
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      WBSTOKOF,TOKOPATH
          TRAPCLR   IO
          IF        OVRCD=1
            MOVE      FIVE,PRODAIND
            GOTO      PROPN500
          ENDIF
          CLOSE     WBSTOKOF
.
PROPN500
.
