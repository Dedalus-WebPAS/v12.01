PAGSEP    IF        OPTION=2 
            PRINT     *19,"|--------------------------------------------------":
                      "--------------------------------------------|"
          ELSE
            PRINT     *1,"|----------------------------------------------":
                         "-----------------------------------------------":
                         "-------------------------------|"
          ENDIF
          ADD       ONE,CLNO
