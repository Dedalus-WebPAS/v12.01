          MATCH     SP70,BKRXDROR
          IF        !@EQUAL
            MOVE      BKRXDROR,TDESC
            MOVE      "Ba",CATEGORY
            MOVE      BKRXDROR,CODE
            CALL      GETCOD00
          ENDIF
          WRITE     HTMLFILE;"#",":
