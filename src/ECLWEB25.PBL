          BRANCH    OVRCD,GETATD99
.
          MATCH     DURNO,URNUMBER
          GOTO      GETATD99 IF NOT EQUAL
.
          MOVE      DDATE,FIRSTDAT
          UNPACK    DTIME,DIM5
          REP       ": ",DIM5
          SQUEEZE   DIM5
          MOVE      DIM5,FIRSTIME
.
          MOVE      ADATE,LASTDATE
          MOVE      ATIME,DIM5
          REP       ": ",DIM5
          SQUEEZE   DIM5
          MOVE      DIM5,LASTTIME
.
          CALL      TIMEDIFF
          MOVE      CALCTIME,MINSDIFF
          LJUSTIFY  MINSDIFF
          STRIP     MINSDIFF
          MOVELPTR  MINSDIFF,FORM6
          SFORMAT   VAR,FORM6
          MOVE      MINSDIFF,VAR
          WRITE     HTMLFILE;VAR;
