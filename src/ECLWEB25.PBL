.
          MATCH     "1",MULTLT01
          IF        @EQUAL
            MOVE      "2",DIM1C
            PACK      REMITKEY,WBEIHOSP,WBEIFTID,WBEIRADV,WBEIPNUM,SP70
            CALL      PRNRMM00
          ENDIF
.
          MATCH     "1",WBEISTAT
          IF        @EQUAL
            MOVE      "Payment record partially released.",WEBTITLE
          ELSE
            MOVE      "Payment record released.",WEBTITLE
          ENDIF
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      RPAM9999
.
