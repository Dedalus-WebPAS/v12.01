          MOVE      KEY3,KEYSTARR[1]        * key for weberhaf
          MOVE      KEY24,KEYSTARR[2]
          MOVE      KEY30,KEYSTARR[3]
          MOVE      KEY4,KEYSTARR[4]
          MOVE      USERID,KEYSTARR[5]
          MOVE      DIM1C,KEYSTARR[6]
          MOVE      SIX,KEYSTCNT            * Number of Keyins
