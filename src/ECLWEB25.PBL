            MOVE      SP70,FILTHOSP         * Not multi hospital so blank
            CALL      ERTI0000
            GOTO      ERTV9999
          ENDIF
.
.   Multi Hospital
.
          MATCH     SP70,FILTHOSP           * User has selected a hospital
          IF        !@EQUAL
            CALL      ERTI0000
            GOTO      ERTV9999
          ENDIF
.
          PACK      KEY3,SP70               * Show all users available
          CALL      RSPTHSP1                * hospitals
