          CALL      RSPRDT4
          CALL      RKPRDT4
          IF        OVRCD=0
            MATCH     DIM8,PRDTINVN
            IF        @EQUAL
              PACK      KEY27,PRDTUNIQ,PRDTPRAC,PRDTDOCT,PRDTCODE,Sp70
              CALL      RDPRHR1
              IF        OVRCD=0
                MATCH     SP70,PRHRPRAC
                IF        !@EQUAL
                  PACK      KEY6,PRHRPRAC,SP70
