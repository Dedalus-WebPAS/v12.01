            APPEND    WBEJARID,SERDIM50
            APPEND    " - priinvof record not found",SERDIM50
            RESET     SERDIM50
            GOTO      RPAM6000
          ENDIF

.
          MATCH     "1",PRINCNST
.         GOTO      RPAM6000 IF EQUAL
          IF        @EQUAL
