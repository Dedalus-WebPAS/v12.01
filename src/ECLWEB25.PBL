          MOVE      KEY3,KEYSTARR[1]        * key for webdvwaf
          MOVE      KEY8,KEYSTARR[2]
          MOVE      KEY4,KEYSTARR[3]
          MOVE      KEY8A,KEYSTARR[4]
          MOVE      KEY24,KEYSTARR[5]
          MOVE      USERID,KEYSTARR[6]
          MOVE      DIM1C,KEYSTARR[7]
