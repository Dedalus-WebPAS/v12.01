            PACK      KEY5,ANSB,ANST,WEFRBT,SP70
            CALL      RDCODE1
            IF        OVRCD=0.
              MATCH     "S",THCSCOD             * EOU Unit
              IF        @EQUAL
                MATCH     ANSO,TCINDC8
                IF        @EQUAL
                  MOVE      WWARD,WARDCODE      * Set wardcode cgi
                ENDIF
              ENDIF
            ENDIF
          ENDIF
.
.                       write the select option.
