..
.              PACK      KEY3,WBEIHOSP,SP70
.              CALL      RDPTHSP1
.              IF        OVRCD=0
.                MATCH     SAVELOCN,PTHSLOCN
.                IF        !@EQUAL
.                  SUB       ONE,ERDCOUNT
.                  GOTO      RPAM1000
.                ENDIF
.              ELSE
.                  SUB       ONE,ERDCOUNT
.                  GOTO      RPAM1000
.              ENDIF
.            ENDIF
.          ENDIF
