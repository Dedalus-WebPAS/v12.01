.*****************************************************************************
.*                            GBEN0000                                       *
.*              Get the benefit amount total for the ERA                     *
.* Requires:  Valid read on pmserhaf record (ERA Header)                     *
.* Returns:   BENETOTL - total benefit amount for the ERA batch              *
.*****************************************************************************
.
GBEN0000  MOVE      ZERO,BENETOTL                * initialise total amount
.
.         Loop through weberdaf looking for payments from the selected ERAW
.
          PACK      KEY82,WBEIFTID,WBEIRADV,WBEIPNUM,SP30,SP30,SP1
          CALL      RSWBERD1                     * position in file
GBEN1000  CALL      RKWBERD1                     * read next record
          BRANCH    OVRCD,GBEN9999               * end of file
.
          MATCH     WBEIFTID,WBEJFTID            * same participant still ?
          GOTO      GBEN9999 IF NOT EQUAL        * no - finished ERA
.
          MATCH     WBEIRADV,WBEJRADV            * same remittance advice still?
          GOTO      GBEN9999 IF NOT EQUAL        * no - finished ERA
.
          MATCH     WBEIPNUM,WBEJPNUM            * same part number still ?
          GOTO      GBEN9999 IF NOT EQUAL        * no - finished ERA
.
          UNPACK    SP70,DIM7,DIM2,DIM10
.
          RJUSTIFY  WBEJBAMT
          UNPACK    WBEJBAMT,DIM7,DIM2           * set amount paid
          PACK      DIM10,DIM7,DOT,DIM2
          MOVE      ZERO,FORM7P2D
.
          SCAN      "-",DIM10
          IF        @EQUAL
            REP        "-0",DIM10
            MOVE       ZERO,FORM7P2D
            MOVE       DIM10,FORM7P2D
            ASSIGN     (FORM7P2D*SEQ),FORM7P2D
          ELSE
            MOVE       DIM10,FORM7P2D
          ENDIF
.
          ADD       FORM7P2D,BENETOTL            * update ERA benefit total
          GOTO      GBEN1000                     * get next payment
.
GBEN9999  RETURN
+
.---------------------------------------------------------------------------
.         WRRDTU00 - Write to rcpdteaf - Deposit Amt < Total Benefit Amt
.---------------------------------------------------------------------------
.
WRRDTU00  CALL      IBACLOCK
          PACK      RCDTTDAT,CCC,CYY,CMM,CDD
          REP       " 0",RCDTTDAT
          MOVE      HRECPT,RCDTRECN
          MOVE      SP70,RCDTINVN
          MOVE      SP70,RCDTVIST
          MOVE      SP70,RCDTURNO
.
          MOVE      SP70,RCDTNAME
          MOVE      SP70,RCDTADD1
          MOVE      SP70,RCDTADD2
          MOVE      SP70,RCDTADD3
          MOVE      SP70,RCDTADD4
          MOVE      SP70,RCDTPOST
.
          MOVE      SP70,RCDTSFLG
          MOVE      WBEIPREF,RCDTREFR
          MOVE      WBEIHOSP,RCDTMHOS
.
          MOVE      RCCNERRE,RCDTREGI
          IF        IBCNMHOS=1
.
            MATCH     SP70,RCDTMHOS
            IF        !@EQUAL & !@EOS
              PACK      KEY3,RCDTMHOS,SP70
              CALL      RDPTHSP1
              IF        OVRCD=0
                MATCH     SP70,PTHSERRE
                IF        !@EQUAL & !@EOS
                  MOVE      PTHSERRE,RCDTREGI
                ENDIF
              ENDIF
            ENDIF
.
          ENDIF
.
          MOVE      SP70,RCDTGSTC
          CALL      GTACCT00
.
          RJUSTIFY  WBEIDAMT
          UNPACK    WBEIDAMT,DIM7,DIM2
          PACK      DIM10,DIM7,FULLSTOP,DIM2
          MOVE      ZERO,FORM7P2C
.
          SCAN      "-",DIM10
          IF        @EQUAL
            REP        "-0",DIM10
            MOVE       DIM10,FORM7P2C
            ASSIGN     (FORM7P2C*SEQ),FORM7P2C
          ELSE
            MOVE      DIM10,FORM7P2C
          ENDIF
          MOVE      FORM7P2C,RCDTAMNT
.
          MOVE      ZERO,RCDTDISC
          MOVE      CHOSPNUM,RCDTMDHS
          MOVE      SP70,RCDTGSTA
          MOVE      SP70,RCDTALLO
          CALL      IBACLOCK
.         PACK      RCDTRELD,CCC,CYY,CMM,CDD
.         REP       " 0",RCDTRELD
.         MOVE      CTIMEIS,RCDTRELT
          MOVE      SP70,RCDTRELD
          MOVE      SP70,RCDTRELT
          PACK      RCDTCDAT,CCC,CYY,CMM,CDD
          REP       " 0",RCDTCDAT
          MOVE      SP70,RCDTPRAC
          MOVE      CTIMEIS,RCDTCTIM
          MOVE      WBSEUID,RCDTCUID
          MOVE      SP70,RCDTUDAT
          MOVE      SP70,RCDTUTIM
          MOVE      SP70,RCDTUUID
          MOVE      SP70,RCDTDPTY
          MOVE      SP70,RCDTGLEX
          MOVE      SP70,RCDTBATN
          MOVE      SP70,RCDTSPAR
.
          PACK      KEY17,RCDTRECN,RCDTTCNT,SP70
          CALL      WRRCDTE1
.
WRRDTU99  RETURN
