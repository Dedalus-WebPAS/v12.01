                             "#"",PTBDLOSD,"#",":  *9
                             "#"",PTBDOPPE,"#",":  *10
                             "#"",PTBDSTAT,"#",":  *11
                             "#"",PTBDOVER,"#",":  *12
                             "#"",PTBDBRQN,"#",":  *13
                             "#"",ADIAG1,"#",":  *14
                             "#"",DOCFNAME,"#",":  *15
                             "#"",ADATE,"#",":     *16
                             "#"",DDATE,"#",":     *17
                             "#"",PMWOEDAT,PMWOEDTM,"#",":   *18
                             "#"",SPECDESC,"#",":  *19
                             "#"",FUNDDESC,"#",":  *20
                             "#"",RGMNAM01,"#",":  *21
                             "#"",RGMNAM02,"#",":  *22
                             "#"",RGMNAM03,"#",":  *23
                             "#"",RGMSTA01,"#",":  *24
