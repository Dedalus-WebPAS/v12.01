            GOTO      ERTD1000 IF EQUAL
.
            IF        EXDTBLNK=0
              MATCH     FRSTDATE,WBBHPDAT
              GOTO      ERTD1000 IF LESS
.
              MATCH     WBBHPDAT,LASTDATE
              GOTO      ERTD1000 IF LESS
            ENDIF
.
          ENDIF
.
          MOVE      SP70,DIM11
          MATCH     SP70,WBBHPDAT
