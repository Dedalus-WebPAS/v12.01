            IF        @EQUAL
              MOVE      FORM7P2C,WBIMAMMP
              ASSIGN    (PRINMAMT-WBIMAMMP),PRINMAMT
            ELSE
              MOVE      FORM7P2C,WBIMAMFP
              ASSIGN    (PRINHAMT-WBIMAMFP),PRINHAMT
            ENDIF
          ENDIF
.
          CALL      IBACLOCK                     * get current date
          PACK      CURRDATE,CCC,CYY,CMM,CDD
          REP       " 0",CURRDATE
          MOVE      CTIMEIS,CURRTIME
.
