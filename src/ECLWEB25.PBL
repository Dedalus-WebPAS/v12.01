          BRANCH    OVRCD,RGMCOD99
          MATCH     ADMISSNO,PTRGADMN
          GOTO      RGMCOD99 IF NOT EQUAL
.
          PACK      KEY13,PTRGREGM,SP1,SP1,ZERO,SP70
          CALL      RDPMREG1 
