          MATCH     "C",MODE
          GOTO      SFIELD IF EQUAL
          GOTO      KDOCTYP
.
INVCOD1   DISPLAY   *P1:24,*EL,*B,*V2LON:
                    "** To Code must be > or = From Code **    ";
          CALL      HITENTER
          GOTO      KTOPR
.
INVCODX   DISPLAY   *P1:24,*EL,*B,*V2LON,"** Code does not exist **    ";
          CALL      HITENTER
          GOTO      KTOPR
.
