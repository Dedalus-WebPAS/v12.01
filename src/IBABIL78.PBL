          RESET     PRIMFLG
          GOTO      NXT01 IF NOT EOS
          MOVE      "N",PRIMFLG
          DISPLAY   *P36:12,*V2LON,PRIMFLG;
          GOTO      NXT03
.
NXT01     MATCH     "Y",PRIMFLG
