          INC        COMMON/INC
.
KEY2      DIM        2
TASKID    DIM        8
VERSION   INIT      "V12.01.00"
.
          EXECUTE    "sh",TASKID
          CLOCK      FORK,KEY2
          MATCH      " 0",KEY2
          IF         @EQUAL
            CHAIN       PGM
          ELSE
            STOP
          ENDIF
          
