          EXECUTE   CMDLIN1,TASKID               * create temporary index file
.
          OPEN      FACTEMP1,TEMPFILE         * open temp index file
.
          COMPARE   TWO,COPTION
          IF        @EQUAL
             OPEN      FACTEMP2,TEMPFILE
          ELSE
             COMPARE   THREE,COPTION
             IF        @EQUAL
                OPEN      FACTEMP3,TEMPFILE
             ENDIF
          ENDIF
