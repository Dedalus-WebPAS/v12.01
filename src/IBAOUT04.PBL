            MOVE      "GEN.21-Private Practice invoice ",ERRMSG
            ENDSET    ERRMSG
            SQUEEZE   KEY8
            APPEND    KEY8,ERRMSG
            APPEND    " not on file",ERRMSG
            RESET     ERRMSG
            MOVE      SP9,DIM9
            CALL      PERR0000
            GOTO      VALD0500
          ENDIF
