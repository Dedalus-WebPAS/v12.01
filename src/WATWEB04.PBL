          MOVE      SP70,KEY4
          IF        WTCNCSID = 1
            PACK      KEY9,WMCODE,SP8
            CALL      RDPROA1
            IF        OVRCD<>1
              MOVE      ZERO,FORM4
              MOVE      WPRHAT,FORM4
              MOVE      FORM4,KEY4           * CPAC scoring ID
              REP       " 0",KEY4
            ENDIF
          ELSE
            IF        WTCNCSID = 2
              PACK      KEY5,ANSA,SP1,WMPCAT,SP8
              CALL      RDCODE1
              IF        OVRCD<>1
                MOVE      TCFORM6,FORM4
                MOVE      FORM4,KEY4
                REP       " 0",KEY4
              ENDIF       
            ELSE
              IF        WTCNCSID = 3
                PACK      KEY5,ANSW,ANSU,WMUNIT,SP8 
                CALL      RDCODE1
                IF        OVRCD<>1
                  MOVE      TCFORM6,FORM4
                  MOVE      FORM4,KEY4
                  REP       " 0",KEY4
                ENDIF
              ENDIF
            ENDIF
          ENDIF
          WRITE     HTMLFILE;KEY4;
          GOTO      MISC9999
