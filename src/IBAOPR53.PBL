          COMPARE   TWO,OPCNR53A 
          IF        @EQUAL
            PACK      KEY18,OPPERDC2,OPDATYPE * type & pers. type are the key
          ELSE
            PACK      KEY18,OPPERDC1,OPDATYPE
          ENDIF
          COMPARE   TWO,OPCNR53A            * Report Layout 2 ?
          GOTO      PDET0500 IF EQUAL       * yes
          MOVE      TDESC,OPTYPDC1          * no
          GOTO      PDET1000
