                      *75,"|",*84,"| ",OPDAADMN:
                      *95,"| Case ",OPDACASE,"    Team ",OPSGTMNO:
                      *120,"|",*126,"|",*132,"|";
          ELSE
            PRINT     *1,"|",DIM10:                * line 1
                      *12,"| ",DESC1:
                      *45,"| ",OPDCDURA:
                      *52,"| ",OPTYPDC1:
                      *75,"| ",DIM6:
                      *84,"| ",AURNO:
                      *95,"| ",PATNAME1:
                      *120,"|  ",PSEX:
                      *126,"| ",PSAGEYRS,*132,"|":
                      *N,"|  ",OPDACLIN:            * line 2
                      *12,"| ",DESC2:
                      *45,"| Op",OPDCNOOP:
                      *52,"| ",OPPERDC1:
                      *75,"|",*84,"| ",OPDAADMN:
                      *95,"| Case ",OPDACASE,"    Team ",OPDCTEAM:
                      *120,"|",*126,"|",*132,"|"; 
