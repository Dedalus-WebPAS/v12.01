.*                  HEAD0000                                              *
.*            print the heading                                           *
.**************************************************************************
HEAD0000  IF        CLNO=0
            CALL      FRSTH000
          ENDIF
          PRINT     *N;
          CALL      UND132
          UNPACK    XOPRDATE,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,FORM2           * move month to a form field
          LOAD      DSPMONTH,FORM2,JAN,FEB,MAR,APRIL,MAY,JUNE:
                                   JULY,AUG,SEPT,OCT,NOV,DEC
.
          PRINT     *N,*1,"Date : ",CDAY,SP1,DSPMONTH,SP1,CCENT,CYEAR;
.
.
