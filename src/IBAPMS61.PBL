                    *37,"|",*42,"|",*51,"|":
                    *100,"|Adm ",*106,"|Op Start":
                    *115,"|UR",*124,"|Ward",*132,"|";
          PRINT     *N,*1,"|Room",*8,"|Patient Name",*28,"|Adm Time":
                    *37,"|Age",*42,"|Adm no",*51,"|Procedure":
                    *100,"|Stat",*106,"|Time":
                    *115,"|Number",*124,"|Bed ",*132,"|";
.
