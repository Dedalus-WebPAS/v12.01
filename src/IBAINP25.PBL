.*****************************************************************************
.*                              KADM0000                                     *
.*                     Keyin the Admission Number                            *
.*****************************************************************************
KADM0000  MOVE      ZERO,SPECCODE
          MOVE      ZERO,EXIT
.
          KEYIN     *P1:4,*EF,"Admission  : ",*EL,*V2LON,AADMNO;
          MATCH     ZEROVISN,AADMNO
          GOTO      KADM9000 IF EQUAL
.
          MOVE      AADMNO,KEY8
          CALL      RDMISS1
          BRANCH    OVRCD OF KADM8000
          CALL      RDVISA1
          BRANCH    OVRCD OF KADM8000
.
          PACK      DIM8,ADATE
          REP       " 0",DIM8
          MATCH     DIM8,TODAY
          IF        @EQUAL
            DISPLAY   *P1:24,*EL,*B,"Patient was admitted today. ";
            CALL      HITENTER
            GOTO      KADM0000
          ENDIF
.
          MOVE      SP8,DDATE
          MOVE      AADMNO,KEY8
          CALL      RDDSCH1
          MOVE      OVRCD,DISFLG
.
          MOVE      AURNO,KEY8
          CALL      RDMAST1
          IF        OVRCD = 1
            DISPLAY   *P1:24,*B,*EL,"Patient PMI details missing. ";
            CALL      HITENTER
            GOTO      KADM0000
          ENDIF
.
          MOVE      PURNO,KEY8
          CALL      RDPMPX21
          IF        OVRCD = 1
            DISPLAY   *P1:24,*B,*EL,"Patient PMI details missing. ";
            CALL      HITENTER
            GOTO      KADM0000
          ENDIF
.
. If Using the Invoice Pending File
.
          IF        PTCNIPEN = 0
            OPEN      PATIPEN1,"patipenf"
            MOVE      AADMNO,KEY8
            CALL      RDIPEN1
            IF        OVRCD = 1
           DISPLAY   *P1:24,*B,*EL,"Patient does not have an Invoice pending. ";
              CALL      HITENTER
              GOTO      KADM0000
            ENDIF
          ENDIF
.
          CALL      PATHEAD
          GOTO      KADM9999
.
KADM8000  DISPLAY   *P1:24,*EL,*B:
                    "Admission Number does not exist. ";
          CALL      HITENTER
          GOTO      KADM0000
.
KADM9000  MOVE      ONE,EXIT
.
KADM9999  RETURN
.
