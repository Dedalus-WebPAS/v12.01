            MOVE      SP70,SIIADES
            MOVE      SIPCSUT,SIIAISS
            MOVE      ZERO,SIIAAVC
            MOVE      ONE,SIIANON
          ELSE
            MOVE      SIPCCAT,KEY7
            CALL      RDSIIA1
............MOVE      SIIASUB,SIPCPRD        * SRF127291
          ENDIF
.
          MOVE      SP70,SICADESC
          MOVE      SIPCCST,KEY5
          CALL      RDSICA1
.
          MATCH     SP70,SIPCWAR
          IF        @EQUAL
            MOVE      "N/A",SIPCWAR
            MOVE      SP70,SIWADES
          ELSE
            MOVE      SIPCWAR,KEY5
            CALL      RDSIWA1
          ENDIF
.
          MOVE      SIPCPRD,KEY5
          CALL      RDSISA1
.
REDITB99  RETURN
.
SELITB00  MOVE      SIPF1MKY,KEY20
          CALL      RDSIPF1
          IF        OVRCD=1
