          IF        @EQUAL
            MOVE      ONE,CSEARCH
          ELSE
            MOVE      ZERO,CSEARCH
          ENDIF
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HSIPCZ08
.
.
HSIPCZ60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HSIPCZ70 IF EQUAL
          IF        POCTYPE=1
            MATCH     SCHFLD01,SIPCUR1
            GOTO      HSIPCZ72 IF NOT EQUAL
          ELSE
            MATCH     SCHFLD01,SIPCUR2
            GOTO      HSIPCZ72 IF NOT EQUAL
          ENDIF
HSIPCZ70
          GOTO      HSIPCZ75
HSIPCZ72  MOVE      ONE,OVRCD
HSIPCZ75  RETURN
HSIPCZ85
