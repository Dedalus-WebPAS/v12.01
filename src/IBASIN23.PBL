.----------------------------------------------------------------------
. Special Positional Read
.----------------------------------------------------------------------
RSPOC000  CALL      RSSIPCB1
          CALL      RSSIPCB2                 * Position both at start
.
RSPOC999  RETURN
.----------------------------------------------------------------------
. Special Read Forward
.----------------------------------------------------------------------
RKPOC000  CALL      RKSIPCB1
          IF        OVRCD=0
            PACK      KEY25SAV,SIPCUR1,SIPCPON,SIPCITM,SP70
            CALL      RKSIPCB2
            IF        OVRCD=0
              PACK      KEY25,SIPCUR2,SIPCPON,SIPCITM,SP70
              MATCH     KEY25SAV,KEY25
              IF        @LESS
                MOVE      TWO,POCTYPE
                CALL      RPSIPCB1
                CALL      RDSIPCB2           * Read 1st Back
              ELSE
                MOVE      ONE,POCTYPE
                CALL      RPSIPCB2
                MOVE      KEY25SAV,KEY25
                CALL      RDSIPCB1           * Read 1st Back
              ENDIF
            ELSE
              MOVE      ONE,POCTYPE
              CALL      RPSIPCB2
              MOVE      KEY25SAV,KEY25
              CALL      RDSIPCB1           * Read 1st Back
            ENDIF
          ELSE
            CALL      RPSIPCB1
            CALL      RKSIPCB2
            BRANCH    OVRCD,RKPOC900
            MOVE      TWO,POCTYPE
            PACK      KEY25,SIPCUR2,SIPCPON,SIPCITM,SP70
          ENDIF
          MOVE      ZERO,OVRCD
          GOTO      RKPOC999

RKPOC900  MOVE      ONE,OVRCD
.
RKPOC999  RETURN
.----------------------------------------------------------------------
. Special Read Backward
.----------------------------------------------------------------------
RPPOC000  CALL      RPSIPCB1
          MATCH     SIPCUR1,SP70
          IF        @EQUAL & OVRCD=0
            MOVE      ONE,OVRCD
          ENDIF
          IF        OVRCD=0
            PACK      KEY25SAV,SIPCUR1,SIPCPON,SIPCITM,SP70
            CALL      RPSIPCB2
            IF        OVRCD=0
              PACK      KEY25,SIPCUR2,SIPCPON,SIPCITM,SP70
              MATCH     KEY25,KEY25SAV
              IF        @LESS
                MOVE      TWO,POCTYPE
                CALL      RKSIPCB1
                CALL      RDSIPCB2           * Read 1st Back
              ELSE
                CALL      RKSIPCB2
                MOVE      KEY25SAV,KEY25
                CALL      RDSIPCB1           * Read 1st Back
                MOVE      ONE,POCTYPE
              ENDIF
            ELSE
              CALL      RKSIPCB2
              MOVE      KEY25SAV,KEY25
              CALL      RDSIPCB1           * Read 1st Back
              CALL      RDSIPCB1           * Read 1st Back
            ENDIF
          ELSE
            CALL      RPSIPCB2
            BRANCH    OVRCD,RPPOC900
            MATCH     SIPCUR2,SP70
            IF        @EQUAL & OVRCD=0
              GOTO      RPPOC900
            ENDIF
            MOVE      TWO,POCTYPE
            PACK      KEY25,SIPCUR2,SIPCPON,SIPCITM,SP70
            CALL      RKSIPCB1
          ENDIF
          MOVE      ZERO,OVRCD
          GOTO      RPPOC999

RPPOC900  MOVE      ONE,OVRCD
.
RPPOC999  RETURN
.----------------------------------------------------------------------
. Special Read Exact
.----------------------------------------------------------------------
RDPOC000  IF        POCTYPE=1
            CALL      RDSIPCB2
            CALL      RDSIPCB1
          ELSE
            CALL      RDSIPCB1
            CALL      RDSIPCB2
          ENDIF
.
RDPOC999  RETURN
.----------------------------------------------------------------------
. check warehouse security
. requires : SIPAPON
. returns  : EXIT = 0 - access granted,
.                   1 - access denied
.----------------------------------------------------------------------
CWSA0000  MOVE      ZERO,EXIT          * access granted
          PACK      KEY10,SIPAPON,SP70
          CALL      RSSIPC1
CWSA0100  CALL      RKSIPC1
          BRANCH    OVRCD,CWSA9999
.
          MATCH     SIPAPON,SIPCPON
          GOTO      CWSA9999 IF NOT EQUAL
.
          MATCH     SP5,SIPCWAR
          GOTO      CWSA0100 IF EQUAL
.
          PACK      KEY9,SIPCWAR,PASSCODE,SP70
          CALL      RDSIWS2
          BRANCH    OVRCD,CWSA9000
.
          GOTO      CWSA0100
.
CWSA9000  MOVE      ONE,EXIT           * access denied
          CLEAR     DISPMSG
          APPEND    "P/O ",DISPMSG
          APPEND    SIPAPON,DISPMSG
          APPEND    SLASH,DISPMSG
          APPEND    SIPCITM,DISPMSG
          APPEND    ", from W/H ",DISPMSG
          APPEND    SIPCWAR,DISPMSG
          APPEND    ". No Access - ",DISPMSG
          CALL      MESSAGE1
          GOTO      CWSA9999
.
CWSA9999  RETURN
.
.----------------------------------------------------------------------
. Get Next Order
.----------------------------------------------------------------------
NXTORD00  MOVE      SIPAPON,ORDERNO
          PACK      KEY25,REQCODE,ORDERNO,Z70
          CALL      RSSIPCB1
          CALL      RKSIPCB1
          MATCH     REQCODE,SIPCUR1
          IF        !@EQUAL | OVRCD=1
            CALL      RSSIPCB2
            CALL      RKSIPCB2
            MATCH     REQCODE,SIPCUR2
            IF        !@EQUAL | OVRCD=1
              MOVE      "No More Orders for this Reference - ",DISPMSG
              CALL      MESSAGE1
              MOVE      ORDERNO,SIPCPON
              MOVE      REQITEM,SIPCITM
            ENDIF
          ENDIF
.
          MOVE      SIPCITM,REQITEM
          PACK      KEY7,SIPCPON,SP70
          CALL      RDSIPA1
          BRANCH    OVRCD,NXTORD00
.
          CALL      GETORD00       * Get Order Info
.
          RETURN
.
CHKPRICE  IF        (SIPCECT<0)
            MOVE      "        T.B.A.",UNITCOST
          ELSE
            MOVE      "9,999,999.9999",UNITCOST
            FEDIT     SIPCECT,UNITCOST
            MATCH     "*",UNITCOST
            IF        @EQUAL
              MOVE      SIPCECT,F9P4
              MOVE      F9P4,UNITCOST
            ENDIF
          ENDIF
.
          MOVE     SIPCCST,KEY5
          MOVE     SP70,SICADESC
          CALL     RDSICA1
          IF       OVRCD=1
            MOVE     SP70,SIPCCST
            MOVE     SP70,SICADESC
          ENDIF
.
          RETURN
.
.--------------------------------------------------
. Total Cost of Order
.--------------------------------------------------
TOTORD00  MOVE     ZERO,TOTALORD
          PACK     KEY11,SIPCPON,SIPCITM,SP70    * save current record
          PACK     KEY10,SIPAPON,SP70
          CALL     RSSIPC1
TOTORD10  CALL     RKSIPC1
          BRANCH   OVRCD,TOTORD90
          MATCH    SIPCPON,SIPAPON
          GOTO     TOTORD90 IF NOT EQUAL
          IF       SIPCECT>0
            ASSIGN   TOTALORD+(SIPCECT*SIPCOQT),TOTALORD
          ENDIF
          GOTO     TOTORD10
TOTORD90
          PACK     KEY10,KEY11,SP70              * restore original record
          CALL     RDSIPC1
          RETURN
SELITM00  MOVE      SIPC1MKY,KEY20
          CALL      RDSIPC1
          IF        OVRCD=1
            MOVE      "Invalid Selection - ",DISPMSG
            BEEP
            CALL      MESSAGE1
          ELSE
            CALL      REDITM00
            CALL      SC01L000
          ENDIF
.
