.----------------------------------------------------------------------
. Keyin REQCODE
.----------------------------------------------------------------------
KSIPOC00  CALL      OPSPEC00
          BRANCH    EXIT,KSIPOC95
.
KSIPOC10  MOVE      ZERO,CKEYTYP
          MOVE      SP70,REQCODE
          MOVE      SP70,REQITEM
          KEYIN     *PCCOL:CVERT,*V2LON,REQCODE:
                    *PCCOL:CVERT,*V2LON,*DV,REQCODE
          PACK      REQCODE,REQCODE,SP70
          MATCH     SP70,REQCODE
          GOTO      KSIPOC95 IF EQUAL
.
          MATCH     EXITCHAR,REQCODE
          GOTO      KSIPOC95 IF EQUAL
.
          MATCH     QUEST,REQCODE
          IF        @EQUAL
            CALL      HSIPCZ00
            BRANCH    EXIT,KSIPOC10
          ELSE
            PACK      KEY25,REQCODE,SP70
            CALL      RSSIPCB1
            CALL      RKSIPCB1
            MATCH     REQCODE,SIPCUR1
            IF        !@EQUAL | OVRCD=1
              CALL      RSSIPCB2
              CALL      RKSIPCB2
              MATCH     REQCODE,SIPCUR2
              IF        !@EQUAL | OVRCD=1
                MOVE      "Requisition Code does not exist - ",DISPMSG
                CALL      MESSAGE1
                GOTO      KSIPOC00
              ENDIF
            ENDIF
          ENDIF
          MOVE      SIPCITM,REQITEM
          PACK      KEY7,SIPCPON,SP70
          CALL      RDSIPA1
          IF        OVRCD=1
            MOVE      "Purchase Order Header does not exist - ",DISPMSG
            CALL      MESSAGE1
            GOTO      KSIPOC00
          ENDIF
.
          CALL      CWSA0000       * check warehouse security
          BRANCH    EXIT,KSIPOC00
.
          CALL      GETORD00       * Get Order Info
.
