          GOTO      KSIPOC99
.
KSIPOC95  MOVE      TWO,EXIT
.
KSIPOC99  RETURN
.------------------------------------------------------------
. Get Order Details
.------------------------------------------------------------
GETORD00  MATCH     SP70,SIPADCN
          IF        !@EQUAL
            UNPACK    SIPADCN,CCENT,CYEAR,CMON,CDAY
            CALL      PACDATE
            CLEAR     DISPMSG
            APPEND    "WARNING: Order Cancelled on ",DISPMSG
            APPEND    CPCDATE,DISPMSG
            APPEND    " : ",DISPMSG
            RESET     DISPMSG
            BEEP
            CALL      MESSAGE1
          ENDIF
.
          UNPACK    SP70,SIPEYEAR,SIPEPER
          MOVE      SIPADIN,SIPEDATE
          CALL      RSIPE000                * get period
.
          MOVE      SP70,APMASN1
          PACK      KEY5,SIPASUP,SP70
          CALL      RDAPMA1
.
          MOVE      SP70,SIORNAME
          PACK      KEY3,SIPAORI,SP70
          CALL      RDSIOR1
.
          MOVE      SP70,SICDDESC
          MOVE      "ME ",KEY3
          PACK      KEY6,KEY3,SIPAMES,SP70
          MATCH     SP70,SIPAMES
          CALL      RDSICD1 IF NOT EQUAL
          MOVE      SICDDESC,MESSDESC
.
          MOVE      SP70,SICDDESC
          MOVE      "CF ",KEY3
          PACK      KEY6,KEY3,SIPACON,SP70
          MATCH     SP70,SIPACON
          CALL      RDSICD1 IF NOT EQUAL
          MOVE      SICDDESC,CONFDESC
.
          MOVE      SP70,SICDDESC
          MOVE      "pa1",KEY3
          PACK      KEY6,KEY3,SIPAUC1,SP70
          MATCH     SP70,SIPAUC1
          CALL      RDSICD1 IF NOT EQUAL
          MOVE      SICDDESC,CODEDES1
.
          MOVE      SP70,SICDDESC
          MOVE      "pa2",KEY3
          PACK      KEY6,KEY3,SIPAUC2,SP70
          MATCH     SP70,SIPAUC2
          CALL      RDSICD1 IF NOT EQUAL
          MOVE      SICDDESC,CODEDES2
.
          PREP      TEMPWORD,FILSIPB1          * I SRF 19076
          MOVE      SP70,VAR                   * Initialise the temp word
          WRITE     TEMPWORD,SEQ;VAR           * processor file to empty
          PACK      KEY10,SIPAPON,SP70
          MOVE      KEY10,SAVKEY80
          SETLPTR   SAVKEY80,7
          CALL      RDSIPB1
          IF        OVRCD=0
            CALL      RPSIPB1
          ENDIF
.
GETORD10
          CALL      RKSIPB1                    * Obtain purchase order
          BRANCH    OVRCD,GETORD20             * comments
          PACK      KEY10,SIPBPON,SIPBLIN      *
          MATCH     SAVKEY80,KEY10             * If not found then leave
          GOTO      GETORD20 IF NOT EQUAL      * temp word processor file
          PACK      VAR,SIPBCOM,SP70,SP70      * empty
          STRIP     VAR
          MOVELPTR  VAR,F3
          SFORMAT   VAR,F3
          MOVE      SIPBCOM,VAR
          IF        F3=0
            WRITE     TEMPWORD,SEQ;
          ELSE
            WRITE     TEMPWORD,SEQ;VAR
          ENDIF
          SFORMAT   VAR,127
          GOTO      GETORD10
GETORD20  CLOSE     TEMPWORD                   * END I SRF 19076
.
          CALL      TOTORD00
.
          MOVE      "**  UNKNOWN  **    ",TRANSSTA
          UNPACK    SP70,SITRPURC,SITRSTAT,SITRUSER,SITRCREA:
                         SITRORG,SITREDAT,SITRTDAT
          MOVE      "000",SITRTNUM
          PACK      KEY7,SIPAPON,SP70
          CALL      RDSITR1
          IF        OVRCD=1
            MOVE      "No Transmission    ",TRANSSTA
          ELSE
            MATCH     "0",SITRSTAT
            IF        @EQUAL
              MOVE      "Await. Transmit    ",TRANSSTA
            ENDIF
            MATCH     "1",SITRSTAT
            IF        @EQUAL
              MOVE      "In Progress        ",TRANSSTA
            ENDIF
            MATCH     "3",SITRSTAT
            IF        @EQUAL
              MOVE      "Ack. Received      ",TRANSSTA
            ENDIF
            MATCH     "9",SITRSTAT
            IF        @EQUAL
              MOVE      "Cancelled          ",TRANSSTA
            ENDIF
          ENDIF
.
          RETURN
.
