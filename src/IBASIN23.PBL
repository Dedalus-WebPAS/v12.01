            CALL     RPSIPF1 
            IF       OVRCD=1
              MOVE     SIPF1SNO,SECTOR
            ENDIF
            PACK     SIPF1MKY,SIPFPON,SIPFITM,SIPFREC                                                        
            MATCH    SIPF1EKY,SIPF1MKY
            IF       !@EQUAL
              MOVE     SIPF1SNO,SECTOR
