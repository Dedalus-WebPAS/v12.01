          GOTO      HSIPCZ20
HSIPCZ91
          MOVE      SP70,SIPCPON
          MOVE      SP70,SIPCPON
          MOVE      SP70,SIPCITM
          MOVE      SP70,SIPCCST
          MOVE      SP70,SIPCPRD
          MOVE      SP70,SIPCDAT
          MOVE      SP70,SIPCWAR
          MOVE      SP70,SIPCCAT
          MOVE      SP70,SIPCPN
          MOVE      SP70,SIPCPD
          MOVE      SP70,SIPCCON
          MOVE      SP70,SIPCCDT
          MOVE      SP70,SIPCSUT
          MOVE      SP70,SIPCEDD
          MOVE      SP70,SIPCFDD
          MOVE      ZERO,SIPCOQT
          MOVE      ZERO,SIPCIQT
          MOVE      ZERO,SIPCRQT
          MOVE      ZERO,SIPCECT
          MOVE      ZERO,SIPCTCR
          MOVE      ZERO,SIPCTIN
          MOVE      ZERO,SIPCOQO
          MOVE      ZERO,SIPCOOP
          MOVE      SP70,SIPCUR1
          MOVE      SP70,SIPCUR2
          MOVE      SP70,SIPCUD1
          MOVE      SP70,SIPCUD2
          MOVE      SP70,SIPCUY1
          MOVE      SP70,SIPCUY2
          MOVE      SP70,SIPCSPA
          MOVE      ONE,EXIT
          GOTO      HSIPCZ96
HSIPCZ92
          MOVE      SP70,KEY26
          MOVE      SP70,SIPAPON
          LOAD      KEY26,CCITEM,ST1,ST2,ST3,ST4,ST5:
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY26,KEY25,ANS
          MOVE      ANS,POCTYPE
          IF        POCTYPE=1
            UNPACK      KEY25,SIPCUR1,SIPCPON,SIPCITM
          ELSE
            UNPACK      KEY25,SIPCUR2,SIPCPON,SIPCITM
          ENDIF
          MOVE      KEY25,REQCODE
HSIPCZ96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HSIPCZ99
