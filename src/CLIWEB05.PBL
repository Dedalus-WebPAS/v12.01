          CALL      RDREPM1
          BRANCH    OVRCD,WRTLNK10
.
          PACK      KEY6,REPMHWD,SP70
          CALL      RDWARD1
          IF        OVRCD=0
            MOVE      "04",LINKTYPE           * Link Ward/Dept from Result Text
            PACK      LINKKEYS,WWARD,SP70     * as well as Patients Ward
            CALL      NEWLNK00
          ENDIF
          GOTO      WRTLNK20
.
