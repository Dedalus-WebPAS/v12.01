            CALL      SPCHR000                   * convert special characters
          ENDIF
          APPEND    CARET,ZSD001
          APPEND    FIELDSTR,ZSD001
          APPEND    CARET,ZSD001
          APPEND    "webPAS",ZSD001
          RESET     ZSD001
.
WRZSD200  MATCH     SP3,H7CIHOSP
          IF        @EQUAL
            MOVE      BLANKSTR,ZSD002
            GOTO      WRZSD300
          ENDIF
.
          CLEAR     PTHSNAME
          PACK      KEY3,H7CIHOSP
          CALL      RDPTHSP1
