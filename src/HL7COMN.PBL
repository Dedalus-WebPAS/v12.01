          MATCH     SP50,EMVICOM6                * admitting diagnosis 6 blank?
          IF        !@EQUAL
            ENDSET    DG1004                     * no - append diagnosis 6
            APPEND    SP1,DG1004
            MOVE      EMVICOM6,FIELDSTR
            CALL      SPCHR000                   * convert special characters
            APPEND    FIELDSTR,DG1004            * load diagnosis 6
            RESET     DG1004
            STRIP     DG1004
          ENDIF
