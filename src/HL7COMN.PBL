          IF        @EQUAL
            APPEND    BLANKSTR,PID011            * yes
          ELSE
            MOVE      PMCEADD2,FIELDSTR          * load address line 2
            CALL      SPCHR000                   * convert special characters
            APPEND    FIELDSTR,PID011
          ENDIF
