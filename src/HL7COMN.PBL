          APPEND    "webPAS",ZXP003
          RESET     ZXP003
.
.         Load Privacy Indicator
.
WRZXP100  MATCH     SP3,PMPXPRVI
          IF        @EQUAL
            MOVE      BLANKSTR,ZXP004
            GOTO      WRZXP200
          ENDIF
.
          MOVE      PMPXPRVI,FIELDSTR
          CALL      SPCHR000                     * convert special characters
          MOVE      FIELDSTR,ZXP004
          ENDSET    ZXP004
.
          MOVE      "Unknown Code",FIELDSTR
