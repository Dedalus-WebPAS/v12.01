          KEYIN      *P1:24,*EL,"Ok to Continue (":
                     *V2LON,*DV,ANSY,*HOFF,*DV,SLASH:
                     *V2LON,*DV,ANSN,*HOFF,*DV,SLASH:
                     *V2LON,*DV,ANSC,*HOFF,") ? ",*V2LON,ANS;
