SELLIN00  MOVE      SIOD2MKY,KEY20
          CALL      RDSIOD2
          IF        OVRCD=1
            BEEP
            MOVE      "Record Not on File - ",DISPMSG
            CALL      MESSAGE1
            GOTO      SELLIN99
          ENDIF
          MOVE      SIODCAT,KEY7
          CALL      RDSIIA1
          PACK      KEY30,SIIAISS,SIODSUT
          CALL      RDSIFA1
          MATCH     SP70,SIODCON
          IF        @EQUAL
            MOVE      SP70,SISCDES
            MOVE      SP70,SISCSDT
            MOVE      SP70,SISCEDT
          ELSE
            MOVE      SIODCON,KEY10
            CALL      RDSISC1
          ENDIF
.
          PACK      KEY27,SIODCAT,SIODSUP,SIODSUT
          CALL      RDSIIC1
.
          IF        SIODCST<0
            MOVE      SEQ,SIODCST
            MOVE      "        T.B.A.",UNITCOST
            MOVE      "        T.B.A.",IUNITCST
          ELSE
            MOVE      "9,999,999.9999",UNITCOST
            MOVE      "9,999,999.9999",IUNITCST
            ASSIGN    SIODCST/SIFACTOR,F12X4
            FEDIT     F12X4,IUNITCST
            FEDIT     SIODCST,UNITCOST
            MATCH     "*",IUNITCST
            IF        @EQUAL
              MOVE      F12X4,F9P4
              MOVE      F9P4,UNITCOST
            ENDIF
            MATCH     "*",UNITCOST
            IF        @EQUAL
              MOVE      SIODCST,F9P4
              MOVE      F9P4,UNITCOST
            ENDIF
          ENDIF
.
.         ASSIGN    SIODQTY/SIFACTOR,F12P2
.         MOVE      F12P2,QTYFORM8
.         MOVE      QTYFORM8,SUPPQTY
.
          ASSIGN    SIODQTY*SIFACTOR,ISSQTY
.
          CALL      CHKNOT00
          MATCH     SP70,SIODEDT
          IF        @EQUAL
            CALL      CALDD000
          ELSE
            MOVE      SIODEDT,EXPDDATE
          ENDIF
.
          CALL      SC01L000
.
