                   *P1:7,"Enter starting name  : ",*DV,*V2LON,STRTNAM:
                   *P24:7,*RV,STRTNAM,*HOFF:
                   *P1:9,"Enter ending name    : ",*DV,*V2LON,ENDNAM:
                   *P24:9,*RV,ENDNAM
.
         MATCH     "Start ",STRTNAM
         IF        @EQUAL
           MOVE      "A     ",STRTNAM
         ENDIF
