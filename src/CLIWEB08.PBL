          IF        @EQUAL
            OPEN      PMSEHBA1,"pmsehbaf"
          ENDIF
.
          MATCH     "3",PTCNUNET
          IF        @EQUAL
            OPEN      PMSNPLA1,"pmsnplaf"
            OPEN      PMSNDBA2,"pmsndbaf"
          ENDIF
.
