          PACK      KEY10,USERID,SP70
          CALL      RDWBSE1
          IF        OVRCD=1
            MOVE      SP70,WBSEDALL
          ENDIF
.
          MOVE      ZERO,EVNTFLAG
          COMPARE   "9",PVITYPE
          GOTO      CKAL9999 IF NOT EQUAL
          MATCH     " 1",PVISYST
          GOTO      CKAL1000 IF NOT EQUAL
