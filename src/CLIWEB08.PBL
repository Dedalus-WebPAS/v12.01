.
          PACK      KEY4,OBPCSLID
          CALL      RDOBPT1
          STRIP     OBPTSDIR
.
          MATCH     "00000000",OBPCCVIS
          IF        @EQUAL
            MOVE      "UR",KEY2
            PACK      CMDLINE,KEY2,OBPCURNO,DASH,OBPCCPID,OBPCFEXT
