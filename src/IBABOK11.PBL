START     HLINE     *G37,2,48,80
          DISPLAY   *P1:3,*EF:
                    *P1:4,*V2LON,ZERO,*HOFF," = Exit":
                    *P1:5,*V2LON,ONE,*HOFF," = ",OPTDES1:
                    *P1:6,*V2LON,TWO,*HOFF," = ",OPTDES2:
                    *P1:7,*V2LON,THREE,*HOFF," = ",OPTDES3:
                    *P1:8,*V2LON,FOUR,*HOFF," = ",OPTDES4;
.
          MOVE      TEN,CVERT
          COMPARE   ONE,HBWAIT
          GOTO      BEGIN IF NOT EQUAL
