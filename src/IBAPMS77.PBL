                    *50,"|":
                    *75,"|":
                    *100,"|":
                    *110,"|":
                    *132,"|" 
.                                                        * Line 3
          PRINT     *1,"|":                              * Column 1
                    *25,"|",*28,DSPLURNO:                *        2
                    *50,"|":
                    *75,"|":
                    *100,"|":
                    *110,"|":
