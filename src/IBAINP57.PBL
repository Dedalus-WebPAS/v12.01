          MATCH     ADATE,TDATE
          IF        @EQUAL
            MOVE      TDATE,EPSSDATE        * initial Episode start date
            MOVE      TATYPE,CARECLSS       * initial Episode care class
            GOTO      LRAT1000              * ignore adm record
          ELSE
            MOVE      TDATE,EPSEDATE        * Episode end date
          ENDIF
