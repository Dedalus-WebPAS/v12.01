          GOTO      SNPACN99 IF NOT EQUAL
.
          PRINT    *1,"|",AADMNO,*10,"| is a public patient with a care ":
                   "type of ",ACARE,", and has no HoNOS score",*80,"|"
          GOTO      SNPACN99
.
