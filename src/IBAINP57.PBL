          PACK      KEY8,AADMNO,SP70
          CALL      RDDSCH1                 * Read the discharge file
          IF        OVRCD<>1
            PACK      LASTDATE,DDATE
          ENDIF
.
          PACK      KEY30,AADMNO,SP70
          CALL      RDSTRAN2
LRAT1000  CALL      RDKTRAN2
          BRANCH    OVRCD,LRAT3000
