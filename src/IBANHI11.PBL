          IF        VCHKFLAG=1
            APPEND    "Validation Run ",DIM107
          ENDIF
          APPEND    "started on ",DIM107
          APPEND    CPCDATE,DIM107
          APPEND    " at ",DIM107
          APPEND    CTIMEIS,DIM107          
          APPEND    SP70,DIM107
          APPEND    SP70,DIM107
          RESET     DIM107
          WRITE     TEMP3,SEQ;DIM107
.
LOGS9999  RETURN
