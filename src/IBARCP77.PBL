.******************************************************************************
.*                                  RJUS0000                                  *
.*                         Right Justify a field                              *
.******************************************************************************
RJUS0000  MOVE      SP70,FLD60OUT
          SQUEEZE   FLD60IN
          MOVELPTR  FLD60IN,LENIN
          MOVE      LENGTH,LENOUT
          SUB       LENIN,LENOUT
          IF        LENOUT > 0
            SUB       ONE,LENOUT
            BUMP      FLD60OUT,LENOUT
            APPEND    FLD60IN,FLD60OUT
          ELSE
            MOVE      FLD60IN,FLD60OUT
          ENDIF
          RESET     FLD60OUT
.
RJUS9999  RETURN
