          APPEND    "ibarcp77.us2 ",CMDLINE
          APPEND    KEY120,CMDLINE
          APPEND    SP1,CMDLINE
          APPEND    KEY110,CMDLINE        * file name without pathname
          APPEND    SP70,CMDLINE
