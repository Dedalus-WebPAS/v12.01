.*     SCRT was copied from WAT10 patient procedure details enquire screen    *
.******************************************************************************
SCRT0000  DISPLAY   *P1:3,*EF:
                    *P1:4,"U/R No.   : ":
                    *P28:4,"Name : ":
                    *P28:5,"Sex  : ":
                    *P42:5,"Born : ":
                    *P60:5,"Age : ":
                    *P28:6,"Priv : ":
                    *P55:6,"Bus : ":
                    *P13:4,*V2LON,PURNO:
                    *P35:4,PATNAME:
                    *P35:5,DSEX:
                    *P49:5,PATDOB:
                    *P66:5,PSAGE:
                    *P35:6,PTELEP:
                    *P61:6,PTELEB;
.
          MOVE      "11111111",ALRTMASK
          CALL      PATALERT
.
          DISPLAY   *P1:8,*EF
          MOVE      "01",SCRID
          CALL      DISP0000                      * display background
          BRANCH    EXIT,SCRT9999
          CALL      DSPFIELD                      * display fields
.
          IF        STCNWLAD<>2
            GOTO      SCRT9999
          ENDIF
.
          CALL      SEEMORE
