.******************************************************************************
.*        Print Date given date in KEY16                                      *
.******************************************************************************
PDAT0000  UNPACK    KEY16,FDATE1,FDATE2
          MATCH     SP8,FDATE1
          GOTO      PDAT2222 IF NOT EQUAL
          PRINT     START," To ";
          GOTO      PDAT4444
PDAT2222
          UNPACK    FDATE1,CCENT,CYEAR,CMON,CDAY
          CALL      PACDATE
          PRINT     CPCDATE," To ";
PDAT4444
          MATCH     "999999",FDATE2
          GOTO      PDAT5555 IF NOT EQUAL
          PRINT     FINISH;
          GOTO      PDAT9999
PDAT5555
          MATCH     "zzzzzz",FDATE2
          GOTO      PDAT6666 IF NOT EQUAL
          PRINT     FINISH;
          GOTO      PDAT9999
PDAT6666
          UNPACK    FDATE2,CCENT,CYEAR,CMON,CDAY
          CALL      PACDATE
          PRINT     CPCDATE;
.
PDAT9999  RETURN
