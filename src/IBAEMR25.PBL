.****************************************************************************
.*        IO ROUTINES FOR TEMPORARY FILE                                    *
.****************************************************************************
RSTEMP1   READ      REPTEMP1,KEY14;;
          RETURN
.
RDTEMP1   MOVE      ZERO,OVRCD
          READ      REPTEMP1,KEY14;TEMPFUND,TEMPDATE,TEMPNATT:
                                   TEMPNADM,TEMPSPAR
          GOTO      OVERCOND IF OVER
          RETURN
.
RKTEMP1   MOVE      ZERO,OVRCD
          READKS    REPTEMP1;TEMPFUND,TEMPDATE,TEMPNATT:
                                   TEMPNADM,TEMPSPAR
          GOTO      OVERCOND IF OVER
          RETURN
.
WRTEMP1   WRITE     REPTEMP1,KEY14;TEMPFUND,TEMPDATE,TEMPNATT:
                                   TEMPNADM,TEMPSPAR
          RETURN
.
UPTEMP1   UPDATE    REPTEMP1;TEMPFUND,TEMPDATE,TEMPNATT:
                                   TEMPNADM,TEMPSPAR
          RETURN
.
