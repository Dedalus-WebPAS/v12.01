          MATCH     URNUMBER,REPHPID
          GOTO      URCOL900 IF NOT EQUAL
          ADD       ONE,RECNUMB
          IF        STARTNUM>=RECNUMB
            GOTO      URCOL100
          ENDIF
          MATCH     SP70,COLLDATE
          GOTO      URCOL300 IF EQUAL
          MATCH     REPHCDT,COLLDATE
          GOTO      URCOL200 IF NOT EQUAL
          MATCH     REPHCTM,COLLTIME
          GOTO      URCOL200 IF NOT EQUAL
