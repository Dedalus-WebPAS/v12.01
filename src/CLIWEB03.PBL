          GOTO      SETCOL99
SETCOL08  MOVE      "DarkGrey    ",COLORSTR
          GOTO      SETCOL99
SETCOL09  MOVE      "LightBlue   ",COLORSTR
          GOTO      SETCOL99
SETCOL10  MOVE      "LightGreen  ",COLORSTR
          GOTO      SETCOL99
