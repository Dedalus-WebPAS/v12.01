            WRITE      TEMPFILE,KEY45;LOCATION,REQDATE,REQTIME,MRRSKEY:
                                      RESVDATE,RECORDID,REQSTOR,REASON
            GOTO       WRTP9999
. 
WRTP2000    PACK       KEY45,REQSTOR,REQDATE,REQTIME,MRRSKEY,SP20
            WRITE      TEMPFILE,KEY45;LOCATION,REQDATE,REQTIME,MRRSKEY:
                                      RESVDATE,RECORDID,REQSTOR,REASON
            GOTO       WRTP9999
.
WRTP3000    PACK       KEY45,RECORDID,REQDATE,REQTIME,MRRSKEY,SP20
            WRITE      TEMPFILE,KEY45;LOCATION,REQDATE,REQTIME,MRRSKEY:
                                      RESVDATE,RECORDID,REQSTOR,REASON
. 
WRTP9999    RETURN
