PRNT0000  MOVE      "80",CLNO
          MOVE      ZERO,CPAGENO
          MOVE      ZERO,WTNUMDOC
          MOVE      ZERO,WTTOTNEW
          MOVE      ZERO,WTTOTREV
          MOVE      SP6,SAVCTYPE
.
