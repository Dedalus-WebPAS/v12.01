.
          RESET     OPSEM005
          MATCH     SP70,OPSEM005
          GOTO      CHKSEM87 IF EQUAL
.
          RESET     OPSEM006
          MATCH     SP70,OPSEM006
          GOTO      CHKSEM88 IF EQUAL
.
          GOTO      CHKSEM98
.
