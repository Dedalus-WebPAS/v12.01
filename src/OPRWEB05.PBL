          BRANCH    EXIT,WEBRSP95
.
          WRITE     HTMLFILE;"<script type=#"text/javascript#" ":
                             " src=#"../js/Standard.js#"></script>":
                             "<script type=#"text/javascript#" ":
                             " src=#"../js/Custom.js#"></script>":
                             "<script type=#"text/javascript#"> {":
                             " if (self.name==#"PopUpFrame#") {":
                             "   alert(#"",WEBTITLE,"#"); ":
                             "   reloadParentFrame();}":
                             " else { ":
                             "   window.history.go(-1);":
