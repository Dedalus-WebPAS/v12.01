                    *V2LON,*DV,ANSY,*HOFF,*DV,SLASH,*V2LON,*DV,ANSN,*HOFF:
                    ")    :":
                    *P56:6,*V2LON,ANS
.
          PACK      ANS,ANS,SP1
          REP       "nNyY",ANS
.
          MATCH     ANSY,ANS
          IF        @EQUAL
            MOVE      ONE,PENSFLAG
            DISPLAY   *P56:6,*V2LON,DYES
            GOTO      KPEN1000
          ENDIF
.
          MATCH     ANSN,ANS
          IF        @EQUAL
            MOVE      ZERO,PENSFLAG
            DISPLAY   *P56:6,*V2LON,DNO
            GOTO      KPEN1000
          ENDIF
.
          GOTO      KPEN0000
.
