.
          MATCH     "4",TCINDC1                  * pension card code ?
          IF        !@EQUAL
            DISPLAY   *P1:24,*EL,*B,"Concession card type is not for a ":
                      "pension card.  ";
            CALL      HITENTER
            DISPLAY   *P1:24,*EL
            GOTO      KPEN0000
          ENDIF
.
          DISPLAY   *P61:7,*EL,TDESC             * display description
          MOVE      ACODE,PENSCODE
          MOVE      ZERO,EXIT
          GOTO      KPEN9999
