          IF        ADDRFLG4 = 0 & ADDRVFLG = 0
            MOVE      PTMXECPC,PCODEVAR
            MOVE      PTMXECA3,SUBRBVAR
            MOVE      PTMXECA4,STATEVAR
            MOVE      FOUR,ADDRTYPE
            CALL      VADD0000              * valid address ?
            IF        EXIT = 1
              MOVE      ONE,ERORFLAG        * no
