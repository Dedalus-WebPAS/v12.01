          IF        ADDRFLG2 = 0 & ADDRVFLG = 0
            MOVE      PTMXPOST,PCODEVAR
            MOVE      PTMXADD3,SUBRBVAR
            MOVE      PTMXADD4,STATEVAR
            MOVE      TWO,ADDRTYPE
            CALL      VADD0000              * valid address ?
            IF        EXIT = 1
              MOVE      ONE,ERORFLAG        * no
