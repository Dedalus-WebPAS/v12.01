.******************************************************************************
.*                                  GFND0000                                  *
.*                        Generate Fees of a health fund                      *
.******************************************************************************
GFND0000  MOVE      ZERO,LINECNTR
.
          OPEN      UBFEETXT,FNAMEI
.
GFND1000  READ      UBFEETXT,SEQ;XFLD0001,XFLD0002,XFLD0003,XFLD0004,XFLD0005:
                                 XFLD0006,XFLD0007,XFLD0008,XFLD0009,XFLD0010:
                                 XFLD0011
          GOTO      GFND9000 IF OVER
.
          ADD       ONE,LINECNTR            * Text file line counter for error
.
          IF        VALDPASS=1
            CALL      VFLD0000                * Validation pass so
            BRANCH    EXIT,GFND1000           * validate fields
          ENDIF
.
          PACK      XCNTID,XFLD0001,SP70
          MATCH     XCNTID,CONTRCID
          IF        !@EQUAL
            MOVE      "Contract Id Mismatch",ERRORMSG
            CALL      PERR0000
            GOTO      GFND1000
          ENDIF
.
          PACK      XCLMCD,XFLD0002,SP70
          PACK      XHFCOD,XFLD0003,SP70
.
          MATCH     XCLMCD,KYCLAIM
          IF        !@EQUAL
            MOVE      "Claim Code Mismatch",ERRORMSG
            CALL      PERR0000
            GOTO      GFND1000
          ENDIF
.
          BRANCH    OPTION,GFND2100,GFND2200,GFND2400
          GOTO      GFND1000
.
GFND2100  PACK      XHFCOD,KYHFUND,SP70
          PACK      XHFTYP,XFLD0004,SP70
          GOTO      GFND3000
.
GFND2200  MATCH     XHFCOD,KYHFUND
          IF        !@EQUAL
            MOVE      "Health Fund Mismatch",ERRORMSG
            CALL      PERR0000
            GOTO      GFND1000
          ENDIF
.
          PACK      XHFTYP,XFLD0004,SP70
          GOTO      GFND3000
.
GFND2400  MOVE      SP70,XHFCOD
          MOVE      SP70,XHFTYP
.
GFND3000  PACK      XADMTYP,XFLD0005,SP70
          PACK      XBEDTYP,XFLD0006,SP70
.
          PACK      KEY3,SP70
          PACK      KEY3,XFLD0007,SP70
          SQUEEZE   KEY3
          MOVE      ZERO,XHIGHDY
          MOVE      KEY3,XHIGHDY
.
          PACK      XBFDESC,XFLD0008,SP70
.
.         No of Invoices must be 1 or 2
.
          MOVE      SP70,XNOINVN
          MATCH     "1",PTCNPPIN
          IF        !@EQUAL
            MOVE      "1",XNOINVN              * default to '1'
            PACK      KEY1,XFLD0009,SP70
            MATCH     "2",KEY1
            IF        @EQUAL
              MOVE      "2",XNOINVN
            ENDIF
          ENDIF
.
          MOVE      XFLD0010,XBFREB
          MOVE      XFLD0011,XBFPAT
.
          COMPARE   THREE,OPTION
          GOTO      GFND3800 IF EQUAL       * Default blank HF/table
.
          BRANCH    KYHFTAB,GFND4000        * All Health fund table
.
.         as per Upload file
.
GFND3800  CALL      LBFE0000
          GOTO      GFND1000
.
.         All health fund table type of the health fund
.
GFND4000  PACK      KEY14,KYHFUND,SP70
          CALL      RDSFUND1
GFND5000  CALL      RDKFUND1
          BRANCH    OVRCD,GFND1000
          MATCH     KYHFUND,HCODE
          GOTO      GFND1000 IF NOT EQUAL
.
          MATCH     "0000    ",HFTABL
          GOTO      GFND5000 IF EQUAL  
.
          BRANCH    PHFTACT,GFND5000        * Inactive
.
          MOVE      PTHFBCAT,XHFTYP         * Health fund Table type
          CALL      LBFE0000
          GOTO      GFND5000                * get next record
.
GFND9000  CLOSE     UBFEETXT
.
GFND9999  RETURN
+
.******************************************************************************
.*                                  LBFE0000                                  *
.*                             Write bed fees record                          *
.******************************************************************************
LBFE0000  UNPACK    SP70,BFCLM,BFHFUND,PTBFHTYP,BFATYPE,BFBTYPE,PTBFCNID
          MOVE      ZERO,BFENDDY
.
          MATCH     SP70,XBFDESC
          GOTO      LBFE9000 IF EQUAL
.
          PACK      KEY27,XCLMCD,XHFCOD,XHFTYP,XADMTYP,XBEDTYP:
                          XCNTID,XHIGHDY,SP70
          CALL      RDBFEE1
          IF        OVRCD=1
            UNPACK    KEY27,BFCLM,BFHFUND,PTBFHTYP,BFATYPE,BFBTYPE:
                            PTBFCNID,DBFENDDY
            MOVE      XBFREB,BFHF
            MOVE      XBFPAT,BFPAT
            MOVE      XNOINVN,BFNINV
            MOVE      XBFDESC,BFDESC
            UNPACK    SP70,BFSPARE
.
            IF        VALDPASS=0
              ADD       ONE,ADDCOUNT          * Total records added
              CALL      WRBFEE1             * Write if not validation pass
            ENDIF
.
          ELSE
            MOVE      XBFREB,BFHF
            MOVE      XBFPAT,BFPAT
            MOVE      XNOINVN,BFNINV
            MOVE      XBFDESC,BFDESC
.
            IF        VALDPASS=0
              ADD       ONE,UPDCOUNT        * Total records updated
              CALL      UPBFEE1             * Update if not validation pass
            ENDIF                         
.
          ENDIF
          MOVE      ZERO,EXIT
          GOTO      LBFE9999
.
LBFE9000  MOVE      ONE,EXIT
LBFE9999  RETURN
+
.******************************************************************************
.         Remove Bed Fees
.******************************************************************************
RMBF0000  BRANCH    OPTION,RMBF2000         * by health fund group
          IF        OPTION=3
            MOVE      SP70,KYHFUND          * blank health fund
          ENDIF
          CALL      DREC0000                * Deleting existing records
          GOTO      RMBF9999
.
.         Get health funds for the HF group
.
RMBF2000  MOVE      SP70,SAVKEY20
          PACK      KEY20,KYHFGRP,SP70
          CALL      RDSFUND3
RMBF4000  CALL      RDKFUND3
          BRANCH    OVRCD,RMBF9999
          MATCH     KYHFGRP,PTHFHGRP    * Same group?
          GOTO      RMBF9999 IF NOT EQUAL
          MATCH     "0000    ",HFTABL
          GOTO      RMBF4000 IF NOT EQUAL
.
          MOVE      HCODE,KYHFUND
          PACK      SAVKEY20,PTHFHGRP,HCODE,HFTABL,SP70
.
          CALL      DREC0000                * Deleting existing records
.
          MOVE      SAVKEY20,KEY20
          CALL      RDFUND3                 * Reposition
          GOTO      RMBF4000
.
RMBF9999  RETURN
+
.******************************************************************************
.         Deleting existing records
.******************************************************************************
DREC0000  PACK      KEY27,CONTRCID,KYCLAIM,KYHFUND,SP70
          CALL      RDSBFEE3
          CALL      RDKBFEE3
          BRANCH    OVRCD,DREC9999
.
          MATCH     CONTRCID,PTBFCNID            * Same Contract ID
          GOTO      DREC9999 IF NOT EQUAL
          MATCH     KYCLAIM,BFCLM
          GOTO      DREC9999 IF NOT EQUAL
          MATCH     KYHFUND,BFHFUND
          GOTO      DREC9999 IF NOT EQUAL
.
          PACK      KEY27,PTBFCNID,BFCLM,BFHFUND,PTBFHTYP,BFATYPE:
                          BFBTYPE,BFENDDY,SP70
          MATCH     SP70,KEY27
          GOTO      DREC9999 IF EQUAL
          CALL      DEBFEE3
          GOTO      DREC0000
.
DREC9999  RETURN
+
.******************************************************************************

.         Execute us1 command before processing
.******************************************************************************
EXCUS000  CLEAR     CMDLINE
          APPEND    "ibapms92.us1 ",CMDLINE
          APPEND    FNAMEI,CMDLINE
          APPEND    SP70,CMDLINE
          RESET     CMDLINE
          EXECUTE   CMDLINE,TASKID
EXCUS999  RETURN
+
.******************************************************************************
. Validate bed fee input text file fields
.******************************************************************************
VFLD0000  MOVE      ZERO,EXIT
.
          MATCH     SP70,XFLD0001
          IF        !@EQUAL
            PACK      KEY6,XFLD0001,SP70
            CALL      RDPMCID1
            IF        OVRCD=1
              MOVE      "Invalid Contract Id",ERRORMSG
              CALL      PERR0000
              MOVE      ONE,EXIT
            ENDIF
          ENDIF
.
          MATCH     SP70,XFLD0002
          IF        !@EQUAL
            PACK      KEY5,ANSC,ANSL,XFLD0002,SP70
            CALL      RDCODE1      
            IF        OVRCD=1
              MOVE      "Invalid Claim Code",ERRORMSG
              CALL      PERR0000
              MOVE      ONE,EXIT
            ENDIF
          ENDIF
.
          PACK      XFLD0003,XFLD0003,SP70
          MATCH     SP70,XFLD0003
          IF        !@EQUAL
            PACK      KEY6,XFLD0003,SP70
            PACK      KEY14,KEY6,ZERO8,SP70
            CALL      RDFUND1
            IF        OVRCD=1
              MOVE      "Invalid Health Fund",ERRORMSG
              CALL      PERR0000
              MOVE      ONE,EXIT
            ENDIF
          ENDIF
.
          MATCH     SP70,XFLD0004
          IF        !@EQUAL
            PACK      KEY5,ANSH,ANST,XFLD0004,SP70
            CALL      RDCODE1
            IF        OVRCD=1
              MOVE      "Invalid Health Fund Table Type",ERRORMSG
              CALL      PERR0000
              MOVE      ONE,EXIT
            ENDIF
          ENDIF
.
          MATCH     SP70,XFLD0005
          IF        !@EQUAL
            PACK      KEY5,ANSA,SP1,XFLD0005,SP70
            CALL      RDCODE1
            IF        OVRCD=1
              MOVE      "Invalid Admission Type",ERRORMSG
              CALL      PERR0000
              MOVE      ONE,EXIT
            ENDIF
          ENDIF
.
          MATCH     SP70,XFLD0006
          IF        !@EQUAL
            PACK      KEY5,ANSB,ANST,XFLD0006,SP70
            CALL      RDCODE1
            IF        OVRCD=1
              MOVE      "Invalid Bed Type",ERRORMSG
              CALL      PERR0000
              MOVE      ONE,EXIT
            ENDIF
          ENDIF
.
          TYPE      XFLD0007
          IF        !@EQUAL | @EOS
            MOVE      "Invalid Step Down",ERRORMSG
            CALL      PERR0000
            MOVE      ONE,EXIT
          ENDIF
.      
          MATCH     SP70,XFLD0008
          IF        @EQUAL | @EOS
            MOVE      "Description is Mandatory",ERRORMSG
            CALL      PERR0000
            MOVE      ONE,EXIT
          ENDIF
.
          TYPE      XFLD0009
          IF        !@EQUAL | @EOS
            MOVE      "Invalid Number of Invoices",ERRORMSG
            CALL      PERR0000
            MOVE      ONE,EXIT
          ENDIF
.      
          TYPE      XFLD0010
          IF        !@EQUAL | @EOS
            MOVE      "Invalid Rebate Amount",ERRORMSG
            CALL      PERR0000
            MOVE      ONE,EXIT
          ENDIF
.      
          TYPE      XFLD0011
          IF        !@EQUAL | @EOS
            MOVE      "Invalid Patient Amount",ERRORMSG
            CALL      PERR0000
            MOVE      ONE,EXIT
          ENDIF
.      
VFLD9999  RETURN
+
.******************************************************************************
. Print error line
.******************************************************************************
PERR0000  ADD       ONE,ERRCOUNT            * Total error count
.
          PRINT     *1,"|",LINECNTR,*12,"|",ERRORMSG,*44,"|":
                    XFLD0001,XFLD0002,XFLD0003,XFLD0004,XFLD0005:
                    XFLD0006,XFLD0007,XFLD0008,XFLD0009,XFLD0010:
                    XFLD0011,*132,"|"
.
PERR9999  RETURN
+
