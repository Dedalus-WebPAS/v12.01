          MATCH     SP8,KEY8
          GOTO      CHKU2000 IF NOT EQUAL
.
          PACK      WEBTITLE,SP70,SP70
          CLEAR     WEBTITLE
          APPEND    "U/R Number is not entered. ",WEBTITLE
          RESET     WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
