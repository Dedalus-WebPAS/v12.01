          CALL      RDCHCO1
          IF        OVRCD <> 1
            MOVE      CODECC,CHCATG
            MOVE      SAVCARE,CHCODE
            MOVE      WBSEPCD,CHUPUSID
            PACK      CHUPDATE,CURRDATE
            MOVE      CTIMEIS,CHUPTIME
            MOVE      F2,CHEPISNO
            MOVE      SP10,CHCODCMP
            CALL      UPCHCO1
          ELSE
            MOVE      CODECC,CHCATG
            MOVE      SAVCARE,CHCODE
            MOVE      AADMNO,CHADMN
            MOVE      TRANDATE,CHDATE
            MOVE      TRANTIME,CHTIME
            MOVE      WBSEPCD,CHCRUSID
            PACK      CHCRDATE,CURRDATE
            MOVE      CTIMEIS,CHCRTIME
            MOVE      SP70,CHUPDATE
            MOVE      SP70,CHUPTIME
            MOVE      SP70,CHUPUSID
            MOVE      F2,CHEPISNO
            UNPACK    SP70,CHCODCMP,CHSPARE
            CALL      WRCHCO1
          ENDIF
          GOTO      CHCC9999
