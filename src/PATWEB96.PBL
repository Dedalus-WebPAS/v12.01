            ELSE
              WRITE     HTMLFILE;012;       * Insert 'new line' character
            ENDIF
            WRITE     HTMLFILE;"Five Day Ward was selected and the planned ":
                               "length of stay falls on a weekend.";
            GOTO      CHKDAY99
          ENDIF
