          MATCH     "A",TMOVE
          GOTO      CHTRNS10 IF EQUAL
.
          MATCH     TDATE,PTMIS001
          GOTO      CHTRNS90 IF LESS
          GOTO      CHTRNS92 IF NOT EQUAL
