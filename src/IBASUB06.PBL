          IF        @EQUAL
            MOVE      SISDUC1,KEY19
          ENDIF
          FEDIT     SISDUC2,KEY20
          MATCH     "**",KEY16
          IF        @EQUAL
            MOVE      SISDUC2,KEY20
          ENDIF
          FEDIT     SISDUC3,KEY21
          MATCH     "**",KEY16
          IF        @EQUAL
            MOVE      SISDUC3,KEY21
          ENDIF
          FEDIT     SISDUC4,KEY22
          MATCH     "**",KEY16
          IF        @EQUAL
            MOVE      SISDUC4,KEY22
          ENDIF
          SQUEEZE   KEY16
          SQUEEZE   KEY17
          SQUEEZE   KEY18
          SQUEEZE   KEY19
          SQUEEZE   KEY20
          SQUEEZE   KEY21
          SQUEEZE   KEY22
.
          PRINT     "| ",*132,"|",*N:
                    *+,"| ",SP10,SP2,SISDSUP,SP6,SISDCAT,SP4,CPCDATE:
                    *50,"0.00 to ",KEY16," = $",KEY19,SP5:
                    *90,KEY16," to ",KEY17," = $",KEY20,SP5:
                    *132,"|",*N:
                    "| ",SP10,SP2:
                    SISDSUT,SP2,KEY15:
                    *50,KEY17," to ",KEY18," = $",KEY21,SP5:
                    *90,"over ",KEY18," = $",KEY22,SP5:
                    *132,"|"
          ADD       "3",CLNO           * add one to line number
          GOTO      WRIB3000
.
WRIB4000
          PRINT     "| ",*132,"|"
          ADD       "1",CLNO           * add one to line number
.
WRIB9999  RETURN
