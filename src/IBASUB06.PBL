          PACK      KEY45,SISDCON,SISDSUP,SISDCAT,SP70
          PACK      KEY22,SISDCON,SISDSUP,SISDCAT,SP70
          CALL      RSSISD1
DEL34000
          CALL      RKSISD1
          BRANCH    OVRCD,DEL37000
          PACK      KEY45,SISDCON,SISDSUP,SISDCAT,SISDSUT,SP70
          MATCH     KEY22,KEY45
          GOTO      DEL37000 IF NOT EQUAL
          MATCH     KEY37,KEY45
          GOTO      DEL34000 IF NOT EQUAL
          GOTO      DEL38000

