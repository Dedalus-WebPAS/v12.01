          GOTO      OVERCOND IF OVER
          RETURN
.
WRTEMP1   RESET     KEY3
          WRITE     TEMPFILE,KEY3;KEY3
          RETURN
.
RSTEMP1  RESET     KEY3
         READ      TEMPFILE,KEY3;;
         RETURN
.
RKTEMP1  MOVE      ZERO,OVRCD
         READKS    TEMPFILE;DIM3
         GOTO      OVERCOND IF OVER
         RETURN
.
DETEMP1  MOVE      ZERO,OVRCD
         RESET     KEY3
         DELETE    TEMPFILE,KEY3
         RETURN
