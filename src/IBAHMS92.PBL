                CLEAR     ERRORMSG
                APPEND    "Invalid Eff.From Date-",ERRORMSG
                APPEND    XEFDAT,ERRORMSG
                APPEND    SP70,ERRORMSG
                RESET     ERRORMSG
                CALL      PERR0000
                MOVE      ONE,EXIT
              ENDIF
              PACK      XEDATE,XFLD0005,SP70
              PACK      XFLD0005,XFLD0005,SP70
              MATCH     PMCITDAT,XFLD0005
              IF        !@EQUAL
                CLEAR     ERRORMSG
                APPEND    "Invalid Eff.To Date-",ERRORMSG
