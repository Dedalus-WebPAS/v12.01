.         default to the current date.
.
          MOVE      SP8,MRMAEPDT
          MATCH     SP25,MASTFILM
          IF        !@EQUAL
            MATCH     SP8,MASTEPDT
            IF        @EQUAL
              MOVE      CURRDATE,MRMAEPDT
            ELSE
              MOVE      MASTEPDT,MRMAEPDT
            ENDIF
          ENDIF
.
.         If the originating hospital is blank, then default it
.         to the home hospital
.
          MATCH     SP3,MASTOHOS
          IF        @EQUAL
            MOVE      MASTHHOS,MRMAOHOS
          ELSE
            MOVE      MASTOHOS,MRMAOHOS
          ENDIF
          IF        HOSPLFLG = 1
            MOVE      MRMAOHOS,KEY3
            CALL      RDTEMP1                    * hospital code on map file ?
            IF        OVRCD = 0
              MOVE      NEWHOSPL,MRMAOHOS        * yes - load mapped hospital
            ENDIF
