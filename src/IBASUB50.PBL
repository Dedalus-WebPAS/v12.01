ZTOT0000  MOVE      SP70,LASTCODE
          MOVE      ZERO,ATOTAL01
          MOVE      ZERO,ATOTAL02
          MOVE      ZERO,ATOTAL03
          MOVE      ZERO,ATOTAL04
          MOVE      ZERO,ATOTAL05
          MOVE      ZERO,ATOTAL06
          MOVE      ZERO,ATOTAL07
          MOVE      ZERO,ATOTAL08
          MOVE      ZERO,ATOTAL09
          MOVE      ZERO,ATOTAL10
          MOVE      ZERO,ATOTAL11
          MOVE      ZERO,ATOTAL12
          MOVE      ZERO,QTOTAL09
          MOVE      ZERO,QTOTAL10
          MOVE      ZERO,QTOTAL11
          MOVE      ZERO,QTOTAL12
.
