.********************************************************************
. BKLIN - Break up KEY60 into two neat halves        Called by lots
.         Requires - KEY60   (original line)
.         Returns  - KEY30   (line 1)
.                    KEY60   (line 2)
.********************************************************************
BKLIN000  RESET     KEY60,31
          CMATCH    SP70,KEY60
          WHILE     !@EQUAL&!@EOS
            BUMP      KEY60,-1
            IF        !@EOS
              CMATCH    SP70,KEY60
            ENDIF
          DO
.
          IF        @EOS
            SETLPTR   KEY60
            RESET     KEY60
            UNPACK    KEY60,KEY30,KEY60
          ELSE
            LENSET    KEY60
            RESET     KEY60
            MOVE      KEY60,KEY30
            PACK      KEY30,KEY30,SP70
.
            ENDSET    KEY60
            SETLPTR   KEY60
            BUMP      KEY60
            PACK      KEY60,KEY60,SP70
          ENDIF
.
BKLIN999  RETURN
.----------------------------------------------------------
. Print Heading
.----------------------------------------------------------
HEAD0000  CALL      HEAD132                 * Print Page Header
          IF        CPAGENO=1
            STRIP       INCLDESC
            STRIP       SEQDESC
            CLEAR       VAR
            APPEND      "Including ",VAR
            APPEND      INCLDESC,VAR
            APPEND      " in ",VAR
            APPEND      SEQDESC,VAR
            APPEND      " Sequence ",VAR
            RESET       VAR
            STRIP       FROMCATA
            STRIP       TOCATA
            STRIP       FROMGRP
            STRIP       TOGRP
            STRIP       FROMDESC
            STRIP       TODESC
            STRIP       FROMCOST
            STRIP       TOCOST
            STRIP       FROMWAR
            STRIP       TOWAR
            UNPACK      SIPESDT,CCENT,CYEAR,CMON,CDAY
            CALL        PACDATE
            PACK        KEY10,CPCDATE,SP70
            UNPACK      SIPEEDT,CCENT,CYEAR,CMON,CDAY
            CALL        PACDATE
            PACK        PRNDESC1,FROMCATA
            PACK        PRNDESC2,TOCATA
            PRINT       *+,"Usage Period : ",SIPEPER,"/",SIPEYEAR:
                        SP2,KEY10," to ",CPCDATE:
                        *N,"Catalog Code  Range : ",PRNDESC1," to ",PRNDESC2:
                        *N,"Product Group Range : ",FROMGRP," to ",TOGRP:
                        *N,"Catalog Name  Range : ",FROMDESC," to ",TODESC:
                        *N,"Cost Centre   Range : ",FROMCOST," to ",TOCOST
            IF          INCLTYPE<>2
              PRINT     *+,"Warehouse     Range : ",FROMWAR," to ",TOWAR
              ADD       "1",CLNO
            ENDIF
            PRINT       VAR
            ADD       "6",CLNO
          ELSE
            PRINT       VAR
            ADD       "1",CLNO
          ENDIF
          IF        REPTSEQ=3
            MOVE      SP70,SISADESC
            PACK      KEY5,SIIASUB,SP70
            CALL      RDSISA1
            PRINT     "Product Group : ",SIIASUB,SP1,SISADESC
          ENDIF
.
          CALL      UND132
          PRINT     *1,"| Catalog Description":
                    *70,"|             Current Period   ":
                         "              Year to Date    ":
                    *132,"|":
                    *N,*1,"|",*10,"Cost Centre Description":
                    *70,"|       Quantity          Value":
                         "       Quantity          Value":
                    *132,"|"
          ADD       "2",CLNO
          CALL      UND132
.
HEAD9999  RETURN
.----------------------------------------------------------
. Calculate Totals
.----------------------------------------------------------
CALC0000  MOVE      ZERO,ATOTAL01
          MOVE      ZERO,ATOTAL02
          MOVE      ZERO,ATOTAL03
          MOVE      ZERO,ATOTAL04
          MOVE      ONE,NEWCAT
          MOVE      ZERO,ENDFLAG
          MOVE      SP5,SAVCST
. ------- Calculate from Catalog usage by Warehouse File
          PACK      KEY18,SIIACAT,SP5,SIPEYEAR,SP70
          PACK      KEY18CHK,SIIACAT,SP5,SIPEYEAR,SIPEPER
          CALL      RSSIAL5
CALC1000  CALL      RKSIAL5
          BRANCH    OVRCD,CALC8000
          MATCH     SIALCAT,SIIACAT
          GOTO      CALC8000 IF NOT EQUAL
.
          IF        REPTSEQ=3
            MATCH     CHECKSUB,SIALSUB
            GOTO      CALC1000 IF NOT EQUAL
          ENDIF
.
          MATCH     SIIACAT,SP70              * Don't worry if Non-Stock/Catalog
          IF        !@EQUAL & SIIANON=0
            MATCH     SP70,SIALWAR            * all warehouses ?       *C-F0901
            IF        !@EQUAL
              MATCH     FWAREHS,SIALWAR       * Must be between        *C-F0901
              GOTO      CALC1000 IF LESS      * From and To            *C-F0901
              MATCH     SIALWAR,TWAREHS       * warehouse              *C-F0901
              GOTO      CALC1000 IF LESS                               *C-F0901
            ENDIF                                                      *C-F0901
.
. check warehouse security
.
            PACK      KEY9,PASSCODE,SIALWAR,SP70   * check warehouse security
            CALL      RDSIWS1
            BRANCH    OVRCD,CALC1000
          ENDIF
.
          MATCH     SP5,SAVCST
          IF        !@EQUAL
            MATCH     SIALCST,SAVCST
            CALL      CALCP000 IF NOT EQUAL
          ENDIF
.
          ADD       SIALUQT,ATOTAL03        * Stock Usage
          ADD       SIALUAM,ATOTAL04
.
          MATCH     DAT6,SIALDAT              * current period ?
          IF        @EQUAL
            ADD       SIALUQT,ATOTAL01      * Stock Usage
            ADD       SIALUAM,ATOTAL02
          ENDIF
          MOVE      SIALCST,SAVCST
          GOTO      CALC1000
.
CALC8000  CALL      CALCP000
          CALL      CALCT000
.
CALC9999  RETURN
.
CALCP000  IF        (ATOTAL01=0 & ATOTAL02=0 & ATOTAL03=0 & ATOTAL04=0)
            MOVE      SIALCST,SAVCST
            GOTO      CALCP999
          ENDIF
.
          PACK      KEY7,SIIACAT,SP70
          CALL      RDSIIA1
          IF        OVRCD=1
            CALL      SNON0000
          ENDIF
.
          COMPARE   "55",CLNO               * Number of Lines Test
          CALL      HEAD0000 IF NOT LESS
.
          IF        NEWCAT=1
            MOVE      ZERO,NEWCAT
            PRINT     *1,"|",*2,SIIACAT,*10,SIIADES,*70,"|",*132,"|"
            ADD       ONE,CLNO
          ENDIF
.
          MOVE      SP70,SICADESC
          PACK      KEY5,SAVCST,SP70
          CALL      RDSICA1
.
          COMPARE   "55",CLNO               * Number of Lines Test
          CALL      HEAD0000 IF NOT LESS
.
          CALL      WRTEXP00
          PRINT     *1,"|",*10,SICACODE,*18,SICADESC:
                    *70,"|",ATOTAL01,ATOTAL02,ATOTAL03,SP1,ATOTAL04:
                    *132,"|"
          ADD       "1",CLNO
          MOVE      SIALCST,SAVCST
.
          ADD       ATOTAL01,ATOTAL05
          ADD       ATOTAL02,ATOTAL06
          ADD       ATOTAL03,ATOTAL07
          ADD       ATOTAL04,ATOTAL08
          ADD       ATOTAL01,ATOTAL09
          ADD       ATOTAL02,ATOTAL10
          ADD       ATOTAL03,ATOTAL11
          ADD       ATOTAL04,ATOTAL12
          MOVE      ZERO,ATOTAL01
          MOVE      ZERO,ATOTAL02
          MOVE      ZERO,ATOTAL03
          MOVE      ZERO,ATOTAL04
.
CALCP999  RETURN
.
CALCT000  IF        (ATOTAL09=0 & ATOTAL10=0 & ATOTAL11=0 & ATOTAL12=0)
            GOTO      CALCT999
          ENDIF
.
          COMPARE   "55",CLNO               * Number of Lines Test
          CALL      HEAD0000 IF NOT LESS
.
          CALL      UND132
          PRINT     *1,"|",*2,SIIACAT,*10,SIIADES,*70,"|":
                    ATOTAL09,ATOTAL10,ATOTAL11,SP1,ATOTAL12:
                    *132,"|"
          ADD       "1",CLNO
          CALL      UND132
.
          ADD       ATOTAL09,QTOTAL09
          ADD       ATOTAL10,QTOTAL10
          ADD       ATOTAL11,QTOTAL11
          ADD       ATOTAL12,QTOTAL12
          MOVE      ZERO,ATOTAL09
          MOVE      ZERO,ATOTAL10
          MOVE      ZERO,ATOTAL11
          MOVE      ZERO,ATOTAL12
.
CALCT999  RETURN
