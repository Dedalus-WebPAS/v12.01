          MATCH     SIWAWAR,SP70
          IF        @EQUAL
            CALL      KSIIAA00
          ELSE
            CALL      KSIIEA00
          ENDIF
          BRANCH    EXIT,ECAT1000,ECAT9999
          MOVE      SIIACAT,FCATALOG
          PACK      FROMCATA,SIIACAT,SP1,SIIADES,SP70
          GOTO      ECAT2000
.
