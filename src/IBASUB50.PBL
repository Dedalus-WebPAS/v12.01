          MOVE      SISADESC,KEY30
.
          PRINT     *1,"|",CHECKSUB,SP1,KEY30,*45,"|":
                    *70,"|",QTOTAL09,QTOTAL10,QTOTAL11,SP1,QTOTAL12:
                    *132,"|"
          MOVE      SIALSUB,CHECKSUB
          MOVE      SIALCAT,CHECKCAT
          MOVE      "599",CLNO
          MOVE      ZERO,ATOTAL01
          MOVE      ZERO,ATOTAL02
          MOVE      ZERO,ATOTAL03
          MOVE      ZERO,ATOTAL04
          MOVE      ZERO,QTOTAL09
          MOVE      ZERO,QTOTAL10
          MOVE      ZERO,QTOTAL11
          MOVE      ZERO,QTOTAL12
.
