          MOVE      "Start                                           ",FROMCATA
.
ECAT2000  MOVE      FROMCATA,VAR
          CALL      DISPITEM
.
          MOVE      "    2",FLDID
          CALL      GETPOSIT
          MOVE      SP70,VAR
          CALL      DISPITEM
.
          MOVE      ZERO,CKEYTYP
          MATCH     SIWAWAR,SP70
          IF        @EQUAL
            CALL      KSIIAA00
          ELSE
            CALL      KSIIEA00
          ENDIF
          BRANCH    EXIT,ECAT3000,ECAT9999
          MOVE      SIIACAT,TCATALOG
          PACK      TOCATA,SIIACAT,SP1,SIIADES,SP70
          MATCH     FCATALOG,TCATALOG
          GOTO      ECAT9000 IF EQUAL
          GOTO      ECAT9000 IF NOT LESS
