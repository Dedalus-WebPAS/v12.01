          MOVE      HL6AALN1,PMCEADD1
          MOVE      HL6AALN2,PMCEADD2
          MOVE      HL6AALN3,PMCEADD3
          MOVE      HL6AALN4,PMCEADD4
          MOVE      HL6AAPOS,PMCEPOST
