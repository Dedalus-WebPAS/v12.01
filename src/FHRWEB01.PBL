          MATCH     "ANONYMOUS",HLP7NUSE
          IF        @EQUAL
            CALL      WRALI000
          ENDIF
.
          MATCH     "OLD",HLP7NUSE
          IF        @EQUAL
            CALL      WRALI000
          ENDIF
.
          GOTO      GTALIA10
.
