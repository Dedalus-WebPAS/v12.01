.
          MATCH     HLP1RSID,HLP6RSID
          GOTO      UPXCON99 IF NOT EQUAL
.
          MATCH     HLP1VRID,HLP6VRID
          GOTO      UPXCON99 IF NOT EQUAL
.
          MATCH     HLP1CNT1,HLP6CNT1
          GOTO      UPXCON99 IF NOT EQUAL
.
          PACK      KEY82,HLP6RSID,HLP6VRID,HLP6CNT1,HLP6CNT2,SP70
          CALL      RDHLP6A1
          IF        OVRCD=1
