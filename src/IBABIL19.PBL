         MOVE      "12",FORM2
         PACK      KEY16,TWO,FORM2,TILDA20       * Discount
         CALL      RDWORK                        * Read in the record
.
         MOVE      "LESS DISCOUNT                 ",SUBDESC1
         MOVE      SP20,SUBDCOD
.
         CALL      PRTD0000                      * Print the data
         CALL      BLNK0000                      * Blank line
         CALL      SUBT0000                      * Subtract from the totals
