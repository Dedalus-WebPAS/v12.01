DRNG0000  DISPLAY   *P1:20,"Starting Date :":
                    *P1:21,"  Ending Date :"
.
          MOVE       "20",CVERT
          PACK       KEY8 WITH FTCC,FTYY,FTMM,FTDD
          UNPACK     KEY8,XCENT,XYEAR,XMON,XDAY
          MOVE       XDAY,IDAY
          MOVE       XMON,IMON
          MOVE       XYEAR,IYEAR
          MOVE       XCENT,ICENT
.
          CALL       KEYDTE00
          PACK       INVFDATE WITH ICENT,IYEAR,IMON,IDAY
          REP        " 0",INVFDATE
.
          MOVE       "21",CVERT
.
