.           PACK      KEY8,ADATE,SP10
.           MATCH     KEY8,LEAVDATE
.           IF        @EQUAL
.             SUB       ONE,CDYSDAYS
.           ENDIF
          ENDIF
          ADD       CDYSDAYS,LEAVDAY              * set leave days
          MOVE      SP8,LEAVDATE
          MOVE      SP8,RETNDATE
