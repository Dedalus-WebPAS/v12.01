              ENDIF
            ENDIF
            APPEND    "provider no. check digit not valid",ERRMSG
            RESET     ERRMSG
            CALL      PERR0000
            GOTO      VPRN9100
          ENDIF
.
          MOVE      ZERO,EXIT
          GOTO      VPRN9999
.
VPRN9100  MOVE      ONE,EXIT
