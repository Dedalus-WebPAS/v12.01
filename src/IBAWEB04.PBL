.
          MATCH     SP70,MORSEG09
          IF        @EQUAL
            PACK      KEY70,MORSEG01,MORSEG02,MORSEG03,MORSEG06,MORSEG07,MORSEG08,SP70
            MATCH     SP70,KEY70
            IF        !@EQUAL
              MOVE      "MOR.09-AnsnapVersion is blank.",ERRMSG  * yes
              CALL      PERR0000
