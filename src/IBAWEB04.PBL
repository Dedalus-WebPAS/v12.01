.
          MATCH     SP70,MORSEG06
          IF        @EQUAL
            PACK      KEY70,MORSEG01,MORSEG02,MORSEG03,MORSEG07,MORSEG08,MORSEG09,SP70
            MATCH     SP70,KEY70
            IF        !@EQUAL
              MOVE      "MOR.04-ANSNAPId is blank.",ERRMSG  * yes
              CALL      PERR0000
