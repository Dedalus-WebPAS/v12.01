.
          MATCH     SP70,MORSEG03
          IF        @EQUAL
            PACK      KEY70,MORSEG01,MORSEG02,MORSEG06,MORSEG07,MORSEG08,MORSEG09,SP70
            MATCH     SP70,KEY70
            IF        !@EQUAL
              MOVE      "MOR.03-AssessmentOnlyCode is blank.",ERRMSG  * yes
              CALL      PERR0000
