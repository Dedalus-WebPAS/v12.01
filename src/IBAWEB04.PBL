.******************************************************************************
.*                                  PROC0000              Called by: WRIT0000 *
.*    Check if re-submission, Update records in webihcaf accordingly          *
.******************************************************************************
.
PROC0000  MOVE      SP10,SAVPRVBN
.
          MOVE      SAVBTHN,WBICBATN             * load the batch number
          MOVE      TWO,WBICFLAG                 * update status to "extracted"
          MOVE      CIDSEG04,WBICCCFL            * save contiguous claim flag
          MOVE      LISTAMNT,WBICAMTC            * load claim amount
          MOVE      CIDSEG02,WBICCTYP            * IHCW Type PU/PR
          CALL      UPWBIHC1                     * update record
.
.         Write a record to the claims audit/history table for this claim
.
          MOVE      WBICINVN,WBIIINVN
          CALL      IBACLOCK
          PACK      WBIIDATE,CCC,CYY,CMM,CDD
          REP       " 0",WBIIDATE
          MOVE      CTIMEIS,WBIITIME
          MOVE      SAVBTHN,WBIIBATN
          MOVE      TWO,WBIISTAT
          MOVE      " 0",WBIITYPE
          MOVE      WBIJOPER,WBIIOPER
          MOVE      SP30,WBIITRID
          MOVE      SP30,WBIISPAR
          CALL      WRWBIHA1
.
          MATCH     "1",ERRORIND
          IF        @EQUAL
.
            MOVE      THREE,WBICFLAG
            CALL      UPWBIHC1
.
PROC0100    MOVE      WBICINVN,WBIIINVN
            CALL      IBACLOCK
            PACK      WBIIDATE,CCC,CYY,CMM,CDD
            REP       " 0",WBIIDATE
            MOVE      CTIMEIS,WBIITIME
            MOVE      SAVBTHN,WBIIBATN
            MOVE      ONE,WBIISTAT
            MOVE      " 0",WBIITYPE
            MOVE      WBIJOPER,WBIIOPER
            MOVE      WBICTRID,WBIITRID
            MOVE      SP70,WBIIEROR
            MOVE      ERRORDSC,WBIIEROT
            MOVE      WBICMSID,WBIIMSID
            PACK      WBIISPAR,SP70,SP70
.
            PACK      KEY26,WBIIINVN,WBIIDATE,WBIITIME,WBIITYPE,SP70
            CALL      RAWBIHA1
            IF        OVRCD=0
              GOTO      PROC0100
            ENDIF
.
            CALL      WRWBIHA1
.
          ENDIF
.
.         Check if this claim was the result of a resubmission
.
          MATCH     SP8,WBICPBAT                 * blank previous batch ?
          GOTO      PROC9999 IF EQUAL            * yes - not a resubmission
.
.         Update the next batch number in the original source claim for this
.         resubmission
. 
          MOVE      WBICPBAT,SAVPRVBN            * save previous batch number
          PACK      KEY35,WBICHOSP,TEN6,WBICHFND,WBICADMN,WBICINVN,SAVPRVBN
          CALL      RDWBIHC1
          BRANCH    OVRCD,PROC9999        
.
          MOVE      SAVBTHN,WBICNBAT
          CALL      UPWBIHC1
.
PROC9999  RETURN
