          IF        @EQUAL
            PACK      KEY70,MORSEG01,MORSEG03,MORSEG06,MORSEG07,MORSEG08,MORSEG09,SP70
            MATCH     SP70,KEY70
            IF        !@EQUAL
              MOVE      "MOR.02-AROCImpairmentCode is blank.",ERRMSG  * yes
              CALL      PERR0000
            ENDIF
