.****************************************************************************
.*                                INIT0000             Called by: ML0000    *
.*                Open files and initialise variables                       *
.****************************************************************************
.
INIT0000  CALL      DISPHEAD                  * display heading
.
          DISPLAY   *P56:24,*EL,"Opening ":
                    *P64:24,"controlf"
          OPEN      CONTROLF,"controlf"
.
          DISPLAY   *P64:24,"allrefaf";          * referral file
          OPEN      ALLREFA3,"allrefaf"
.
          DISPLAY   *P64:24,"allstsaf";          * referral status history  file
          OPEN      ALLSTSA1,"allstsaf"
.
          DISPLAY   *P64:24,"alllnkaf";          * referral status history  file
          OPEN      ALLLNKA1,"alllnkaf"
.
          OPEN      ALLAUDA1,"allaudaf"
          OPEN      PATCODE1,"patcodes"
          OPEN      PATMA1A1,"patma1af"
          OPEN      PATMX1A1,"patmx1af"
          OPEN      PMSPX2A1,"pmspx2af"
          OPEN      WEBSECA1,"websecaf"
          OPEN      PRISECA1,"prisecaf"
.
          OPEN      PATVISA1,"patvisaf"
.
          READ      CONTROLF,HUND06;*27,ALCNRAUD,*28,ALCNEAUD

          MATCH     "1",ALCNRAUD            
          IF        @EQUAL
            OPEN      ALLAUDRE,"allaudre"
          ENDIF
.
          MOVE      "out",CFILEPRE
          OPEN      OUTSITA1,"outsitaf"
          CALL      OPNOUT00               * Open prefixed Outpatient Files
.
INIT9999  RETURN
