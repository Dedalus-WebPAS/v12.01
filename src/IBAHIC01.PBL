.****************************************************************************
.*                                 PROC0000                                 *
.*             Perform the report                                           *
.*  Only allow to close referrals with status of 1 (active) and 3 (Inactive)*
.****************************************************************************
PROC0000  MOVE       ZERO,CPAGENO
          CLOCK      TIME,CTIMEIS
          MOVE       "60",CLNO
.
          DISPLAY    *P10:24,"Printing Referrals: ";
.
PROC0200  PACK      KEY17,ONE,SP70
          CALL      RSALREF3                * position on the item file
.
.------ read through the item file ------
.
PROC1000  CALL      RKALREF3
          BRANCH    OVRCD,PROC9000
.
          DISPLAY   *P35:24,*EL,*V2LON,ALREVISN;
.
          MATCH     SP70,CLINICTY
          IF        !@EQUAL
            MATCH     ALRECTYP,CLINICTY     * Same clinic type ?
            GOTO      PROC1000 IF NOT EQUAL
          ENDIF
.
          MATCH     SP70,CLINICID
          IF        !@EQUAL
            MATCH     ALRECLID,CLINICID     * Same clinic id ?
            GOTO      PROC1000 IF NOT EQUAL
          ENDIF
.
          MATCH     SP70,ALREREXP
          GOTO      PROC1000 IF EQUAL       * expiry date is blank
.
          MATCH     "2",ALRESTAT
          IF        @EQUAL
            PACK      KEY17,THREE,SP70      * Skip to status 3 Inactive
            CALL      RSALREF3              * position on the ref file
            GOTO      PROC1000
          ENDIF
.
          MATCH     "4",ALRESTAT            * status cancelled so exit
          GOTO      PROC9000 IF EQUAL
.
.         Expiry date must be greater than 30 days prior to the current date
.
          PACK      CDYSTDTE,CCC,CYY,CMM,CDD
          REP       " 0",CDYSTDTE
          MOVE      ALREREXP,CDYSFDTE
          CALL      CALCDAYS
          IF        CDYSDAYS<30
            GOTO      PROC1000
          ENDIF
.
.         Check if there was at least one attendance for this visit
.
          PACK      KEY16,ALREVISN,SP70
          CALL      RSALLNK1
PROC2000  CALL      RKALLNK1
          BRANCH    OVRCD,PROC1000
.
          MATCH     ALREVISN,ALLNVISN            * Same referral
          GOTO      PROC1000 IF NOT EQUAL
.
          MATCH     "1",ALLNSTAT                 * OP visit cancelled
          GOTO      PROC2000 IF EQUAL
.
          CALL      ROTFIL00                     * Check outpatient file prefix
.
          MOVE      ALLNLNKV,KEY8
          CALL      RDVISA1
          BRANCH    OVRCD,PROC1000
.
          PACK      KEY36,ALLNLNKV,SP70
          CALL      RDSBOKA6
          CALL      RDKBOKA6
          BRANCH    OVRCD,PROC1000
.
          MATCH     DOBAOUTN,ALLNLNKV       * Check series booking admiss no
          GOTO      PROC1000 IF NOT EQUAL
.
          COMPARE   FOUR,OBASTAT
          GOTO      PROC2000 IF NOT EQUAL   * not attended, try next booking
.
.         Read outmasaf file to get the medical practice
.
          PACK      KEY18,OBASITE,OBACLIN,OBADAY,OBASTRT,SP70
          CALL      RDMASA1
          IF        OVRCD=0
            PACK      KEY16,OTMAMPRC,USERID,SP70
            CALL      RDPRSEC1
            BRANCH    OVRCD,PROC1000     * User got no access for this Med Pract
.
            MATCH     SP70,PROVNUMB
            IF        !@EQUAL
              MATCH     PROVNUMB,OTMAPROV
              GOTO      PROC1000 IF NOT EQUAL
            ENDIF
.
          ENDIF
.
          MOVE      SP70,PATNAME
          MOVE      OBAURNO,KEY8
          CALL      RDMAST1
          IF        OVRCD=0
            MOVE      PSNAME,PACSNAME
            MOVE      PGNAME,PACGNAME
            MOVE      PTITL,PACTITLE
            MOVE      TWO,PACFRMT
            CALL      PACNAME                 * Format patients name
            MOVE      PACFNAME,PATNAME
          ENDIF
.
          MOVE      OBAURNO,KEY8
          CALL      RDPMPX21
          IF        OVRCD=0
            CALL      CLPMSPX2
          ENDIF
.
          UNPACK    ALREREXP,CCENT,CYEAR,CMON,CDAY     * expiry date
          CALL      PACDATE
.
          MOVE      SP70,CTYPDESC
          PACK      KEY12,PVISITE,ALRECTYP,SP70
          CALL      RDCTYA1                  * Read Clinic Type File
          IF        OVRCD=0
            UNPACK    OCTDESC,CTYPDESC
          ENDIF
.
.         Get description of clinic id
.
          MOVE      SP70,CLIDDESC
          PACK      KEY20,PVISITE,ALRECLID,Z70
          CALL      RDSCLIA1
          CALL      RDPCLIA1
          IF        OVRCD=0
            MATCH     PVISITE,OCASITE
            IF        @EQUAL
              MATCH     ALRECLID,OCACLIN
              IF        @EQUAL
                UNPACK    OCADESC,CLIDDESC
              ENDIF
            ENDIF
          ENDIF
.
          PACK      TDESC,SP70
          PACK      KEY5,ANSR,ANSI,ALRERTYP
          CALL      RDCODE1
.
          COMPARE   "48",CLNO
          CALL      HEAD0000 IF NOT LESS
.
          PRINT     *1,"|",OBAURNO,*12,"|",PATNAME,*40,"|",PMEDI:
                    *52,"|",CTYPDESC,*74,"|",CLIDDESC:
                    *96,"|",TDESC,*118,"| ",CPCDATE,*132,"|"
          ADD       ONE,CLNO
.
          BRANCH    COPTION,PROC1000        * Report only
.
          PACK      SKEY17,ALRESTAT,ALRERDAT,ALREVISN  * Save position in file
.
          MOVE      TWO,AUDTTYPE            * before history record
          CALL      WAALRE00
.
          MOVE      TWO,ALRESTAT
          MOVE      "2",ALRESTAT            * Populate all the fields and write
          MOVE      SP3,ALRERINA            * in database
          MOVE      SP8,ALREDINA
          MOVE      SP8,ALRETINA
          MOVE      SP8,ALREDINU
          MOVE      SP10,ALREUINA
.
          CALL      IBACLOCK
          PACK      ALREUDAT,CCC,CYY,CMM,CDD * current date
          REP       " 0",ALREUDAT
          CLOCK     TIME,ALREUTIM           * current time
          MOVE      USERID,ALREUUID        * user id
.
          PACK      ALREDCLO,CCC,CYY,CMM,CDD
          REP       " 0",ALREDCLO
          CLOCK     TIME,ALRETCLO
          MOVE      USERID,ALREUCLO
          CALL      UPALREF3
.
          CALL      PMIGTNID                * get national id for dgate write
          MOVE      NMPNUMB,PTNINMPI
          MOVE      ONE,HL7TRGID
          MOVE      SP8,HL7INCLD
          CALL      DGCLII13                * broadcast Update Ref. (REF^I13)
.
          MOVE      THREE,AUDTTYPE          * after history record
          CALL      WAALRE00
.                                   ***** write to Referral History Table
          MOVE      ALREDCLO,ALSTSDAT
          CALL      WRTRSH00                * write a record to allstsaf
.
          CALL      WRTRAU00                * write to the referral audit table
.
          MOVE      SKEY17,KEY17            * re position on record after update
          CALL      RSALREF3
.
          GOTO      PROC1000
.
PROC9000  COMPARE    ZERO,CPAGENO
          GOTO       PROC9999 IF EQUAL
.
          CALL       UND132
          PRINT      *N,"*** END OF REPORT  ***";
.
PROC9999  RETURN
+
.******************************************************************************
.      write a record to the Referral Status History Table
.******************************************************************************
WRTRSH00  MOVE      ALREVISN,ALSTVISN       * Populate all the fields and write
          MOVE      ALRESTAT,ALSTSTAT
          PACK      ALSTCDAT,CCC,CYY,CMM,CDD
          REP       " 0",ALSTCDAT
          CLOCK     TIME,ALSTCTIM
          MOVE      USERID,ALSTCUID
          PACK      KEY17,ALSTVISN,ALSTSTAT,ALSTSDAT,SP70
          CALL      RAALSTS1
          IF        OVRCD=0
            CALL      UPALSTS1                * update to the file
          ELSE
            CALL      WRALSTS1                * write to the file
          ENDIF
.
WRTRSH99  RETURN
.******************************************************************************
.      write a record to the Referral Audit Table
.******************************************************************************
WRTRAU00  CALL      CLALLAUD                     * clear allaudaf variables
.
          MOVE      ALREVISN,ALAUVISN
          PACK      ALAUADAT,CCC,CYY,CMM,CDD
          REP       " 0",ALAUADAT
          CLOCK     TIME,ALAUATIM
          MOVE      "C",ALAUUPDT            * Update type
          MOVE      USERID,ALAUAUID
.
          PACK      KEY24,ALAUVISN,ALAUADAT,ALAUATIM,SP70
          CALL      RDALAUD1
          IF        OVRCD=1
            MOVE      ZERO,OVRCD
            TRAP      OVERCOND IF IO
            CALL      WRALAUD1                * write to the file
            TRAPCLR   IO
            BRANCH    OVRCD,WRTRAU99
          ENDIF
WRTRAU99  RETURN
.
.******************************************************************************
.      write a record to the Referral Audit Table
.******************************************************************************
HEAD0000  CALL      HEAD132
          CALL      SHED0000
          PRINT     *N,*40,"Clinic Type : ",CLINICTY," - ",CTYPDESC:
                    *N,*40,"Clinic ID   : ",CLINICID," - ",CLIDDESC:
                    *N,*40,"Provider No : ",PROVNUMB
          CALL      UND132
.
          PRINT     *1,"|U/R Number",*12,"|Patient Name",*40,"|Medicare":
                    *52,"|Clinic Type",*74,"|Clinic ID":
                    *96,"|Referrals Type",*118,"|Expiry Date",*132,"|"
.
          ADD       SIX,CLNO
          CALL      UND132
HEAD9999  RETURN
+
.******************************************************************************
.      Set heading
.******************************************************************************
SHED0000  MOVE      SP70,CTYPDESC
          PACK      KEY12,USERSITE,CLINICTY,SP70
          CALL      RDCTYA1                  * Read Clinic Type File
          IF        OVRCD=0
            UNPACK    OCTDESC,CTYPDESC
          ENDIF
.
.         Get description of clinic id
.
          MOVE      SP70,CLIDDESC
          PACK      KEY20,USERSITE,CLINICID,Z70
          CALL      RDSCLIA1
          CALL      RDPCLIA1
          IF        OVRCD=0
            MATCH     USERSITE,OCASITE
            IF        @EQUAL
              MATCH     ALRECLID,OCACLIN
              IF        @EQUAL
                UNPACK    OCADESC,CLIDDESC
              ENDIF
            ENDIF
          ENDIF
SHED9999  RETURN
