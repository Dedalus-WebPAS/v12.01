                                  PRFNPR07,PRFNPR08,PRFNPR09:
                                  PRFNPR10,PRFNPR11,PRFNPR12:
                                  PRFNPR13,PRFNLAST
.
          ADD       FSTAMNT,PRFNPERA
.
          IF        FGSTFLAG=1
            CALL      UPPRFG1                 * Update the record
          ELSE
            CALL      UPPRFN1                 * Update the record
          ENDIF
          CALL      RELSLK00        * Release System Lock Audits
.
          GOTO      PRYFS999
