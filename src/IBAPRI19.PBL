          CALL      LINE0000                     * end of section line
.
          PRINT     *1,PIPE,*33,PIPE,*38,"PERIOD",*46,PIPE,*53,"Y.T.D.":
                    *59,PIPE,*60,MDESC1,*132,PIPE 
