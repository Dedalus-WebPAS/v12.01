            GOTO      YNZM1000 IF NOT EQUAL
          ENDIF
.
          MATCH     SP70,FROMEDAT          * Effective Date
          IF        !@EQUAL
            MATCH     FROMEDAT,NZMCEFDT
            GOTO      YNZM1000 IF NOT EQUAL
          ENDIF
.
YNZM7500  IF        INCRFLAG=1
            ADD       INCRAMNT,NZMCPATP    * Add fix dollar 
          ENDIF 
