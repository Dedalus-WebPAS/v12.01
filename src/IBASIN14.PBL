          IF       OVRCD=1
            CALL     RPSICE1 
          ENDIF
          MOVE     ZERO,SECTOR
          WHILE    SECTOR<SICE1SNO
            CALL     RPSICE1 
            IF       OVRCD=1
              MOVE     SICE1SNO,SECTOR
            ENDIF
            PACK     SICE1MKY,SICECST,SICEDAT,SICECOM                                                        
            MATCH    SICE1EKY,SICE1MKY
            IF       !@EQUAL
              MOVE     SICE1SNO,SECTOR
