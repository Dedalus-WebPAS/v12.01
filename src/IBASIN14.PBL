              IF        SICEAMT<>0 & SICEBUD=0
                MOVE      HUNDRED,PVAPERXX
              ELSE
                ASSIGN    (VARPERXX/SICEBUD)*100.0,PVAPERXX
              ENDIF
            ENDIF
          ENDIF
          GOTO      ECOMY999
.
ECOMY990  MOVE      TWO,EXIT
.
