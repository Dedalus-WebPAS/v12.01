            MOVE     ONE,SIFACTOR
          ELSE
            MOVE     SIPCCAT,KEY7
            CALL     RDSIIA1
            PACK     ITEMDESC,SIIADES
            PACK     KEY30,SIIAISS,SIPCSUT
            CALL     RDSIFA1
            IF       OVRCD=1
              MOVE     ONE,SIFACTOR
            ENDIF
          ENDIF
          ASSIGN   SIPCOQT*SIFACTOR,ORDQTY
          ASSIGN   SIPCECT*SIPCOQT,F12P2
          IF       SIPCECT<0
            MOVE     "      T.B.A,",ITEMCOST
          ELSE
            MOVE     F12P2,F9P2
            MOVE     F9P2,ITEMCOST
          ENDIF
          MOVE     SIPCPON,KEY7
          CALL     RDSIPA1
          MOVE     SIPASUP,KEY5
          CALL     RDAPMA1
