          MOVE     ZERO,SECTOR
          IF       SICE1CNT=SICE1SNO
            SUB      ONE,SICE1CNT
          ENDIF
          WHILE    SECTOR<=SICE1CNT
            CALL     RPSICE1 
            IF       OVRCD=1
              MOVE     SICE1SNO,SECTOR
            ENDIF
            PACK     SICE1MKY,SICECST,SICEDAT,SICECOM                                                        
            MATCH    SICE1EKY,SICE1MKY
