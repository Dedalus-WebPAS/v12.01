          ENDIF
          PACK     KEY10,SIPCPON,SIPCITM
          CALL     RDSIPC1
          IF       OVRCD=1
            BEEP
            MOVE     "Purchase Not Found - ",DISPMSG
            CALL     MESSAGE1
            GOTO     SELORD99
          ENDIF
          MATCH    SP70,SIPCCAT
          IF       @EQUAL
