          CALL      RSEMVCD2
SETBRQ10  CALL      RKEMVCD2
          BRANCH    OVRCD,SETBRQ40
.
          MATCH     ADMISSNO,EMVCVIST
          GOTO      SETBRQ40 IF NOT EQUAL
.
          MATCH     "005",EMVCTYPE
          GOTO      SETBRQ40 IF NOT EQUAL
.
          MATCH     "000",EMVCSEQU
          GOTO      SETBRQ40 IF NOT EQUAL
.
          MOVE      EMVCMNCD,DIM10
          MOVE      EMVCFTXT,DIM100
.
          GOTO      SETBRQ50
.
