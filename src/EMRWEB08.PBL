              MOVE      EMVIDDAT,EMHIDSD            *New nurse seen date/time
              REP       " 0",EMHIDSD                *set discharge date/time
              UNPACK    EMVIDTIM,HOUR,D1,MIN,D1,SEC
              PACK      EMHIDST,HOUR,MIN,SEC
              REP       " 0",EMHIDST
              GOTO      WRTHIS30
.
WRTHIS18      PACK      EMHIDSD,CCC,CYY,CMM,CDD * New doctor so seen date/time
              REP       " 0",EMHIDSD            * are set the current/time
              PACK      EMHIDST,HOUR,MIN,SEC
              REP       " 0",EMHIDST
              GOTO      WRTHIS30
            ENDIF
          ENDIF
.
WRTHIS30  MATCH     SAVENURS,EMVIATNS       * Is this a new nurse?
          IF        @EQUAL
            MOVE      SAVENRDT,EMHINSD      * Restore current nurse date and
            MOVE      SAVENRTM,EMHINST      * time
          ELSE
            MATCH     "1",EMVITRIG          * Triage cat of RED
            IF        @EQUAL
              MATCH     SP70,SAVENURS
              IF        @EQUAL
                MOVE      EMVIDATE,EMHINSD   * Make seen date/time the same as
                UNPACK    EMVITIME,HOUR,D1,MIN,D1,SEC * arrival date/time if
                PACK      EMHINST,HOUR,MIN,SEC        * first doctor
                REP       " 0",EMHINST
              ELSE
                MATCH     SP8,EMVIDDAT                 * Check Discharged
                GOTO      WRTHIS35 IF EQUAL            * yes
.
                MOVE      EMVIDDAT,EMHINSD            *New nurse seen date/time
                REP       " 0",EMHINSD                *set discharge date/time  
.
                UNPACK    EMVIDTIM,HOUR,D1,MIN,D1,SEC
                PACK      EMHINST,HOUR,MIN,SEC
                REP       " 0",EMHINST
                GOTO      WRTHIS40

WRTHIS35        PACK      EMHINSD,CCC,CYY,CMM,CDD *New nurse so seen date/time
                REP       " 0",EMHINSD            *are set the current and time
                PACK      EMHINST,HOUR,MIN,SEC
                REP       " 0",EMHINST
                GOTO      WRTHIS40
              ENDIF
            ELSE
              MATCH     SP8,EMVIDDAT                 * Check Discharged
              GOTO      WRTHIS38 IF EQUAL            * not discharged
.
              MOVE      EMVIDDAT,EMHINSD            *New nurse seen date/time
              REP       " 0",EMHINSD                *set discharge date/time
              UNPACK    EMVIDTIM,HOUR,D1,MIN,D1,SEC
