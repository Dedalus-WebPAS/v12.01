              IF        @EQUAL
                MOVE      EMVIDATE,EMHIDSD   * Make seen date/time the same as
                UNPACK    EMVITIME,HOUR,D1,MIN,D1,SEC * arrival date/time if
                PACK      EMHIDST,HOUR,MIN,SEC        * first doctor
                REP       " 0",EMHIDST
              ELSE
                MATCH     SP8,EMVIDDAT
                GOTO      WRTHIS15 IF EQUAL
.
                MOVE      EMVIDDAT,EMHIDSD
                REP       " 0",EMHIDSD
                UNPACK    EMVIDTIM,HOUR,D1,MIN,D1,SEC
                PACK      EMHIDST,HOUR,MIN,SEC
                REP       " 0",EMHIDST
                GOTO      WRTHIS30
.
WRTHIS15        PACK      EMHIDSD,CCC,CYY,CMM,CDD * New doctor so seen date/time
                REP       " 0",EMHIDSD            * are set the current/time
                PACK      EMHIDST,HOUR,MIN,SEC
                REP       " 0",EMHIDST
