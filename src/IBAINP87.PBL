          IF        OPTION = 1
            PRINT     *30,"From ",DIM9," Date : ",FROMDATE," To : ",TODATE
          ELSE
            PRINT     *30,"All un-registered or re-registered Cancers"
          ENDIF
.
          IF        IBCNMHOS =1
            PRINT     *30,"Hospital : ",HEADHOSP,"    ",NAMEHOSP
            MOVE      SIX,CLNO
          ELSE
            MOVE      FIVE,CLNO
          ENDIF
.
          CALL      UND132
