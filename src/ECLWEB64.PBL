.         Vertical movement is required for the barcode otherwise
.         it will print above the top line of the large labels (verttab1/2)
.
VERTTAB1  INIT      "*p+30Y"    * move 30 PCL units down (for barcode)
VERTTAB2  INIT      "*p-30Y"    * move 30 PCL units up (for barcode)
VERTTAB3  INIT      "*p+5Y"     * move 5  PCL units up (vertical space between
VERTTAB5  INIT      "*p+5Y"     * move 2  PCL units up (vertical space between
CONST1    INIT      "ED  UR"
CONST2    INIT      "A/T"
CONST3    INIT      "GP:"
.-------------------------------------------------------------------------------
NSETBOLD  INIT      "(s0bT"
SETBOLD   INIT      "(s7bT"
.
ALRCOMM   DIM       100
MULTILIN  FORM      1  
.-------------------------------------------------------------------------------
PRGID     INIT      "ECLWEB64"
VERSION   INIT      "V12.01.00"
PRGNAM    INIT      "Medicare WebServices OECW - ECM Polling Response Server"
.-------------------------------------------------------------------------------
.
          CALL      WEBINT00       * Initialise Fifo Etc.
.
          CALL      INIT0000       * Open Files
.
MAIN0000  COMMIT
.
          BEGIN
.
          DISPLAY   *W2
.
          MATCH     "1",PTCNWSIN
          GOTO      MAIN0000 IF NOT EQUAL
.
          MATCH     "1",PTCNOECW
          GOTO      MAIN0000 IF NOT EQUAL
.
          MOVE      ZERO,EXIT
.
          PACK      KEY16,SP1,ONE,SP70
          CALL      RSWBOEM2
MAIN1000  CALL      RKWBOEM2
          BRANCH    OVRCD,MAIN9999
.
          MATCH     " 1",WBOESTAT                * Processed Record?
          GOTO      MAIN9999 IF NOT EQUAL
.
          MATCH     SP70,WBOETRID
          GOTO      MAIN1000 IF EQUAL
.
          PACK      KEY71,WBOEHOSP,SP70          * Read B2B Device Table
          CALL      RSWBB2B3
          CALL      RKWBB2B3
          BRANCH    OVRCD,MAIN1000
.
          MATCH     WBOEHOSP,WBB2HOSP            * Same Hospital
          GOTO      MAIN1000 IF NOT EQUAL
.
          MATCH     "ACTIVE",WBB2DSTA            * Device Active
          GOTO      MAIN1000 IF NOT EQUAL
.
          MATCH     SP70,WBB2DPTH
          GOTO      MAIN1000 IF EQUAL
.
          CALL      PROPN000                     * Valid Properties File?
.
          MATCH     "1",PRODAIND
          GOTO      MAIN1000 IF EQUAL
.
          PACK      SAVKEY16,WBOESTAT,WBOEHOSP,WBOEVISN,WBOECNTR,SP70
.
          OPEN      WBSPROPF,PRDAPATH
.
. Parse _oecw file (OECW ECM Response JSON file)
. **************************************************
.
MAIN2000  CLEAR     CMDLINE
          APPEND    WBB2DPTH,CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    "/webPAS_claims/oecwecm/",CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    WBOETRID,CMDLINE
          APPEND    OECRLITR,CMDLINE
          RESET     CMDLINE
.
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      OECRFILE,CMDLINE
          TRAPCLR   IO
          IF        OVRCD=1
            GOTO      MAIN3000
          ENDIF
.
          CALL      OECPRS00                   * Parse OECW Response JSON
.
          GOTO      MAIN8000
.
. Parse _oecwecm_errors file (OECW ECM Stderrs/Stdout file)
. *********************************************************
.
MAIN3000  CLEAR     CMDLINE
          APPEND    WBB2DPTH,CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    "/webPAS_claims/oecwecm/",CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    WBOETRID,CMDLINE
          APPEND    OECELITR,CMDLINE
          RESET     CMDLINE
.
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      OECEFILE,CMDLINE
          TRAPCLR   IO
          IF        OVRCD=1
            MOVE      "Response files not found.",OECELINE  
            GOTO      MAIN3500
          ENDIF
.
MAIN3100  READ      OECEFILE,SEQ;OECELINE
          IF        @OVER
            MOVE      "Refer to OECW Details.",OECELINE
            GOTO      MAIN3500 
          ENDIF
.
          SCAN      "error",OECELINE
          IF        @EQUAL
            GOTO      MAIN3500
          ENDIF
.
          GOTO      MAIN3100
.
MAIN3500  PACK      KEY16,WBOESTAT,WBOEHOSP,WBOEVISN,WBOECNTR,SP70
          CALL      RDWBOEM2
          IF        OVRCD=0
            MOVE      " 3",WBOESTAT
            MOVE      OECELINE,WBOEERRD
            CALL      UPWBOEM2
          ENDIF
.
          GOTO      MAIN9000
.
. ******************************************************************************
.
MAIN8000  PACK      KEY16,WBOESTAT,WBOEHOSP,WBOEVISN,WBOECNTR,SP70
          CALL      RDWBOEM2
          IF        OVRCD=0
            MOVE      " 6",WBOESTAT
            CALL      UPWBOEM2
          ENDIF
.
MAIN9000  CLOSE     WBSPROPF
          CLOSE     OECRFILE
          CLOSE     OECEFILE
.
          GOTO      MAIN1000
.
MAIN9999  CLOSE     WBSPROPF
          CLOSE     OECRFILE
          CLOSE     OECEFILE
.
          GOTO      MAIN0000
+
.------------------------------------------------------------
.  Parse OECW Response JSON
.------------------------------------------------------------
OECPRS00  CALL      CLJSON00
.
OECPRS01  READ      OECRFILE,SEQ;OECRLINE
          GOTO      OECPRS99 IF OVER
.
. ***********************************************************
. Errors
. ***********************************************************
.
          SCAN      "highestSeverity",OECRLINE
          IF        @EQUAL
.
OECPRS05    READ      OECRFILE,SEQ;OECRLINE
            GOTO      OECPRS90 IF OVER
.
.           *************************************************
.           serviceMessage
.           *************************************************
.
            SCAN      "serviceMessage",OECRLINE
            IF        @EQUAL
.
OECPRS07      READ      OECRFILE,SEQ;OECRLINE
              GOTO      OECPRS20 IF OVER
.
.             ***********************************************
.             code
.             ***********************************************
.
              SCAN      "code",OECRLINE
              IF        @EQUAL
                RESET     OECRLINE
.               UNPACK    OECRLINE,DIM14,DIM4
.
                SCAN      ":",OECRLINE
                IF        @EQUAL
                  MOVE      OECRLINE,OECRWORK
                  REP       ": ",OECRWORK
                  REP       "#" ",OECRWORK
                  LJUSTIFY  OECRWORK
                  MOVE      OECRWORK,DIM4
                  RESET     OECRLINE
                ENDIF
.
                TYPE      DIM4
                IF        @EQUAL
                  MOVE      DIM4,JSONERCD
                ENDIF 
              ENDIF
.
.             ***********************************************
.             text
.             ***********************************************
.
              SCAN      "reason",OECRLINE
              IF        @EQUAL
                RESET     OECRLINE
.               UNPACK    OECRLINE,DIM16,JSONERRE
.
                SCAN      ":",OECRLINE
                IF        @EQUAL
                  MOVE      OECRLINE,OECRWORK
                  REP       ": ",OECRWORK
                  REP       "#" ",OECRWORK
                  LJUSTIFY  OECRWORK
                  MOVE      OECRWORK,JSONERRE
                  RESET     OECRLINE
                ENDIF
.
                REP       "#" ",JSONERRE
              ENDIF
.
              GOTO      OECPRS07
.
            ENDIF
.
OECPRS20    GOTO      OECPRS05   
.
          ENDIF
.
. ***********************************************************
. DHSEIN
. ***********************************************************
.
          SCAN      "DHSEIN",OECRLINE
          IF        @EQUAL
            RESET     OECRLINE
            UNPACK    OECRLINE,JSONERRE
            REP       "#" ",JSONERRE
          ENDIF
.
. ***********************************************************
. status
. ***********************************************************
.
          SCAN      "status",OECRLINE
          IF        @EQUAL
            RESET     OECRLINE
.           UNPACK    OECRLINE,DIM14,JSONSTAT
.
                SCAN      ":",OECRLINE
                IF        @EQUAL
                  MOVE      OECRLINE,OECRWORK
                  REP       ": ",OECRWORK
                  REP       "#" ",OECRWORK
                  LJUSTIFY  OECRWORK
                  MOVE      OECRWORK,JSONSTAT
                  RESET     OECRLINE
                ENDIF
.
            REP       "#" ",JSONSTAT
            REP       ", ",JSONSTAT
          ENDIF
.
.
. ***********************************************************
. claimSummary 
. ***********************************************************
.
          SCAN      "claimSummary",OECRLINE
          IF        @EQUAL
.
OECPRS25    READ      OECRFILE,SEQ;OECRLINE
            GOTO      OECPRS90 IF OVER
.
.           *************************************************
.           accountReferenceId
.           *************************************************
.
            SCAN      "accountReferenceId",OECRLINE
            IF        @EQUAL
              RESET     OECRLINE
.             UNPACK    OECRLINE,DIM28,JSONARID
.
                SCAN      ":",OECRLINE
                IF        @EQUAL
                  MOVE      OECRLINE,OECRWORK
                  REP       ": ",OECRWORK
                  REP       "#" ",OECRWORK
                  LJUSTIFY  OECRWORK
                  MOVE      OECRWORK,JSONARID
                  RESET     OECRLINE
                ENDIF
.
              REP       "#" ",JSONARID
            ENDIF
.
.           ***********************************************
.           close bracket
.           ***********************************************
.
            SCAN      "}",OECRLINE
            IF        @EQUAL
              GOTO      OECPRS90
            ENDIF
.
            GOTO      OECPRS25
.
          ENDIF
.
. ***********************************************************
. medicareStatus
. ***********************************************************
.
          SCAN      "medicareStatus",OECRLINE
          IF        @EQUAL
.
OECPRS30    READ      OECRFILE,SEQ;OECRLINE
            GOTO      OECPRS90 IF OVER
.
.           *************************************************
.           status
.           *************************************************
.
            SCAN      "status",OECRLINE
            IF        @EQUAL
.
OECPRS35      READ      OECRFILE,SEQ;OECRLINE
              GOTO      OECPRS50 IF OVER
.
.             ***********************************************
.             code
.             ***********************************************
.
              SCAN      "code",OECRLINE
              IF        @EQUAL
                RESET     OECRLINE
.               UNPACK    OECRLINE,DIM15,JSONMECD
.
                SCAN      ":",OECRLINE
                IF        @EQUAL
                  MOVE      OECRLINE,OECRWORK
                  REP       ": ",OECRWORK
                  REP       "#" ",OECRWORK
                  LJUSTIFY  OECRWORK
                  MOVE      OECRWORK,JSONMECD
                  RESET     OECRLINE
                ENDIF
.
                REP       ", ",JSONMECD
.               UNPACK    OECRLINE,DIM15,JSONERCD
.
                SCAN      ":",OECRLINE
                IF        @EQUAL
                  MOVE      OECRLINE,OECRWORK
                  REP       ": ",OECRWORK
                  REP       "#" ",OECRWORK
                  LJUSTIFY  OECRWORK
                  MOVE      OECRWORK,JSONERCD
                  RESET     OECRLINE
                ENDIF
.
                REP       ", ",JSONERCD
              ENDIF
.
.             ***********************************************
.             text
.             ***********************************************
.
              SCAN      "text",OECRLINE
              IF        @EQUAL
                RESET     OECRLINE
.               UNPACK    OECRLINE,DIM16,JSONMETX
.
                SCAN      ":",OECRLINE
                IF        @EQUAL
                  MOVE      OECRLINE,OECRWORK
                  REP       ": ",OECRWORK
                  REP       "#" ",OECRWORK
                  LJUSTIFY  OECRWORK
                  MOVE      OECRWORK,JSONMETX
                  RESET     OECRLINE
                ENDIF
.
                REP       "#" ",JSONMETX
.               UNPACK    OECRLINE,DIM16,JSONERRE
.
                SCAN      ":",OECRLINE
                IF        @EQUAL
                  MOVE      OECRLINE,OECRWORK
                  REP       ": ",OECRWORK
                  REP       "#" ",OECRWORK
                  LJUSTIFY  OECRWORK
                  MOVE      OECRWORK,JSONERRE
                  RESET     OECRLINE
                ENDIF
.
                REP       "#" ",JSONERRE
              ENDIF
.
.             ***********************************************
.             close bracket
.             ***********************************************
.
              SCAN      "}",OECRLINE
              IF        @EQUAL
                GOTO      OECPRS50
              ENDIF
.
              GOTO      OECPRS35
.
            ENDIF
.
.           ***********************************************
.           close bracket
.           ***********************************************
.
            SCAN      "}",OECRLINE
            IF        @EQUAL
              GOTO      OECPRS90
            ENDIF
.
OECPRS50    GOTO      OECPRS30
.
          ENDIF
.
. ***********************************************************
. medicareClaimEstimation
. ***********************************************************
.
          SCAN      "medicareClaimEstimation",OECRLINE
          IF        @EQUAL
.
OECPRS60    READ      OECRFILE,SEQ;OECRLINE
            GOTO      OECPRS90 IF OVER
.
.           *************************************************
.           medicalEvent
.           *************************************************
.
            SCAN      "medicalEvent",OECRLINE
            IF        @EQUAL
.
OECPR60A      READ      OECRFILE,SEQ;OECRLINE
              GOTO      OECPRS70 IF OVER
.
.             ***********************************************
.             id
.             ***********************************************
.
              SCAN      "id",OECRLINE
              IF        @EQUAL
                RESET     OECRLINE
.               UNPACK    OECRLINE,DIM14,JSONMEID
.
                SCAN      ":",OECRLINE
                IF        @EQUAL
                  MOVE      OECRLINE,OECRWORK
                  REP       ": ",OECRWORK
                  REP       "#" ",OECRWORK
                  LJUSTIFY  OECRWORK
                  MOVE      OECRWORK,JSONMEID
                  RESET     OECRLINE
                ENDIF
.
              ENDIF
.
.             *************************************************
.             service
.             *************************************************
.
              SCAN      "service",OECRLINE
              IF        @EQUAL
.
                MOVE      ONE,FORM2
.
OECP60AA        READ      OECRFILE,SEQ;OECRLINE
                GOTO      OECPR60Z IF OVER
.
                COMPARE   FORM2,FIVE
                GOTO      OECP60AY IF LESS
.
.               ***********************************************
.               chargeAmount
.               ***********************************************
.
                SCAN      "chargeAmount",OECRLINE
                IF        @EQUAL
                  RESET     OECRLINE
.                 UNPACK    OECRLINE,DIM26,JSONSVCA[FORM2]
.
                  SCAN      ":",OECRLINE
                  IF        @EQUAL
                    MOVE      OECRLINE,OECRWORK
                    REP       ": ",OECRWORK
                    REP       "#" ",OECRWORK
                    LJUSTIFY  OECRWORK
                    MOVE      OECRWORK,JSONSVCA[FORM2]
                    RESET     OECRLINE
                  ENDIF
.
                  REP       "#" ",JSONSVCA[FORM2]
                  REP       ", ",JSONSVCA[FORM2]
                ENDIF
.
.               ***********************************************
.               dateOfService
.               ***********************************************
.
                SCAN      "dateOfService",OECRLINE
                IF        @EQUAL
                  RESET     OECRLINE
.                 UNPACK    OECRLINE,DIM27,JSONSVDT[FORM2]
.
                  SCAN      ":",OECRLINE
                  IF        @EQUAL
                    MOVE      OECRLINE,OECRWORK
                    REP       ": ",OECRWORK
                    REP       "#" ",OECRWORK
                    LJUSTIFY  OECRWORK
                    MOVE      OECRWORK,JSONSVDT[FORM2]
                    RESET     OECRLINE
                  ENDIF
.
                ENDIF
.
.               ***********************************************
.               benefit
.               ***********************************************
.
                SCAN      "benefit",OECRLINE
                IF        @EQUAL
                  RESET     OECRLINE
.                 UNPACK    OECRLINE,DIM20,JSONSVBE[FORM2]
.
                  SCAN      ":",OECRLINE
                  IF        @EQUAL
                    MOVE      OECRLINE,OECRWORK
                    REP       ": ",OECRWORK
                    REP       "#" ",OECRWORK
                    LJUSTIFY  OECRWORK
                    MOVE      OECRWORK,JSONSVBE[FORM2]
                    RESET     OECRLINE
                  ENDIF
.
                  REP       ", ",JSONSVBE[FORM2]
                ENDIF
.
.               ***********************************************
.               explanationCode
.               ***********************************************
.
OECP6AAB        SCAN      "#"explanationCode#"",OECRLINE
                IF        @EQUAL
                  RESET     OECRLINE
.                 UNPACK    OECRLINE,DIM25,JSONSVEC[FORM2]
.
                  SCAN      ":",OECRLINE
                  IF        @EQUAL
                    MOVE      OECRLINE,OECRWORK
                    REP       ": ",OECRWORK
                    REP       "#" ",OECRWORK
                    LJUSTIFY  OECRWORK
                    MOVE      OECRWORK,JSONSVEC[FORM2]
                    RESET     OECRLINE
                  ENDIF
.
                  REP       "#" ",JSONSVEC[FORM2]
                  REP       ", ",JSONSVEC[FORM2]
                ENDIF
.
.               ***********************************************
.               scheduleFee
.               ***********************************************
.
                SCAN      "scheduleFee",OECRLINE
                IF        @EQUAL
                  RESET     OECRLINE
.                 UNPACK    OECRLINE,DIM32,JSONSVSF[FORM2]
.
                  SCAN      ":",OECRLINE
                  IF        @EQUAL
                    MOVE      OECRLINE,OECRWORK
                    REP       ": ",OECRWORK
                    REP       "#" ",OECRWORK
                    LJUSTIFY  OECRWORK
                    MOVE      OECRWORK,JSONSVSF[FORM2]
                    RESET     OECRLINE
                  ENDIF
.
                  REP       ", ",JSONSVSF[FORM2]
.
                ENDIF
.
.               ***********************************************
.               itemNumber
.               ***********************************************
.
                SCAN      "itemNumber",OECRLINE
                IF        @EQUAL
                  RESET     OECRLINE
.                 UNPACK    OECRLINE,DIM24,JSONSVIT[FORM2]
.
                  SCAN      ":",OECRLINE
                  IF        @EQUAL
                    MOVE      OECRLINE,OECRWORK
                    REP       ": ",OECRWORK
                    REP       "#" ",OECRWORK
                    LJUSTIFY  OECRWORK
                    MOVE      OECRWORK,JSONSVIT[FORM2]
                    RESET     OECRLINE
                  ENDIF
.
                  REP       "#" ",JSONSVIT[FORM2]
                  REP       ", ",JSONSVIT[FORM2]
                ENDIF
.
.               ***********************************************
.               id
.               ***********************************************
.
                SCAN      "id",OECRLINE
                IF        @EQUAL
                  RESET     OECRLINE
.                 UNPACK    OECRLINE,DIM16,JSONSVID[FORM2]
.
                  SCAN      ":",OECRLINE
                  IF        @EQUAL
                    MOVE      OECRLINE,OECRWORK
                    REP       ": ",OECRWORK
                    REP       "#" ",OECRWORK
                    LJUSTIFY  OECRWORK
                    MOVE      OECRWORK,JSONSVID[FORM2]
                    RESET     OECRLINE
                  ENDIF
.
                ENDIF
.
.               ***********************************************
.               close bracket with comma
.               ***********************************************
.
                SCAN      "},",OECRLINE
                IF        @EQUAL
                  ADD       ONE,FORM2
                  GOTO      OECP60AA
                ENDIF
.
.               ***********************************************
.               close square bracket
.               ***********************************************
.
OECP60AY        SCAN      "]",OECRLINE
                IF        @EQUAL
                  GOTO      OECPR60Z
                ENDIF
.
OECP60AZ        GOTO      OECP60AA
.
              ENDIF
.
.             ***********************************************
.             close square bracket
.             ***********************************************
.
              SCAN      "]",OECRLINE
              IF        @EQUAL
                GOTO      OECPRS70
              ENDIF
.
OECPR60Z      GOTO      OECPR60A
.
            ENDIF
.
.           ***********************************************
.           fundLocationId
.           ***********************************************
.
            SCAN      "fundLocationId",OECRLINE
            IF        @EQUAL
              RESET     OECRLINE
.             UNPACK    OECRLINE,DIM28,JSONMEFL
.
              SCAN      ":",OECRLINE
              IF        @EQUAL
                MOVE      OECRLINE,OECRWORK
                REP       ": ",OECRWORK
                REP       "#" ",OECRWORK
                LJUSTIFY  OECRWORK
                MOVE      OECRWORK,JSONMEFL
                RESET     OECRLINE
              ENDIF
.
              REP       "#" ",JSONMEFL
              REP       ", ",JSONMEFL
            ENDIF
.
.           ***********************************************
.           lodgementDate
.           ***********************************************
.
            SCAN      "lodgementDate",OECRLINE
            IF        @EQUAL
              RESET     OECRLINE
.             UNPACK    OECRLINE,DIM28,JSONMELD
.
              SCAN      ":",OECRLINE
              IF        @EQUAL
                MOVE      OECRLINE,OECRWORK
                REP       ": ",OECRWORK
                REP       "#" ",OECRWORK
                LJUSTIFY  OECRWORK
                MOVE      OECRWORK,JSONMELD
                RESET     OECRLINE
              ENDIF
.
              REP       "#" ",JSONMELD
              REP       ", ",JSONMELD
.
            ENDIF
.
.           ***********************************************
.           close bracket
.           ***********************************************
.
            SCAN      "}",OECRLINE
            IF        @EQUAL
              GOTO      OECPRS90
            ENDIF
.
OECPRS70    GOTO      OECPRS60
.
          ENDIF
.
. ***********************************************************
.
OECPRS90  GOTO      OECPRS01
.
OECPRS99  CALL      OECTAB00
.
          RETURN
