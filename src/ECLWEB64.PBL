.---------------------------------------------------------------
. Clear JSON Work Variables
.---------------------------------------------------------------
CLJSON00  MOVE      SP70,JSONSTAT
          MOVE      SP70,JSONARID
          MOVE      SP70,JSONMECD
          PACK      JSONMETX,SP70,SP70,SP70,SP70,SP70,SP70,SP70,SP70,SP70,SP70:
                             SP70,SP70,SP70,SP70,SP70,SP70,SP70,SP70,SP70,SP70
          MOVE      SP70,JSONMEMN
          MOVE      SP70,JSONMEMR
          MOVE      SP70,JSONMEGN
          MOVE      SP70,JSONMEFL
          MOVE      SP70,JSONMELD
          MOVE      SP70,JSONMEID
.
          MOVE      SP70,JSONHISV
          MOVE      SP70,JSONERCD
          MOVE      SP70,JSONERSV
          PACK      JSONERRE,SP70,SP70,SP70,SP70,SP70,SP70,SP70,SP70,SP70,SP70
.
          MOVE      ONE,F2
.
          WHILE     F2 <= 50
.
            MOVE      SP70,JSONSVID[F2]
            MOVE      SP70,JSONSVBE[F2]
            MOVE      SP70,JSONSVCA[F2]
            MOVE      SP70,JSONSVDT[F2]
            MOVE      SP70,JSONSVEC[F2]
            MOVE      SP70,JSONSVIT[F2]
            MOVE      SP70,JSONSVSF[F2]
.
            ADD       ONE,F2
          DO    
.
CLJSON99  RETURN
.---------------------------------------------------------------
. Write/Update OEC Response Tables
.---------------------------------------------------------------
OECTAB00  PACK      KEY16,WBOESTAT,WBOEHOSP,WBOEVISN,WBOECNTR,SP70
          CALL      RDWBOEM2
          IF        OVRCD=0
.
            MOVE      JSONERCD,WBOEERRC
            MOVE      JSONERRE,WBOEERRD
            CALL      UPWBOEM2
.
          ENDIF
.
          MATCH    SP70,JSONSTAT
          GOTO     OECTAB99 IF EQUAL
.
          PACK     KEY23,WBOEARID,WBOECNTR,SP70
          CALL     RDWBOMG1
          IF       OVRCD=0
.
            MOVE     WBOETRID,WBOGCTID
.
            MOVE     JSONSTAT,WBOGSTAT
.
            MOVE     JSONMECD,WBOGMECD
.
            MOVE     JSONMETX,WBOGMETX
.
            MOVE     JSONMEMN,WBOGMEMN
.
            MOVE     JSONMEMR,WBOGMEMR
.
            MOVE     JSONMEGN,WBOGMEGN
.
            MOVE     JSONMEFL,WBOGFLID
.
            REP      "- ",JSONMELD
            SQUEEZE  JSONMELD
            LJUSTIFY JSONMELD
            MOVE     JSONMELD,WBOGLDDT
.
            CALL     IBACLOCK
            PACK     WBOGUDTE,CCC,CYY,CMM,CDD
            REP       " 0",WBOGUDTE
            MOVE      CTIMEIS,WBOGUTIM
.
            MOVE     WBOEMSID,WBOGMSID
.
            CALL     UPWBOMG1
.
          ELSE
.
            CALL     CLWEBOMG
.
            MOVE     WBOEARID,WBOGARID
.
            MOVE     WBOECNTR,WBOGRPTC
.
            MOVE     WBOETRID,WBOGCTID
.
            MOVE     JSONSTAT,WBOGSTAT
.
            MOVE     JSONMECD,WBOGMECD
.
            MOVE     JSONMETX,WBOGMETX
.
            MOVE     JSONMEMN,WBOGMEMN
.
            MOVE     JSONMEMR,WBOGMEMR
.
            MOVE     JSONMEGN,WBOGMEGN
.
            MOVE     JSONMEFL,WBOGFLID
.
            REP      "- ",JSONMELD
            SQUEEZE  JSONMELD
            LJUSTIFY JSONMELD
            MOVE     JSONMELD,WBOGLDDT
.
            CALL     IBACLOCK
            PACK     WBOGCDTE,CCC,CYY,CMM,CDD
            REP       " 0",WBOGCDTE
            MOVE      CTIMEIS,WBOGCTIM
.
            MOVE     WBOEMSID,WBOGMSID
.
            CALL     WRWBOMG1
.
          ENDIF
.
          MATCH    SP70,JSONMEID
          IF       !@EQUAL
.
            PACK     KEY25,WBOEARID,WBOECNTR,SP1,ONE,SP70
            CALL     RDWBOMH1
            IF       OVRCD=0
.
              MOVE     JSONMEID,WBOHMEID
.
              MOVE     WBOETRID,WBOHCTID
. 
              MOVE     WBOEMSID,WBOHMSID
.
              CALL     UPWBOMH1
.
            ELSE
.
              CALL     CLWEBOMH
.
              MOVE     WBOEARID,WBOHARID
.
              MOVE     WBOECNTR,WBOHRPTC
.
              MOVE     " 1",WBOHMECT
.
              MOVE     JSONMEID,WBOHMEID
.
              MOVE     WBOETRID,WBOHCTID
.
              MOVE     WBOEMSID,WBOHMSID 
.
              CALL     WRWBOMH1 
.
            ENDIF
.
          ENDIF
.
OECTAB90  MOVE      ONE,F2
.
          WHILE     F2 <= 50
.
            MATCH     SP70,JSONSVID[F2]
            GOTO      OECTAB99 IF EQUAL
.
            MOVE      F2,DIM4
            RJUSTIFY  DIM4
.
            PACK      KEY29,WBOEARID,WBOECNTR,SP1,ONE,DIM4,SP70
            CALL      RDWBOMI1
            IF        OVRCD=0
.
              MOVE      JSONSVID[F2],WBOISVID
.
              MOVE      JSONSVBE[F2],WBOISVBE
.
              MOVE      JSONSVCA[F2],WBOISVCH
.
              UNPACK    JSONSVDT[F2],CCENT,CYEAR,DIM1,CMON,DIM1,CDAY
              PACK      WBOISVDS,CCENT,CYEAR,CMON,CDAY
              REP       " 0",WBOISVDS
.
              MOVE      JSONSVEC[F2],WBOISVEC
.
              MOVE      JSONSVIT[F2],WBOISVIT
.
              MOVE      JSONSVSF[F2],WBOISVSC
.
              MOVE      WBOETRID,WBOICTID
.
              MOVE      WBOEMSID,WBOIMSID
.
              CALL      UPWBOMI1
.
            ELSE
.
              CALL      CLWEBOMI
.
              MOVE      WBOEARID,WBOIARID
.
              MOVE      WBOECNTR,WBOIRPTC
.
              MOVE      " 1",WBOIMECT
.
              MOVE      DIM4,WBOISVCT
.
              MOVE      JSONSVID[F2],WBOISVID
.
              MOVE      JSONSVBE[F2],WBOISVBE
.
              MOVE      JSONSVCA[F2],WBOISVCH
.
              UNPACK    JSONSVDT[F2],CCENT,CYEAR,DIM1,CMON,DIM1,CDAY
              PACK      WBOISVDS,CCENT,CYEAR,CMON,CDAY
              REP       " 0",WBOISVDS
.
              MOVE      JSONSVEC[F2],WBOISVEC
.
              MOVE      JSONSVIT[F2],WBOISVIT
.
              MOVE      JSONSVSF[F2],WBOISVSC
.
              MOVE      WBOETRID,WBOICTID
.
              MOVE      WBOEMSID,WBOIMSID
.
              CALL      WRWBOMI1 
.
            ENDIF            
.
            ADD       ONE,F2  
          DO
.
OECTAB99  RETURN
