.
INIT0000
         CALL      DISPHEAD
         DISPLAY   *P57:24,"Opening bokletrf"
         OPEN      BOKLETR1,"bokletrf"
         DISPLAY   *P65:24,"bokletrf"
         OPEN      BOKLETR2,"bokletrf"
