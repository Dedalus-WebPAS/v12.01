         MOVE      DIM70,ANS
         REPLACE   REPSTR,ANS
         MATCH     "0",ANS
         GOTO      GETV9999 IF NOT EQUAL
         GOTO      GETV1000
.
GETV9000
         MOVE      TRUE,EXIT
.
GETV9999
         RETURN
