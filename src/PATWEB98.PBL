            COMPARE   PTSTATUS,ASTAT       * Is the status selected
            IF        !@EQUAL
              GOTO      FHRLST10
            ENDIF
          ENDIF
.
          ADD       ONE,RECNUMB
          IF        STARTNUM>=RECNUMB
            GOTO      FHRLST10
          ENDIF
          IF        DISNUMB>0
            WRITE     HTMLFILE;*+,",";
