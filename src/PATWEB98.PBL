          GOTO      TRNSUP11 IF EQUAL
.
          MATCH     ANSM,TCINDC3
          GOTO      TRNSUP11 IF EQUAL
.
          MATCH     ANSM,TCINDC4
          GOTO      TRNSUP11 IF EQUAL
.
          MATCH     ANSM,TCINDC5
          GOTO      TRNSUP11 IF EQUAL
