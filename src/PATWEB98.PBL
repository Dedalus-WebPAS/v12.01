          ENDIF
.
          MATCH     ANSD,TMOVE
          IF        @EQUAL
            APPEND    "javascript:DispDscError()",HTMLLINK
            GOTO      TRNSUP15
          ENDIF
.
          MATCH     ANSC,TMOVE
