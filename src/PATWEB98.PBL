                             "#"",TRATEF,"#",":            * 5
                             "#"";
          IF        TRNSUPTY=3
            IF        RECCNT<>1 | F1=1
            WRITE     HTMLFILE;"<input type=checkbox name=uptrn",DRECCNT:
                      " onClick=UpdateArray(this.value); value='",BEDTRKEY,"'>";
