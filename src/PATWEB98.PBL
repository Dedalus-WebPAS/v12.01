          WRITE     HTMLFILE;"t.addRow(#"",HTMLLINK,"#",": * 0
                             "#"",TDATE,TTIME,"#",":       * 1
                             "#"",WARDNAME,"#",":          * 2
                             "#"",TBED,"#",":              * 3
                             "#"";
          IF        TRNSUPTY=1
