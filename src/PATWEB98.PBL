          MOVE      SP70,KEY5
          IF        PVITYPE=1
            MATCH      SP70,EMVIDDAT
            GOTO       GFREDT99 IF EQUAL
            UNPACK     EMVIDDAT,CCENT,CYEAR,CMON,CDAY
            MOVE       EMVIDTIM,KEY5
            GOTO       GFREDT90              * EMR
          ENDIF
