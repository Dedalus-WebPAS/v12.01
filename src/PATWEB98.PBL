.-----------------------------------------------------------------------------
.         Validate Transfer Date/Time
.-----------------------------------------------------------------------------
VALDDT00  MOVE      ZERO,OVRCD
          MATCH     "C",TMOVE
          GOTO      VALDDT99 IF NOT EQUAL    * Not a change record
.
          UNPACK    TRANSKEY,KEY8,KEY16,SAVWRDBD  * Admission,Date/Time,Ward/Bed
.
.         Check if this is the last transfer record for the admission
.
          CALL      RDKTRAN2
          BRANCH    OVRCD,VALDDT10
.
          MATCH     DTADMN,KEY8           * check same admision no ?
          GOTO      VALDDT10 IF NOT EQUAL
.
.         This is not the last Transfer record
.         Check if the change date/time is after the Last rec. in transfer file
.
          PACK      KEY30,KEY8,Z70
          CALL      RDSTRAN2
          CALL      RDPTRAN2
          BRANCH    OVRCD,VALDDT80
.
          MATCH     DTADMN,KEY8          * check same admision no ?
          GOTO      VALDDT80 IF NOT EQUAL
.
.         The new transfer date/time must be before the last Date/time change
.
          MATCH     TDATE,TRNTODAT
          GOTO      VALDDT80 IF LESS
          GOTO      VALDDT91 IF NOT EQUAL
.
          MATCH     TTIME,TRNTOTIM
          GOTO      VALDDT91 IF NOT LESS
.
          GOTO      VALDDT80
.
.         This is the last Transfer record
.         Check for previous Ward/bed or Adm.type change
.
VALDDT10  PACK      KEY30,TRANSKEY,SP70
          CALL      RDTRAN2              * reposition
          BRANCH    OVRCD,VALDDT80
.
          MOVE      ATYPE,SAVATYPE       * save Adm.type
.
          CALL      RDPTRAN2
          BRANCH    OVRCD,VALDDT80
.
          MATCH     DTADMN,KEY8          * check same admision no ?
          GOTO      VALDDT80 IF NOT EQUAL
.
          PACK      KEY6,TWARD,TBED
          MATCH     KEY6,SAVWRDBD        * check ward/bed
          GOTO      VALDDT20 IF NOT EQUAL
.
          MATCH     TATYPE,SAVATYPE      * check admission type
          GOTO      VALDDT10 IF EQUAL
.
.         The new transfer date/time must be after the previous change
.
VALDDT20  MATCH     TRNTODAT,TDATE
          GOTO      VALDDT80 IF LESS
          GOTO      VALDDT92 IF NOT EQUAL
.
          MATCH     TRNTOTIM,TTIME
          GOTO      VALDDT92 IF NOT LESS
.
VALDDT80  PACK      KEY30,TRANSKEY,SP70  
          CALL      RDTRAN2              * reposition
.
          MOVE      ZERO,OVRCD
          GOTO      VALDDT99
.
VALDDT91  MOVE      ONE,OVRCD
          GOTO      VALDDT99
.
VALDDT92  MOVE      TWO,OVRCD
          GOTO      VALDDT99
.
VALDDT99  RETURN
