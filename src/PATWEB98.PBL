          ADD       ONE,RECCNT
          COMPARE   FIFTY,RECCNT
          IF        @EQUAL
            MOVE      ONE,FINFLAG
            WRITE     HTMLFILE;"alert('Too Many Visits to View');"
            GOTO      TACLST99
          ENDIF
.
          GOTO      TACLST10
.
