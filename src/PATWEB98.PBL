            GOTO      ALLVIS95 IF NOT EQUAL
          ENDIF
.
          COMPARE   TEN1,FILTVTYP             * Allied Health Visit Types
          IF        @EQUAL
            MATCH     "10",PTVITYPE 
            GOTO      ALLVIS95 IF NOT EQUAL
.
            PACK     KEY8,PVIBILL,SP70       * Waiting and Active Referrals
            CALL     RDALREF1
            BRANCH   OVRCD,ALLVIS10
.
            CALL     INDHCP00
.
            MATCH    "0",ALRESTAT
            IF       !@EQUAL
              MATCH    "1",ALRESTAT          
              GOTO      ALLVIS10 IF NOT EQUAL
            ENDIF
          ENDIF
