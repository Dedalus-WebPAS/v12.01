                            "UpdateArray(this.value);":
                            " value='",BEDTRKEY,"' disabled>";
                ELSE
                  WRITE     HTMLFILE;"<input type=checkbox name=uptrn",DRECCNT:
                            " onClick=showFundingSrc(this.checked);":
