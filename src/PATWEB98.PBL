          IF        !@EQUAL
            PACK      KEY6,BKRXPOWD,SP70
            CALL      RDWARD1
            IF        OVRCD<>1
              PACK      KEY6,BKRXPOWD,BKRXPOBD,SP70
              PACK      KEY3,BKRXPOWD,SP70
              CALL      FWBD0000
              PACK      VISTLOC,WARDBED,SP70
              COMPARE   ONE,IBCNMHOS
              IF        @EQUAL
                PACK      KEY3,PTWDHOSN,SP70
