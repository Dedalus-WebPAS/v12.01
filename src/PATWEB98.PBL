          PACK      KEY8,ADMISSNO,SP70
          CALL      RDMISS1
          MOVE      "CC",KEY2
          PACK      KEY5,KEY2,ACARE,SP70
          CALL      RDCODE1
          MOVE      TDESC,CODDESC
          MOVE      SP70,LASTWBED
          MOVE      SP70,LASTDOCT
          MOVE      SP70,LASTTYPE
.
          PACK      KEY30,ADMISSNO,Z70
          CALL      RDSTRAN2
TRNINS10  CALL      RDPTRAN2
          BRANCH    OVRCD,TRNINS99
          MATCH     ADMISSNO,DTADMN
          GOTO      TRNINS99 IF NOT EQUAL
          CALL      GETTRN00
          CALL      GETCAR00              * get care type
