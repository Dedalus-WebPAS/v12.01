          APPEND    "','",HTMLLINK
          APPEND    DPVIBILL,HTMLLINK
          APPEND    "','",HTMLLINK
          APPEND    PVITYPE,HTMLLINK
          APPEND    "','",HTMLLINK
          APPEND    SECOPT,HTMLLINK
          APPEND    "')",HTMLLINK
          RESET     HTMLLINK
