                CALL      RDMLSEC1
                IF        OVRCD=1
                  PACK      KEY13,USERID,VISTHOSP,SP70   * This hospital access
                  CALL      RDMLSEC1
                  BRANCH    OVRCD,BOKLST10
                ENDIF
              ELSE
                PACK     KEY10,USERID,SP70        * Re read logged in user to
                CALL     RDWBSE1                  * get hospital
                BRANCH   OVRCD,BOKLST10
