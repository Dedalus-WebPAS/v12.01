XPSG6000  REP       " 0",PSGSEG07
          IF        FORM4 = 0
            MOVE      "PSG.04-Warning: ADM type: ",ERRMSG
            PACK      ERRMSG,ERRMSG,ATYPE
            ENDSET    ERRMSG
