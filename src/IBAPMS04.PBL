          PACK      KEY5,ANSO,ANSA,CODEOA        * no
          CALL      RDCODE1                      * code on file ?
          IF        OVRCD = 1
            IF        SAVSAMED <> 2
              MOVE      "PSG.06-Anaesthetic Type code (",ERRMSG
              ENDSET    ERRMSG
              APPEND    CODEOA,ERRMSG
              APPEND    " - Cat OA) not found",ERRMSG
              RESET     ERRMSG
