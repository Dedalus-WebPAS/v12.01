              MOVE      "PSG.09-Warning: Anaesthetic Type code (",ERRMSG
              ENDSET    ERRMSG
              APPEND    CODEOA,ERRMSG
              APPEND    " - Cat OA, Ind. 1) not numeric",ERRMSG
              RESET     ERRMSG
              CALL      PWRN0000
