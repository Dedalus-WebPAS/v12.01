          IF        OVRCD = 1
            MOVE      "GEN.02-Missing admission file record",ERRMSG
            CALL      PERR0000
          ELSE
            MOVE      LISTADMN,KEY8
            CALL      RDPMVX11                   * visit extension found ?
            IF        OVRCD = 1
              MOVE      "GEN.10-Missing visit extension record",ERRMSG
              CALL      PERR0000
            ENDIF
          ENDIF
