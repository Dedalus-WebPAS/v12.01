            MOVE      "PSG.11-Warning: Anaesthetic Type code (",ERRMSG
            ENDSET    ERRMSG
            APPEND    CODEOA,ERRMSG
            APPEND    " - Cat OA, Ind. 1) not valid",ERRMSG
            RESET     ERRMSG
            CALL      PWRN0000
          ENDIF
.
XPSG8000  BRANCH    VALDFLG,XPSG9000             * validation only
