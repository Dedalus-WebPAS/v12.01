PRTEND00  CALL      UND132
          MOVE      "999,999,999.99",PRTTOT
          FEDIT     GRANDCST,PRTTOT
          PRINT     "|Report Total Stock Value : ",*117,"|",PRTTOT,"|"
          CALL      UND132
          PRINT     *50,"***** End of Report *****"
