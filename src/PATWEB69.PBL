          MOVE      TEN9,F2
          MATCH     UPDTFLDS[F2],Z70
          IF        !@EQUAL
            UNPACK    UPDTFLDS[F2],DIM10,DIM2
            MOVE      DIM10,PMEDI                * Medicare Number
            MOVE      DIM2,PTMXMCCD              * IRN
          ENDIF
.
          MOVE      TWENTY,F2
          MATCH     UPDTFLDS[F2],Z70
          IF        !@EQUAL
            MOVE      UPDTFLDS[F2],PMPXMEDC      * Medicare Valid To Date
          ENDIF
.
MVUPDF99  RETURN
