          MOVE     "EXPRT2",KEY6
          PACK     KEY8,KEY6,PORT
          REP      " 0",KEY8
          PREP     EXPORTF2,KEY8
          WRITE    EXPORTF2,SEQ;"Supplier";
.
          MOVE     ZERO,F3
MAKEXP10  ADD      ONE,F3
          UNPACK   PERLST[F3],CCENT,CYEAR,CMON
          WRITE    EXPORTF2,SEQ;TABDELM,CMON,"/",CCENT,CYEAR;
          COMPARE  PERCNT,F3
          GOTO     MAKEXP10 IF NOT EQUAL
          WRITE    EXPORTF2,SEQ;TABDELM,"Total"
