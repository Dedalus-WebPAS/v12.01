          GOTO     MAKEXP99 IF NOT EQUAL
.
          MOVE     "EXPRT1",KEY6
          PACK     KEY8,KEY6,PORT
          REP      " 0",KEY8
          PREP     EXPORTF1,KEY8
          WRITE    EXPORTF1,SEQ;"Supplier",TABDELM:
                               "Period",TABDELM:
                               "Fill Rate",TABDELM,"No. Orders",TABDELM:
                               "No. Lines",TABDELM,"Total Cost",TABDELM:
                               "Av. Lines per Order",TABDELM:
                               "Av. Cost per Order"
