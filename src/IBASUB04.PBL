.          PRINT     *50,"ORDER EXCEPTIONS LIST"
.          PRINT     "Order/Line   Expected    Delivered    Days      ":
.                    "Order Qty       Recd Qty  Description    ":
.                    "                Part Number"
.          CALL      UND132
.
