.-------------------------------------------------------------
.
. Audit & Locking Standard Routine to Post Changes.
.
.-------------------------------------------------------------
POSA0000  PACK     KEY4,HLTATID,SP70  * Setup Key
          CALL     RAHLTA1            * Check for record on file
          IF       OVRCD=0
            CALL     UPHLTA1          * Update Changes
            CALL     UUHLTA1          * Unloc
          ELSE
            CALL     WRHLTA1          * Write New Record
          ENDIF
          RETURN                      * Return to program.
.-------------------------------------------------------------
. Cancel Table Modification
.-------------------------------------------------------------
CANA0000  PACK     KEY4,HLTATID,SP70  * Setup Key
          CALL     RAHLTA1            * Check for record on file
          IF       OVRCD=0
            CALL     UUHLTA1          * Unlock
          ENDIF
          RETURN                      * Return to program.
.
.-------------------------------------------------------------
. Audit & Locking Standard Routine to Delete Record.
.-------------------------------------------------------------
DELA0000  MOVE     SP70,DISPMSG        * Clear Line 24 Display Message
          CALL     MESSAGE2            * Save and Clear Line 24
          CALL     DELQST              * Delete (Y/N/C) CEXIT 1/2/3
          CALL     MESSAGE3            * Restore Line 24
          BRANCH   CEXIT,DELA1000,DELA9000,DELA9100
.
DELA1000  PACK     KEY4,HLTATID,SP70  * Setup Key to File
          CALL     RAHLTA1             * Read Record
          IF       OVRCD=0
            CALL     DEHLTA1           * Delete Record
          ENDIF
          MOVE     TWO,EXIT            * Restart Screen
          GOTO     DELA9999
DELA9000  MOVE     ONE,EXIT            * Return to Prompt on "No"
          GOTO     DELA9999
DELA9100  MOVE     TWO,EXIT            * Restart Screen on "Cancel"
          GOTO     DELA9999
DELA9999  RETURN
.-------------------------------------------------------------
. Audit & Locking Standard Routine to Enter Key Field
.-------------------------------------------------------------
EKEYA000  MOVE      TWO,CKEYTYP             * Record Does not have to be on file
.                                           * This allows add and change.
          CALL      KHLTAA00                * Call Standard Keyin Routine with ?
          BRANCH    EXIT,EKEYA999,EKEYA999  * Exit 1 Null Entry 2 Exitchar
          COMPARE   ONE,OVRCD               * New Record so no Lock
          GOTO      EKEYA999 IF EQUAL       * Return
.
EKEYA100  PACK      KEY4,HLTATID,SP70       * Setup Key to File
          CALL      RLHLTA1                 * Read and Lock Record
          BRANCH    OVRCD,EKEYA999,EKEYA200 * OVRCD=1 Not on File 2 Locked
          MOVE      ZERO,EXIT               * Setup Exit Field
          MOVE      ZERO,OVRCD              * Setup Ovrcd Field
          GOTO      EKEYA999
.
EKEYA200  MOVE      SP70,DISPMSG            * Clear Line 24 Message Display
          CALL      MESSAGE2                * Clear Line 24 and Save
          KEYIN     *P1:24,*EL,"Record Locked - (":
                    *V2LON,"R",*HOFF,")etry or (":
                    *V2LON,"E",*HOFF,")xit ? ",ANS
          CALL      MESSAGE3                * Restore Line 24
          REP       UPPLOW,ANS              * Convert to Upper Case
          MATCH     ANSR,ANS                * Re-try
          GOTO      EKEYA100 IF EQUAL       * Return to locking
          MATCH     ANSE,ANS                * Exit
          GOTO      EKEYA200 IF NOT EQUAL   * Must be a valid response
          GOTO      EKEYA000                * Return to Keyin
.
EKEYA999  RETURN                            * End of Routine
.-------------------------------------------------------------
. Audit & Locking Standard Routine to Post Changes.
.-------------------------------------------------------------
POSB0000  PACK     KEY14,HLCOTID,HLCOCOD,SP70     * Setup Key
          CALL     RAHLCO1            * Check for record on file
          IF       OVRCD=0
            CALL     UPHLCO1          * Update Changes
            CALL     UUHLCO1          * Unloc
          ELSE
            CALL     WRHLCO1          * Write New Record
          ENDIF
          RETURN                      * Return to program.
.-------------------------------------------------------------
. Cancel
.-------------------------------------------------------------
CANB0000  PACK     KEY14,HLCOTID,HLCOCOD,SP70     * Setup Key
          CALL     RAHLCO1            * Check for record on file
          IF       OVRCD=0
            CALL     UUHLCO1          * Unloc
          ENDIF
          RETURN                      * Return to program.
.-------------------------------------------------------------
. Audit & Locking Standard Routine to Delete Record.
.-------------------------------------------------------------
DELT0000  MOVE     SP70,DISPMSG        * Clear Line 24 Display Message
          CALL     MESSAGE2            * Save and Clear Line 24
          CALL     DELQST              * Delete (Y/N/C) CEXIT 1/2/3
          CALL     MESSAGE3            * Restore Line 24
          BRANCH   CEXIT,DELT1000,DELT9000,DELT9100
.
DELT1000  PACK     KEY14,HLCOTID,HLCOCOD,SP70  * Setup Key to File
          CALL     RAHLCO1             * Read Record
          IF       OVRCD=0
            CALL     DEHLCO1           * Delete Record
          ENDIF
          MOVE     TWO,EXIT            * Restart Screen
          GOTO     DELT9999
DELT9000  MOVE     ONE,EXIT            * Return to Prompt on "No"
          GOTO     DELT9999
DELT9100  MOVE     TWO,EXIT            * Restart Screen on "Cancel"
          GOTO     DELT9999
DELT9999  RETURN
.-------------------------------------------------------------
. Audit & Locking Standard Routine to Enter Key Field
.-------------------------------------------------------------
EKEYB000  MOVE      TWO,CKEYTYP             * Record Does not have to be on file
.                                           * This allows add and change.
          CALL      KHLCOA00                * Call Standard Keyin Routine with ?
          BRANCH    EXIT,EKEYB999,EKEYB999  * Exit 1 Null Entry 2 Exitchar
          COMPARE   ONE,OVRCD               * New Record so no Lock
          GOTO      EKEYB999 IF EQUAL       * Return
.
EKEYB100  PACK      KEY14,HLCOTID,HLCOCOD,SP70      * Setup Key to File
          CALL      RLHLCO1                 * Read and Lock Record
          BRANCH    OVRCD,EKEYB999,EKEYB200 * OVRCD=1 Not on File 2 Locked
          MOVE      ZERO,EXIT               * Setup Exit Field
          MOVE      ZERO,OVRCD              * Setup Ovrcd Field
          GOTO      EKEYB999
.
EKEYB200  MOVE      SP70,DISPMSG            * Clear Line 24 Message Display
          CALL      MESSAGE2                * Clear Line 24 and Save
          KEYIN     *P1:24,*EL,"Record Locked - (":
                    *V2LON,"R",*HOFF,")etry or (":
                    *V2LON,"E",*HOFF,")xit ? ",ANS
          CALL      MESSAGE3                * Restore Line 24
          REP       UPPLOW,ANS              * Convert to Upper Case
          MATCH     ANSR,ANS                * Re-try
          GOTO      EKEYB100 IF EQUAL       * Return to locking
          MATCH     ANSE,ANS                * Exit
          GOTO      EKEYB200 IF NOT EQUAL   * Must be a valid response
          GOTO      EKEYB000                * Return to Keyin
