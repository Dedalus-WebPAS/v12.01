.
          REP       "+ ",URNUMBER
          REP       "+ ",ADMISSNO
          MATCH     "1",PMBISTAT
          IF        @EQUAL
            MATCH     "1",PMBIPRNT
            IF        @EQUAL
              WRITE     HTMLFILE;"<td><input type=checkbox name=deltstat":
