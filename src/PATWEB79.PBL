.
          MOVE      PMSBI001,KEY4
          CALL      RDPMBIS1
          BRANCH    OVRCD,DISBIL99
.
          PACK      KEY12,PMSBI001,SP70
          CALL      RSPMBBI1
DISBIL10  CALL      RKPMBBI1
          BRANCH    OVRCD,DISBIL99
.
          MATCH     PMSBI001,PMBBSEID
          GOTO      DISBIL99 IF NOT EQUAL
.
          ADD       ONE,RECNUMB
          IF        STARTNUM>=RECNUMB
            GOTO      DISBIL10
          ENDIF
.
          CALL      PBIROW00
.
          MATCH     "1",SORTFLAG
          IF        !@EQUAL
            ADD       ONE,DISNUMB
            COMPARE   NORECORD,DISNUMB
            GOTO      DISBIL10 IF NOT EQUAL
            GOTO      DISBIL99
          ELSE
            GOTO      DISBIL10
          ENDIF
.
