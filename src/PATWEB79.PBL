                             "#"",PMBBVISN,"#",":           *4
                             "#"",PMBBURNO,"#",":           *5
                             "#"",PMBBADDT,"#",":           *6
                             "#"",PMBBIFDT,"#",":           *7
                             "#"",PMBBITDT,"#",":           *8
                             "#"",PMBBAMNT,"#",":           *9
                             "#"",PMBBPDRG,"#",":           *10
                             "#"",PMBBCFLG,"#",":           *11
                             "#"",PMBBSTAT,"#",":           *12
                             "#"",PMBBPRNT,"#",":           *13
                             "#"",PMBBWARD,"#",":           *14
                             "#"",PMBBAMBS,"#",":           *15
                             "#"",PMBBCARE,"#",":           *16
                             "#"",CODCOMPL,"#",":           *17
                             "#"",PRINTED,"#",":            *18
                             "#"",CAREDESC,"#",";           *19
