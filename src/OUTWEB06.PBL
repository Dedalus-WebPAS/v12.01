                             ",#"",KEY3,"#"":                        *19
                             ",#"",DOCFNAME,"#"":                    *20
                             ",#"",ADIAG1,"#","                     *21
.
          IF        NOMRFLAG=1                                       *22  
            WRITE     HTMLFILE;"#"#",";                               
          ELSE
            WRITE     HTMLFILE;"#"<input type=checkbox name=mrkeplst",DRECCNT:
                      " id=mrkeplst",DRECCNT:
                      " value='",KEY40,"'checked onclick=RemoveTable(this);>":
                      "#",";
