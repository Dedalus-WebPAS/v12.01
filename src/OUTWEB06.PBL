          PACK      KEY20,WBSESIT,CLINICID,FRSTDATE,SP70
.
          MATCH     KEY20,KEY28
          GOTO      FILTB999 IF NOT EQUAL                      
.                                                                     Car 233039
          MATCH     SP70,FILTVITY
          IF        @EQUAL
            GOTO      FILTB115
          ENDIF
.
          MATCH     OBAVISIT,FILTVITY
          IF        !@EQUAL
            GOTO      FILTB100
          ENDIF
.
