            ENDIF
            MOVE      PTWDHOSN,MEDILOCH
            MOVE      PTWDLOCN,MEDILOCN     * Set MR Location
          ENDIF
.                                           * end I SRF 17693
          MATCH     SP70,MRMACLOC
          IF        !@EQUAL
            IF        EXCEPTNS<>1
              MATCH     MRMACHOS,MEDILOCH          * check to see is current loc
              IF        @EQUAL
                MATCH     MRMACLOC,MEDILOCN        * check to see is current loc
                GOTO      PRETB100 IF EQUAL
