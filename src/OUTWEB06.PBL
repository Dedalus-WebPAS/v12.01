            MATCH     SP70,FILTDOTY              * All document type filter
            IF        @EQUAL
              MATCH     "1",MRCNDISA             * If not displaying all volumes
              IF        !@EQUAL
                MATCH     SP70,SAVEDOTY
                IF        !@EQUAL
                  MATCH     SAVEDOTY,MRMADOTY    * Display the highest volume of
