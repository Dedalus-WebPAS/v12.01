          MOVE      ZERO,EXIT
          GOTO      KSIWA999
.
KSIWA950  MOVE      TWO,EXIT
.
KSIWA999  RETURN
.----------------------------------------------------------------------
. Check if warehouse ok for stocktake
.----------------------------------------------------------------------
CWARE000  PACK      KEY5,SIWAWAR,SP70            * lock warehouse
          REPEAT
            CALL      RLSIWA1
            IF        OVRCD=1
              CLEAR     DISPMSG
              APPEND    "Warehouse Missing - ",DISPMSG
              CALL      MESSAGE1
              GOTO      CWARE950
            ENDIF
            IF        OVRCD=2
              PACK      DISPMSG,DISPMSG,SP70
              CALL      MESSAGE2
              KEYIN     *P1:24,*EL,"Warehouse Locked - (":
                        *V2LON,"R",*HOFF,")etry or e(":
                        *V2LON,"X",*HOFF,")it ? ",*V2LON,ANS
              CALL      MESSAGE3
              PACK      ANS,ANS,SP70
              REP       UPPLOW,ANS
              MATCH     "X",ANS
              GOTO      CWARE950 IF EQUAL
              MATCH     "E",ANS
              GOTO      CWARE950 IF EQUAL
            ENDIF
          UNTIL     OVRCD=0
.
          CALL      OPSIWK1            * check if stocktake file exists
          IF        OVRCD=1
            CLOSE     SINWRKA1
            CLEAR     DISPMSG
            APPEND    "Stocktake Not in Progress for this Warehouse - ",DISPMSG
            CALL      MESSAGE1
            GOTO      CWARE950
          ENDIF
.
.
CWARE900  MOVE      ZERO,EXIT          * continue
          GOTO      CWARE999
.
CWARE950  MOVE      ONE,EXIT           * quit
          CALL      UUSIWA1
.
CWARE999  RETURN
.--------------------------------------------------------
. Read Current Page of Multi Items
.--------------------------------------------------------
DX02L000  MOVE     ZERO,SIWK1END
          PACK     KEY15,PRGID,SCRID,SP70
          CALL     RSSCPS1
.
DX02L010  CALL     RKSCPS1
          BRANCH   OVRCD,DX02L600
          MATCH    PRGID,SCPSPID
          GOTO     DX02L600 IF NOT EQUAL
          MATCH    SCRID,SCPSSID
          GOTO     DX02L600 IF NOT EQUAL
          MATCH    NAMSSFFI,SCPSMTY
          GOTO     DX02L010 IF NOT EQUAL
          MOVE     SCPSITM,FIELDNO
          BRANCH    FIELDNO,DX02L010,DX02L010,DX02L010,DX02L010:
                            DX02L101,DX02L010,DX02L010,DX02L010:
                           DX02L010,DX02L010,DX02L010,DX02L102:
                            DX02L103,DX02L104,DX02L010
          GOTO      DX02L010
.
DX02L101  PACK      SIWK1MKY,SP70,SP70
          CALL      UPTSIWK1
          IF        SIWK1END=0
            CALL      RKSIWK1
            IF        OVRCD=0
              PACK      SIWK1MKY,SIWKWAR,SIWKRACK,SIWKCAT
              MATCH     SIWK1EKY,SIWK1MKY
              IF        @EQUAL
                MOVE      ZERO,SIWK1END
                CALL      UPTSIWK1
          CALL      REDITM00
                MOVE      SIWKRACK,VAR
                CALL      DISPITEM
              ELSE
                MOVE      ONE,SIWK1END
              ENDIF
            ELSE
              MOVE      ONE,SIWK1END
            ENDIF
          ENDIF
          IF        SIWK1END=1
            PACK      VAR,SP70,SP70
            CALL      DISPITEM
          ENDIF
          GOTO      DX02L010
.
DX02L102  IF        SIWK1END=0
            MOVE      SIWKCAT,VAR
            CALL      DISPITEM
          ELSE
            PACK      VAR,SP70,SP70
            CALL      DISPITEM
          ENDIF
          GOTO      DX02L010
.
DX02L103  IF        SIWK1END=0
            MOVE      SIIADES,VAR
            CALL      DISPITEM
          ELSE
            PACK      VAR,SP70,SP70
            CALL      DISPITEM
          ENDIF
          GOTO      DX02L010
.
DX02L104  IF        SIWK1END=0
            MOVE      SIWKCNT,VAR
            CALL      DISPITEM
          ELSE
            PACK      VAR,SP70,SP70
            CALL      DISPITEM
          ENDIF
          CALL      KEYDAT00                *******  KEYIN DATA  *******
          IF        1=ABORTNOW
            PACK      DISPMSG,SP70
            CALL      MESSAGE2
            KEYIN     *P1:24,"Ok to Quit Insert Mode ? ",*V2LON,ANS;
            CALL      MESSAGE3
            PACK      ANS,ANS,SP70
            REP       UPPLOW,ANS
            MATCH     "Y",ANS
            IF        @EQUAL
              GOTO      DX02L999
            ELSE
              GOTO      DX02L104
            ENDIF
          ENDIF
          GOTO      DX02L010
.
DX02L600  MATCH    "SIWK1",NAMSSFFI
          GOTO     DX02L605 IF NOT EQUAL
          GOTO     DX02L999
.
DX02L605
.
DX02L999  RETURN
.----------------------------------------------------------------------
. Keyin Data
.----------------------------------------------------------------------
KEYDAT00  PACK      KEY20,SIWK1MKY,SP70
          CALL      RDSIWK1
          BRANCH    OVRCD,KEYDAT99     * note: record should be there !!
.
          MOVE      "   14",KEY5       * get screen position
          PACK      KEY20,PRGID,SCRID,KEY5,CVERT,SP70
          UNPACK    SP70,SCPSPID,SCPSSID,SCPSITM,SCPSROW,SCPSCOL
          CALL      RSSCPS2
          CALL      RKSCPS2
          PACK      KEY17,SCPSPID,SCPSSID,SCPSITM,SCPSROW,SP70
          MATCH     KEY20,KEY17
          IF        !@EQUAL
            MOVE      "10",SCPSLEN
            MOVE      "69",SCPSCOL
          ENDIF
          SFORMAT   VAR,SCPSLEN
          MOVE      SCPSCOL,CCOL
.
          MOVE      "99,999,999",VAR   * default to current amount
          MOVE      SIWKCNT,FORM10
          MOVE      FORM10,KEY10
          FEDIT     FORM10,VAR
          KEYIN     *PCCOL:CVERT,*V2LON,*RV,*JR,KEY10
          RESET     KEY10
          SCAN      EXITCHAR,KEY10
          IF        @EQUAL
            GOTO    KEYDAT95
          ENDIF
.
          TYPE      KEY10
          IF        !@EQUAL
            BEEP
            MOVE      "Invalid Quantity - ",DISPMSG
            CALL      MESSAGE1
            GOTO      KEYDAT00
          ENDIF
.
          SCAN      DOT,KEY10
          IF        @EQUAL
            BEEP
            MOVE      "Invalid Partial Quantity - ",DISPMSG
            CALL      MESSAGE1
            GOTO      KEYDAT00
          ENDIF
.
          MOVE      KEY10,FORM10
          IF        FORM10 < 0
            BEEP
            MOVE      "Amount cannot be negative - ",DISPMSG
            CALL      MESSAGE1
            GOTO      KEYDAT00
          ENDIF
.
          FEDIT     FORM10,VAR
          MOVE      FORM10,SIWKCNT
          CALL      UPSIWK1
.
KEYDAT90  MOVE      ZERO,ABORTNOW
          GOTO      KEYDAT99
.
KEYDAT95  MOVE      ONE,ABORTNOW
.
KEYDAT99
          SFORMAT   VAR,127
          RETURN
.----------------------------------------------------------------------
. Search for Catalog
.----------------------------------------------------------------------
SEARC000
          PACK      DISPMSG,DISPMSG,SP70
          CALL      MESSAGE2
          DISPLAY   *P1:24,"Enter Catalog Code : ";
.
          MOVE      "24",CVERT
          MOVE      "22",CCOL
          CALL      KSIIAA00           * keyin catalog code
          BRANCH    EXIT,SEARC999,SEARC999
.
          UNPACK    SP70,SIWKWAR,SIWKRACK,SIWKCAT
          PACK      KEY20,SIWAWAR,SIIACAT,SP70
          CALL      RDSIWK2
          IF        OVRCD=1
            CALL      RKSIWK2
          ENDIF
.
          PACK      KEY20,SIWKWAR,SIWKRACK,SIWKCAT,SP70
          CALL      RSSIWK1            * reposition before catalog code
          CALL      RPSIWK1
          CALL      NP02L000           * get next page
.
SEARC999
          CALL      MESSAGE3
          RETURN
.********************************************************************
. PRNA - print report for SIWK                      Called by ML
.         Returns  : EXIT            (1=illegal range entered)
.********************************************************************
PRNA0000
          DISPLAY   *P1:24,"Printing :   ";
          MOVE      ZERO,CPAGENO                 * reset page no.
          MOVE      ZERO,CNOUNDLN                * no underlines
          CLOCK     TIME,CTIMEIS                 * get current time
          MOVE      "999",CLNO                   * init last line number
          MOVE      ZERO,CONTQTY                 * clear item count
          MOVE      ZERO,FORM12D
          MOVE      ZERO,FORM12E
          MOVE      ZERO,FORM12F
          MOVE      ZERO,ATOTAL02
          MOVE      ZERO,ATOTAL03
          MOVE      ZERO,ATOTAL04
          MOVE      ONE,OPTIONA
.
          CALL      FILA0000                     * go to correct posit in file
.
PRNA1000  CALL      NREA0000                     * get next record
          BRANCH    OVRCD,PRNA8000               * end of file ?
.
          DISPLAY   *P12:24,*V2LON,SIWKCAT,*EL;
          ADD       ONE,CONTQTY                  * increment item count
.
          CALL      WRIA0000                     * print details
          GOTO      PRNA1000                     * get next record
.
PRNA8000  COMPARE   ZERO,CPAGENO
          CALL      HEAA0000 IF EQUAL            * print head if nothing printed
          CALL      PTOT0000                     * print totals
          PACK      KEY20,CONTQTY,SP70
          SQUEEZE   KEY20
          PRINT     "Note : '*' Indicates items still set for stocktake",*N:
                    *+,KEY20," Items Printed":
                    *N,*N:
                    "*** END OF REPORT ***"
.
.
PRNA9000  MOVE      ZERO,EXIT                    * exit screen
          GOTO      PRNA9999
.
PRNA9500  MOVE      ONE,EXIT                     * continue
.
PRNA9999  RETURN
.**********************************************************************
.  FILA - Set file pointer                             Called By PRNA
.**********************************************************************
FILA0000  BRANCH    OPTIONA,FILA0100,FILA9999,FILA9999,FILA9999:
                            FILA9999,FILA9999,FILA9999,FILA9999
.
.---- print by type 1 ----
.
FILA0100  PACK      KEY20,SIWAWAR,SP70
          CALL      RSSIWK1
          CALL      RPSIWK1
          GOTO      FILA9999
.
FILA9999  RETURN
.**********************************************************************
.  NREA - Get next record                              Called By PRNA
.**********************************************************************
NREA0000  BRANCH    OPTIONA,NREA0100,NREA9999,NREA9999,NREA9999:
                            NREA9999,NREA9999,NREA9999,NREA9999
.
.---- print by type 1 ----
.
NREA0100  CALL      RKSIWK1
          BRANCH    OVRCD,NREA9500
          MATCH     SIWAWAR,SIWKWAR
          GOTO      NREA9500 IF NOT EQUAL
.
          GOTO      NREA9000
.
NREA9000  MOVE      ZERO,OVRCD         * continue
          GOTO      NREA9999
.
NREA9500  MOVE      ONE,OVRCD          * quit
.
NREA9999  RETURN
.********************************************************************
. HEAA - print report                               Called by PRNA
.********************************************************************
HEAA0000  CALL      HEAD132                      * print heading
.
          ADD       "2",CLNO
          PRINT     *40,"Warehouse : ",SIWAWAR,SP2,SIWADES,*N
          CALL      UND132                       * print line
          PRINT     *3,"Catalog",*46,"Rack & Shelf",*61,"Counted Stock":
                    *78,"Difference     Comments",*N:
                    *46,"Issue Unit",*61,"Stock on Hand",*78,"     Value"
          ADD       "2",CLNO
          CALL      UND132                       * print line
.
HEAA9999  RETURN
.********************************************************************
. WRIA - print data line                            Called by PRNA
.********************************************************************
WRIA0000
          COMPARE   "54",CLNO                    * end of page ?
          CALL      HEAA0000 IF NOT LESS         * start new page
.
          UNPACK    SP70,SIIADES,SIIAISS
          PACK      KEY7,SIWKCAT,SP70
          CALL      RDSIIA1
.
          MOVE      ZERO,SIIESTI
          PACK      KEY12,SIWKCAT,SIWKWAR,SP70
          CALL      RDSIIE1
          IF        SIIESTI=1
            MOVE      "*",ANS
          ELSE
            MOVE      " ",ANS
          ENDIF
.
          PACK      KEY60,SIIADES,SP70
          CALL      BKLIN000           * break desc in half
          PACK      KEY33,KEY60,SP70
.
          MOVE      SIWKCNT,FORM12A
          MOVE      SIWKSOH,FORM12B
          ASSIGN    SIWKCNT-SIWKSOH,F12P2
          MOVE      F12P2,FORM12C
          ASSIGN    FORM12C*SIIAAVC,ATOTAL01
          ADD       FORM12C,FORM12D
          ADD       ATOTAL01,ATOTAL02
          IF        FORM12C>0
            ADD       FORM12C,FORM12E
            ADD       ATOTAL01,ATOTAL03
          ELSE
            SUB       FORM12C,FORM12F
            SUB       ATOTAL01,ATOTAL04
          ENDIF
.
          PRINT     ANS,SP1,SIWKCAT,SP2,KEY30,SP4,SIWKRACK,SP8,FORM12A:
                    SP2,FORM12C,*N:
                    SP2,SP9,KEY33,SP1,SIIAISS,SP1,FORM12B,SP2,ATOTAL01
.
          ADD       "2",CLNO           * add one to line number
          CALL      UND132
.
WRIA9999  RETURN
.
.********************************************************************
. PTOT - Print Totals
.********************************************************************
PTOT0000
          COMPARE   "54",CLNO                    * end of page ?
          CALL      HEAA0000 IF NOT LESS         * start new page
.
          PRINT     "Total Surplusses    : ",FORM12E:
                    "           Total Surplus Value     : ",ATOTAL03,*N:
                    "Total Deficits      : ",FORM12F:
                    "           Total Deficit Value     : ",ATOTAL04,*N:
                    "Total Discrepancies : ",FORM12D:
                    "           Total Discrepancy Value : ",ATOTAL02
          ADD       "3",CLNO           * add one to line number
          CALL      UND132
.
PTOT9999  RETURN
.********************************************************************
. BKLIN - Break up KEY60 into two neat halves        Called by lots
.         Requires - KEY60   (original line)
.         Returns  - KEY30   (line 1)
.                    KEY60   (line 2)
.********************************************************************
BKLIN000  RESET     KEY60,31           * reset to length(line1)+1
          CMATCH    SP70,KEY60
          WHILE     !@EQUAL&!@EOS
            BUMP      KEY60,-1
            IF        !@EOS
              CMATCH    SP70,KEY60
            ENDIF
          DO
.
          IF        @EOS
            SETLPTR   KEY60
            RESET     KEY60
            UNPACK    KEY60,KEY30,KEY60
          ELSE
            LENSET    KEY60
            RESET     KEY60
            MOVE      KEY60,KEY30
            PACK      KEY30,KEY30,SP70
.
            ENDSET    KEY60
            SETLPTR   KEY60
            BUMP      KEY60
            PACK      KEY60,KEY60,SP70
          ENDIF
.
BKLIN999  RETURN
.----------------------------------------------------------------------
. Set Warehouse
.----------------------------------------------------------------------
SETWAR00  MOVE      SP70,WARCODE
          RETURN
.----------------------------------------------------------------------
. Process Import File
.----------------------------------------------------------------------
IMPT0000  CALL      OPEN0000          * Open Import
          BRANCH    EXIT,IMPT9999
.
          READ      CONTROLF,SIXTY3;*233,SNCOCAT6                      *I-f0901
.
          CALL      VERFY000          * Verify Format
          BRANCH    EXIT,IMPT9999
.
IMPT1000  READ      IMPORTFL,SEQ;IMPCAT,IMPQTY
          GOTO      IMPT9000 IF OVER
.
          IF        IMPQTY=999999
            BEEP
            CLEAR     DISPMSG
            APPEND    "Catalog ",DISPMSG
            APPEND    IMPCAT,DISPMSG
            APPEND    " Skipped. Stock On Hand Not Recorded - ",DISPMSG
            RESET     DISPMSG
            CALL      MESSAGE1
            GOTO      IMPT1000
          ENDIF
.
. ----------------------------------------- * Start of Additional code *I-f0901
.                                           * Check SNCOCAT6 to determine if
.                                           * Catalogue No. to be right aligned
          IF        SNCOCAT6 = 1
            RJUSTIFY  IMPCAT                * Right align Cat.No
          ENDIF
. ----------------------------------------- * End of Additional code   *I-f0901
.
          PACK      KEY12,IMPCAT,SIWAWAR
          CALL      RDSIIE1
.
          PACK      KEY20,SIWAWAR,SIIERAS,IMPCAT
          CALL      RDSIWK1
.
          MOVE      IMPQTY,SIWKCNT
          CALL      UPSIWK1
          GOTO      IMPT1000
.
IMPT9000  BEEP
          CLOSE     IMPORTFL,DELETE
          MOVE      "Y",ANS
          KEYIN    *P1:24,*EL,"Import Complete. ":
                    "Print Discrepancy Report (Y/N) ? ",*V2LON,*RV,ANS
          REP       UPPLOW,ANS
          MATCH     ANSN,ANS
          GOTO      IMPT9999 IF EQUAL
          MATCH     ANSY,ANS
          GOTO      IMPT9000 IF NOT EQUAL
.
          CALL      PRNA0000           * Print Report
.
IMPT9999  RETURN
.------------------------------------------------------------
. Open Import File
.------------------------------------------------------------
OPEN0000  MOVE     ZERO,OVRCD
          TRAP     OVERCOND IF IO
          OPEN     IMPORTFL,"ibasin37"
          TRAPCLR  IO
          MOVE     ZERO,EXIT
          COMPARE  ZERO,OVRCD
          GOTO     OPEN9999 IF EQUAL
.
          MOVE     "ibasin37.us1 ibasin37.txt",KEY80
          MOVE     ZERO,HLEF
          CALL     GETSCR00
          EXECUTE  KEY80,TASKID
          CALL     PUTSCR00
          MOVE     ZERO,OVRCD
          TRAP     OVERCOND IF IO
          OPEN     IMPORTFL,"ibasin37"
          TRAPCLR  IO
          MOVE     ZERO,EXIT
          IF       OVRCD=1
            BEEP
            MOVE     "Can't Open Import File ibasin37.txt - ",DISPMSG
            CALL     MESSAGE1
            MOVE     ONE,EXIT
          ENDIF
OPEN9999  RETURN
.
VERFY000  MOVE      ZERO,SECTOR
          MOVE      ZERO,TOTALQTY
VERFY100  MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF FORMAT
          READ      IMPORTFL,SEQ;IMPCAT,IMPQTY
          GOTO      VERFY900 IF OVER
          TRAPCLR   FORMAT
          BRANCH    OVRCD,VERFY800
          ADD       ONE,SECTOR
          ADD       IMPQTY,TOTALQTY
.
. ----------------------------------------- * Start of Additional code *I-f0901
.                                           * Check SNCOCAT6 to determine if
.                                           * Catalogue No. to be right aligned
          IF        SNCOCAT6 = 1
            RJUSTIFY  IMPCAT                * Right align Cat.No
          ENDIF
. ----------------------------------------- * End of Additional code   *I-f0901
.
          PACK      KEY12,IMPCAT,SIWAWAR
          CALL      RDSIIE1
          BRANCH    OVRCD,VERFY810
.
          PACK      KEY20,SIWAWAR,SIIERAS,IMPCAT
          CALL      RDSIWK1
          BRANCH    OVRCD,VERFY820
.
          GOTO      VERFY100
.
VERFY800  BEEP
          CLEAR     DISPMSG
          APPEND    "File Format Error Record ",DISPMSG
          APPEND    SECTOR,DISPMSG
          APPEND    " - ",DISPMSG
          RESET     DISPMSG
          CALL      MESSAGE1
          MOVE      ONE,EXIT
          GOTO      VERFY999
.
VERFY810  BEEP
          CLEAR     DISPMSG
          APPEND    "Invalid Catalog/Warehouse Record ",DISPMSG
          APPEND    SECTOR,DISPMSG
          APPEND    " - ",DISPMSG
          RESET     DISPMSG
          CALL      MESSAGE1
          MOVE      ONE,EXIT
          GOTO      VERFY999
.
VERFY820  BEEP
          CLEAR     DISPMSG
          APPEND    "Catalog/Warehouse Not in Stocktake Record ",DISPMSG
          APPEND    SECTOR,DISPMSG
          APPEND    " - ",DISPMSG
          RESET     DISPMSG
          CALL      MESSAGE1
          MOVE      ONE,EXIT
          GOTO      VERFY999
.
VERFY900  OPEN      IMPORTFL,"ibasin37"
          MOVE      ZERO,EXIT
          MOVE      ANSY,ANS
          BEEP
          DISPLAY   *P1:19,*EL,"Import File Verify Complete":
                    *P1:20,*EL,"---------------------------":
                    *P1:21,*EL,"Records Counted : ",*V2LON,SECTOR,*HOFF:
                    *P1:22,*EL,"Total Quantity  : ",*V2LON,TOTALQTY
          KEYIN     *P1:24,*EL,"Continue (Y/N) ? ",*V2LON,*RV,ANS
          REP       UPPLOW,ANS
          MATCH     ANSY,ANS
          GOTO      VERFY999 IF EQUAL
          MATCH     ANSN,ANS
          GOTO      VERFY900 IF NOT EQUAL
          MOVE      ONE,EXIT
.
VERFY999  RETURN
.
