          CALL      RDSIIE1
          IF        SIIESTI=1
            PACK      SIWAWAR,SIIEWAR,SP70
            UNPACK    SP70,SIWKWAR,SIWKCAT
            MOVE      "zzzzzzzzzz",KEY10
            PACK      KEY20,SIIEWAR,SIIECAT,KEY10
            CALL      RSSIWK2
            CALL      RPSIWK2
            PACK      KEY12,SIWKWAR,SIWKCAT,SP70
            MATCH     KEY12,KEY20
            IF        !@EQUAL
              MOVE      ONE,OVRCD
            ENDIF
          ELSE
            MOVE      ONE,OVRCD
          ENDIF
          RETURN
