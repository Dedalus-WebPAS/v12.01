.----------------------------------------------------------------------
. Keyin Counted Amount
.----------------------------------------------------------------------
KCOUNT00  MOVE      "9999,999,999",KEY12   * default to current amount
          MOVE      SIWKCNT,WHOLESOH
          FEDIT     WHOLESOH,KEY12
          KEYIN     *PCCOL:CVERT,*V2LON,*RV,*JR,KEY12
          RESET     KEY12
          SCAN      EXITCHAR,KEY12
          IF        @EQUAL
            GOTO      KCOUNT95
          ENDIF
.
          TYPE      KEY12
          IF        !@EQUAL
            BEEP
            MOVE      "Invalid Quantity - ",DISPMSG
            CALL      MESSAGE1
            GOTO      KCOUNT00
          ENDIF
.
          SCAN      DOT,KEY12
          IF        @EQUAL
            BEEP
            MOVE      "Invalid Partial Quantity - ",DISPMSG
            CALL      MESSAGE1
            GOTO      KCOUNT00
          ENDIF
.
          MOVE      ZERO,WHOLESOH        * update amount
          MOVE      KEY12,WHOLESOH
.
          IF        WHOLESOH < 0
            BEEP
            MOVE      "Quantity cannot be negative - ",DISPMSG
            CALL      MESSAGE1
            GOTO      KCOUNT00
          ENDIF
.
          MOVE      WHOLESOH,SIWKCNT
.
