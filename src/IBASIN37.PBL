                            DP02L010
          GOTO      DP02L010
.
DP02L101
          PACK      SIWK1MKY,SP70,SP70
          CALL      UPTSIWK1
          IF        SIWK1END=0
            CALL      RKSIWK1 
            IF        OVRCD=0
              PACK      SIWK1MKY,SIWKWAR,SIWKRACK,SIWKCAT                                                       
              MATCH     SIWK1EKY,SIWK1MKY
              IF        @EQUAL
                MOVE      ZERO,SIWK1END
                ADD       ONE,SIWK1CNT
                CALL      UPTSIWK1
          CALL      REDITM00
                MOVE      SIWKRACK,VAR     
                CALL      DISPITEM
              ELSE
                MOVE      ONE,SIWK1END
              ENDIF
            ELSE
              MOVE      ONE,SIWK1END
            ENDIF
          ENDIF
          IF        SIWK1END=1
            PACK      VAR,SP70,SP70
            CALL      DISPITEM
          ENDIF
          GOTO      DP02L010
.
