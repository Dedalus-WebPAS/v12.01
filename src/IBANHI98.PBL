              GOTO      SFI1L113
            ENDIF
            BRANCH    EXIT,SFI1L113,SFI1L990
          ELSE
            COMPARE   TWO,EXIT
            GOTO      SFI1L990 IF EQUAL
          ENDIF
          MOVE      PTCNDOMF,VAR     
          CALL      DISPITEM
