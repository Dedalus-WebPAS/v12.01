          GOTO      VISTAB90 IF NOT EQUAL
          ADD       ONE,VISCOUNT
.
          CALL      GETVIS00
.
          WRITE     HTMLFILE;"<tr><td><a href='javascript:linkPatient(#"":
                                URNUMBER,"#",#"",PVIBILL,"#",#"":
                                PVITYPE,"#")'>":
                                PVIBILL,"</a></td>":
